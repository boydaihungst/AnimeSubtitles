Dialogue: 0,0:00:40.28,0:00:43.20,Default,,0,0,0,,CHÀO MỪNG ĐẾN VỚI\NPHỐ CASINO KING TRÁNG LỆ
Dialogue: 0,0:00:43.29,0:00:48.21,Default,,0,0,0,,{\an8}LOS ANGELES, MỸ
Dialogue: 0,0:00:52.84,0:00:53.84,Default,,0,0,0,,Là villain!
Dialogue: 0,0:00:53.92,0:00:54.97,Default,,0,0,0,,Chạy đi!
Dialogue: 0,0:01:00.51,0:01:02.68,Default,,0,0,0,,Ta sẽ lấy mười triệu đô la!
Dialogue: 0,0:01:02.77,0:01:04.64,Default,,0,0,0,,Chuyện này dễ như ăn kẹo!
Dialogue: 0,0:01:05.39,0:01:06.69,Default,,0,0,0,,- Đại ca ơi!\N- Cái gì?
Dialogue: 0,0:01:12.57,0:01:14.03,Default,,0,0,0,,Các ngươi xong đời rồi…
Dialogue: 0,0:01:15.61,0:01:16.61,Default,,0,0,0,,villain!
Dialogue: 0,0:01:18.49,0:01:19.91,Default,,0,0,0,,Chốt hạ đi, Cow Lady!
Dialogue: 0,0:01:22.99,0:01:24.29,Default,,0,0,0,,Im mồm đi!
Dialogue: 0,0:01:24.37,0:01:26.71,Default,,0,0,0,,Còn lâu!
Dialogue: 0,0:01:30.58,0:01:32.04,Default,,0,0,0,,SÒNG BẠC
Dialogue: 0,0:01:32.13,0:01:33.46,Default,,0,0,0,,Bố ơi!
Dialogue: 0,0:01:33.55,0:01:35.97,Default,,0,0,0,,Các ngươi đang cản đường đấy! Cút đi!
Dialogue: 0,0:01:49.31,0:01:50.77,Default,,0,0,0,,Không sao rồi.
Dialogue: 0,0:01:51.27,0:01:52.23,Default,,0,0,0,,Tại sao ư?
Dialogue: 0,0:01:55.28,0:01:57.45,Default,,0,0,0,,Vì đã có tôi ở đây!
Dialogue: 0,0:01:59.28,0:02:02.45,Default,,0,0,0,,Này, tên kia, ngươi là ai?
Dialogue: 0,0:02:03.16,0:02:07.12,Default,,0,0,0,,Ta chỉ là một anh hùng người Nhật\Ntình cờ đi ngang qua thôi.
Dialogue: 0,0:02:08.12,0:02:10.50,Default,,0,0,0,,Vậy ta sẽ tiễn ngươi về nhà!
Dialogue: 0,0:02:10.58,0:02:12.50,Default,,0,0,0,,Bằng quan tài luôn!
Dialogue: 0,0:02:19.55,0:02:22.09,Default,,0,0,0,,{\an8}Ai vừa bảo sẽ tiễn ta về ấy nhỉ…
Dialogue: 0,0:02:22.68,0:02:23.51,Default,,0,0,0,,Ủa?
Dialogue: 0,0:02:24.68,0:02:26.10,Default,,0,0,0,,Các ngươi tẩu thoát nhanh thật!
Dialogue: 0,0:02:27.64,0:02:29.85,Default,,0,0,0,,Cậu lố quá rồi đấy, Toshi!
Dialogue: 0,0:02:32.65,0:02:33.61,Default,,0,0,0,,Hắn chạy thoát rồi!
Dialogue: 0,0:02:33.69,0:02:34.65,Default,,0,0,0,,Đuổi theo hắn, Dave!
Dialogue: 0,0:02:34.73,0:02:37.78,Default,,0,0,0,,Cậu vẫn hành động\Ntrước khi kịp suy nghĩ như mọi khi.
Dialogue: 0,0:02:47.70,0:02:49.25,Default,,0,0,0,,MỤC TIÊU: VILLAIN TO LỚN
Dialogue: 0,0:02:52.96,0:02:54.96,Default,,0,0,0,,Đại ca, một chiếc xe\Nđang đuổi theo chúng ta!
Dialogue: 0,0:02:55.59,0:02:57.17,Default,,0,0,0,,Lũ anh hùng chết tiệt!
Dialogue: 0,0:03:05.76,0:03:07.06,Default,,0,0,0,,Cái quái gì thế?
Dialogue: 0,0:03:09.06,0:03:11.27,Default,,0,0,0,,Toshi, chúng ta trễ giờ lên lớp mất.
Dialogue: 0,0:03:11.35,0:03:12.48,Default,,0,0,0,,Giải quyết nhanh gọn đi!
Dialogue: 0,0:03:12.56,0:03:14.31,Default,,0,0,0,,Tớ cũng định thế đấy, Dave!
Dialogue: 0,0:03:17.57,0:03:19.90,Default,,0,0,0,,Chúng mày dai như đỉa vậy!
Dialogue: 0,0:03:19.99,0:03:22.07,Default,,0,0,0,,Detroit…
Dialogue: 0,0:03:22.15,0:03:24.41,Default,,0,0,0,,Smash!
Dialogue: 0,0:03:41.30,0:03:43.34,Default,,0,0,0,,{\i1}Mình sẽ kết thúc bằng chiêu tiếp theo.{\i0}
Dialogue: 0,0:03:44.47,0:03:47.22,Default,,0,0,0,,{\i1}Dave, chiêu này đặt tên theo\Ntiểu bang nơi cậu sinh ra đấy…{\i0}
Dialogue: 0,0:03:47.30,0:03:48.76,Default,,0,0,0,,Tiến lên đi!
Dialogue: 0,0:03:48.85,0:03:51.10,Default,,0,0,0,,California…
Dialogue: 0,0:03:52.73,0:03:56.23,Default,,0,0,0,,Smash!
Dialogue: 0,0:04:09.29,0:04:10.37,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:04:12.25,0:04:13.75,Default,,0,0,0,,Anh hùng đó là ai vậy?
Dialogue: 0,0:04:13.83,0:04:15.00,Default,,0,0,0,,Chưa gặp bao giờ.
Dialogue: 0,0:04:15.08,0:04:16.50,Default,,0,0,0,,Nhưng anh ấy lợi hại lắm!
Dialogue: 0,0:04:17.59,0:04:19.88,Default,,0,0,0,,Cậu ấy là học sinh trao đổi đến từ Nhật!
Dialogue: 0,0:04:20.67,0:04:22.84,Default,,0,0,0,,Tên anh hùng của cậu ấy là All Might.
Dialogue: 0,0:04:24.80,0:04:25.97,Default,,0,0,0,,- All Might…\N- All Might…
Dialogue: 0,0:04:27.14,0:04:28.35,Default,,0,0,0,,Một ngày nào đó…
Dialogue: 0,0:04:28.43,0:04:31.18,Default,,0,0,0,,Không, tôi dám chắc là sớm thôi,
Dialogue: 0,0:04:32.06,0:04:34.44,Default,,0,0,0,,cậu ấy sẽ trở thành Biểu tượng Hòa bình.
Dialogue: 0,0:04:38.69,0:04:39.77,Default,,0,0,0,,Cảm ơn nhé, Dave!
Dialogue: 0,0:04:40.48,0:04:44.03,Default,,0,0,0,,Chúng ta sẽ kịp thôi,\Nnhờ bộ đồ chiến đấu cậu làm đấy!
Dialogue: 0,0:04:44.11,0:04:47.11,Default,,0,0,0,,Nhanh lên! Nếu còn muộn nữa\Nthì chúng ta sẽ không đủ tín chỉ…
Dialogue: 0,0:04:47.20,0:04:50.41,Default,,0,0,0,,{\i1}Chuyến bay SFO UA-857\Nđã bị không tặc tấn công.{\i0}
Dialogue: 0,0:04:50.49,0:04:52.29,Default,,0,0,0,,{\i1}- Chuyến bay SFO UA-857…{\i0}\N- Dave!
Dialogue: 0,0:04:52.37,0:04:53.20,Default,,0,0,0,,Mạo hiểm lắm!
Dialogue: 0,0:04:58.42,0:05:00.63,Default,,0,0,0,,Cậu không thể nhắm mắt làm ngơ,\Nđúng không?
Dialogue: 0,0:05:02.76,0:05:04.13,Default,,0,0,0,,Đi thôi, Dave!
Dialogue: 0,0:05:04.22,0:05:05.09,Default,,0,0,0,,Được!
Dialogue: 0,0:05:05.80,0:05:09.47,Default,,0,0,0,,Nhưng mà làm anh hùng khó thật đấy!
Dialogue: 0,0:05:11.81,0:05:14.02,Default,,0,0,0,,{\i1}Hôm nay tớ sẽ khao cậu ăn trưa!{\i0}
Dialogue: 0,0:05:16.81,0:05:17.98,Default,,0,0,0,,{\i1}All Might.{\i0}
Dialogue: 0,0:05:18.06,0:05:21.11,Default,,0,0,0,,All Might!
Dialogue: 0,0:05:22.98,0:05:25.07,Default,,0,0,0,,Sao thế, nhóc Midoriya?
Dialogue: 0,0:05:25.15,0:05:26.11,Default,,0,0,0,,Nhìn thấy được rồi.
Dialogue: 0,0:05:26.20,0:05:27.36,Default,,0,0,0,,Nhìn kìa!
Dialogue: 0,0:05:28.36,0:05:30.78,Default,,0,0,0,,Thành phố di động nhân tạo,
Dialogue: 0,0:05:30.87,0:05:33.79,Default,,0,0,0,,nơi hơn 10.000 nhà khoa học đang ở,\NI-Island!
Dialogue: 0,0:05:34.70,0:05:38.62,Default,,0,0,0,,Không tin nổi là cháu có thể đến I-Island\Nvào đầu kỳ nghỉ hè!
Dialogue: 0,0:05:38.71,0:05:41.38,Default,,0,0,0,,Ta không ngờ là cháu lại hóng đến thế đấy!
Dialogue: 0,0:05:41.46,0:05:42.96,Default,,0,0,0,,Mừng là ta đã mời cháu đi.
Dialogue: 0,0:05:44.34,0:05:48.01,Default,,0,0,0,,Nhưng cháu đi theo chú có sao không ạ?
Dialogue: 0,0:05:48.09,0:05:50.01,Default,,0,0,0,,Cháu không phải rụt rè gì cả.
Dialogue: 0,0:05:50.09,0:05:53.51,Default,,0,0,0,,Thiệp mời ghi là\Nta có thể dẫn người theo mà.
Dialogue: 0,0:05:53.60,0:05:56.39,Default,,0,0,0,,Chẳng phải đó là ám chỉ người nhà…
Dialogue: 0,0:05:57.14,0:05:59.52,Default,,0,0,0,,Hai chúng ta được kết nối bằng một thứ
Dialogue: 0,0:05:59.60,0:06:01.73,Default,,0,0,0,,sâu sắc hơn cả huyết thống mà, phải không?
Dialogue: 0,0:06:02.65,0:06:04.98,Default,,0,0,0,,Mối liên kết của One For All.
Dialogue: 0,0:06:05.73,0:06:06.57,Default,,0,0,0,,Vâng!
Dialogue: 0,0:06:06.65,0:06:07.61,Default,,0,0,0,,- Ủa?\N- Ủa?
Dialogue: 0,0:06:07.70,0:06:11.91,Default,,0,0,0,,{\i1}Máy bay sắp hạ cánh xuống I-Island.{\i0}
Dialogue: 0,0:06:11.99,0:06:14.91,Default,,0,0,0,,Đến lúc mệt mỏi rồi đây.
Dialogue: 0,0:06:15.74,0:06:17.91,Default,,0,0,0,,Bởi vì khi đến đảo…
Dialogue: 0,0:06:19.58,0:06:22.13,Default,,0,0,0,,ta phải liên tục duy trì hình dạng cơ bắp.
Dialogue: 0,0:06:22.84,0:06:25.13,Default,,0,0,0,,Nào! Cháu cũng thay đồ đi, nhóc Midoriya.
Dialogue: 0,0:06:25.21,0:06:28.97,Default,,0,0,0,,Cháu đã xin phép trường\Nmang trang phục chiến đấu theo nhỉ?
Dialogue: 0,0:06:29.05,0:06:30.05,Default,,0,0,0,,{\i1}Vâng!{\i0}
Dialogue: 0,0:06:33.22,0:06:35.60,Default,,0,0,0,,{\i1}Thế giới này đã trở thành\Nmột xã hội siêu nhân{\i0}
Dialogue: 0,0:06:35.68,0:06:39.14,Default,,0,0,0,,{\i1}với khoảng 80 phần trăm dân số\Nsở hữu những khả năng đặc biệt.{\i0}
Dialogue: 0,0:06:40.02,0:06:42.35,Default,,0,0,0,,{\i1}Nghề làm anh hùng\Nchiến đấu với những villain{\i0}
Dialogue: 0,0:06:42.44,0:06:47.69,Default,,0,0,0,,{\i1}dùng Kosei làm việc xấu, cứu giúp\Nngười gặp nạn rất được xã hội hoan nghênh.{\i0}
Dialogue: 0,0:06:51.86,0:06:52.95,Default,,0,0,0,,{\i1}Không sao rồi.{\i0}
Dialogue: 0,0:06:53.66,0:06:54.53,Default,,0,0,0,,{\i1}Tại sao ư?{\i0}
Dialogue: 0,0:06:56.33,0:06:57.70,Default,,0,0,0,,{\i1}Vì đã có tôi ở đây!{\i0}
Dialogue: 0,0:06:58.37,0:07:00.37,Default,,0,0,0,,Chú ấy ngầu quá!
Dialogue: 0,0:07:00.46,0:07:04.21,Default,,0,0,0,,Khi mình có Kosei,\Nmình muốn được giống như chú ấy!
Dialogue: 0,0:07:05.50,0:07:06.92,Default,,0,0,0,,Cháu nên bỏ cuộc đi.
Dialogue: 0,0:07:07.59,0:07:10.01,Default,,0,0,0,,{\i1}Nhưng mà tôi, Midoriya Izuku,{\i0}
Dialogue: 0,0:07:10.09,0:07:12.47,Default,,0,0,0,,{\i1}sinh ra đã không có Kosei,{\i0}
Dialogue: 0,0:07:13.05,0:07:16.10,Default,,0,0,0,,{\i1}và suýt nữa tôi đã từ bỏ\Ngiấc mơ trở thành một anh hùng.{\i0}
Dialogue: 0,0:07:17.43,0:07:22.27,Default,,0,0,0,,{\i1}Cuộc đời tôi đã hoàn toàn thay đổi\Nvào cái ngày định mệnh{\i0}
Dialogue: 0,0:07:22.35,0:07:24.56,Default,,0,0,0,,{\i1}tôi gặp anh hùng hạng nhất, All Might.{\i0}
Dialogue: 0,0:07:26.07,0:07:28.90,Default,,0,0,0,,Cháu có thể thành anh hùng\Ndù không có Kosei không?
Dialogue: 0,0:07:29.74,0:07:31.90,Default,,0,0,0,,Một kẻ không có Kosei có thể
Dialogue: 0,0:07:32.66,0:07:34.49,Default,,0,0,0,,trở thành người như chú được không?
Dialogue: 0,0:07:35.24,0:07:38.37,Default,,0,0,0,,{\i1}Các anh hùng hàng đầu đều có\Nnhững câu chuyện nổi danh từ thời đi học.{\i0}
Dialogue: 0,0:07:39.04,0:07:41.91,Default,,0,0,0,,{\i1}Hầu hết bọn họ đều có một điểm chung:{\i0}
Dialogue: 0,0:07:42.62,0:07:46.54,Default,,0,0,0,,{\i1}"Cơ thể tôi phản ứng\Ntrước khi kịp suy nghĩ".{\i0}
Dialogue: 0,0:07:46.63,0:07:47.54,Default,,0,0,0,,Kacchan!
Dialogue: 0,0:07:47.63,0:07:49.88,Default,,0,0,0,,Sao lại là mày?
Dialogue: 0,0:07:51.63,0:07:55.39,Default,,0,0,0,,Trông cậu như thể đang cầu cứu.
Dialogue: 0,0:07:57.10,0:08:00.02,Default,,0,0,0,,Cháu có thể trở thành anh Hùng.
Dialogue: 0,0:08:01.27,0:08:04.31,Default,,0,0,0,,{\i1}Nhóc à, ta sẽ kể cho cháu nghe sự thật.{\i0}
Dialogue: 0,0:08:06.15,0:08:10.23,Default,,0,0,0,,Kosei của ta được truyền cho ta\Ntựa như một ngọn đuốc thiêng.
Dialogue: 0,0:08:11.15,0:08:12.99,Default,,0,0,0,,Một sức mạnh truyền lại sức mạnh.
Dialogue: 0,0:08:13.49,0:08:16.07,Default,,0,0,0,,Đó chính là Kosei mà ta được thừa hưởng!
Dialogue: 0,0:08:17.87,0:08:19.33,Default,,0,0,0,,Được vinh danh bằng cái tên
Dialogue: 0,0:08:19.95,0:08:21.29,Default,,0,0,0,,"One For All".
Dialogue: 0,0:08:22.12,0:08:25.17,Default,,0,0,0,,One For All…
Dialogue: 0,0:08:25.25,0:08:27.58,Default,,0,0,0,,{\i1}Một người có được sức mạnh{\i0}
Dialogue: 0,0:08:27.67,0:08:30.42,Default,,0,0,0,,{\i1}và truyền lại cho người khác,{\i0}
Dialogue: 0,0:08:30.50,0:08:32.13,Default,,0,0,0,,{\i1}người đó lại truyền cho người khác.{\i0}
Dialogue: 0,0:08:33.51,0:08:38.60,Default,,0,0,0,,Sức mạnh này là\Nthành quả của những lời kêu cứu
Dialogue: 0,0:08:38.68,0:08:40.18,Default,,0,0,0,,và những trái tim quả cảm.
Dialogue: 0,0:08:40.93,0:08:42.06,Default,,0,0,0,,{\i1}Phải.{\i0}
Dialogue: 0,0:08:42.14,0:08:45.56,Default,,0,0,0,,{\i1}Và tiếp theo sẽ đến lượt cháu.{\i0}
Dialogue: 0,0:08:46.27,0:08:50.61,Default,,0,0,0,,{\i1}Sau khi nhận được\NKosei One For All của All Might,{\i0}
Dialogue: 0,0:08:50.69,0:08:53.03,Default,,0,0,0,,{\i1}tôi đã vào học Khoa Anh hùng\Ncủa Trung học UA,{\i0}
Dialogue: 0,0:08:53.11,0:08:55.45,Default,,0,0,0,,{\i1}và bắt đầu lao mình về phía ước mơ.{\i0}
Dialogue: 0,0:08:56.45,0:08:58.20,Default,,0,0,0,,{\i1}Tôi sẽ vượt qua mọi khó khăn{\i0}
Dialogue: 0,0:08:58.70,0:09:00.58,Default,,0,0,0,,{\i1}và cứu người với một nụ cười trên môi,{\i0}
Dialogue: 0,0:09:01.41,0:09:03.50,Default,,0,0,0,,{\i1}để trở thành anh hùng vĩ đại nhất!{\i0}
Dialogue: 0,0:09:08.33,0:09:09.63,Default,,0,0,0,,{\an8}I-ISLAND, SÂN BAY
Dialogue: 0,0:09:09.71,0:09:12.09,Default,,0,0,0,,{\an8\i1}Chúng tôi sẽ bắt đầu kiểm tra nhập cảnh.{\i0}
Dialogue: 0,0:09:14.13,0:09:16.43,Default,,0,0,0,,Một câu hỏi cho cháu đây, nhóc Midoriya.
Dialogue: 0,0:09:16.51,0:09:19.14,Default,,0,0,0,,Hòn đảo nhân tạo này\Nđược tạo ra để làm gì?
Dialogue: 0,0:09:19.22,0:09:20.43,Default,,0,0,0,,Để xem nào…
Dialogue: 0,0:09:20.51,0:09:23.18,Default,,0,0,0,,Để tạo ra một nơi\Ncác nhân tài trên thế giới có thể tụ hội,
Dialogue: 0,0:09:23.27,0:09:25.93,Default,,0,0,0,,nghiên cứu Kosei\Nvà phát triển các vật phẩm chiến đấu!
Dialogue: 0,0:09:26.56,0:09:28.10,Default,,0,0,0,,{\i1}Hòn đảo này có khả năng di động{\i0}
Dialogue: 0,0:09:28.19,0:09:31.23,Default,,0,0,0,,{\i1}là để bảo vệ các nhà khoa học\Nvà nghiên cứu của họ khỏi bọn villain.{\i0}
Dialogue: 0,0:09:31.32,0:09:34.40,Default,,0,0,0,,{\i1}Hệ thống an ninh đó\Ntương đương với nhà tù Tartarus,{\i0}
Dialogue: 0,0:09:34.49,0:09:36.70,Default,,0,0,0,,{\i1}và chưa có tên villain nào\Nphạm tội ở đây cả.{\i0}
Dialogue: 0,0:09:36.78,0:09:39.70,Default,,0,0,0,,Cháu biết nhiều về mấy thứ này thật đấy!
Dialogue: 0,0:09:39.78,0:09:41.16,Default,,0,0,0,,YAGI TOSHINORI\NMIDORIYA IZUKU
Dialogue: 0,0:09:41.24,0:09:42.95,Default,,0,0,0,,{\i1}Kiểm tra nhập cảnh đã hoàn tất.{\i0}
Dialogue: 0,0:09:43.66,0:09:46.50,Default,,0,0,0,,{\i1}Hiện tại, trên I-Island\Nđang diễn ra tiền khai mạc I-Expo,{\i0}
Dialogue: 0,0:09:46.58,0:09:51.17,Default,,0,0,0,,{\i1}một triển lãm trưng bày các kết quả\Ncủa nhiều nghiên cứu và phát triển.{\i0}
Dialogue: 0,0:09:51.25,0:09:54.38,Default,,0,0,0,,{\i1}Nếu bạn nhận được thiệp mời,\Nxin mời đến và xem triển lãm.{\i0}
Dialogue: 0,0:09:55.51,0:09:56.92,Default,,0,0,0,,Ồ!
Dialogue: 0,0:10:02.81,0:10:04.60,Default,,0,0,0,,Không ngờ lại có thật nhiều người
Dialogue: 0,0:10:04.68,0:10:07.89,Default,,0,0,0,,dù buổi triển lãm\Nvẫn chưa chính thức mở cửa.
Dialogue: 0,0:10:07.98,0:10:10.90,Default,,0,0,0,,{\an8}Được thấy tận mắt mới biết\Nnơi này tuyệt vời thế nào!
Dialogue: 0,0:10:10.98,0:10:15.53,Default,,0,0,0,,Khác với Nhật Bản, mọi người\Nđược tự do sử dụng Kosei trên I-Island.
Dialogue: 0,0:10:16.28,0:10:19.07,Default,,0,0,0,,Ta nghe nói có rất nhiều\Nđiểm tham quan ứng dụng Kosei.
Dialogue: 0,0:10:19.82,0:10:20.99,Default,,0,0,0,,Cháu nên ghé chơi thử.
Dialogue: 0,0:10:21.07,0:10:22.07,Default,,0,0,0,,Vâng ạ!
Dialogue: 0,0:10:22.16,0:10:24.74,Default,,0,0,0,,Giờ thì khách sạn của chúng ta ở…
Dialogue: 0,0:10:24.83,0:10:26.58,Default,,0,0,0,,Chào mừng đến với I-Expo!
Dialogue: 0,0:10:26.66,0:10:29.71,Default,,0,0,0,,Ơ kìa, anh là All Might à?
Dialogue: 0,0:10:29.79,0:10:30.75,Default,,0,0,0,,All Might?
Dialogue: 0,0:10:30.83,0:10:32.38,Default,,0,0,0,,Anh hùng hạng nhất?
Dialogue: 0,0:10:32.46,0:10:35.09,Default,,0,0,0,,- Là anh ấy thật!\N- Ôi!
Dialogue: 0,0:10:35.17,0:10:39.63,Default,,0,0,0,,- Xin hãy nói vài lời.\N- Cảm ơn mọi người đã chào đón nồng nhiệt!
Dialogue: 0,0:10:39.72,0:10:41.64,Default,,0,0,0,,{\i1}Xếp hàng để xin chữ ký nhé!{\i0}
Dialogue: 0,0:10:45.64,0:10:48.35,Default,,0,0,0,,Ta không ngờ là sẽ bị ngăn lại lâu thế.
Dialogue: 0,0:10:48.93,0:10:51.73,Default,,0,0,0,,Chúng ta sắp trễ hẹn mất rồi!
Dialogue: 0,0:10:51.81,0:10:52.73,Default,,0,0,0,,Hẹn?
Dialogue: 0,0:10:52.81,0:10:57.78,Default,,0,0,0,,Phải. Ta định họp mặt\Nvới một người bạn lâu năm.
Dialogue: 0,0:10:57.86,0:11:00.15,Default,,0,0,0,,Xin lỗi nhé, cháu đi với ta được không?
Dialogue: 0,0:11:00.24,0:11:02.78,Default,,0,0,0,,Bạn của All Might…
Dialogue: 0,0:11:02.87,0:11:04.16,Default,,0,0,0,,Tất nhiên là cháu sẽ đi rồi!
Dialogue: 0,0:11:04.24,0:11:09.37,Default,,0,0,0,,Ta vẫn chưa nói với cậu ấy về One For All\Ncũng như việc truyền Kosei lại cho cháu,
Dialogue: 0,0:11:09.45,0:11:10.71,Default,,0,0,0,,cháu nhớ kỹ nhé.
Dialogue: 0,0:11:10.79,0:11:13.13,Default,,0,0,0,,Chú giữ bí mật với cả bạn ạ?
Dialogue: 0,0:11:13.21,0:11:17.88,Default,,0,0,0,,Bởi vì những người biết được\Nbí mật của One For All sẽ gặp nguy hiểm.
Dialogue: 0,0:11:17.96,0:11:19.01,Default,,0,0,0,,Ra là thế.
Dialogue: 0,0:11:20.01,0:11:21.05,Default,,0,0,0,,Chú nói đúng.
Dialogue: 0,0:11:21.88,0:11:25.10,Default,,0,0,0,,{\i1}One For All là một bí mật rất quan trọng.{\i0}
Dialogue: 0,0:11:26.22,0:11:30.68,Default,,0,0,0,,Nhóc Midoriya,\Nngười kế thừa One For All từ ta
Dialogue: 0,0:11:31.18,0:11:36.27,Default,,0,0,0,,phải gánh lấy số phận đối mặt với tà ác\Nvà cả All For One trong tương lai.
Dialogue: 0,0:11:37.07,0:11:38.48,Default,,0,0,0,,{\i1}Đúng vậy.{\i0}
Dialogue: 0,0:11:38.57,0:11:40.32,Default,,0,0,0,,{\i1}Để chuẩn bị cho điều đó,{\i0}
Dialogue: 0,0:11:40.40,0:11:44.78,Default,,0,0,0,,{\i1}mình phải học được cách thi triển\NOne For All chính xác càng sớm càng tốt.{\i0}
Dialogue: 0,0:11:48.29,0:11:49.20,Default,,0,0,0,,- Ủa?\N- Ủa?
Dialogue: 0,0:11:49.70,0:11:50.54,Default,,0,0,0,,A!
Dialogue: 0,0:11:50.62,0:11:52.58,Default,,0,0,0,,Chú ơi!
Dialogue: 0,0:11:54.04,0:11:55.79,Default,,0,0,0,,Chú Might!
Dialogue: 0,0:11:55.88,0:11:58.21,Default,,0,0,0,,Ồ, Melissa!
Dialogue: 0,0:11:58.30,0:11:59.46,Default,,0,0,0,,Đã lâu không gặp chú!
Dialogue: 0,0:11:59.55,0:12:01.47,Default,,0,0,0,,Cháu rất vui vì chú đã đến!
Dialogue: 0,0:12:01.55,0:12:04.38,Default,,0,0,0,,Cảm ơn cháu đã mời chú!
Dialogue: 0,0:12:04.47,0:12:06.39,Default,,0,0,0,,Nhưng suýt nữa chú không nhận ra cháu đấy!
Dialogue: 0,0:12:06.47,0:12:08.14,Default,,0,0,0,,Cháu lớn thật rồi!
Dialogue: 0,0:12:08.22,0:12:10.31,Default,,0,0,0,,Cháu 17 tuổi rồi mà.
Dialogue: 0,0:12:10.39,0:12:12.14,Default,,0,0,0,,Cháu nặng hơn trước rồi đúng không?
Dialogue: 0,0:12:12.23,0:12:14.06,Default,,0,0,0,,Không hề!
Dialogue: 0,0:12:16.40,0:12:19.57,Default,,0,0,0,,Cháu mừng là chú vẫn mạnh khỏe, chú Might.
Dialogue: 0,0:12:19.65,0:12:23.24,Default,,0,0,0,,{\i1}- Chị ấy là bạn cũ của All Might à?{\i0}\N- Cháu xin lỗi vì đến muộn.
Dialogue: 0,0:12:23.32,0:12:25.07,Default,,0,0,0,,- Cháu quên mất giờ giấc.\N{\i1}- Trông chị ấy đâu lớn tuổi đến thế.{\i0}
Dialogue: 0,0:12:25.16,0:12:26.53,Default,,0,0,0,,{\i1}- Là do Kosei à?{\i0}\N- Không sao.
Dialogue: 0,0:12:26.62,0:12:28.99,Default,,0,0,0,,À, phải rồi. Dave đâu rồi?
Dialogue: 0,0:12:30.12,0:12:31.37,Default,,0,0,0,,Ở phòng thí nghiệm ạ.
Dialogue: 0,0:12:31.45,0:12:34.79,Default,,0,0,0,,Có vẻ như cuối cùng bố cũng đã\Nhoàn thành bước đầu của nghiên cứu,
Dialogue: 0,0:12:34.87,0:12:36.88,Default,,0,0,0,,thế nên cháu mời chú đến đảo
Dialogue: 0,0:12:36.96,0:12:39.54,Default,,0,0,0,,để gây bất ngờ chúc mừng việc đó.
Dialogue: 0,0:12:39.63,0:12:41.30,Default,,0,0,0,,Thì ra là kế hoạch của cháu.
Dialogue: 0,0:12:41.92,0:12:44.38,Default,,0,0,0,,À mà lần này Dave nghiên cứu gì thế?
Dialogue: 0,0:12:44.47,0:12:46.51,Default,,0,0,0,,Bố bảo là tuyệt mật,
Dialogue: 0,0:12:46.59,0:12:48.18,Default,,0,0,0,,bố còn chẳng chịu nói cho cháu.
Dialogue: 0,0:12:48.26,0:12:50.39,Default,,0,0,0,,Mấy nhà khoa học\Nxem trọng việc đó thật nhỉ?
Dialogue: 0,0:12:50.47,0:12:52.27,Default,,0,0,0,,À, nhóc Midoriya.
Dialogue: 0,0:12:52.77,0:12:55.48,Default,,0,0,0,,Đây là con gái của bạn tốt của ta.
Dialogue: 0,0:12:55.56,0:12:56.90,Default,,0,0,0,,Chị là Melissa Shield.
Dialogue: 0,0:12:57.40,0:12:58.36,Default,,0,0,0,,Rất vui được gặp em!
Dialogue: 0,0:12:59.44,0:13:00.77,Default,,0,0,0,,Thì ra là thế…
Dialogue: 0,0:13:00.86,0:13:01.86,Default,,0,0,0,,- Hả?\N- Gì vậy?
Dialogue: 0,0:13:01.94,0:13:03.36,Default,,0,0,0,,À, không có gì!
Dialogue: 0,0:13:04.82,0:13:06.24,Default,,0,0,0,,Rất vui được gặp chị!
Dialogue: 0,0:13:06.95,0:13:10.49,Default,,0,0,0,,Em là Midoriya Izuku, năm nhất\NKhoa Anh hùng của trường Trung học UA.
Dialogue: 0,0:13:11.28,0:13:14.66,Default,,0,0,0,,Trường Trung học UA?\NVậy em và chú Might là…
Dialogue: 0,0:13:14.75,0:13:17.87,Default,,0,0,0,,- Vâng, em là học trò của chú ấy!\N- Một anh hùng tương lai đấy!
Dialogue: 0,0:13:18.50,0:13:21.50,Default,,0,0,0,,Ồ, không thể tin được\Nem lại là học trò của chú Might!
Dialogue: 0,0:13:21.59,0:13:23.51,Default,,0,0,0,,Chắc chắn tương lai của em sẽ xán lạn lắm!
Dialogue: 0,0:13:23.59,0:13:27.01,Default,,0,0,0,,Không, em vẫn đang rèn luyện. À, ý em là…
Dialogue: 0,0:13:27.09,0:13:28.76,Default,,0,0,0,,Vậy Kosei của em là gì?
Dialogue: 0,0:13:28.84,0:13:30.43,Default,,0,0,0,,Là Kosei hệ sức mạnh!
Dialogue: 0,0:13:31.14,0:13:34.52,Default,,0,0,0,,{\an8}Trang phục của em tuyệt lắm,\Nnhưng thiết kế lại khá chính thống.
Dialogue: 0,0:13:34.60,0:13:37.31,Default,,0,0,0,,{\an8}Cũng không có vật phẩm hỗ trợ nào.
Dialogue: 0,0:13:37.39,0:13:39.15,Default,,0,0,0,,{\i1}Chị ấy đứng gần quá!{\i0}
Dialogue: 0,0:13:41.90,0:13:44.86,Default,,0,0,0,,Có lẽ em nên cải tiến trang phục.
Dialogue: 0,0:13:46.36,0:13:47.45,Default,,0,0,0,,Melissa, đến lúc rồi.
Dialogue: 0,0:13:47.53,0:13:50.49,Default,,0,0,0,,Ôi, xin lỗi ạ! Cháu nhập tâm quá.
Dialogue: 0,0:13:51.74,0:13:54.49,Default,,0,0,0,,Chúng ta mau chóng\Nmang niềm vui đến cho bố nào!
Dialogue: 0,0:13:55.62,0:13:58.04,Default,,0,0,0,,Lối này ạ, chú Might!
Dialogue: 0,0:14:06.55,0:14:09.13,Default,,0,0,0,,Chúng tôi có thể trà trộn vào triển lãm\Nmột cách suôn sẻ.
Dialogue: 0,0:14:09.22,0:14:11.47,Default,,0,0,0,,Vậy khi nào hàng sẽ đến?
Dialogue: 0,0:14:11.55,0:14:14.77,Default,,0,0,0,,{\i1}Đến lấy hàng lúc 3:00 chiều ở Cổng 66.{\i0}
Dialogue: 0,0:14:14.85,0:14:16.06,Default,,0,0,0,,Tôi hiểu rồi.
Dialogue: 0,0:14:27.94,0:14:29.07,Default,,0,0,0,,Tiến sĩ.
Dialogue: 0,0:14:29.91,0:14:31.03,Default,,0,0,0,,Tiến sĩ David.
Dialogue: 0,0:14:31.11,0:14:31.95,Default,,0,0,0,,Sao?
Dialogue: 0,0:14:32.57,0:14:34.66,Default,,0,0,0,,Chỗ này tôi cũng dọn xong rồi.
Dialogue: 0,0:14:35.54,0:14:37.58,Default,,0,0,0,,Thế à? Cảm ơn nhé, Sam.
Dialogue: 0,0:14:39.16,0:14:42.33,Default,,0,0,0,,Sao anh không đi ăn trưa\Nvới con gái như mọi khi?
Dialogue: 0,0:14:42.42,0:14:44.54,Default,,0,0,0,,Hôm nay con bé vẫn phải đi học.
Dialogue: 0,0:14:44.63,0:14:46.80,Default,,0,0,0,,Tôi tưởng trường cho nghỉ học\Nvì I-Expo chứ?
Dialogue: 0,0:14:47.30,0:14:49.42,Default,,0,0,0,,Con bé đang nghiên cứu độc lập.
Dialogue: 0,0:14:49.51,0:14:51.97,Default,,0,0,0,,Vì cháu là con gái của bố.
Dialogue: 0,0:14:52.05,0:14:53.05,Default,,0,0,0,,Cháu giống bố mà.
Dialogue: 0,0:14:53.14,0:14:54.60,Default,,0,0,0,,Melissa.
Dialogue: 0,0:14:54.68,0:14:56.39,Default,,0,0,0,,Chào cô Melissa.
Dialogue: 0,0:14:56.47,0:14:57.93,Default,,0,0,0,,Chào chú Sam.
Dialogue: 0,0:14:58.52,0:15:00.35,Default,,0,0,0,,Cảm ơn chú đã luôn chăm sóc cho ông bố
Dialogue: 0,0:15:00.44,0:15:02.31,Default,,0,0,0,,chỉ biết lao đầu vào nghiên cứu của cháu.
Dialogue: 0,0:15:02.85,0:15:04.27,Default,,0,0,0,,Bố hết đường chối cãi thật.
Dialogue: 0,0:15:04.86,0:15:06.65,Default,,0,0,0,,Quan trọng hơn là con làm gì ở đây vậy?
Dialogue: 0,0:15:07.15,0:15:10.57,Default,,0,0,0,,Để chúc mừng bố\Nhoàn thành bước đầu của nghiên cứu,
Dialogue: 0,0:15:10.65,0:15:12.41,Default,,0,0,0,,con đã mời một người.
Dialogue: 0,0:15:12.49,0:15:13.62,Default,,0,0,0,,Một người?
Dialogue: 0,0:15:14.12,0:15:16.12,Default,,0,0,0,,Một người bố rất yêu quý!
Dialogue: 0,0:15:17.16,0:15:18.79,Default,,0,0,0,,Tớ ở đây,
Dialogue: 0,0:15:18.87,0:15:22.37,Default,,0,0,0,,run rẩy trong cảm xúc trào dâng\Ncủa cuộc hội ngộ giữa hai ta!
Dialogue: 0,0:15:23.04,0:15:23.92,Default,,0,0,0,,Toshi…
Dialogue: 0,0:15:24.84,0:15:25.71,Default,,0,0,0,,All Might?
Dialogue: 0,0:15:27.00,0:15:28.42,Default,,0,0,0,,Hàng thật giá thật?
Dialogue: 0,0:15:30.47,0:15:33.30,Default,,0,0,0,,Tớ đến tận đây để gặp cậu đấy, Dave!
Dialogue: 0,0:15:34.09,0:15:35.51,Default,,0,0,0,,Sao? Bố có ngạc nhiên không?
Dialogue: 0,0:15:36.56,0:15:38.27,Default,,0,0,0,,Ừ…
Dialogue: 0,0:15:38.97,0:15:40.64,Default,,0,0,0,,{\an8}Bố ngạc nhiên lắm…
Dialogue: 0,0:15:41.48,0:15:44.06,Default,,0,0,0,,Hai ta phải cảm ơn Melissa đấy!
Dialogue: 0,0:15:44.15,0:15:47.44,Default,,0,0,0,,- Trời ạ, bao lâu rồi nhỉ?\N- Đừng gợi nhắc đến số tuổi của tớ chứ.
Dialogue: 0,0:15:47.52,0:15:50.03,Default,,0,0,0,,Cả hai ta đều không muốn\Nnghĩ đến điều đó mà nhỉ?
Dialogue: 0,0:15:51.65,0:15:52.61,Default,,0,0,0,,Phải, đúng vậy!
Dialogue: 0,0:15:54.32,0:15:56.49,Default,,0,0,0,,{\an8}Tớ rất vui khi được gặp cậu, Dave.
Dialogue: 0,0:15:57.74,0:15:59.70,Default,,0,0,0,,{\an8}Tớ cũng rất vui\Nkhi được gặp cậu, All Might.
Dialogue: 0,0:16:02.41,0:16:05.00,Default,,0,0,0,,Nhóc Midoriya, để ta giới thiệu với cháu,
Dialogue: 0,0:16:05.08,0:16:07.25,Default,,0,0,0,,bạn thân của chú, David Shield…
Dialogue: 0,0:16:07.34,0:16:08.34,Default,,0,0,0,,Cháu biết chú ấy!
Dialogue: 0,0:16:08.42,0:16:09.96,Default,,0,0,0,,Tiến sĩ David Shield!
Dialogue: 0,0:16:10.05,0:16:13.01,Default,,0,0,0,,{\i1}Nhà khoa học hàng đầu nghiên cứu Kosei\Nđược trao Giải Nobel Kosei!{\i0}
Dialogue: 0,0:16:13.09,0:16:15.72,Default,,0,0,0,,{\i1}Chú ấy là cộng sự của All Might\Nkhi chú còn sống ở Mỹ,{\i0}
Dialogue: 0,0:16:15.80,0:16:17.68,Default,,0,0,0,,và trang phục chiến đấu của All Might…
Dialogue: 0,0:16:18.35,0:16:19.43,Default,,0,0,0,,{\i1}"Thời Trẻ",{\i0}
Dialogue: 0,0:16:19.51,0:16:21.18,Default,,0,0,0,,{\i1}"Thời đại Đồng",{\i0}
Dialogue: 0,0:16:21.27,0:16:23.06,Default,,0,0,0,,{\i1}"Thời đại Bạc",{\i0}
Dialogue: 0,0:16:23.73,0:16:26.73,Default,,0,0,0,,{\i1}và "Thời đại Hoàng kim!"{\i0}
Dialogue: 0,0:16:26.81,0:16:29.44,Default,,0,0,0,,Chú ấy là nhà phát minh thiên tài\Nđã chế tạo ra chúng!
Dialogue: 0,0:16:29.52,0:16:31.82,Default,,0,0,0,,Cháu chưa từng nghĩ\Nmình có thể gặp được chú ấy!
Dialogue: 0,0:16:31.90,0:16:33.24,Default,,0,0,0,,Cháu cảm động lắm!
Dialogue: 0,0:16:36.45,0:16:38.91,Default,,0,0,0,,Xem ra chú không cần tự giới thiệu rồi.
Dialogue: 0,0:16:38.99,0:16:40.54,Default,,0,0,0,,Cháu xin lỗi! Chỉ là…
Dialogue: 0,0:16:40.62,0:16:42.04,Default,,0,0,0,,Không, không sao đâu.
Dialogue: 0,0:16:46.37,0:16:48.63,Default,,0,0,0,,Đã lâu rồi chú mới gặp All Might.
Dialogue: 0,0:16:49.21,0:16:51.67,Default,,0,0,0,,Xin lỗi, nhưng bọn chú\Nnói chuyện riêng một chút nhé?
Dialogue: 0,0:16:51.76,0:16:53.34,Default,,0,0,0,,À, vâng ạ.
Dialogue: 0,0:16:53.42,0:16:56.93,Default,,0,0,0,,Melissa, dẫn Midoriya\Nđi tham quan I-Expo đi con.
Dialogue: 0,0:16:57.64,0:16:59.18,Default,,0,0,0,,Vâng, thưa bố.
Dialogue: 0,0:16:59.26,0:17:00.31,Default,,0,0,0,,Được ạ?
Dialogue: 0,0:17:00.89,0:17:03.68,Default,,0,0,0,,Rất hân hạnh được đi cùng\Nmột anh hùng tương lai!
Dialogue: 0,0:17:03.77,0:17:04.94,Default,,0,0,0,,Đi thôi!
Dialogue: 0,0:17:05.02,0:17:06.94,Default,,0,0,0,,Vâng! Cảm ơn chị rất nhiều!
Dialogue: 0,0:17:08.48,0:17:10.02,Default,,0,0,0,,- Sam à.\N- Vâng?
Dialogue: 0,0:17:10.52,0:17:11.73,Default,,0,0,0,,Anh cũng đi nghỉ đi.
Dialogue: 0,0:17:13.82,0:17:15.86,Default,,0,0,0,,Chị nên gọi em thế nào?
Dialogue: 0,0:17:15.95,0:17:18.03,Default,,0,0,0,,Midoriya? Hay Izuku?
Dialogue: 0,0:17:18.53,0:17:19.45,Default,,0,0,0,,Cứ gọi em là…
Dialogue: 0,0:17:22.24,0:17:23.33,Default,,0,0,0,,Deku thôi ạ.
Dialogue: 0,0:17:24.08,0:17:24.91,Default,,0,0,0,,Deku?
Dialogue: 0,0:17:25.00,0:17:26.71,Default,,0,0,0,,Biệt danh lạ thật.
Dialogue: 0,0:17:27.58,0:17:29.46,Default,,0,0,0,,Em cứ gọi chị là Melissa.
Dialogue: 0,0:17:29.54,0:17:31.17,Default,,0,0,0,,Tôi xin phép.
Dialogue: 0,0:17:33.55,0:17:35.42,Default,,0,0,0,,Này, cậu không sao chứ, Toshi?
Dialogue: 0,0:17:36.51,0:17:37.84,Default,,0,0,0,,Cảm ơn cậu.
Dialogue: 0,0:17:38.34,0:17:41.43,Default,,0,0,0,,Thời gian tớ có thể duy trì\Ndạng cơ bắp càng ngắn hơn rồi.
Dialogue: 0,0:17:41.51,0:17:43.43,Default,,0,0,0,,{\an8}Cậu có kể những triệu chứng qua email rồi,
Dialogue: 0,0:17:43.52,0:17:46.06,Default,,0,0,0,,{\an8}nhưng tớ không biết là\Ntình hình đã tệ hơn.
Dialogue: 0,0:17:53.11,0:17:54.82,Default,,0,0,0,,Tuyệt thật đấy.
Dialogue: 0,0:17:54.90,0:17:56.40,Default,,0,0,0,,Nhìn những thứ này xem,
Dialogue: 0,0:17:56.49,0:17:58.78,Default,,0,0,0,,không thể tin nổi\Nđây là một hòn đảo nhân tạo!
Dialogue: 0,0:17:59.49,0:18:02.20,Default,,0,0,0,,Ở đây có đủ các cơ sở vật chất\Ncủa một thành phố lớn.
Dialogue: 0,0:18:02.28,0:18:04.70,Default,,0,0,0,,Điều duy nhất bọn chị\Nkhông được làm là du lịch.
Dialogue: 0,0:18:04.79,0:18:06.29,Default,,0,0,0,,Thật ạ?
Dialogue: 0,0:18:06.37,0:18:08.62,Default,,0,0,0,,Đó là vì các nhà khoa học ở đây\Nvà gia đình của họ
Dialogue: 0,0:18:08.71,0:18:11.25,Default,,0,0,0,,có nghĩa vụ giữ bí mật\Nđể tránh tiết lộ thông tin.
Dialogue: 0,0:18:11.33,0:18:12.92,Default,,0,0,0,,Ồ!
Dialogue: 0,0:18:13.71,0:18:16.26,Default,,0,0,0,,Đó là Anh hùng Quái vật: Godzillo!
Dialogue: 0,0:18:16.84,0:18:18.84,Default,,0,0,0,,Lần đầu tiên tôi nhìn thấy người thật đấy!
Dialogue: 0,0:18:18.93,0:18:20.43,Default,,0,0,0,,Kosei của anh ấy tuyệt vời quá!
Dialogue: 0,0:18:20.51,0:18:23.89,Default,,0,0,0,,Hẳn là anh ấy được nhà tài trợ mời.
Dialogue: 0,0:18:24.60,0:18:27.27,Default,,0,0,0,,Có vẻ như có rất nhiều sự kiện khác nhau,
Dialogue: 0,0:18:27.35,0:18:29.52,Default,,0,0,0,,ví dụ như trưng bày vật phẩm và ký tặng.
Dialogue: 0,0:18:29.60,0:18:31.23,Default,,0,0,0,,I-Expo tuyệt thật!
Dialogue: 0,0:18:31.86,0:18:34.40,Default,,0,0,0,,Tối nay sẽ có tiệc cho khách mời nữa.
Dialogue: 0,0:18:34.48,0:18:38.07,Default,,0,0,0,,Em cũng sẽ tham gia mà nhỉ?
Dialogue: 0,0:18:38.15,0:18:40.41,Default,,0,0,0,,Vì em đi với chú Might mà.
Dialogue: 0,0:18:40.49,0:18:43.87,Default,,0,0,0,,{\i1}Thế nên All Might mới bảo mình{\i0}
Dialogue: 0,0:18:43.95,0:18:45.33,Default,,0,0,0,,{\i1}mang quần áo trang trọng theo?{\i0}
Dialogue: 0,0:18:45.41,0:18:46.75,Default,,0,0,0,,- Ồ, Deku.\N- Sao ạ?
Dialogue: 0,0:18:47.45,0:18:49.66,Default,,0,0,0,,Mình sang xem sảnh đường đó đi!
Dialogue: 0,0:18:50.62,0:18:53.96,Default,,0,0,0,,Có nhiều vật phẩm chiến đấu mới ở đây ghê!
Dialogue: 0,0:18:54.04,0:18:55.96,Default,,0,0,0,,Deku, nhìn này!
Dialogue: 0,0:18:56.05,0:19:00.59,Default,,0,0,0,,Chiếc Beagle đa năng này ngoài bay ra\Ncòn có thể di chuyển dưới nước nữa!
Dialogue: 0,0:19:00.68,0:19:01.84,Default,,0,0,0,,Tuyệt thật!
Dialogue: 0,0:19:01.93,0:19:03.35,Default,,0,0,0,,Em có thể mặc bộ đồ lặn này
Dialogue: 0,0:19:03.43,0:19:06.35,Default,,0,0,0,,{\an8}lặn sâu xuống tận 7.000 mét\Ndưới mực nước biển.
Dialogue: 0,0:19:06.43,0:19:07.81,Default,,0,0,0,,Sâu thật!
Dialogue: 0,0:19:07.89,0:19:11.98,Default,,0,0,0,,Chiếc kính này được trang bị\N36 cảm biến khác nhau.
Dialogue: 0,0:19:12.06,0:19:13.56,Default,,0,0,0,,{\i1}Em thấy nhiều quá rồi!{\i0}
Dialogue: 0,0:19:14.69,0:19:16.82,Default,,0,0,0,,Thật ra hầu hết những thứ ở đây
Dialogue: 0,0:19:16.90,0:19:19.82,Default,,0,0,0,,đều được tạo ra dựa trên\Nnhững sáng tạo của bố chị.
Dialogue: 0,0:19:19.90,0:19:22.32,Default,,0,0,0,,Ồ, tuyệt thật đấy!
Dialogue: 0,0:19:22.82,0:19:28.33,Default,,0,0,0,,Mọi món đồ ở đây đều giúp\Ncác anh hùng trên thế giới làm việc.
Dialogue: 0,0:19:29.87,0:19:32.37,Default,,0,0,0,,Chị rất kính trọng bố mình nhỉ?
Dialogue: 0,0:19:33.54,0:19:36.17,Default,,0,0,0,,Vì ước mơ của chị là trở thành\Nmột nhà khoa học giống bố.
Dialogue: 0,0:19:37.42,0:19:38.25,Default,,0,0,0,,Phải rồi.
Dialogue: 0,0:19:38.34,0:19:41.51,Default,,0,0,0,,{\an8}Nhắc mới nhớ, chị đang theo học\Ntại Học viện ở đây nhỉ?
Dialogue: 0,0:19:41.59,0:19:43.39,Default,,0,0,0,,Ừ, chị đang học năm ba.
Dialogue: 0,0:19:43.47,0:19:45.64,Default,,0,0,0,,Học viện I-Island
Dialogue: 0,0:19:45.72,0:19:49.31,Default,,0,0,0,,là nơi các nhà khoa học có chí lớn\Nmuốn theo học nhỉ?
Dialogue: 0,0:19:49.39,0:19:51.18,Default,,0,0,0,,Chị vẫn chưa tới đâu cả.
Dialogue: 0,0:19:52.52,0:19:54.65,Default,,0,0,0,,Chị còn phải học nhiều lắm.
Dialogue: 0,0:19:56.77,0:20:01.40,Default,,0,0,0,,Em cũng phải cố gắng nhiều hơn\Nđể được như All Might.
Dialogue: 0,0:20:02.95,0:20:06.24,Default,,0,0,0,,Em thật sự yêu quý chú Might nhỉ?
Dialogue: 0,0:20:06.32,0:20:08.79,Default,,0,0,0,,Lúc nãy em nhiệt tình lắm.\NChị ngạc nhiên đấy!
Dialogue: 0,0:20:08.87,0:20:12.29,Default,,0,0,0,,Ối, xin lỗi, em không kiềm chế được.
Dialogue: 0,0:20:13.96,0:20:15.88,Default,,0,0,0,,Trông cậu vui thật nhỉ, Deku.
Dialogue: 0,0:20:15.96,0:20:18.34,Default,,0,0,0,,Uraraka?
Dialogue: 0,0:20:18.42,0:20:19.46,Default,,0,0,0,,Cậu làm gì ở đây thế?
Dialogue: 0,0:20:20.09,0:20:21.34,Default,,0,0,0,,Trông cậu vui thật nhỉ.
Dialogue: 0,0:20:21.42,0:20:22.34,Default,,0,0,0,,Cậu lặp lại kìa!
Dialogue: 0,0:20:24.13,0:20:25.84,Default,,0,0,0,,Yaoyorozu?
Dialogue: 0,0:20:25.93,0:20:28.01,Default,,0,0,0,,Trông cậu có vẻ vui lắm đấy.
Dialogue: 0,0:20:28.10,0:20:30.56,Default,,0,0,0,,Midoriya, tớ nghe cậu nói hết rồi.
Dialogue: 0,0:20:30.64,0:20:33.06,Default,,0,0,0,,{\i1}Giắc Cắm Tai Nghe của Jiro đáng sợ quá!{\i0}
Dialogue: 0,0:20:33.64,0:20:34.52,Default,,0,0,0,,Bạn em à?
Dialogue: 0,0:20:34.60,0:20:36.73,Default,,0,0,0,,Là bạn cùng lớp của em.
Dialogue: 0,0:20:36.81,0:20:38.77,Default,,0,0,0,,Hình như có chút hiểu lầm ở đây rồi.
Dialogue: 0,0:20:38.86,0:20:43.78,Default,,0,0,0,,Chuyện là chị Melissa đang đưa tớ\Nđi tham quan triển lãm.
Dialogue: 0,0:20:43.86,0:20:46.66,Default,,0,0,0,,Đúng vậy! Bố chị và chú Might là…
Dialogue: 0,0:20:47.37,0:20:48.45,Default,,0,0,0,,Xin chị đấy!
Dialogue: 0,0:20:48.53,0:20:51.87,Default,,0,0,0,,{\an8}Đừng nói với họ\Nlà em đến đây với All Might!
Dialogue: 0,0:20:51.95,0:20:53.16,Default,,0,0,0,,{\an8}- Nhìn kìa…\N- Tại sao?
Dialogue: 0,0:20:53.25,0:20:55.33,Default,,0,0,0,,{\an8}- Nhìn kìa…\N- Nhiều lý do lắm ạ.
Dialogue: 0,0:20:55.92,0:20:56.79,Default,,0,0,0,,{\an8}Được rồi.
Dialogue: 0,0:20:58.67,0:21:00.75,Default,,0,0,0,,Các em có muốn\Ncùng ra quán uống trà không?
Dialogue: 0,0:21:03.21,0:21:04.26,Default,,0,0,0,,{\i1}Ồ!{\i0}
Dialogue: 0,0:21:04.34,0:21:07.97,Default,,0,0,0,,{\i1}Vậy là các em đều có kinh nghiệm\Nlàm việc với Anh hùng Chuyên nghiệp rồi à?{\i0}
Dialogue: 0,0:21:08.05,0:21:10.89,Default,,0,0,0,,Chỉ tập luyện và đi tuần tra thôi ạ.
Dialogue: 0,0:21:10.97,0:21:14.64,Default,,0,0,0,,Em gặp phải một tai nạn\Nnhưng em chỉ di tản mọi người thôi.
Dialogue: 0,0:21:14.73,0:21:16.35,Default,,0,0,0,,Nhưng mà vẫn tuyệt lắm!
Dialogue: 0,0:21:16.44,0:21:19.61,Default,,0,0,0,,Chẳng biết tại sao em lại\Nđi đóng quảng cáo truyền hình.
Dialogue: 0,0:21:19.69,0:21:22.23,Default,,0,0,0,,Bình thường có làm thế được đâu.\NTuyệt thật đấy!
Dialogue: 0,0:21:22.94,0:21:25.32,Default,,0,0,0,,Ngày mai bọn em định đến sảnh đường,
Dialogue: 0,0:21:25.40,0:21:27.11,Default,,0,0,0,,nơi Học viện triển lãm.
Dialogue: 0,0:21:27.20,0:21:28.82,Default,,0,0,0,,- Em háo hức lắm!\N{\i1}- Tốt quá rồi.{\i0}
Dialogue: 0,0:21:28.91,0:21:31.24,Default,,0,0,0,,- Chị có làm gì ở đó không?\N{\i1}- Xem ra đã ổn thỏa rồi.{\i0}
Dialogue: 0,0:21:31.33,0:21:32.16,Default,,0,0,0,,Ừ, tất nhiên rồi!
Dialogue: 0,0:21:32.24,0:21:35.12,Default,,0,0,0,,- Cảm ơn vì đã đợi.\N- Giọng nói đó…
Dialogue: 0,0:21:35.21,0:21:38.92,Default,,0,0,0,,- Kaminari!\N- Cả Mineta nữa?
Dialogue: 0,0:21:39.00,0:21:40.79,Default,,0,0,0,,Hai cậu đang làm gì vậy?
Dialogue: 0,0:21:40.88,0:21:43.51,Default,,0,0,0,,Họ cần nhân viên tạm thời cho triển lãm
Dialogue: 0,0:21:43.59,0:21:44.80,Default,,0,0,0,,nên bọn tớ đã xin vào.
Dialogue: 0,0:21:44.88,0:21:45.72,Default,,0,0,0,,Nhỉ?
Dialogue: 0,0:21:45.80,0:21:49.72,Default,,0,0,0,,Bọn tớ có thể đi xem triển lãm\Nvào giờ nghỉ và còn được trả lương nữa.
Dialogue: 0,0:21:49.80,0:21:53.26,Default,,0,0,0,,Biết đâu bọn tớ còn may mắn gặp được
Dialogue: 0,0:21:53.35,0:21:54.89,Default,,0,0,0,,mấy cô nàng xinh xắn đến tham gia!
Dialogue: 0,0:21:54.97,0:21:57.85,Default,,0,0,0,,Này, Midoriya, cậu gặp nàng đó ở đâu thế?
Dialogue: 0,0:21:57.94,0:21:59.52,Default,,0,0,0,,Giới thiệu với bọn này đi! Đi nào!
Dialogue: 0,0:21:59.60,0:22:00.73,Default,,0,0,0,,À, thì…
Dialogue: 0,0:22:00.81,0:22:02.77,Default,,0,0,0,,Họ cũng là học sinh của UA à?
Dialogue: 0,0:22:02.86,0:22:04.11,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,0:22:04.19,0:22:05.28,Default,,0,0,0,,Bọn này sẽ là anh hùng.
Dialogue: 0,0:22:05.36,0:22:07.45,Default,,0,0,0,,Sao các cậu lề mề thế hả?
Dialogue: 0,0:22:07.53,0:22:10.99,Default,,0,0,0,,Muốn nhận tiền\Nthì phải làm việc chăm chỉ hơn chứ!
Dialogue: 0,0:22:11.95,0:22:14.45,Default,,0,0,0,,- Iida!\N- Cậu cũng ở đây à?
Dialogue: 0,0:22:14.54,0:22:17.04,Default,,0,0,0,,Tớ xuất thân từ một gia đình anh hùng mà,
Dialogue: 0,0:22:17.12,0:22:19.46,Default,,0,0,0,,thế nên nhà tớ được I-Expo mời.
Dialogue: 0,0:22:20.25,0:22:23.17,Default,,0,0,0,,Nhưng nhà tớ bận lắm\Nnên chỉ có mỗi tớ thôi.
Dialogue: 0,0:22:23.25,0:22:24.63,Default,,0,0,0,,Cả cậu nữa à, Iida?
Dialogue: 0,0:22:25.13,0:22:27.05,Default,,0,0,0,,Tớ cũng được mời
Dialogue: 0,0:22:27.13,0:22:31.59,Default,,0,0,0,,vì bố tớ nắm cổ phần\Ncủa các nhà tài trợ I-Expo.
Dialogue: 0,0:22:31.68,0:22:34.89,Default,,0,0,0,,Và vì Yaomomo thừa hai vé mời…
Dialogue: 0,0:22:35.93,0:22:39.27,Default,,0,0,0,,{\i1}- Oẳn tù tì ra cái gì ra cái này\N- Oẳn tù tì ra cái gì ra cái này{\i0}
Dialogue: 0,0:22:39.35,0:22:41.40,Default,,0,0,0,,Mình thua mất rồi!
Dialogue: 0,0:22:41.90,0:22:43.65,Default,,0,0,0,,{\i1}Sau khi chơi trò may rủi công bằng,{\i0}
Dialogue: 0,0:22:44.23,0:22:46.73,Default,,0,0,0,,bọn tớ đã được chọn đi với cậu ấy.
Dialogue: 0,0:22:46.82,0:22:49.86,Default,,0,0,0,,Phải! Các bạn nữ khác cũng ở trên đảo.
Dialogue: 0,0:22:49.95,0:22:51.16,Default,,0,0,0,,Ra là thế.
Dialogue: 0,0:22:51.66,0:22:55.62,Default,,0,0,0,,Bọn tớ định cùng nhau đi chơi\Nkhi triển lãm mở cửa vào ngày mai.
Dialogue: 0,0:22:55.70,0:22:58.66,Default,,0,0,0,,Các em có cần chị làm hướng dẫn không?
Dialogue: 0,0:22:58.75,0:23:00.71,Default,,0,0,0,,- Có được không ạ?\N- Được chứ!
Dialogue: 0,0:23:00.79,0:23:02.04,Default,,0,0,0,,- Được rồi!\N- Ừ!
Dialogue: 0,0:23:02.13,0:23:04.29,Default,,0,0,0,,- Cho bọn em theo với!\N- Cho bọn em theo với!
Dialogue: 0,0:23:04.38,0:23:05.75,Default,,0,0,0,,Cái gì thế?
Dialogue: 0,0:23:09.80,0:23:12.05,Default,,0,0,0,,Thời gian hoàn thành: 33 giây!
Dialogue: 0,0:23:12.14,0:23:13.64,Default,,0,0,0,,Hạng tám!
Dialogue: 0,0:23:13.72,0:23:15.43,Default,,0,0,0,,Kirishima?
Dialogue: 0,0:23:15.51,0:23:17.68,Default,,0,0,0,,Deku, em biết cậu ấy à?
Dialogue: 0,0:23:17.77,0:23:19.85,Default,,0,0,0,,Vâng, bạn cùng lớp của em.
Dialogue: 0,0:23:19.93,0:23:22.27,Default,,0,0,0,,Người khiêu chiến tiếp theo là…
Dialogue: 0,0:23:22.77,0:23:24.81,Default,,0,0,0,,Kacchan?
Dialogue: 0,0:23:25.32,0:23:27.57,Default,,0,0,0,,Giờ thì, Villain Attack!
Dialogue: 0,0:23:27.65,0:23:29.32,Default,,0,0,0,,Sẵn sàng, bắt đầu!
Dialogue: 0,0:23:36.16,0:23:37.66,Default,,0,0,0,,Chết đi!
Dialogue: 0,0:23:38.29,0:23:39.12,Default,,0,0,0,,{\i1}Chết ư?{\i0}
Dialogue: 0,0:23:40.20,0:23:41.83,Default,,0,0,0,,Thật tuyệt vời!
Dialogue: 0,0:23:41.91,0:23:43.12,Default,,0,0,0,,Vượt qua trong 15 giây!
Dialogue: 0,0:23:43.21,0:23:44.29,Default,,0,0,0,,Cậu ấy đứng đầu!
Dialogue: 0,0:23:47.38,0:23:50.01,Default,,0,0,0,,- Ủa, bên kia là Midoriya phải không nhỉ?\N- Hả?
Dialogue: 0,0:23:53.89,0:23:56.76,Default,,0,0,0,,{\an8}Sao mày lại ở đây hả?
Dialogue: 0,0:23:56.85,0:23:58.47,Default,,0,0,0,,Dừng lại đi, Kacchan.
Dialogue: 0,0:23:58.56,0:24:00.43,Default,,0,0,0,,- Mọi người đang nhìn…\N- Thì sao?
Dialogue: 0,0:24:00.52,0:24:01.93,Default,,0,0,0,,Dừng lại đi, Bakugo!
Dialogue: 0,0:24:02.02,0:24:04.35,Default,,0,0,0,,- Sao cậu ấy nổi giận vậy?\N- Cóc liên quan đến mày!
Dialogue: 0,0:24:04.44,0:24:05.65,Default,,0,0,0,,- Cậu ấy lúc nào cũng vậy.\N- Đừng ra vẻ lớp trưởng ở đây!
Dialogue: 0,0:24:05.73,0:24:08.44,Default,,0,0,0,,- Cuộc chiến định mệnh giữa đấng nam nhi!\N- Lớp trưởng lúc nào cũng phải thế!
Dialogue: 0,0:24:08.52,0:24:11.53,Default,,0,0,0,,Kirishima, hai cậu đều được mời\Nđến triển lãm à?
Dialogue: 0,0:24:11.61,0:24:12.45,Default,,0,0,0,,Không.
Dialogue: 0,0:24:12.53,0:24:15.82,Default,,0,0,0,,Bakugo là người duy nhất được mời\Nvì cậu ấy là quán quân Hội thao UA mà.
Dialogue: 0,0:24:15.91,0:24:17.20,Default,,0,0,0,,Tớ chỉ đi cùng thôi.
Dialogue: 0,0:24:17.78,0:24:20.20,Default,,0,0,0,,Sao nào? Các cậu định thử à?
Dialogue: 0,0:24:20.29,0:24:21.91,Default,,0,0,0,,Vô ích thôi!
Dialogue: 0,0:24:22.00,0:24:23.96,Default,,0,0,0,,Vì tao chắc chắn sẽ giỏi hơn mày!
Dialogue: 0,0:24:24.04,0:24:25.75,Default,,0,0,0,,Phải, cậu nói đúng.
Dialogue: 0,0:24:25.83,0:24:28.63,Default,,0,0,0,,Nhưng chưa thử thì sao biết được.
Dialogue: 0,0:24:28.71,0:24:31.21,Default,,0,0,0,,Phải, cậu nói đúng. Khoan đã.
Dialogue: 0,0:24:31.30,0:24:36.47,Default,,0,0,0,,{\an8}Vậy thì mau thể hiện cái kết quả thảm hại\Ncủa mày đi, đồ mọt sách chết tiệt!
Dialogue: 0,0:24:36.55,0:24:37.93,Default,,0,0,0,,Được!
Dialogue: 0,0:24:39.06,0:24:42.14,Default,,0,0,0,,Bây giờ là một người thách đấu bất ngờ!
Dialogue: 0,0:24:42.77,0:24:45.40,Default,,0,0,0,,Thành tích của cậu ấy sẽ là bao nhiêu đây?
Dialogue: 0,0:24:46.44,0:24:48.69,Default,,0,0,0,,{\i1}Vì mình đã quyết làm thế rồi…{\i0}
Dialogue: 0,0:24:49.48,0:24:51.36,Default,,0,0,0,,{\i1}One For All…{\i0}
Dialogue: 0,0:24:51.44,0:24:53.28,Default,,0,0,0,,{\i1}Full Cowl!{\i0}
Dialogue: 0,0:24:54.78,0:24:55.78,Default,,0,0,0,,Villain Attack!
Dialogue: 0,0:24:55.86,0:24:58.24,Default,,0,0,0,,Sẵn sàng, bắt đầu!
Dialogue: 0,0:24:59.20,0:25:02.12,Default,,0,0,0,,- Hả?\N{\i1}- Để không làm cánh tay bị thương…{\i0}
Dialogue: 0,0:25:02.20,0:25:04.58,Default,,0,0,0,,{\i1}mình sẽ dùng năm phần trăm sức mạnh!{\i0}
Dialogue: 0,0:25:13.17,0:25:14.72,Default,,0,0,0,,Cậu ấy cũng rất cừ!
Dialogue: 0,0:25:14.80,0:25:16.55,Default,,0,0,0,,Cậu ấy đứng thứ hai với 16 giây!
Dialogue: 0,0:25:16.63,0:25:19.05,Default,,0,0,0,,- Ồ!\N- Thật phi thường!
Dialogue: 0,0:25:19.64,0:25:21.51,Default,,0,0,0,,{\i1}Cậu ấy có môt sức mạnh bùng nổ tuyệt vời.{\i0}
Dialogue: 0,0:25:21.60,0:25:22.89,Default,,0,0,0,,{\i1}Và sức hủy diệt…{\i0}
Dialogue: 0,0:25:23.93,0:25:25.89,Default,,0,0,0,,{\i1}Cũng giống như năng lực của chú Might vậy.{\i0}
Dialogue: 0,0:25:27.19,0:25:28.06,Default,,0,0,0,,{\i1}Nhưng mà…{\i0}
Dialogue: 0,0:25:28.73,0:25:30.36,Default,,0,0,0,,Ôi, suýt tí là được!
Dialogue: 0,0:25:30.44,0:25:32.53,Default,,0,0,0,,Đúng như tớ mong đợi, Midoriya.
Dialogue: 0,0:25:32.61,0:25:35.24,Default,,0,0,0,,Tớ không ngờ là thời gian\Nlại sát nút với Kacchan như thế…
Dialogue: 0,0:25:35.32,0:25:37.24,Default,,0,0,0,,Cái quái gì thế hả? Không thể nào!
Dialogue: 0,0:25:37.32,0:25:39.37,Default,,0,0,0,,Tao sẽ làm lại và đá tung…
Dialogue: 0,0:25:39.45,0:25:42.37,Default,,0,0,0,,Ôi! Tuyệt vời quá đi mất!
Dialogue: 0,0:25:42.45,0:25:45.04,Default,,0,0,0,,Mười bốn giây!
Dialogue: 0,0:25:45.12,0:25:47.62,Default,,0,0,0,,Cậu ấy nhảy thẳng lên vị trí dẫn đầu!
Dialogue: 0,0:25:48.12,0:25:49.88,Default,,0,0,0,,Todoroki!
Dialogue: 0,0:25:49.96,0:25:51.79,Default,,0,0,0,,- Cũng là bạn học của em à?\N- Vâng!
Dialogue: 0,0:25:52.38,0:25:54.17,Default,,0,0,0,,Các em đều tuyệt lắm!
Dialogue: 0,0:25:54.26,0:25:57.30,Default,,0,0,0,,- Không hổ là những anh hùng tương lai.\N- Chị đừng nói thế ạ.
Dialogue: 0,0:25:58.18,0:26:01.51,Default,,0,0,0,,- Chết tiệt, thằng Nửa Này Nửa Nọ kia!\N- Bakugo.
Dialogue: 0,0:26:01.60,0:26:04.68,Default,,0,0,0,,Cố tình đột ngột xuất hiện\Nvà gây ấn tượng với mọi người hả?
Dialogue: 0,0:26:04.77,0:26:07.64,Default,,0,0,0,,- Midoriya và mọi người cũng ở đây à?\N- Đừng có bơ tao!
Dialogue: 0,0:26:07.73,0:26:10.56,Default,,0,0,0,,Mà sao mày lại ở đây?
Dialogue: 0,0:26:10.65,0:26:12.65,Default,,0,0,0,,Bố tôi được mời nên tôi thay mặt ông ấy.
Dialogue: 0,0:26:12.73,0:26:16.11,Default,,0,0,0,,- Xin lỗi, nhưng người tiếp theo đang đợi…\N- Im miệng! Tiếp theo là tôi!
Dialogue: 0,0:26:16.19,0:26:17.70,Default,,0,0,0,,Các cậu dừng lại đi!
Dialogue: 0,0:26:17.78,0:26:20.32,Default,,0,0,0,,Những bí mật xấu hổ của UA\Nsẽ bị phơi bày mất!
Dialogue: 0,0:26:20.41,0:26:21.41,Default,,0,0,0,,- Phải rồi!\N- Ừ!
Dialogue: 0,0:26:21.49,0:26:23.45,Default,,0,0,0,,Kacchan, bình tĩnh nào!
Dialogue: 0,0:26:23.95,0:26:25.29,Default,,0,0,0,,- Chị xin lỗi.\N- Đừng làm bọn này xấu hổ thêm nữa!
Dialogue: 0,0:26:25.37,0:26:26.41,Default,,0,0,0,,- Mày bảo ai xấu hổ?\N- Thôi đi, Bakugo!
Dialogue: 0,0:26:26.50,0:26:29.04,Default,,0,0,0,,Có vẻ như bọn em học ở UA khá là vui.
Dialogue: 0,0:26:29.75,0:26:32.88,Default,,0,0,0,,- Ít nhất là sẽ không nhàm chán.\N- Bọn mày bị cái giống gì vậy? Bỏ ra!
Dialogue: 0,0:26:32.96,0:26:34.80,Default,,0,0,0,,- Đúng vậy.\N- Tao sẽ đốt trụi bọn mày!
Dialogue: 0,0:26:42.43,0:26:44.51,Default,,0,0,0,,Tôi đã nhận được hàng theo kế hoạch.
Dialogue: 0,0:26:46.22,0:26:48.35,Default,,0,0,0,,Cái gì? All Might?
Dialogue: 0,0:26:50.14,0:26:53.36,Default,,0,0,0,,Đừng hoảng sợ. Tôi sẽ xử lý.
Dialogue: 0,0:26:55.82,0:26:58.53,Default,,0,0,0,,All Might đang ở trên hòn đảo này…
Dialogue: 0,0:27:08.33,0:27:09.37,Default,,0,0,0,,{\an8}Cái gì…
Dialogue: 0,0:27:15.21,0:27:16.71,Default,,0,0,0,,Chuyện này là sao, Toshi?
Dialogue: 0,0:27:17.88,0:27:20.51,Default,,0,0,0,,Sao chỉ số Kosei của cậu giảm mạnh vậy?
Dialogue: 0,0:27:21.18,0:27:24.68,Default,,0,0,0,,Dù cho cậu có bị thương nặng\Ntrong trận chiến với All For One,
Dialogue: 0,0:27:24.76,0:27:26.64,Default,,0,0,0,,thì chỉ số thế này vẫn quá bất thường.
Dialogue: 0,0:27:27.35,0:27:30.48,Default,,0,0,0,,Rốt cuộc cơ thể cậu bị gì vậy?
Dialogue: 0,0:27:32.52,0:27:36.44,Default,,0,0,0,,Khi cậu làm anh hùng quá lâu,\Ncơ thể cậu rồi sẽ vụn vỡ.
Dialogue: 0,0:27:38.03,0:27:40.40,Default,,0,0,0,,{\i1}Mình không thể nói cho cậu ấy\Nbí mật của One For All.{\i0}
Dialogue: 0,0:27:40.49,0:27:41.90,Default,,0,0,0,,{\i1}Nếu mình nói ra,{\i0}
Dialogue: 0,0:27:41.99,0:27:46.37,Default,,0,0,0,,{\i1}Dave và Melissa sẽ bị vướng vào\Ncuộc chiến với All For One.{\i0}
Dialogue: 0,0:27:47.95,0:27:51.08,Default,,0,0,0,,Cứ đà này,\Nchúng ta sẽ mất đi Biểu tượng Hòa bình.
Dialogue: 0,0:27:52.04,0:27:56.09,Default,,0,0,0,,{\i1}Lý do duy nhất Nhật Bản có thể\Nduy trì tỷ lệ tội phạm ở mức 6 phần trăm{\i0}
Dialogue: 0,0:27:56.17,0:27:57.67,Default,,0,0,0,,{\i1}là nhờ có cậu.{\i0}
Dialogue: 0,0:27:58.17,0:28:01.30,Default,,0,0,0,,{\i1}Nhìn chung các nước khác\Ncó tỷ lệ tội phạm trên 20 phần trăm.{\i0}
Dialogue: 0,0:28:03.34,0:28:07.35,Default,,0,0,0,,{\an8}Rất nhiều lần tớ ước rằng cậu ở lại Mỹ.
Dialogue: 0,0:28:10.35,0:28:13.02,Default,,0,0,0,,Không cần phải bi quan như thế.
Dialogue: 0,0:28:13.90,0:28:16.15,Default,,0,0,0,,Có rất nhiều\NAnh hùng Chuyên nghiệp đầy tiềm năng,
Dialogue: 0,0:28:16.23,0:28:19.07,Default,,0,0,0,,và rất nhiều người hỗ trợ các anh hùng\Ngiống như cậu vậy.
Dialogue: 0,0:28:19.78,0:28:20.69,Default,,0,0,0,,Tớ vẫn có thể
Dialogue: 0,0:28:20.78,0:28:23.40,Default,,0,0,0,,làm All Might vài giờ mỗi ngày…
Dialogue: 0,0:28:23.49,0:28:24.41,Default,,0,0,0,,Nhưng…
Dialogue: 0,0:28:25.16,0:28:29.95,Default,,0,0,0,,có khả năng một villain khác\Nnhư All For One sẽ lại xuất hiện.
Dialogue: 0,0:28:30.54,0:28:31.37,Default,,0,0,0,,Dave.
Dialogue: 0,0:28:31.45,0:28:32.29,Default,,0,0,0,,Sao?
Dialogue: 0,0:28:33.00,0:28:38.04,Default,,0,0,0,,{\an8}Vì thời điểm đó, tớ không có ý định\Ntừ bỏ danh hiệu Biểu tượng Hòa bình.
Dialogue: 0,0:28:39.05,0:28:41.80,Default,,0,0,0,,{\i1}Hơn nữa, chúng ta vẫn còn hy vọng.{\i0}
Dialogue: 0,0:28:43.76,0:28:45.80,Default,,0,0,0,,{\i1}Niềm hy vọng rằng ý chí của One For All{\i0}
Dialogue: 0,0:28:45.89,0:28:47.85,Default,,0,0,0,,{\i1}và Biểu tượng Hòa bình{\i0}
Dialogue: 0,0:28:49.39,0:28:52.14,Default,,0,0,0,,{\i1}sẽ kết nối với thế hệ tiếp theo.{\i0}
Dialogue: 0,0:28:56.44,0:28:59.44,Default,,0,0,0,,{\i1}Chúng tôi sẽ đóng cửa vào 6:00 tối nay.{\i0}
Dialogue: 0,0:29:00.61,0:29:02.90,Default,,0,0,0,,{\i1}Cảm ơn quý khách đã đến tham quan.{\i0}
Dialogue: 0,0:29:04.03,0:29:05.49,Default,,0,0,0,,Trời ạ.
Dialogue: 0,0:29:05.57,0:29:08.66,Default,,0,0,0,,{\an8}Mớ chuẩn bị mở cửa thôi\Nmà đã bận thế này rồi.
Dialogue: 0,0:29:08.74,0:29:11.16,Default,,0,0,0,,Ngày mai sẽ ra sao đây?
Dialogue: 0,0:29:11.24,0:29:13.20,Default,,0,0,0,,Thôi đi. Tớ không muốn\Nnghĩ đến việc đó đâu!
Dialogue: 0,0:29:13.79,0:29:15.25,Default,,0,0,0,,Mineta và Kaminari!
Dialogue: 0,0:29:15.33,0:29:17.50,Default,,0,0,0,,- Hả?\N- Các cậu làm tốt lắm!
Dialogue: 0,0:29:17.58,0:29:19.34,Default,,0,0,0,,Cả hai cậu đều vất vả rồi!
Dialogue: 0,0:29:20.88,0:29:21.71,Default,,0,0,0,,Cái gì đây?
Dialogue: 0,0:29:22.30,0:29:24.72,Default,,0,0,0,,Thiệp mời dự tiệc.
Dialogue: 0,0:29:25.76,0:29:26.63,Default,,0,0,0,,Tiệc ư?
Dialogue: 0,0:29:26.72,0:29:27.55,Default,,0,0,0,,Cho bọn tớ?
Dialogue: 0,0:29:27.64,0:29:31.68,Default,,0,0,0,,- Chị Melissa mời đấy.\N- Chị ấy bảo hai cậu cũng nên tận hưởng!
Dialogue: 0,0:29:32.27,0:29:35.18,Default,,0,0,0,,Vẫn còn nữa\Nnên nếu thích thì các em cứ lấy nhé.
Dialogue: 0,0:29:35.27,0:29:36.85,Default,,0,0,0,,Kaminari…
Dialogue: 0,0:29:36.94,0:29:38.56,Default,,0,0,0,,Mineta…
Dialogue: 0,0:29:38.65,0:29:41.94,Default,,0,0,0,,- Nỗ lực của ta được đền đáp rồi!\N- Nỗ lực của ta được đền đáp rồi!
Dialogue: 0,0:29:42.57,0:29:45.95,Default,,0,0,0,,{\i1}Tớ nghe nói có khá nhiều\NAnh hùng Chuyên nghiệp tham dự bữa tiệc.{\i0}
Dialogue: 0,0:29:46.03,0:29:48.41,Default,,0,0,0,,Đừng làm xấu mặt học sinh UA đấy!
Dialogue: 0,0:29:48.49,0:29:51.45,Default,,0,0,0,,Thay quần áo lịch sự\Nvà cùng nhau đi dự tiệc nào!
Dialogue: 0,0:29:51.53,0:29:55.96,Default,,0,0,0,,Chúng ta sẽ gặp nhau lúc 6:30 tối\Ntại Sảnh số 7 của tháp trung tâm!
Dialogue: 0,0:29:56.04,0:29:57.04,Default,,0,0,0,,Nhớ đến đúng giờ đấy!
Dialogue: 0,0:29:57.62,0:30:00.67,Default,,0,0,0,,Tớ sẽ liên lạc với Todoroki và Bakugo.
Dialogue: 0,0:30:00.75,0:30:02.42,Default,,0,0,0,,Giải tán!
Dialogue: 0,0:30:03.96,0:30:06.30,Default,,0,0,0,,Iida đang mở hết ga kìa!
Dialogue: 0,0:30:07.22,0:30:08.72,Default,,0,0,0,,Hẹn gặp lại!
Dialogue: 0,0:30:08.80,0:30:09.64,Default,,0,0,0,,Ừ!
Dialogue: 0,0:30:10.51,0:30:11.55,Default,,0,0,0,,- Deku!\N- Sao ạ?
Dialogue: 0,0:30:12.76,0:30:14.89,Default,,0,0,0,,Em đến chỗ này với chị nhé?
Dialogue: 0,0:30:16.43,0:30:18.52,Default,,0,0,0,,{\i1}Sao tao phải đi dự tiệc?{\i0}
Dialogue: 0,0:30:18.60,0:30:22.52,Default,,0,0,0,,Tao sẽ không đến nghe và vỗ tay\Ncho bài phát biểu của tên nào đâu.
Dialogue: 0,0:30:22.61,0:30:23.86,Default,,0,0,0,,Ngớ ngẩn lắm.
Dialogue: 0,0:30:23.94,0:30:26.15,Default,,0,0,0,,Tớ nghe bảo có thể thỏa sức ăn đồ ăn ngon!
Dialogue: 0,0:30:26.24,0:30:29.20,Default,,0,0,0,,Dù sao tao cũng có mang\Nquần áo lịch sự theo đâu.
Dialogue: 0,0:30:29.28,0:30:31.03,Default,,0,0,0,,- Tớ cũng nghĩ thế!\N- Cái gì?
Dialogue: 0,0:30:31.12,0:30:32.91,Default,,0,0,0,,Tớ có mang một bộ cho cậu này!
Dialogue: 0,0:30:32.99,0:30:35.83,Default,,0,0,0,,Mày chu đáo quá mức rồi, Đầu Chĩa!
Dialogue: 0,0:30:40.67,0:30:42.00,Default,,0,0,0,,Dave, hẹn gặp cậu…
Dialogue: 0,0:30:43.42,0:30:44.84,Default,,0,0,0,,ở bữa tiệc!
Dialogue: 0,0:30:44.92,0:30:45.96,Default,,0,0,0,,Ừ.
Dialogue: 0,0:30:48.76,0:30:49.59,Default,,0,0,0,,Toshi!
Dialogue: 0,0:30:50.18,0:30:51.22,Default,,0,0,0,,Hả?
Dialogue: 0,0:30:51.76,0:30:52.60,Default,,0,0,0,,Sao thế?
Dialogue: 0,0:30:52.68,0:30:54.22,Default,,0,0,0,,À, không có gì.
Dialogue: 0,0:30:56.72,0:30:57.98,Default,,0,0,0,,Hẹn gặp cậu ở bữa tiệc.
Dialogue: 0,0:31:11.36,0:31:14.87,Default,,0,0,0,,- Tai nạn trong lúc thí nghiệm ư?\N- Vẫn còn người chưa kịp chạy ra!
Dialogue: 0,0:31:19.04,0:31:20.12,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:31:31.68,0:31:32.89,Default,,0,0,0,,Cảm ơn.
Dialogue: 0,0:31:33.55,0:31:35.26,Default,,0,0,0,,Tôi là David Shield.
Dialogue: 0,0:31:35.35,0:31:36.35,Default,,0,0,0,,Tên cậu là gì?
Dialogue: 0,0:31:36.43,0:31:37.60,Default,,0,0,0,,Tôi là All Might.
Dialogue: 0,0:31:37.68,0:31:39.64,Default,,0,0,0,,Một học sinh khao khát được làm anh hùng.
Dialogue: 0,0:31:39.73,0:31:40.64,Default,,0,0,0,,- Ơ?\N- Ơ?
Dialogue: 0,0:31:40.73,0:31:44.15,Default,,0,0,0,,Đây là bộ trang phục làm bằng\Nchất liệu mới nhất dành riêng cho cậu.
Dialogue: 0,0:31:44.23,0:31:46.57,Default,,0,0,0,,Cậu có thể làm được nhiều hơn\Nvới bộ trang phục này.
Dialogue: 0,0:31:47.07,0:31:47.90,Default,,0,0,0,,Tuyệt quá.
Dialogue: 0,0:32:10.17,0:32:11.72,Default,,0,0,0,,{\i1}Biểu tượng Hòa bình?{\i0}
Dialogue: 0,0:32:12.51,0:32:16.14,Default,,0,0,0,,Tớ sẽ phấn đấu tạo ra một thế giới,\Nnơi mọi người có thể cùng nhau tươi cười.
Dialogue: 0,0:32:17.01,0:32:19.14,Default,,0,0,0,,Tớ muốn trở thành Biểu tượng Hòa bình
Dialogue: 0,0:32:19.22,0:32:21.02,Default,,0,0,0,,soi sáng thế giới ấy.
Dialogue: 0,0:32:34.16,0:32:35.32,Default,,0,0,0,,Đã khống chế được chúng.
Dialogue: 0,0:32:35.41,0:32:37.99,Default,,0,0,0,,Có năm bảo vệ.\NMọi thứ đang diễn ra đúng theo kế hoạch.
Dialogue: 0,0:32:38.66,0:32:40.62,Default,,0,0,0,,{\i1}Hệ thống an ninh vẫn đang hoạt động.{\i0}
Dialogue: 0,0:32:40.70,0:32:43.21,Default,,0,0,0,,{\i1}- Giam chúng lại, nhưng đừng giết chúng.{\i0}\N- Vâng.
Dialogue: 0,0:32:43.29,0:32:44.87,Default,,0,0,0,,Ta sẽ tiến hành ngay bây giờ.
Dialogue: 0,0:32:45.54,0:32:46.75,Default,,0,0,0,,Rất suôn sẻ.
Dialogue: 0,0:32:50.75,0:32:52.51,Default,,0,0,0,,Bọn tôi cũng sẽ bắt đầu hành động.
Dialogue: 0,0:32:58.10,0:33:00.72,Default,,0,0,0,,{\i1}Đây là khuôn viên Học viện\Nchị đang theo học.{\i0}
Dialogue: 0,0:33:00.81,0:33:04.19,Default,,0,0,0,,Và đây là phòng thí nghiệm của chị.
Dialogue: 0,0:33:06.56,0:33:07.94,Default,,0,0,0,,Xin lỗi, chỗ này bừa bộn quá.
Dialogue: 0,0:33:08.02,0:33:11.11,Default,,0,0,0,,Ồ, nhìn chuyên nghiệp quá.
Dialogue: 0,0:33:11.19,0:33:13.74,Default,,0,0,0,,Khó mà hình dung được\Nviệc nghiên cứu ở nơi thế này.
Dialogue: 0,0:33:15.65,0:33:18.74,Default,,0,0,0,,Melissa, chị tài năng thật đấy.
Dialogue: 0,0:33:18.82,0:33:23.00,Default,,0,0,0,,Thật ra thì điểm số học tập của chị\Nkhông cao đâu.
Dialogue: 0,0:33:23.50,0:33:25.50,Default,,0,0,0,,Thế nên chị đã học tập rất chăm chỉ.
Dialogue: 0,0:33:26.08,0:33:28.42,Default,,0,0,0,,Bởi vì chị muốn trở thành\Nmột anh hùng bằng mọi giá.
Dialogue: 0,0:33:28.50,0:33:30.84,Default,,0,0,0,,Sao ạ? Chị muốn làm\NAnh hùng Chuyên nghiệp ạ?
Dialogue: 0,0:33:30.92,0:33:33.63,Default,,0,0,0,,Không, chị đã từ bỏ ý định đó lâu rồi.
Dialogue: 0,0:33:34.13,0:33:35.93,Default,,0,0,0,,Chị không có Kosei mà.
Dialogue: 0,0:33:37.68,0:33:38.80,Default,,0,0,0,,Không có Kosei…
Dialogue: 0,0:33:39.72,0:33:42.10,Default,,0,0,0,,Chị không có Kosei dù đã lên năm,
Dialogue: 0,0:33:42.18,0:33:43.81,Default,,0,0,0,,thế nên bố đưa chị đi khám bác sĩ.
Dialogue: 0,0:33:44.81,0:33:47.90,Default,,0,0,0,,Chị được chẩn đoán là không thể có Kosei.
Dialogue: 0,0:33:48.81,0:33:50.27,Default,,0,0,0,,{\i1}Chị ấy giống mình.{\i0}
Dialogue: 0,0:33:51.57,0:33:56.90,Default,,0,0,0,,Mẹ, chú ấy cứu người với nụ cười trên môi.
Dialogue: 0,0:33:58.24,0:34:01.58,Default,,0,0,0,,Con có thể giống như chú ấy không?
Dialogue: 0,0:34:02.41,0:34:03.62,Default,,0,0,0,,{\i1}Xin lỗi, Izuku!{\i0}
Dialogue: 0,0:34:03.70,0:34:06.29,Default,,0,0,0,,{\i1}Mẹ xin lỗi…{\i0}
Dialogue: 0,0:34:07.67,0:34:09.29,Default,,0,0,0,,{\an8}Xin lỗi.
Dialogue: 0,0:34:09.38,0:34:10.46,Default,,0,0,0,,{\an8}Em…
Dialogue: 0,0:34:10.54,0:34:12.42,Default,,0,0,0,,Hả? Em sao thế?
Dialogue: 0,0:34:12.50,0:34:14.38,Default,,0,0,0,,À, không có gì.
Dialogue: 0,0:34:15.30,0:34:20.22,Default,,0,0,0,,Chỉ là nghe chị bảo rằng mình không có\Nthứ mà mọi người thường có…
Dialogue: 0,0:34:21.43,0:34:23.18,Default,,0,0,0,,Tất nhiên là chị sốc lắm.
Dialogue: 0,0:34:23.85,0:34:27.18,Default,,0,0,0,,Nhưng chị có một mục tiêu trong tầm với.
Dialogue: 0,0:34:27.27,0:34:29.02,Default,,0,0,0,,Sao ạ? Mục tiêu ư?
Dialogue: 0,0:34:30.35,0:34:31.48,Default,,0,0,0,,Bố chị đấy.
Dialogue: 0,0:34:32.44,0:34:35.61,Default,,0,0,0,,Bố không có một Kosei\Ncó thể giúp bố làm anh hùng,
Dialogue: 0,0:34:35.69,0:34:37.74,Default,,0,0,0,,nhưng bố hỗ trợ chú Might\Nvà các anh hùng khác
Dialogue: 0,0:34:37.82,0:34:39.36,Default,,0,0,0,,bằng sức mạnh của khoa học.
Dialogue: 0,0:34:40.16,0:34:43.49,Default,,0,0,0,,Tuy là gián tiếp\Nnhưng bố đang chiến đấu vì hòa bình.
Dialogue: 0,0:34:44.33,0:34:46.79,Default,,0,0,0,,Người giúp các anh hùng…
Dialogue: 0,0:34:46.87,0:34:47.96,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,0:34:48.54,0:34:51.71,Default,,0,0,0,,Đó là cách chị muốn trở thành anh hùng.
Dialogue: 0,0:34:54.04,0:34:58.80,Default,,0,0,0,,Lúc trước chị đã làm vật phẩm hỗ trợ này\Ntrong lúc quan sát chú Might.
Dialogue: 0,0:34:58.88,0:35:00.05,Default,,0,0,0,,All Might?
Dialogue: 0,0:35:02.80,0:35:03.97,Default,,0,0,0,,Thử bấm vào ô này xem.
Dialogue: 0,0:35:11.73,0:35:12.90,Default,,0,0,0,,Đây là…
Dialogue: 0,0:35:12.98,0:35:15.94,Default,,0,0,0,,Nếu phải đặt tên cho nó\Nthì chị nghĩ sẽ gọi nó là "Full Gauntlet".
Dialogue: 0,0:35:18.94,0:35:20.82,Default,,0,0,0,,Lần đầu gặp em,
Dialogue: 0,0:35:21.70,0:35:23.32,Default,,0,0,0,,{\i1}chị đã thấy vết sẹo trên tay em{\i0}
Dialogue: 0,0:35:24.07,0:35:26.37,Default,,0,0,0,,{\i1}và nhận ra có vẻ như em{\i0}
Dialogue: 0,0:35:26.45,0:35:29.62,Default,,0,0,0,,{\i1}đang hạn chế Kosei của mình\Ntrong sự kiện triển lãm.{\i0}
Dialogue: 0,0:35:30.25,0:35:31.37,Default,,0,0,0,,Đó là lúc chị nhận ra…
Dialogue: 0,0:35:31.96,0:35:35.75,Default,,0,0,0,,Có lẽ là do Kosei của em quá mạnh\Nđến mức cơ thể em không theo kịp.
Dialogue: 0,0:35:37.21,0:35:38.34,Default,,0,0,0,,{\i1}Chị ấy phát hiện ra rồi.{\i0}
Dialogue: 0,0:35:41.26,0:35:45.30,Default,,0,0,0,,Full Gauntlet đủ mạnh\Nđể chịu được ba cú đấm
Dialogue: 0,0:35:45.39,0:35:47.97,Default,,0,0,0,,với sức mạnh tối đa của chú Might.
Dialogue: 0,0:35:48.06,0:35:50.64,Default,,0,0,0,,Chị nghĩ em có thể\Ntung hết sức của mình với cái này.
Dialogue: 0,0:35:51.85,0:35:54.06,Default,,0,0,0,,Sức mạnh của em…
Dialogue: 0,0:35:54.73,0:35:56.27,Default,,0,0,0,,Deku, cho em đấy.
Dialogue: 0,0:35:56.36,0:35:59.49,Default,,0,0,0,,Ơ? Nhưng nó quan trọng với chị lắm mà?
Dialogue: 0,0:35:59.57,0:36:01.20,Default,,0,0,0,,Thế nên chị mới muốn em dùng nó.
Dialogue: 0,0:36:02.36,0:36:06.49,Default,,0,0,0,,Hãy trở thành một anh hùng tuyệt vời\Ncó thể cứu người gặp khó khăn nhé.
Dialogue: 0,0:36:10.25,0:36:11.08,Default,,0,0,0,,Vâng!
Dialogue: 0,0:36:17.34,0:36:18.17,Default,,0,0,0,,Alô?
Dialogue: 0,0:36:18.25,0:36:19.80,Default,,0,0,0,,{\i1}Cậu đang làm gì vậy, Midoriya?{\i0}
Dialogue: 0,0:36:19.88,0:36:21.67,Default,,0,0,0,,{\i1}Quá giờ tập trung lâu rồi!{\i0}
Dialogue: 0,0:36:21.76,0:36:22.68,Default,,0,0,0,,IIDA
Dialogue: 0,0:36:22.76,0:36:23.68,Default,,0,0,0,,{\i1}Cái gì?{\i0}
Dialogue: 0,0:36:31.23,0:36:32.64,Default,,0,0,0,,Tớ xin lỗi vì đã đến muộn!
Dialogue: 0,0:36:32.73,0:36:35.31,Default,,0,0,0,,Ủa? Khoan đã. Các bạn khác đâu rồi?
Dialogue: 0,0:36:35.40,0:36:36.65,Default,,0,0,0,,Họ vẫn chưa đến.
Dialogue: 0,0:36:36.73,0:36:38.65,Default,,0,0,0,,Họ nghĩ "đi theo nhóm" nghĩa là gì chứ?
Dialogue: 0,0:36:40.44,0:36:43.11,Default,,0,0,0,,Tớ xin lỗi vì đã đến muộn!
Dialogue: 0,0:36:43.20,0:36:45.20,Default,,0,0,0,,- Ồ!\N- Ồ!
Dialogue: 0,0:36:45.28,0:36:46.12,Default,,0,0,0,,- Ủa?\N- Ủa?
Dialogue: 0,0:36:47.37,0:36:49.87,Default,,0,0,0,,Tớ xin lỗi, nhưng Jiro…
Dialogue: 0,0:36:49.95,0:36:51.75,Default,,0,0,0,,- Ồ, phải thế chứ!\N- Phải thế chứ!
Dialogue: 0,0:36:52.96,0:36:56.04,Default,,0,0,0,,Tớ không quen ăn mặc thế này.
Dialogue: 0,0:36:56.63,0:36:58.54,Default,,0,0,0,,Người đẹp vì lụa mà nhỉ?
Dialogue: 0,0:36:58.63,0:37:00.25,Default,,0,0,0,,{\an8}Cậu ấy trông như nữ sát thủ vậy.
Dialogue: 0,0:37:01.42,0:37:02.76,Default,,0,0,0,,Im đi.
Dialogue: 0,0:37:02.84,0:37:04.88,Default,,0,0,0,,Sao có tớ nữa? Tớ khen cậu mà!
Dialogue: 0,0:37:04.97,0:37:05.84,Default,,0,0,0,,Không, không hề.
Dialogue: 0,0:37:05.93,0:37:08.35,Default,,0,0,0,,Đây là lần đầu tớ mặc quần áo trang trọng.
Dialogue: 0,0:37:08.43,0:37:09.85,Default,,0,0,0,,Tớ mượn của Yaoyorozu.
Dialogue: 0,0:37:09.93,0:37:11.89,Default,,0,0,0,,Trông hợp với cậu lắm.
Dialogue: 0,0:37:11.97,0:37:13.10,Default,,0,0,0,,Ừ, đẹp lắm!
Dialogue: 0,0:37:13.18,0:37:16.10,Default,,0,0,0,,Thôi đi, Deku!\NCậu không cần tâng bốc tớ đâu!
Dialogue: 0,0:37:16.19,0:37:17.48,Default,,0,0,0,,Uraraka?
Dialogue: 0,0:37:18.40,0:37:19.31,Default,,0,0,0,,- Ơ?\N- Ơ?
Dialogue: 0,0:37:19.40,0:37:20.44,Default,,0,0,0,,Ôi!
Dialogue: 0,0:37:20.52,0:37:22.78,Default,,0,0,0,,Các em đã đến rồi à?
Dialogue: 0,0:37:22.86,0:37:24.95,Default,,0,0,0,,Bữa tiệc bắt đầu rồi!
Dialogue: 0,0:37:25.70,0:37:27.74,Default,,0,0,0,,Nhân vật chính đến rồi kìa!
Dialogue: 0,0:37:27.82,0:37:31.87,Default,,0,0,0,,Chết rồi, Mineta. Tớ không chịu nổi nữa.\NPhải làm sao đây?
Dialogue: 0,0:37:31.95,0:37:33.66,Default,,0,0,0,,Muốn làm gì thì làm.
Dialogue: 0,0:37:35.58,0:37:36.50,Default,,0,0,0,,{\an8}KHÔNG KHÓA
Dialogue: 0,0:37:36.58,0:37:38.42,Default,,0,0,0,,{\an8}KHÓA
Dialogue: 0,0:37:38.50,0:37:39.88,Default,,0,0,0,,{\an8}TẦNG HAI THÁP TRUNG TÂM\NSẢNH TIẾP KHÁCH
Dialogue: 0,0:37:39.96,0:37:41.71,Default,,0,0,0,,{\an8}Kính thưa quý vị,
Dialogue: 0,0:37:41.80,0:37:45.84,Default,,0,0,0,,{\an8}chào mừng đến với\Ntiệc tiếp đãi của I-Expo.
Dialogue: 0,0:37:46.34,0:37:50.64,Default,,0,0,0,,Xin mời anh hùng hạng nhất All Might,\Nngười vừa đến góp vui hôm nay,
Dialogue: 0,0:37:51.39,0:37:53.52,Default,,0,0,0,,phát biểu vài lời\Nvà cùng ta nâng ly chúc mừng!
Dialogue: 0,0:37:54.10,0:37:56.52,Default,,0,0,0,,Quý vị hãy cho anh ấy một tràng pháo tay!
Dialogue: 0,0:37:57.44,0:37:59.44,Default,,0,0,0,,Xin mời lên sân khấu.
Dialogue: 0,0:38:00.06,0:38:02.15,Default,,0,0,0,,Dave, tớ không biết vụ này.
Dialogue: 0,0:38:02.65,0:38:05.32,Default,,0,0,0,,Đây là chuyện hiển nhiên\Nkhi họ biết cậu ở đây mà.
Dialogue: 0,0:38:05.40,0:38:06.45,Default,,0,0,0,,Trời ạ.
Dialogue: 0,0:38:06.53,0:38:08.45,Default,,0,0,0,,{\an8}KHÓA
Dialogue: 0,0:38:08.53,0:38:09.70,Default,,0,0,0,,{\an8}Không hay rồi.
Dialogue: 0,0:38:09.78,0:38:13.79,Default,,0,0,0,,Cả Bakugo và Kirishima đều không bắt máy.
Dialogue: 0,0:38:13.87,0:38:15.04,Default,,0,0,0,,Này.
Dialogue: 0,0:38:15.12,0:38:17.54,Default,,0,0,0,,Mày có chắc lối này là đúng không?
Dialogue: 0,0:38:17.62,0:38:20.13,Default,,0,0,0,,Tớ nghĩ là đúng.
Dialogue: 0,0:38:20.21,0:38:21.75,Default,,0,0,0,,Nghĩ à?
Dialogue: 0,0:38:21.84,0:38:24.88,Default,,0,0,0,,À thì tớ bỏ quên\Nđiện thoại trong phòng rồi.
Dialogue: 0,0:38:25.71,0:38:27.63,Default,,0,0,0,,TÁC VỤ HỆ THỐNG: KHÓA
Dialogue: 0,0:38:27.72,0:38:30.76,Default,,0,0,0,,Cảm ơn đã mời tôi. Tôi là All Might.
Dialogue: 0,0:38:30.84,0:38:32.22,Default,,0,0,0,,Tôi không thích dông dài nên…
Dialogue: 0,0:38:35.35,0:38:38.19,Default,,0,0,0,,{\i1}Đây là thông báo\Ntừ hệ thống an ninh I-Island.{\i0}
Dialogue: 0,0:38:38.77,0:38:41.10,Default,,0,0,0,,{\an8\i1}Chúng tôi nhận được báo cáo\Ntừ hệ thống an ninh rằng{\i0}
Dialogue: 0,0:38:41.19,0:38:43.61,Default,,0,0,0,,{\i1}một thiết bị gây nổ đã được cài vào…{\i0}
Dialogue: 0,0:38:43.69,0:38:45.23,Default,,0,0,0,,- Một quả bom ư?\N- Thật luôn à?
Dialogue: 0,0:38:45.32,0:38:49.91,Default,,0,0,0,,{\an8\i1}Hiện tại I-Island sẽ chuyển sang\Nchế độ báo động cấp cao.{\i0}
Dialogue: 0,0:38:50.41,0:38:52.66,Default,,0,0,0,,{\an8\i1}Người dân và du khách\Nsống trong thành phố…{\i0}
Dialogue: 0,0:38:52.74,0:38:54.54,Default,,0,0,0,,{\an8}- Không thể nào.\N- Mọi chuyện sẽ ổn chứ?
Dialogue: 0,0:38:55.16,0:38:57.20,Default,,0,0,0,,Chỉ cần làm theo hướng dẫn\Nlà sẽ an toàn thôi.
Dialogue: 0,0:38:57.29,0:38:58.29,Default,,0,0,0,,Cậu nói đúng.
Dialogue: 0,0:38:59.00,0:39:00.79,Default,,0,0,0,,{\i1}Bất cứ ai còn ở bên ngoài mười phút sau{\i0}
Dialogue: 0,0:39:00.87,0:39:02.79,Default,,0,0,0,,{\i1}có thể sẽ bị bắt mà không báo trước.{\i0}
Dialogue: 0,0:39:03.34,0:39:05.00,Default,,0,0,0,,- Mau về phòng thôi.\N- Ừ.
Dialogue: 0,0:39:06.17,0:39:10.80,Default,,0,0,0,,{\i1}Ngoài ra, các tòa nhà chính\Nsẽ bị hệ thống an ninh phong tỏa.{\i0}
Dialogue: 0,0:39:11.55,0:39:14.43,Default,,0,0,0,,{\i1}Xin nhắc lại. Hệ thống an ninh…{\i0}
Dialogue: 0,0:39:18.43,0:39:20.23,Default,,0,0,0,,Như các ngươi vừa nghe đấy.
Dialogue: 0,0:39:20.31,0:39:22.94,Default,,0,0,0,,Bọn ta đã khống chế hệ thống an ninh.
Dialogue: 0,0:39:23.94,0:39:26.11,Default,,0,0,0,,Đừng cố phản kháng.
Dialogue: 0,0:39:26.69,0:39:28.49,Default,,0,0,0,,Nếu các ngươi phản kháng…
Dialogue: 0,0:39:30.07,0:39:31.32,Default,,0,0,0,,Các người máy an ninh
Dialogue: 0,0:39:31.82,0:39:35.41,Default,,0,0,0,,{\i1}sẽ tấn công\Nnhững người dân vô tội trên đảo này.{\i0}
Dialogue: 0,0:39:36.74,0:39:37.58,Default,,0,0,0,,Đúng vậy đấy.
Dialogue: 0,0:39:37.66,0:39:41.67,Default,,0,0,0,,Toàn bộ người dân trên đảo này\Nđều là con tin.
Dialogue: 0,0:39:42.50,0:39:45.13,Default,,0,0,0,,Bao gồm cả các ngươi nữa đấy.
Dialogue: 0,0:39:46.55,0:39:47.92,Default,,0,0,0,,{\an8}Làm đi.
Dialogue: 0,0:39:53.05,0:39:54.55,Default,,0,0,0,,Thiết bị khống chế an ninh!
Dialogue: 0,0:39:54.64,0:39:55.55,Default,,0,0,0,,Không hay rồi!
Dialogue: 0,0:39:57.51,0:39:58.64,Default,,0,0,0,,Cấm cử động.
Dialogue: 0,0:39:59.43,0:40:02.73,Default,,0,0,0,,Ngươi mà cử động dù chỉ một bước,\Nta sẽ lập tức giết hết mọi người ở đây.
Dialogue: 0,0:40:02.81,0:40:03.73,Default,,0,0,0,,Chết tiệt!
Dialogue: 0,0:40:05.40,0:40:06.90,Default,,0,0,0,,Ngoan lắm.
Dialogue: 0,0:40:08.40,0:40:11.70,Default,,0,0,0,,Các ngươi hãy học hỏi All Might\Nvà ngừng chống trả đi.
Dialogue: 0,0:40:13.28,0:40:15.82,Default,,0,0,0,,{\i1}Toshi, chúng ta chỉ còn cách\Nnghe theo chúng thôi.{\i0}
Dialogue: 0,0:40:16.41,0:40:17.53,Default,,0,0,0,,{\i1}Nhưng Dave…{\i0}
Dialogue: 0,0:40:18.20,0:40:20.79,Default,,0,0,0,,{\i1}Chắc chắn tớ sẽ nghĩ ra\Ncách cứu mọi người.{\i0}
Dialogue: 0,0:40:20.87,0:40:23.00,Default,,0,0,0,,{\i1}Cố chịu đựng đến lúc đó nhé.{\i0}
Dialogue: 0,0:40:25.42,0:40:26.92,Default,,0,0,0,,Điện thoại mất sóng.
Dialogue: 0,0:40:27.42,0:40:30.21,Default,,0,0,0,,Có vẻ như mọi cách thức liên lạc\Nđều bị chặn rồi.
Dialogue: 0,0:40:30.30,0:40:31.97,Default,,0,0,0,,Thật sao?
Dialogue: 0,0:40:32.05,0:40:33.72,Default,,0,0,0,,Thang máy cũng không hoạt động.
Dialogue: 0,0:40:33.80,0:40:34.93,Default,,0,0,0,,Cậu nói thật đấy à?
Dialogue: 0,0:40:35.59,0:40:37.89,Default,,0,0,0,,Không thể tin được hệ thống an ninh
Dialogue: 0,0:40:37.97,0:40:40.93,Default,,0,0,0,,sẽ chuyển sang chế độ báo động cấp cao\Nchỉ vì có bom được cài vào.
Dialogue: 0,0:40:41.02,0:40:42.14,Default,,0,0,0,,Iida.
Dialogue: 0,0:40:42.89,0:40:44.31,Default,,0,0,0,,Ta đến nơi tổ chức tiệc thôi.
Dialogue: 0,0:40:44.39,0:40:45.44,Default,,0,0,0,,Tại sao?
Dialogue: 0,0:40:45.52,0:40:47.69,Default,,0,0,0,,Vì All Might có mặt ở bữa tiệc.
Dialogue: 0,0:40:47.77,0:40:48.82,Default,,0,0,0,,All Might cũng ở đó à?
Dialogue: 0,0:40:48.90,0:40:51.90,Default,,0,0,0,,Vậy thì không có gì phải lo cả.
Dialogue: 0,0:40:52.57,0:40:56.03,Default,,0,0,0,,Melissa, chị có thể\Nđưa bọn em đến chỗ bữa tiệc không?
Dialogue: 0,0:40:56.53,0:41:00.66,Default,,0,0,0,,Chị nghĩ nếu dùng cầu thang thoát hiểm\Nthì chúng ta có thể đến gần sảnh.
Dialogue: 0,0:41:01.20,0:41:02.79,Default,,0,0,0,,Phiền chị dẫn đường ạ!
Dialogue: 0,0:41:03.75,0:41:07.88,Default,,0,0,0,,Điện thoại không có sóng,\Ncũng không thể lên mạng.
Dialogue: 0,0:41:07.96,0:41:10.13,Default,,0,0,0,,Chúng ta cũng không xem TV được luôn.
Dialogue: 0,0:41:10.63,0:41:11.46,Default,,0,0,0,,Biết làm sao được.
Dialogue: 0,0:41:11.55,0:41:14.63,Default,,0,0,0,,Hay chúng ta chơi Uno\Ntrong lúc chờ họ tìm ra bom nhé?
Dialogue: 0,0:41:15.84,0:41:16.84,Default,,0,0,0,,Đừng lo.
Dialogue: 0,0:41:17.43,0:41:19.85,Default,,0,0,0,,Nếu các ngươi giữ im lặng\Nthì sẽ không bị thương.
Dialogue: 0,0:41:20.60,0:41:23.23,Default,,0,0,0,,Bọn ta đã chuẩn bị\Nđể thả các ngươi khi đến lúc.
Dialogue: 0,0:41:23.31,0:41:25.10,Default,,0,0,0,,Mục đích của các ngươi là gì?
Dialogue: 0,0:41:26.69,0:41:28.36,Default,,0,0,0,,Không nghe thấy à?
Dialogue: 0,0:41:28.44,0:41:29.77,Default,,0,0,0,,Đã bảo im lặng mà.
Dialogue: 0,0:41:32.36,0:41:34.28,Default,,0,0,0,,Vậy sao? Hiểu rồi.
Dialogue: 0,0:41:38.91,0:41:40.95,Default,,0,0,0,,Ngươi là nhà nghiên cứu ở đây phải không?
Dialogue: 0,0:41:41.03,0:41:42.37,Default,,0,0,0,,Vâng.
Dialogue: 0,0:41:42.45,0:41:43.29,Default,,0,0,0,,Đưa hắn đi.
Dialogue: 0,0:41:43.37,0:41:44.45,Default,,0,0,0,,Các người làm gì vậy?
Dialogue: 0,0:41:44.54,0:41:45.66,Default,,0,0,0,,Dừng lại!
Dialogue: 0,0:41:46.46,0:41:48.29,Default,,0,0,0,,Anh ấy là trợ lý của tôi.
Dialogue: 0,0:41:48.38,0:41:49.63,Default,,0,0,0,,Anh định làm gì với anh ấy?
Dialogue: 0,0:41:49.71,0:41:50.54,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:41:51.38,0:41:53.09,Default,,0,0,0,,Chẳng phải David Shield đây sao?
Dialogue: 0,0:41:53.71,0:41:55.05,Default,,0,0,0,,Ngươi cũng đi theo.
Dialogue: 0,0:41:55.13,0:41:56.51,Default,,0,0,0,,Nếu tôi từ chối thì sao?
Dialogue: 0,0:41:57.18,0:42:01.35,Default,,0,0,0,,Vậy ngươi sẽ nghe thấy tiếng ai đó\Nhét lên đâu đó trên đảo này.
Dialogue: 0,0:42:03.35,0:42:05.35,Default,,0,0,0,,Được. Tôi sẽ đi.
Dialogue: 0,0:42:07.69,0:42:08.98,Default,,0,0,0,,{\i1}Dave…{\i0}
Dialogue: 0,0:42:10.36,0:42:14.07,Default,,0,0,0,,{\i1}Mình có thể đánh bại lũ villain\Nvà khôi phục lại hệ thống an ninh không?{\i0}
Dialogue: 0,0:42:14.15,0:42:16.03,Default,,0,0,0,,{\i1}Với cơ thể này ư?{\i0}
Dialogue: 0,0:42:16.90,0:42:19.49,Default,,0,0,0,,{\i1}Không, mình phải làm.{\i0}
Dialogue: 0,0:42:19.57,0:42:22.62,Default,,0,0,0,,{\i1}Vì mình là Biểu tượng Hòa bình!{\i0}
Dialogue: 0,0:42:26.29,0:42:28.37,Default,,0,0,0,,{\i1}Nhóc Midoriya.{\i0}
Dialogue: 0,0:42:29.04,0:42:30.63,Default,,0,0,0,,All Might thấy chúng ta rồi.
Dialogue: 0,0:42:30.71,0:42:32.21,Default,,0,0,0,,Jiro, cậu làm được chứ?
Dialogue: 0,0:42:32.29,0:42:33.13,Default,,0,0,0,,Ừ.
Dialogue: 0,0:42:33.71,0:42:35.46,Default,,0,0,0,,Chú nói gì đi.
Dialogue: 0,0:42:35.55,0:42:36.51,Default,,0,0,0,,Bọn cháu đang nghe!
Dialogue: 0,0:42:37.22,0:42:38.38,Default,,0,0,0,,Cháu có nghe thấy không?
Dialogue: 0,0:42:38.47,0:42:42.35,Default,,0,0,0,,Lũ villain đã chiếm lấy tòa tháp.\NChúng khống chế hệ thống an ninh
Dialogue: 0,0:42:42.43,0:42:45.43,Default,,0,0,0,,và mọi người trên đảo này đều là con tin.
Dialogue: 0,0:42:46.22,0:42:48.52,Default,,0,0,0,,{\i1}Tất cả các anh hùng cũng đã bị bắt.{\i0}
Dialogue: 0,0:42:48.60,0:42:51.02,Default,,0,0,0,,{\i1}Nguy hiểm lắm.\NRời khỏi đây càng sớm càng tốt đi.{\i0}
Dialogue: 0,0:42:52.11,0:42:53.44,Default,,0,0,0,,Không hay rồi, Midoriya.
Dialogue: 0,0:42:57.78,0:43:00.16,Default,,0,0,0,,{\i1}Chúng ta đã nhận được\Nlời nhắn của All Might.{\i0}
Dialogue: 0,0:43:00.74,0:43:04.16,Default,,0,0,0,,Tớ đề nghị chúng ta\Ntuân theo chỉ dẫn của All Might,
Dialogue: 0,0:43:04.74,0:43:07.04,Default,,0,0,0,,giáo viên của chúng ta ở UA,\Nvà thoát khỏi nơi này.
Dialogue: 0,0:43:07.54,0:43:09.46,Default,,0,0,0,,Tớ đồng ý với ý kiến của Iida,
Dialogue: 0,0:43:09.96,0:43:11.54,Default,,0,0,0,,Chúng ta vẫn còn là học sinh.
Dialogue: 0,0:43:11.63,0:43:14.63,Default,,0,0,0,,Ta không thể đấu với villain\Nmà không có giấy phép anh hùng.
Dialogue: 0,0:43:14.71,0:43:17.76,Default,,0,0,0,,Hay chúng ta trốn ra\Nvà báo cho các anh hùng ở bên ngoài?
Dialogue: 0,0:43:18.34,0:43:20.34,Default,,0,0,0,,Chị nghĩ là sẽ rất khó để trốn thoát.
Dialogue: 0,0:43:21.01,0:43:23.35,Default,,0,0,0,,Nơi này được xây dựng\Nvới mức độ an ninh tương đương
Dialogue: 0,0:43:23.43,0:43:26.68,Default,,0,0,0,,với Tartarus,\Nnơi bọn villain trọng phạm bị giam giữ.
Dialogue: 0,0:43:27.27,0:43:30.14,Default,,0,0,0,,Vậy chúng ta chỉ có thể đợi\Nđến khi có tiếp viện.
Dialogue: 0,0:43:30.64,0:43:32.90,Default,,0,0,0,,Kaminari, cậu chấp nhận như thế à?
Dialogue: 0,0:43:32.98,0:43:33.81,Default,,0,0,0,,Ý cậu là sao?
Dialogue: 0,0:43:34.44,0:43:36.27,Default,,0,0,0,,Cậu không nghĩ là ta nên đi giúp họ à?
Dialogue: 0,0:43:36.36,0:43:39.57,Default,,0,0,0,,Này, thôi nào!\NLũ villain đã bắt được All Might đấy!
Dialogue: 0,0:43:39.65,0:43:42.82,Default,,0,0,0,,Chúng ta không thể nào cứu họ\Nbằng chính sức mình được!
Dialogue: 0,0:43:44.70,0:43:46.91,Default,,0,0,0,,Chúng ta là những người\Nkhao khát làm anh hùng.
Dialogue: 0,0:43:46.99,0:43:49.87,Default,,0,0,0,,Phải, nhưng chúng ta\Nvẫn chưa được phép làm anh hùng!
Dialogue: 0,0:43:49.96,0:43:53.83,Default,,0,0,0,,Dù thế chúng ta vẫn không làm gì à?
Dialogue: 0,0:43:53.92,0:43:56.38,Default,,0,0,0,,Chuyện đó…
Dialogue: 0,0:43:59.59,0:44:00.67,Default,,0,0,0,,Tớ muốn cứu họ.
Dialogue: 0,0:44:02.09,0:44:03.14,Default,,0,0,0,,Deku?
Dialogue: 0,0:44:04.55,0:44:05.89,Default,,0,0,0,,Tớ muốn đi cứu họ.
Dialogue: 0,0:44:05.97,0:44:07.97,Default,,0,0,0,,Cậu định đánh nhau với lũ villain à?
Dialogue: 0,0:44:08.06,0:44:10.35,Default,,0,0,0,,Cậu vẫn chưa rút ra bài học\Nsau vụ USJ à, Midoriya?
Dialogue: 0,0:44:10.43,0:44:12.35,Default,,0,0,0,,Không phải đâu, Mineta!
Dialogue: 0,0:44:12.44,0:44:13.81,Default,,0,0,0,,Tớ đã nghĩ kỹ rồi.
Dialogue: 0,0:44:14.60,0:44:16.19,Default,,0,0,0,,Ta cần phải tìm cách cứu All Might
Dialogue: 0,0:44:16.27,0:44:19.65,Default,,0,0,0,,và những người khác mà không phải\Nđánh nhau với lũ villain!
Dialogue: 0,0:44:20.15,0:44:22.70,Default,,0,0,0,,Tớ hiểu cảm giác của cậu.\NNhưng ta có làm được không?
Dialogue: 0,0:44:22.78,0:44:24.32,Default,,0,0,0,,Dù có khó khăn tớ vẫn muốn thử!
Dialogue: 0,0:44:25.24,0:44:28.58,Default,,0,0,0,,Tớ muốn tìm ra giải pháp tốt nhất có thể
Dialogue: 0,0:44:28.66,0:44:30.41,Default,,0,0,0,,và cứu mọi người!
Dialogue: 0,0:44:32.41,0:44:33.87,Default,,0,0,0,,Deku…
Dialogue: 0,0:44:34.37,0:44:38.38,Default,,0,0,0,,Hệ thống an ninh của I-Island\Nnằm ở tầng cao nhất của tòa tháp này.
Dialogue: 0,0:44:39.55,0:44:41.88,Default,,0,0,0,,Nếu lũ villain đã kiểm soát hệ thống,
Dialogue: 0,0:44:41.97,0:44:44.97,Default,,0,0,0,,vậy hẳn là khóa xác thực và mật khẩu\Nđã bị vô hiệu hóa.
Dialogue: 0,0:44:45.93,0:44:48.43,Default,,0,0,0,,{\i1}Chúng ta có thể khởi động lại hệ thống.{\i0}
Dialogue: 0,0:44:49.31,0:44:52.93,Default,,0,0,0,,{\i1}Chỉ cần tránh bị lũ villain phát hiện\Nvà lên tầng cao nhất là được.{\i0}
Dialogue: 0,0:44:53.89,0:44:56.02,Default,,0,0,0,,Và rồi có lẽ\Nchúng ta có thể cứu mọi người.
Dialogue: 0,0:44:57.11,0:44:58.61,Default,,0,0,0,,Chị Melissa…
Dialogue: 0,0:44:59.27,0:45:01.23,Default,,0,0,0,,Làm sao để tránh bị phát hiện?
Dialogue: 0,0:45:01.74,0:45:04.24,Default,,0,0,0,,Hiện tại\Nkhông ai trong chúng ta bị thương.
Dialogue: 0,0:45:04.32,0:45:07.37,Default,,0,0,0,,Chị nghĩ lũ villain vẫn chưa quen\Nvới hệ thống an ninh.
Dialogue: 0,0:45:07.87,0:45:10.95,Default,,0,0,0,,Tránh xung đột\Nvà khiến hệ thống trở lại bình thường?
Dialogue: 0,0:45:11.45,0:45:12.29,Default,,0,0,0,,Tôi hiểu rồi.
Dialogue: 0,0:45:12.37,0:45:14.96,Default,,0,0,0,,- Cậu có nghĩ là được không?\N- Được chứ!
Dialogue: 0,0:45:15.62,0:45:18.59,Default,,0,0,0,,Nhưng lũ villain đang ở tầng trên cùng.
Dialogue: 0,0:45:19.13,0:45:20.42,Default,,0,0,0,,Không cần giao chiến đâu!
Dialogue: 0,0:45:20.50,0:45:21.34,Default,,0,0,0,,Hả?
Dialogue: 0,0:45:21.42,0:45:23.38,Default,,0,0,0,,Khi hệ thống trở lại bình thường,
Dialogue: 0,0:45:23.47,0:45:26.09,Default,,0,0,0,,All Might các con tin sẽ được thả ra.
Dialogue: 0,0:45:26.18,0:45:29.64,Default,,0,0,0,,Đến lúc đó\Nthế cục sẽ thay đổi trong tức khắc!
Dialogue: 0,0:45:31.51,0:45:32.85,Default,,0,0,0,,Deku, đi thôi!
Dialogue: 0,0:45:32.93,0:45:34.56,Default,,0,0,0,,Uraraka!
Dialogue: 0,0:45:34.64,0:45:36.60,Default,,0,0,0,,Tớ không muốn ngồi bàng quan
Dialogue: 0,0:45:36.69,0:45:38.27,Default,,0,0,0,,khi chúng ta có thể làm gì đó.
Dialogue: 0,0:45:38.90,0:45:42.36,Default,,0,0,0,,Không quan trọng\Ntụi mình có là anh hùng hay không!
Dialogue: 0,0:45:43.11,0:45:45.99,Default,,0,0,0,,Phải, hãy giúp những ai cần giúp!
Dialogue: 0,0:45:46.07,0:45:48.24,Default,,0,0,0,,Hãy làm những việc con người nên làm!
Dialogue: 0,0:45:48.32,0:45:49.20,Default,,0,0,0,,Ừ!
Dialogue: 0,0:45:49.28,0:45:50.45,Default,,0,0,0,,Midoriya.
Dialogue: 0,0:45:51.62,0:45:52.45,Default,,0,0,0,,Tôi cũng đi.
Dialogue: 0,0:45:52.54,0:45:53.41,Default,,0,0,0,,Cả tớ nữa.
Dialogue: 0,0:45:53.50,0:45:54.96,Default,,0,0,0,,Todoroki!
Dialogue: 0,0:45:55.04,0:45:56.21,Default,,0,0,0,,Kyoka!
Dialogue: 0,0:45:56.29,0:45:58.79,Default,,0,0,0,,Tớ sẽ cản mọi người lại\Nnếu thấy rằng ta đã làm hết sức.
Dialogue: 0,0:45:59.46,0:46:02.13,Default,,0,0,0,,Nếu cậu có thể chấp nhận thì tớ cũng đi!
Dialogue: 0,0:46:02.21,0:46:03.05,Default,,0,0,0,,Iida!
Dialogue: 0,0:46:03.13,0:46:05.30,Default,,0,0,0,,Nếu vậy thì tính cả tớ nữa!
Dialogue: 0,0:46:05.38,0:46:06.76,Default,,0,0,0,,Được rồi, tớ cũng đi!
Dialogue: 0,0:46:06.84,0:46:08.01,Default,,0,0,0,,Yaoyorozu!
Dialogue: 0,0:46:08.09,0:46:09.09,Default,,0,0,0,,Kaminari!
Dialogue: 0,0:46:09.18,0:46:12.72,Default,,0,0,0,,Được rồi, tớ hiểu rồi!\NTớ cũng phải đi chứ gì?
Dialogue: 0,0:46:12.81,0:46:14.22,Default,,0,0,0,,Cảm ơn cậu, Mineta!
Dialogue: 0,0:46:14.31,0:46:17.69,Default,,0,0,0,,- Chiến thôi, Mineta!\N- Cùng cố gắng nhé, Mineta!
Dialogue: 0,0:46:18.81,0:46:20.86,Default,,0,0,0,,Melissa, chị hãy đợi ở đây.
Dialogue: 0,0:46:20.94,0:46:22.11,Default,,0,0,0,,Chị cũng đi.
Dialogue: 0,0:46:22.19,0:46:25.49,Default,,0,0,0,,Nhưng chị không có Kosei.
Dialogue: 0,0:46:25.57,0:46:26.65,Default,,0,0,0,,Các em có ai
Dialogue: 0,0:46:26.74,0:46:29.41,Default,,0,0,0,,có thể thay đổi thiết lập\Ncủa hệ thống an ninh không?
Dialogue: 0,0:46:30.95,0:46:32.87,Default,,0,0,0,,Chị là học sinh của Học viện.
Dialogue: 0,0:46:32.95,0:46:35.04,Default,,0,0,0,,- Chị nghĩ mình có thể giúp được.\N- Nhưng…
Dialogue: 0,0:46:36.04,0:46:39.92,Default,,0,0,0,,Có thể chị sẽ gây cản trở các em\Ncho đến khi chúng ta đến tầng cao nhất.
Dialogue: 0,0:46:40.50,0:46:42.46,Default,,0,0,0,,Nhưng hãy để chị được giúp đỡ mọi người.
Dialogue: 0,0:46:43.55,0:46:44.55,Default,,0,0,0,,Xin các em đấy!
Dialogue: 0,0:46:47.42,0:46:48.34,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:46:49.59,0:46:52.18,Default,,0,0,0,,Đi cứu mọi người nào!
Dialogue: 0,0:46:54.47,0:46:55.72,Default,,0,0,0,,Ừ!
Dialogue: 0,0:47:00.94,0:47:02.15,Default,,0,0,0,,{\i1}Nhóc Midoriya?{\i0}
Dialogue: 0,0:47:03.69,0:47:04.98,Default,,0,0,0,,{\i1}Ánh mắt đó…{\i0}
Dialogue: 0,0:47:05.07,0:47:05.90,Default,,0,0,0,,{\i1}Đừng nói là…{\i0}
Dialogue: 0,0:47:06.61,0:47:07.94,Default,,0,0,0,,{\i1}Không, chạy đi!{\i0}
Dialogue: 0,0:47:08.53,0:47:10.07,Default,,0,0,0,,{\i1}Không, cháu sẽ không bỏ chạy.{\i0}
Dialogue: 0,0:47:10.16,0:47:11.74,Default,,0,0,0,,{\i1}Nguy hiểm lắm.{\i0}
Dialogue: 0,0:47:11.82,0:47:14.58,Default,,0,0,0,,{\i1}Cháu muốn làm những gì mình có thể!{\i0}
Dialogue: 0,0:47:15.66,0:47:17.20,Default,,0,0,0,,{\i1}Nhóc Midoriya…{\i0}
Dialogue: 0,0:47:17.29,0:47:19.21,Default,,0,0,0,,{\i1}Chắc chắn cháu sẽ cứu được mọi người!{\i0}
Dialogue: 0,0:47:21.08,0:47:22.63,Default,,0,0,0,,{\i1}Chắc hẳn nhóc ấy sẽ đi nhỉ?{\i0}
Dialogue: 0,0:47:22.71,0:47:27.13,Default,,0,0,0,,{\i1}Là giáo viên, mình biết là\Nmình phải trách mắng hành vi của nhóc ấy.{\i0}
Dialogue: 0,0:47:28.59,0:47:33.55,Default,,0,0,0,,{\i1}Nhưng nếu bây giờ cháu không hành động,\Nvậy thì cháu không phải anh hùng.{\i0}
Dialogue: 0,0:47:34.39,0:47:36.39,Default,,0,0,0,,{\i1}Nếu thế, ta sẽ đợi.{\i0}
Dialogue: 0,0:47:37.02,0:47:41.23,Default,,0,0,0,,{\i1}Ta tin rằng mấy đứa có thể cứu mọi người.{\i0}
Dialogue: 0,0:47:41.94,0:47:44.48,Default,,0,0,0,,{\i1}Trông cậy vào mấy đứa đấy, các mầm non!{\i0}
Dialogue: 0,0:47:44.56,0:47:46.02,Default,,0,0,0,,Đi thôi!
Dialogue: 0,0:47:46.11,0:47:47.03,Default,,0,0,0,,- Được!\N- Được!
Dialogue: 0,0:47:50.53,0:47:53.62,Default,,0,0,0,,{\an8}TẦNG TRÊN CÙNG THÁP TRUNG TÂM\NPHÒNG ĐIỀU KHIỂN CHÍNH
Dialogue: 0,0:47:56.66,0:47:58.33,Default,,0,0,0,,Tao đưa hắn đến rồi.
Dialogue: 0,0:47:58.41,0:48:00.58,Default,,0,0,0,,Mở khóa nhanh lên.
Dialogue: 0,0:48:00.66,0:48:02.33,Default,,0,0,0,,Cứ ra vẻ cao ngạo…
Dialogue: 0,0:48:03.42,0:48:04.67,Default,,0,0,0,,Ngươi nghe rồi đấy.
Dialogue: 0,0:48:05.17,0:48:07.34,Default,,0,0,0,,Nếu không muốn chết thì làm ngay đi.
Dialogue: 0,0:48:08.34,0:48:09.71,Default,,0,0,0,,Tôi hiểu rồi.
Dialogue: 0,0:48:09.80,0:48:11.13,Default,,0,0,0,,Đưa hắn đi.
Dialogue: 0,0:48:11.63,0:48:12.47,Default,,0,0,0,,Ra ngoài.
Dialogue: 0,0:48:13.47,0:48:15.89,Default,,0,0,0,,{\an8}CÙNG LÚC ĐÓ\NCẦU THANG THOÁT HIỂM
Dialogue: 0,0:48:18.81,0:48:19.81,Default,,0,0,0,,Đây là tầng 30.
Dialogue: 0,0:48:19.89,0:48:21.52,Default,,0,0,0,,Chị Melissa, cao nhất là bao nhiêu?
Dialogue: 0,0:48:23.48,0:48:24.31,Default,,0,0,0,,Tầng 200.
Dialogue: 0,0:48:24.40,0:48:25.23,Default,,0,0,0,,Thật hả trời?
Dialogue: 0,0:48:25.31,0:48:27.11,Default,,0,0,0,,Chúng ta phải leo cao thế ư?
Dialogue: 0,0:48:27.61,0:48:29.48,Default,,0,0,0,,Vẫn đỡ hơn là gặp phải villain.
Dialogue: 0,0:48:36.24,0:48:39.08,Default,,0,0,0,,Chị Melissa,\Ncó cần em dùng Kosei giúp chị không?
Dialogue: 0,0:48:39.58,0:48:41.62,Default,,0,0,0,,Cảm ơn em, nhưng chị không cần đâu.
Dialogue: 0,0:48:42.25,0:48:45.54,Default,,0,0,0,,Em cứ giữ sức cho lúc cần thiết đi.
Dialogue: 0,0:48:54.34,0:48:55.72,Default,,0,0,0,,Cửa chớp!
Dialogue: 0,0:48:56.22,0:48:58.72,Default,,0,0,0,,Chúng ta nên làm gì đây? Phá cửa?
Dialogue: 0,0:48:58.81,0:49:01.02,Default,,0,0,0,,Nếu làm thế\Nthì hệ thống an ninh sẽ phản ứng
Dialogue: 0,0:49:01.10,0:49:02.98,Default,,0,0,0,,và lũ villain sẽ chú ý đến chúng ta.
Dialogue: 0,0:49:03.06,0:49:05.19,Default,,0,0,0,,Chúng ta không thể đi lối này à?
Dialogue: 0,0:49:05.27,0:49:07.02,Default,,0,0,0,,- Mineta!\N- Đừng!
Dialogue: 0,0:49:07.81,0:49:09.19,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:49:09.27,0:49:10.19,Default,,0,0,0,,Sao thế?
Dialogue: 0,0:49:10.28,0:49:12.07,Default,,0,0,0,,Cửa ở tầng 80 mở ra à?
Dialogue: 0,0:49:12.69,0:49:15.20,Default,,0,0,0,,Này, mày có nhầm lẫn\Ntrong lúc quét các tầng không?
Dialogue: 0,0:49:17.87,0:49:20.12,Default,,0,0,0,,Hạ tất cả vách ngăn trên tầng 80.
Dialogue: 0,0:49:20.20,0:49:21.74,Default,,0,0,0,,Đừng để chúng trốn thoát.
Dialogue: 0,0:49:21.83,0:49:22.70,Default,,0,0,0,,{\i1}Rõ.{\i0}
Dialogue: 0,0:49:25.21,0:49:26.71,Default,,0,0,0,,{\i1}Mấy đứa cẩn thận nhé.{\i0}
Dialogue: 0,0:49:27.33,0:49:29.21,Default,,0,0,0,,{\i1}Lũ villain rất xảo quyệt.{\i0}
Dialogue: 0,0:49:31.38,0:49:32.67,Default,,0,0,0,,Có lối khác để đi lên không?
Dialogue: 0,0:49:32.76,0:49:36.01,Default,,0,0,0,,Có một cầu thang thoát hiểm khác\Ngiống thế này ở phía đối diện.
Dialogue: 0,0:49:36.09,0:49:37.01,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,0:49:39.51,0:49:40.85,Default,,0,0,0,,Cửa chớp…
Dialogue: 0,0:49:41.35,0:49:43.22,Default,,0,0,0,,Cửa phía sau cũng đóng rồi!
Dialogue: 0,0:49:43.31,0:49:45.10,Default,,0,0,0,,- Todorok!\N- Ừ!
Dialogue: 0,0:49:52.86,0:49:54.36,Default,,0,0,0,,Vào trong này!
Dialogue: 0,0:49:54.44,0:49:55.61,Default,,0,0,0,,{\an8}TẦNG 80, NHÀ MÁY THỰC VẬT
Dialogue: 0,0:49:55.70,0:49:57.74,Default,,0,0,0,,{\an8}Đây là đâu?
Dialogue: 0,0:49:57.82,0:49:59.07,Default,,0,0,0,,{\an8}Nhà máy thực vật.
Dialogue: 0,0:49:59.16,0:50:01.53,Default,,0,0,0,,Nơi nghiên cứu\Ncách Kosei ảnh hưởng lên thực vật.
Dialogue: 0,0:50:01.62,0:50:03.49,Default,,0,0,0,,Khoan đã! Nhìn kìa!
Dialogue: 0,0:50:03.58,0:50:05.33,Default,,0,0,0,,Thang máy đang đi lên!
Dialogue: 0,0:50:05.83,0:50:07.75,Default,,0,0,0,,Lũ villain đuổi theo chúng ta à?
Dialogue: 0,0:50:07.83,0:50:09.38,Default,,0,0,0,,Trốn đi và chờ chúng đi qua!
Dialogue: 0,0:50:13.09,0:50:16.55,Default,,0,0,0,,Không biết có thể dùng thang máy đó\Nđể lên tầng thượng không nhỉ?
Dialogue: 0,0:50:16.63,0:50:17.51,Default,,0,0,0,,Không được đâu.
Dialogue: 0,0:50:17.59,0:50:21.22,Default,,0,0,0,,{\i1}Chỉ có nhân viên có thẩm quyền\Nmới được vận hành thang máy đó.{\i0}
Dialogue: 0,0:50:21.30,0:50:24.85,Default,,0,0,0,,{\i1}Và nó kiên cố như một hầm trú bom\Nnên chúng ta cũng không thề phá hủy nó.{\i0}
Dialogue: 0,0:50:24.93,0:50:27.35,Default,,0,0,0,,{\an8}Thôi nào, cho bọn ta\Ntận dụng tiện ích hiện đại đi mà!
Dialogue: 0,0:50:32.15,0:50:33.36,Default,,0,0,0,,Bộ đồ đó…
Dialogue: 0,0:50:33.44,0:50:34.94,Default,,0,0,0,,Lũ villain này có mặt ở bữa tiệc.
Dialogue: 0,0:50:35.53,0:50:37.57,Default,,0,0,0,,Họ bảo là bọn nhóc ở đây.
Dialogue: 0,0:50:37.65,0:50:39.82,Default,,0,0,0,,Chúng đến một chỗ phiền phức đấy.
Dialogue: 0,0:50:39.91,0:50:41.03,Default,,0,0,0,,Bọn chúng đang đến!
Dialogue: 0,0:50:41.12,0:50:41.95,Default,,0,0,0,,Im lặng.
Dialogue: 0,0:50:42.03,0:50:44.49,Default,,0,0,0,,{\i1}Đừng đến.{\i0}
Dialogue: 0,0:50:44.58,0:50:46.25,Default,,0,0,0,,{\i1}Sang bên kia đi.{\i0}
Dialogue: 0,0:50:46.33,0:50:47.16,Default,,0,0,0,,{\i1}Thôi nào!{\i0}
Dialogue: 0,0:50:47.25,0:50:50.04,Default,,0,0,0,,{\i1}Đừng chú ý đến bọn tôi!{\i0}
Dialogue: 0,0:50:51.75,0:50:53.84,Default,,0,0,0,,Tìm thấy chúng mày rồi,\Nbọn nhóc chết tiệt!
Dialogue: 0,0:50:53.92,0:50:56.13,Default,,0,0,0,,{\i1}Phải làm sao đây?{\i0}
Dialogue: 0,0:50:56.21,0:50:57.55,Default,,0,0,0,,{\i1}Phải làm sao đây?{\i0}
Dialogue: 0,0:50:58.30,0:51:01.43,Default,,0,0,0,,Hả? Mày vừa nói gì đấy, thằng khốn?
Dialogue: 0,0:51:01.51,0:51:03.14,Default,,0,0,0,,{\i1}Kacchan? Cả Kirishima luôn?{\i0}
Dialogue: 0,0:51:03.22,0:51:05.64,Default,,0,0,0,,Chúng mày làm gì ở đây?
Dialogue: 0,0:51:05.72,0:51:09.39,Default,,0,0,0,,- Tao mới là người hỏi câu đó…\N- Cứ để cho tớ, được chứ?
Dialogue: 0,0:51:10.06,0:51:12.23,Default,,0,0,0,,À, bọn tôi bị lạc.
Dialogue: 0,0:51:12.31,0:51:15.15,Default,,0,0,0,,Chúng tôi phải đi lối nào\Nđể đến tiệc chiêu đãi thế?
Dialogue: 0,0:51:15.23,0:51:18.86,Default,,0,0,0,,{\an8}Hai cậu bị lạc\Nmà sao lên đến tận tầng 80 thế?
Dialogue: 0,0:51:18.95,0:51:21.70,Default,,0,0,0,,Tao biết hết đấy! Đừng có mà lừa tao!
Dialogue: 0,0:51:21.78,0:51:23.78,Default,,0,0,0,,- Kosei của hắn!\N- Kirishima!
Dialogue: 0,0:51:27.00,0:51:28.25,Default,,0,0,0,,Kosei này…
Dialogue: 0,0:51:29.21,0:51:30.33,Default,,0,0,0,,Todoroki?
Dialogue: 0,0:51:33.13,0:51:35.96,Default,,0,0,0,,Chết tiệt. Bọn tôi sẽ câu giờ ở đây.
Dialogue: 0,0:51:36.05,0:51:37.76,Default,,0,0,0,,Các cậu tìm cách đi lên đi!
Dialogue: 0,0:51:37.84,0:51:39.51,Default,,0,0,0,,- Todoroki?\N- Còn cậu thì sao?
Dialogue: 0,0:51:39.59,0:51:41.55,Default,,0,0,0,,- Tôi không sao! Đi đi!\N- Todoroki!
Dialogue: 0,0:51:41.63,0:51:44.05,Default,,0,0,0,,Tôi sẽ đuổi theo ngay\Nsau khi giải quyết chúng.
Dialogue: 0,0:51:45.47,0:51:46.31,Default,,0,0,0,,Được!
Dialogue: 0,0:51:46.39,0:51:48.52,Default,,0,0,0,,Các cậu cũng ở đây à?
Dialogue: 0,0:51:48.60,0:51:49.98,Default,,0,0,0,,Có chuyện gì vậy, Todoroki?
Dialogue: 0,0:51:50.06,0:51:52.77,Default,,0,0,0,,- Cậu không nghe thông báo à?\N- Cái gì?
Dialogue: 0,0:51:52.85,0:51:54.73,Default,,0,0,0,,Lũ villain đã chiếm lấy tòa tháp này.
Dialogue: 0,0:51:54.81,0:51:56.44,Default,,0,0,0,,- Hả?\N- Cái gì?
Dialogue: 0,0:51:56.52,0:51:58.40,Default,,0,0,0,,Tôi sẽ giải thích thêm sau.
Dialogue: 0,0:51:58.48,0:52:00.03,Default,,0,0,0,,Bây giờ phải xử lý chúng trước đã.
Dialogue: 0,0:52:01.57,0:52:03.20,Default,,0,0,0,,Kosei đó là sao?
Dialogue: 0,0:52:03.28,0:52:04.70,Default,,0,0,0,,Cẩn thận đấy.
Dialogue: 0,0:52:04.78,0:52:06.45,Default,,0,0,0,,Im đi! Tao biết rồi!
Dialogue: 0,0:52:07.08,0:52:08.66,Default,,0,0,0,,Bọn nhóc chết tiệt…
Dialogue: 0,0:52:08.75,0:52:11.71,Default,,0,0,0,,Đừng có lợi dụng bọn tao!
Dialogue: 0,0:52:19.13,0:52:21.34,Default,,0,0,0,,Chết đi!
Dialogue: 0,0:52:23.55,0:52:24.68,Default,,0,0,0,,Bakugo!
Dialogue: 0,0:52:29.39,0:52:30.98,Default,,0,0,0,,- Kirishima!\N- Né đi!
Dialogue: 0,0:52:38.82,0:52:41.44,Default,,0,0,0,,Chúng mày không phải\Ntrẻ con bình thường nhỉ?
Dialogue: 0,0:52:41.53,0:52:43.15,Default,,0,0,0,,Chúng mày là ai?
Dialogue: 0,0:52:43.24,0:52:45.82,Default,,0,0,0,,Ai thèm nói cho bọn mày,\Nlũ villain chết tiệt!
Dialogue: 0,0:52:45.91,0:52:48.08,Default,,0,0,0,,Các người không xứng biết tên tôi.
Dialogue: 0,0:52:53.83,0:52:54.96,Default,,0,0,0,,Đây cũng là ngõ cụt à?
Dialogue: 0,0:52:55.04,0:52:57.04,Default,,0,0,0,,Này. Phải làm sao đây?
Dialogue: 0,0:52:57.54,0:53:00.09,Default,,0,0,0,,Chúng ta bị nhốt thật rồi!
Dialogue: 0,0:53:00.17,0:53:01.84,Default,,0,0,0,,Chúng ta chỉ đi được đến đây thôi à?
Dialogue: 0,0:53:02.34,0:53:03.17,Default,,0,0,0,,Chị Melissa.
Dialogue: 0,0:53:03.26,0:53:05.93,Default,,0,0,0,,Chị có thấy cái gì đó\Ngiống như cửa trên tầng nhà không?
Dialogue: 0,0:53:06.01,0:53:07.35,Default,,0,0,0,,Đằng kia!
Dialogue: 0,0:53:08.35,0:53:10.97,Default,,0,0,0,,Đó là cửa bảo trì\Ncho hệ thống ánh sáng mặt trời!
Dialogue: 0,0:53:12.06,0:53:15.02,Default,,0,0,0,,Trông giống thang khẩn cấp nhỉ?
Dialogue: 0,0:53:15.10,0:53:17.86,Default,,0,0,0,,Đúng là có thang thoát hiểm thủ công,
Dialogue: 0,0:53:17.94,0:53:19.94,Default,,0,0,0,,nhưng chúng ta chỉ có thể\Nmở ra từ bên trong.
Dialogue: 0,0:53:20.78,0:53:22.61,Default,,0,0,0,,Đã đến được tận đây rồi mà…
Dialogue: 0,0:53:23.61,0:53:25.07,Default,,0,0,0,,Vẫn còn cách.
Dialogue: 0,0:53:31.70,0:53:33.91,Default,,0,0,0,,Nếu các cậu ra ngoài\Nqua đường lỗ thông hơi,
Dialogue: 0,0:53:34.00,0:53:35.96,Default,,0,0,0,,các cậu có thể\Ntrèo tường lên tầng cao nhất.
Dialogue: 0,0:53:36.04,0:53:39.09,Default,,0,0,0,,Đúng vậy! Chỉ cần có\Nmột lỗ thông hơi khác ở trên kia…
Dialogue: 0,0:53:39.17,0:53:40.46,Default,,0,0,0,,Chúng ta có thể vào trong!
Dialogue: 0,0:53:41.17,0:53:45.43,Default,,0,0,0,,Ai có thể chui vào lỗ thông hơi\Nvà trèo tường?
Dialogue: 0,0:53:46.59,0:53:47.43,Default,,0,0,0,,Hả?
Dialogue: 0,0:53:47.51,0:53:49.76,Default,,0,0,0,,Khoan đã. Tớ ư?
Dialogue: 0,0:53:49.85,0:53:51.14,Default,,0,0,0,,Xin cậu đấy, Mineta!
Dialogue: 0,0:53:51.22,0:53:52.68,Default,,0,0,0,,Chỉ có cậu mới làm được thôi!
Dialogue: 0,0:53:52.77,0:53:55.69,Default,,0,0,0,,Đồ ngốc! Các cậu có biết\Nchúng ta ở tầng mấy không?
Dialogue: 0,0:53:56.56,0:53:59.06,Default,,0,0,0,,Nếu cậu cống hiến nhiều nhất\Nđể giúp mọi người sống sót,
Dialogue: 0,0:53:59.15,0:54:02.86,Default,,0,0,0,,cậu sẽ được phỏng vấn\Nvà sẽ rất được các cô gái ái mộ đấy!
Dialogue: 0,0:54:02.94,0:54:03.86,Default,,0,0,0,,- Đi mà!\N- Đi mà!
Dialogue: 0,0:54:04.44,0:54:05.86,Default,,0,0,0,,Cậu sẽ có cả một harem!
Dialogue: 0,0:54:06.82,0:54:10.20,Default,,0,0,0,,{\an8}Được thôi! Tớ phải đi chứ gì?
Dialogue: 0,0:54:12.79,0:54:14.62,Default,,0,0,0,,Harem…
Dialogue: 0,0:54:16.00,0:54:17.79,Default,,0,0,0,,Harem…
Dialogue: 0,0:54:18.79,0:54:19.63,Default,,0,0,0,,Harem…
Dialogue: 0,0:54:19.71,0:54:21.79,Default,,0,0,0,,{\an8}PHÒNG BẢO TRÌ
Dialogue: 0,0:54:24.71,0:54:28.09,Default,,0,0,0,,Làm được rồi! Mình làm được rồi!
Dialogue: 0,0:54:29.59,0:54:32.51,Default,,0,0,0,,Nào nào nào! Bày tỏ tình yêu với tớ đi!
Dialogue: 0,0:54:32.60,0:54:34.89,Default,,0,0,0,,Chỉ cần là con gái là được.
Dialogue: 0,0:54:36.52,0:54:38.10,Default,,0,0,0,,Em tuyệt lắm, Mineta.
Dialogue: 0,0:54:38.19,0:54:39.85,Default,,0,0,0,,Đúng là một anh hùng đầy triển vọng!
Dialogue: 0,0:54:43.11,0:54:45.40,Default,,0,0,0,,Máu lên nào, mọi người!
Dialogue: 0,0:54:45.49,0:54:46.82,Default,,0,0,0,,- Ừ!\N- Ừ!
Dialogue: 0,0:54:46.90,0:54:49.28,Default,,0,0,0,,Này, vẫn chưa tìm được à?
Dialogue: 0,0:54:51.41,0:54:52.45,Default,,0,0,0,,Chết tiệt!
Dialogue: 0,0:54:52.95,0:54:55.58,Default,,0,0,0,,Này, tầng 80! Lũ nhóc chạy thoát rồi kìa!
Dialogue: 0,0:54:55.66,0:54:56.54,Default,,0,0,0,,{\i1}Có chuyện gì vậy?{\i0}
Dialogue: 0,0:54:56.62,0:54:59.12,Default,,0,0,0,,Im đi! Câm miệng…
Dialogue: 0,0:55:23.61,0:55:26.73,Default,,0,0,0,,Tên đó không hề đục lỗ trong không gian.
Dialogue: 0,0:55:26.82,0:55:27.74,Default,,0,0,0,,Hắn đào rỗng nó!
Dialogue: 0,0:55:28.24,0:55:29.86,Default,,0,0,0,,Tao biết rồi.
Dialogue: 0,0:55:30.66,0:55:32.41,Default,,0,0,0,,Chết tiệt. Dai như đỉa đói ấy.
Dialogue: 0,0:55:32.91,0:55:37.16,Default,,0,0,0,,Tao không chơi với mày mãi được đâu!
Dialogue: 0,0:55:39.66,0:55:41.67,Default,,0,0,0,,Howitzer…
Dialogue: 0,0:55:41.75,0:55:43.88,Default,,0,0,0,,Impact!
Dialogue: 0,0:55:49.34,0:55:51.05,Default,,0,0,0,,- Sao mày dám!\N- Bakugo!
Dialogue: 0,0:55:55.14,0:55:56.93,Default,,0,0,0,,Chết tiệt. Hả?
Dialogue: 0,0:55:57.01,0:55:58.47,Default,,0,0,0,,Cái quái gì đây?
Dialogue: 0,0:55:58.56,0:56:00.18,Default,,0,0,0,,Là mồ hôi trong lòng bàn tay tao.
Dialogue: 0,0:56:00.89,0:56:02.69,Default,,0,0,0,,Một chất giống như nitroglycerin.
Dialogue: 0,0:56:16.49,0:56:18.12,Default,,0,0,0,,- Kirishima!\N- Cậu không sao chứ?
Dialogue: 0,0:56:18.62,0:56:20.50,Default,,0,0,0,,Tớ không cử động được.
Dialogue: 0,0:56:20.58,0:56:21.50,Default,,0,0,0,,Giúp tớ với.
Dialogue: 0,0:56:22.37,0:56:24.08,Default,,0,0,0,,Mày bị ngu à?
Dialogue: 0,0:56:24.17,0:56:25.92,Default,,0,0,0,,Mày ngừng sử dụng Kosei là được mà.
Dialogue: 0,0:56:26.00,0:56:27.59,Default,,0,0,0,,Ừ nhỉ.
Dialogue: 0,0:56:28.63,0:56:30.38,Default,,0,0,0,,Trời ạ, giật cả mình.
Dialogue: 0,0:56:30.46,0:56:32.93,Default,,0,0,0,,Mừng là cậu không sao.
Dialogue: 0,0:56:33.01,0:56:34.93,Default,,0,0,0,,Ừ, các cậu cũng thế!
Dialogue: 0,0:56:37.47,0:56:38.56,Default,,0,0,0,,Cảm ơn.
Dialogue: 0,0:56:39.06,0:56:41.35,Default,,0,0,0,,Sao thế? Chẳng giống cậu gì cả.\NĐừng lo lắng!
Dialogue: 0,0:56:41.43,0:56:42.56,Default,,0,0,0,,Tao không có lo lắng!
Dialogue: 0,0:56:42.64,0:56:44.81,Default,,0,0,0,,Được rồi, đuổi theo nhóm Midoriya thôi.
Dialogue: 0,0:56:44.90,0:56:46.02,Default,,0,0,0,,Đừng có ra lệnh cho tao!
Dialogue: 0,0:56:46.11,0:56:48.65,Default,,0,0,0,,Todoroki, cậu kể rõ mọi chuyện đi.
Dialogue: 0,0:56:52.99,0:56:55.32,Default,,0,0,0,,Xem ra chúng nghiêm túc rồi.
Dialogue: 0,0:56:56.07,0:56:58.74,Default,,0,0,0,,{\i1}Đại ca, chúng không phải\Ncon nít bình thường!{\i0}
Dialogue: 0,0:56:59.33,0:57:01.12,Default,,0,0,0,,{\an8\i1}Chúng là học sinh Khoa Anh hùng của UA.{\i0}
Dialogue: 0,0:57:01.20,0:57:02.50,Default,,0,0,0,,{\an8\i1}Chúng là Anh hùng Dự bị!{\i0}
Dialogue: 0,0:57:02.58,0:57:06.75,Default,,0,0,0,,{\i1}Hẳn là lũ nhóc đang cố\Nkhôi phục hệ thống an ninh.{\i0}
Dialogue: 0,0:57:07.59,0:57:10.30,Default,,0,0,0,,Mày đã kích hoạt\Nmáy an ninh trên tầng 80 chưa?
Dialogue: 0,0:57:10.38,0:57:11.21,Default,,0,0,0,,{\i1}Rồi!{\i0}
Dialogue: 0,0:57:11.80,0:57:15.34,Default,,0,0,0,,Mở các cửa ngăn từ tầng 100 đến 130.
Dialogue: 0,0:57:15.43,0:57:17.55,Default,,0,0,0,,{\i1}- Sao?{\i0}\N- Làm theo lời tao.
Dialogue: 0,0:57:20.47,0:57:21.72,Default,,0,0,0,,{\i1}Chúng ta may mắn thật nhỉ?{\i0}
Dialogue: 0,0:57:21.81,0:57:25.02,Default,,0,0,0,,Tất cả những cánh cửa chớp đều mở\Nkể từ khi chúng ta lên tầng 100.
Dialogue: 0,0:57:25.10,0:57:27.48,Default,,0,0,0,,Có lẽ là chúng mất dấu chúng ta rồi?
Dialogue: 0,0:57:27.56,0:57:28.86,Default,,0,0,0,,Hẳn là không phải đâu.
Dialogue: 0,0:57:28.94,0:57:31.15,Default,,0,0,0,,Chúng đang dẫn dắt chúng ta\Nđến đâu đó nhỉ?
Dialogue: 0,0:57:31.23,0:57:32.36,Default,,0,0,0,,Ừ.
Dialogue: 0,0:57:32.44,0:57:35.11,Default,,0,0,0,,Dù là vậy chúng ta vẫn phải\Nlên cao hết mức có thể.
Dialogue: 0,0:57:35.20,0:57:37.57,Default,,0,0,0,,{\i1}Thế nên chúng ta buộc phải\Nđi theo hướng chúng muốn.{\i0}
Dialogue: 0,0:57:41.49,0:57:43.25,Default,,0,0,0,,{\an8}Nhìn số lượng người máy kìa!
Dialogue: 0,0:57:43.33,0:57:44.71,Default,,0,0,0,,{\an8}Có vẻ như kẻ địch
Dialogue: 0,0:57:44.79,0:57:47.79,Default,,0,0,0,,{\an8}đã quyết định bắt giữ chúng ta\Nthay vì đuổi chúng ta ra ngoài.
Dialogue: 0,0:57:47.88,0:57:51.30,Default,,0,0,0,,{\an8}Chắc chắn chúng đã nhận ra\Nchúng ta là học sinh của UA.
Dialogue: 0,0:57:52.38,0:57:56.22,Default,,0,0,0,,Nhưng chúng ta đã lường trước chuyện này!
Dialogue: 0,0:57:56.30,0:57:59.14,Default,,0,0,0,,Ừ. Hãy thực kiện kế hoạch A nào.
Dialogue: 0,0:57:59.22,0:58:00.05,Default,,0,0,0,,Kaminari!
Dialogue: 0,0:58:00.14,0:58:02.43,Default,,0,0,0,,Được rồi. Tớ cũng sẽ cố hết sức!
Dialogue: 0,0:58:02.52,0:58:04.52,Default,,0,0,0,,- Nhờ cậu đấy, Iida!\N- Ừ!
Dialogue: 0,0:58:09.23,0:58:14.49,Default,,0,0,0,,Ăn cú Sốc Điện Tràn Lan\N1,3 Triệu Vôn này đi!
Dialogue: 0,0:58:16.11,0:58:17.36,Default,,0,0,0,,Chúng đang phòng thủ!
Dialogue: 0,0:58:17.45,0:58:19.37,Default,,0,0,0,,Chết tiệt. Đã thế thì…
Dialogue: 0,0:58:19.45,0:58:22.66,Default,,0,0,0,,Hai Triệu Vôn!
Dialogue: 0,0:58:22.74,0:58:24.83,Default,,0,0,0,,Đồ ngốc! Nếu cậu làm thế…
Dialogue: 0,0:58:26.04,0:58:27.33,Default,,0,0,0,,Tuyệt…
Dialogue: 0,0:58:27.83,0:58:29.58,Default,,0,0,0,,Cậu sẽ biến thành dở hơi.
Dialogue: 0,0:58:29.67,0:58:32.92,Default,,0,0,0,,Nhưng nhờ cậu ấy mà\Nngười máy an ninh ngừng hoạt động rồi…
Dialogue: 0,0:58:34.34,0:58:35.51,Default,,0,0,0,,Kaminari!
Dialogue: 0,0:58:35.59,0:58:36.97,Default,,0,0,0,,Cứng đầu quá đấy.
Dialogue: 0,0:58:38.26,0:58:39.09,Default,,0,0,0,,Hết cách rồi.
Dialogue: 0,0:58:39.18,0:58:40.47,Default,,0,0,0,,Các cậu, bắt đầu kế hoạch B!
Dialogue: 0,0:58:40.55,0:58:41.60,Default,,0,0,0,,Được!
Dialogue: 0,0:58:45.68,0:58:48.06,Default,,0,0,0,,Chúng ta có thể gây nhiễu liên lạc\Nbằng cái này!
Dialogue: 0,0:58:48.14,0:58:49.56,Default,,0,0,0,,- Xem đây!\N- Đây nữa!
Dialogue: 0,0:58:49.65,0:58:50.48,Default,,0,0,0,,Tiến lên!
Dialogue: 0,0:58:52.94,0:58:53.82,Default,,0,0,0,,Mineta!
Dialogue: 0,0:58:53.90,0:58:55.28,Default,,0,0,0,,Trả Kaminari lại đây!
Dialogue: 0,0:58:55.36,0:58:57.36,Default,,0,0,0,,Cậu ấy có cả một harem đang đợi!
Dialogue: 0,0:59:00.24,0:59:01.99,Default,,0,0,0,,Sao hả? Ơ?
Dialogue: 0,0:59:02.07,0:59:03.99,Default,,0,0,0,,Trời ạ, chúng không chịu dừng lại!
Dialogue: 0,0:59:05.08,0:59:06.54,Default,,0,0,0,,Lên đi, Midoriya!
Dialogue: 0,0:59:06.62,0:59:07.66,Default,,0,0,0,,Được!
Dialogue: 0,0:59:11.92,0:59:15.71,Default,,0,0,0,,{\i1}One For All: Full Cowl!{\i0}
Dialogue: 0,0:59:15.80,0:59:18.34,Default,,0,0,0,,{\i1}Full Gauntlet!{\i0}
Dialogue: 0,0:59:18.42,0:59:23.14,Default,,0,0,0,,Chị nghĩ em có thể\Ntung hết sức của mình với cái này.
Dialogue: 0,0:59:23.22,0:59:26.47,Default,,0,0,0,,{\i1}Đầu tiên mình sẽ dùng\N30 phần trăm sức mạnh…{\i0}
Dialogue: 0,0:59:26.56,0:59:28.98,Default,,0,0,0,,{\i1}Smash!{\i0}
Dialogue: 0,0:59:31.15,0:59:33.31,Default,,0,0,0,,{\i1}Không đau. Tay mình cũng không sao.{\i0}
Dialogue: 0,0:59:33.40,0:59:34.82,Default,,0,0,0,,{\i1}Mình có thể làm được!{\i0}
Dialogue: 0,0:59:37.53,0:59:38.53,Default,,0,0,0,,Jiro!
Dialogue: 0,0:59:39.11,0:59:41.45,Default,,0,0,0,,- Người máy an ninh ở đâu?\N- Chúng đến từ bên trái!
Dialogue: 0,0:59:41.53,0:59:43.49,Default,,0,0,0,,Được rồi, tiếp tục di chuyển sang phải!
Dialogue: 0,0:59:43.57,0:59:44.41,Default,,0,0,0,,- Được!\N- Được!
Dialogue: 0,0:59:44.49,0:59:46.95,Default,,0,0,0,,Deku, tay cậu là sao vậy?\NLợi hại thật đấy!
Dialogue: 0,0:59:47.04,0:59:47.95,Default,,0,0,0,,Ừ!
Dialogue: 0,0:59:48.04,0:59:49.87,Default,,0,0,0,,Chị Melissa, găng tay hoạt động tốt lắm!
Dialogue: 0,0:59:49.96,0:59:51.42,Default,,0,0,0,,Em mang nó theo à?
Dialogue: 0,0:59:51.50,0:59:54.13,Default,,0,0,0,,Em không biết cách tháo ra.
Dialogue: 0,0:59:55.54,0:59:58.63,Default,,0,0,0,,{\i1}Đại ca, có gì đó làm nhiễu\Ncảm biến của người máy an ninh.{\i0}
Dialogue: 0,0:59:58.71,1:00:00.42,Default,,0,0,0,,{\i1}Chúng ta mất dấu lũ nhóc rồi!{\i0}
Dialogue: 0,1:00:00.51,1:00:01.68,Default,,0,0,0,,Đừng hoảng.
Dialogue: 0,1:00:02.26,1:00:06.97,Default,,0,0,0,,{\i1}Có thể một trong số chúng\Ncó Kosei thính giác.{\i0}
Dialogue: 0,1:00:07.06,1:00:10.10,Default,,0,0,0,,Tớ nghe thấy rất nhiều bánh xe\Nngười máy an ninh từ tầng dưới lên.
Dialogue: 0,1:00:10.18,1:00:11.48,Default,,0,0,0,,Ở trên có tiếng gì không?
Dialogue: 0,1:00:11.56,1:00:13.56,Default,,0,0,0,,- Không. Trên đó ổn.\N- Đi thôi!
Dialogue: 0,1:00:13.65,1:00:17.32,Default,,0,0,0,,{\an8}TẦNG 138, PHÒNG MÁY CHỦ
Dialogue: 0,1:00:24.99,1:00:25.91,Default,,0,0,0,,Bẫy ư?
Dialogue: 0,1:00:25.99,1:00:27.70,Default,,0,0,0,,Phá vòng vây thôi, Iida!
Dialogue: 0,1:00:27.79,1:00:28.87,Default,,0,0,0,,Khoan đã!
Dialogue: 0,1:00:28.95,1:00:33.00,Default,,0,0,0,,Nếu máy chủ ở đây bị phá hủy, có thể\Nsẽ ảnh hưởng đến hệ thống an ninh đấy.
Dialogue: 0,1:00:36.38,1:00:38.05,Default,,0,0,0,,Rốt cuộc có bao nhiêu người máy vậy?
Dialogue: 0,1:00:38.75,1:00:41.17,Default,,0,0,0,,Bọn tớ sẽ chặn lũ người máy an ninh lại.
Dialogue: 0,1:00:41.26,1:00:42.22,Default,,0,0,0,,Midoriya.
Dialogue: 0,1:00:42.30,1:00:44.68,Default,,0,0,0,,Dẫn chị Melissa đi tìm đường khác đi!
Dialogue: 0,1:00:45.64,1:00:46.97,Default,,0,0,0,,Chị Melissa, đi theo em!
Dialogue: 0,1:00:49.14,1:00:50.68,Default,,0,0,0,,Em cũng đi với bọn chị đi, Ochako!
Dialogue: 0,1:00:50.77,1:00:51.81,Default,,0,0,0,,Ơ? Nhưng mà…
Dialogue: 0,1:00:51.89,1:00:54.65,Default,,0,0,0,,- Nhờ cậu đấy, Uraraka!\N- Được!
Dialogue: 0,1:00:58.27,1:01:00.11,Default,,0,0,0,,Torque Over…
Dialogue: 0,1:01:01.03,1:01:04.66,Default,,0,0,0,,{\an8}Recipro Burst!
Dialogue: 0,1:01:08.74,1:01:09.99,Default,,0,0,0,,Tớ giao đại bác cho cậu.
Dialogue: 0,1:01:10.49,1:01:12.87,Default,,0,0,0,,- Tớ sẽ chế tạo đạn!\N- Được!
Dialogue: 0,1:01:18.59,1:01:20.67,Default,,0,0,0,,Tao không giao harem cho mày đâu,\Nnghe chưa?
Dialogue: 0,1:01:20.75,1:01:21.76,Default,,0,0,0,,Phải…
Dialogue: 0,1:01:24.97,1:01:26.14,Default,,0,0,0,,Bắn!
Dialogue: 0,1:01:31.02,1:01:33.18,Default,,0,0,0,,Deku, cậu không thể dừng lại!
Dialogue: 0,1:01:33.27,1:01:37.44,Default,,0,0,0,,Nếu chúng ta bị bắt thì nhóm Iida ở lại\Nsẽ là tốn công vô ích đấy!
Dialogue: 0,1:01:37.52,1:01:38.90,Default,,0,0,0,,Phải!
Dialogue: 0,1:01:39.40,1:01:41.94,Default,,0,0,0,,{\an8}Các cậu phải bảo trọng đấy!
Dialogue: 0,1:01:45.86,1:01:46.86,Default,,0,0,0,,{\i1}Động cơ chết máy rồi?{\i0}
Dialogue: 0,1:01:49.07,1:01:50.08,Default,,0,0,0,,Iida!
Dialogue: 0,1:01:50.16,1:01:51.70,Default,,0,0,0,,Yaomomo, đạn!
Dialogue: 0,1:01:51.79,1:01:53.29,Default,,0,0,0,,Yaomomo?
Dialogue: 0,1:01:53.83,1:01:56.21,Default,,0,0,0,,Sáng Tạo của tớ đến giới hạn rồi.
Dialogue: 0,1:01:56.29,1:01:59.13,Default,,0,0,0,,Da đầu của tớ cũng đến giới hạn rồi.
Dialogue: 0,1:02:02.59,1:02:04.26,Default,,0,0,0,,Là vì chúng bây đánh hăng quá đấy!
Dialogue: 0,1:02:04.92,1:02:07.51,Default,,0,0,0,,- Còn ba đứa trốn thoát thì sao?\N- Tao đang tìm chúng đây.
Dialogue: 0,1:02:07.59,1:02:09.60,Default,,0,0,0,,Chết tiệt. Chúng làm tao bực cả mình.
Dialogue: 0,1:02:11.89,1:02:14.68,Default,,0,0,0,,- Nơi này là…\N- Hệ thống máy phát điện năng lượng gió.
Dialogue: 0,1:02:15.27,1:02:16.77,Default,,0,0,0,,Sao chúng ta lại ở đây?
Dialogue: 0,1:02:16.85,1:02:20.61,Default,,0,0,0,,Nếu ta đi lên tầng trên, chắc hẳn\Nlũ người máy đang phục kích chúng ta.
Dialogue: 0,1:02:21.57,1:02:24.24,Default,,0,0,0,,Thế nên chúng ta sẽ leo thẳng lên\Ntầng cao nhất từ đây.
Dialogue: 0,1:02:24.32,1:02:26.57,Default,,0,0,0,,Chúng ta chỉ cần lên cửa thoát hiểm thôi.
Dialogue: 0,1:02:26.65,1:02:28.03,Default,,0,0,0,,Ở tít trên đó à?
Dialogue: 0,1:02:28.99,1:02:32.49,Default,,0,0,0,,Với Kosei có thể biến thứ mình chạm vào\Nthành vô trọng lực của Ochako,
Dialogue: 0,1:02:32.58,1:02:34.08,Default,,0,0,0,,chúng ta có thể làm được.
Dialogue: 0,1:02:35.75,1:02:37.41,Default,,0,0,0,,Được. Để đó cho em.
Dialogue: 0,1:02:39.58,1:02:41.59,Default,,0,0,0,,{\i1}Chị Melissa, bám chắc vào Deku nhé.{\i0}
Dialogue: 0,1:02:41.67,1:02:42.50,Default,,0,0,0,,{\i1}Được!{\i0}
Dialogue: 0,1:02:46.88,1:02:49.13,Default,,0,0,0,,{\an8}Đi đi!
Dialogue: 0,1:02:51.72,1:02:52.68,Default,,0,0,0,,Được rồi!
Dialogue: 0,1:02:54.35,1:02:55.85,Default,,0,0,0,,Không thể nào.
Dialogue: 0,1:02:55.93,1:02:57.18,Default,,0,0,0,,Uraraka!
Dialogue: 0,1:02:57.27,1:02:58.52,Default,,0,0,0,,Giải trừ Kosei và chạy đi!
Dialogue: 0,1:02:58.60,1:02:59.98,Default,,0,0,0,,Em không thể làm thế!
Dialogue: 0,1:03:00.06,1:03:02.40,Default,,0,0,0,,Nếu em làm thế\Nthì chúng ta không thể cứu mọi người!
Dialogue: 0,1:03:02.48,1:03:03.65,Default,,0,0,0,,Ochako!
Dialogue: 0,1:03:04.65,1:03:06.69,Default,,0,0,0,,{\i1}Nhanh lên!{\i0}
Dialogue: 0,1:03:06.78,1:03:09.11,Default,,0,0,0,,{\i1}Nhanh lên!{\i0}
Dialogue: 0,1:03:11.74,1:03:13.20,Default,,0,0,0,,Kacchan!
Dialogue: 0,1:03:16.54,1:03:18.75,Default,,0,0,0,,Todoroki và Kirishima!
Dialogue: 0,1:03:18.83,1:03:20.67,Default,,0,0,0,,Cậu không sao chứ, Uraraka?
Dialogue: 0,1:03:20.75,1:03:22.25,Default,,0,0,0,,Ừ, tớ không sao.
Dialogue: 0,1:03:22.33,1:03:25.50,Default,,0,0,0,,Deku và chị Melissa\Nđang lên tầng cao nhất.
Dialogue: 0,1:03:25.59,1:03:27.26,Default,,0,0,0,,Ừ, tôi thấy rồi.
Dialogue: 0,1:03:27.34,1:03:29.51,Default,,0,0,0,,Chúng ta sẽ ngăn lũ này lại!
Dialogue: 0,1:03:29.59,1:03:31.64,Default,,0,0,0,,Đừng có ra lệnh cho tao!
Dialogue: 0,1:03:31.72,1:03:34.81,Default,,0,0,0,,- Nhưng chúng ta phối hợp ăn ý lắm!\N- Có chết liền!
Dialogue: 0,1:03:34.89,1:03:36.18,Default,,0,0,0,,Cảm ơn mọi người…
Dialogue: 0,1:03:37.89,1:03:39.60,Default,,0,0,0,,Deku! Chị Melissa!
Dialogue: 0,1:03:39.69,1:03:42.10,Default,,0,0,0,,Bakugo! Nhắm cánh quạt vào Midoriya!
Dialogue: 0,1:03:42.19,1:03:44.32,Default,,0,0,0,,Đã bảo là đừng có ra lệnh cho tao!
Dialogue: 0,1:03:55.66,1:03:56.49,Default,,0,0,0,,Một cơn gió nóng!
Dialogue: 0,1:03:56.58,1:03:57.54,Default,,0,0,0,,Tuyệt vời!
Dialogue: 0,1:03:57.62,1:04:00.58,Default,,0,0,0,,{\i1}Todoroki, cậu đã dùng lửa…{\i0}
Dialogue: 0,1:04:00.66,1:04:01.87,Default,,0,0,0,,{\i1}Cảm ơn!{\i0}
Dialogue: 0,1:04:02.83,1:04:05.34,Default,,0,0,0,,Deku! Em sẽ đập vào tường mất!
Dialogue: 0,1:04:06.80,1:04:10.30,Default,,0,0,0,,Full Gauntlet đủ mạnh\Nđể chịu được ba cú đấm
Dialogue: 0,1:04:10.38,1:04:11.93,Default,,0,0,0,,với sức mạnh tối đa của chú Might.
Dialogue: 0,1:04:12.55,1:04:15.14,Default,,0,0,0,,{\i1}Không có thời gian kiềm chế nữa!{\i0}
Dialogue: 0,1:04:15.22,1:04:17.35,Default,,0,0,0,,{\an8}- Bám chắc vào!\N- Được!
Dialogue: 0,1:04:17.43,1:04:20.56,Default,,0,0,0,,{\i1}One For All: Full Cowl!{\i0}
Dialogue: 0,1:04:22.35,1:04:26.57,Default,,0,0,0,,{\i1}Detroit Smash!{\i0}
Dialogue: 0,1:04:27.90,1:04:29.19,Default,,0,0,0,,Họ vào trong tháp rồi!
Dialogue: 0,1:04:29.28,1:04:30.11,Default,,0,0,0,,Giải trừ!
Dialogue: 0,1:04:32.91,1:04:34.99,Default,,0,0,0,,- Bảo Swordkil và những người khác đến!\N{\i1}- Vâng!{\i0}
Dialogue: 0,1:04:35.62,1:04:37.87,Default,,0,0,0,,Bảo vệ phòng điều khiển\Ncho đến khi tao đến!
Dialogue: 0,1:04:37.95,1:04:39.37,Default,,0,0,0,,{\i1}Cố chịu đựng nào…{\i0}
Dialogue: 0,1:04:39.45,1:04:41.41,Default,,0,0,0,,{\i1}Cố chịu đựng nào, All Might!{\i0}
Dialogue: 0,1:04:41.50,1:04:43.37,Default,,0,0,0,,{\i1}Chắc chắn tụi nhỏ sẽ làm được!{\i0}
Dialogue: 0,1:04:43.46,1:04:44.79,Default,,0,0,0,,{\i1}Mình dám chắc!{\i0}
Dialogue: 0,1:04:47.38,1:04:49.88,Default,,0,0,0,,Deku, em không sao chứ?
Dialogue: 0,1:04:49.96,1:04:52.13,Default,,0,0,0,,Em không sao.
Dialogue: 0,1:04:52.22,1:04:53.43,Default,,0,0,0,,Còn chị Melissa thì sao?
Dialogue: 0,1:04:53.51,1:04:56.01,Default,,0,0,0,,Ừ, chị không sao. Xây xước nhẹ thôi.
Dialogue: 0,1:05:01.31,1:05:03.73,Default,,0,0,0,,Bọn nhóc nổi loạn!
Dialogue: 0,1:05:03.81,1:05:05.90,Default,,0,0,0,,Mục đích của các người là gì?
Dialogue: 0,1:05:06.48,1:05:08.69,Default,,0,0,0,,Đừng làm như thể mình là anh hùng!
Dialogue: 0,1:05:11.49,1:05:12.65,Default,,0,0,0,,Dừng lại!
Dialogue: 0,1:05:12.74,1:05:14.36,Default,,0,0,0,,Tránh ra!
Dialogue: 0,1:05:14.86,1:05:18.03,Default,,0,0,0,,{\i1}- Smash!{\i0}\N- Con nhóc…
Dialogue: 0,1:05:22.29,1:05:23.12,Default,,0,0,0,,Chị Melissa!
Dialogue: 0,1:05:23.21,1:05:24.67,Default,,0,0,0,,Chị không sao.
Dialogue: 0,1:05:25.17,1:05:26.54,Default,,0,0,0,,Không có gì nghiêm trọng.
Dialogue: 0,1:05:26.63,1:05:27.88,Default,,0,0,0,,Đưa tay đây cho em.
Dialogue: 0,1:05:29.21,1:05:31.55,Default,,0,0,0,,Xin lỗi vì đã không thể bảo vệ chị.
Dialogue: 0,1:05:33.13,1:05:34.47,Default,,0,0,0,,Em phải nói "cảm ơn" chứ nhỉ?
Dialogue: 0,1:05:39.10,1:05:40.14,Default,,0,0,0,,Vâng!
Dialogue: 0,1:05:42.68,1:05:44.94,Default,,0,0,0,,Đi cứu mọi người nào!
Dialogue: 0,1:05:45.64,1:05:46.48,Default,,0,0,0,,Ừ!
Dialogue: 0,1:05:51.73,1:05:52.82,Default,,0,0,0,,Bọn chúng đến rồi!
Dialogue: 0,1:05:58.37,1:05:59.32,Default,,0,0,0,,{\an8}TẦNG CAO NHẤT
Dialogue: 0,1:05:59.41,1:06:01.70,Default,,0,0,0,,{\an8\i1}Chị Melissa, phòng điều khiển ở đâu?{\i0}
Dialogue: 0,1:06:01.79,1:06:03.87,Default,,0,0,0,,{\i1}Trước thang máy trung tâm.{\i0}
Dialogue: 0,1:06:05.91,1:06:06.75,Default,,0,0,0,,Có người ở đó.
Dialogue: 0,1:06:10.46,1:06:11.42,Default,,0,0,0,,Bố?
Dialogue: 0,1:06:12.30,1:06:13.34,Default,,0,0,0,,Đúng vậy!
Dialogue: 0,1:06:13.92,1:06:15.59,Default,,0,0,0,,Sao bố lại ở tầng trên cùng?
Dialogue: 0,1:06:15.67,1:06:18.80,Default,,0,0,0,,{\an8}Lũ villain đưa bố chị đến đây\Nđể ép chú ấy làm gì đó chăng?
Dialogue: 0,1:06:18.89,1:06:19.80,Default,,0,0,0,,{\an8}Phải cứu bố chị!
Dialogue: 0,1:06:19.89,1:06:20.85,Default,,0,0,0,,{\an8}Vâng.
Dialogue: 0,1:06:22.10,1:06:25.81,Default,,0,0,0,,{\an8}PHÒNG CẤT CHỨA
Dialogue: 0,1:06:28.81,1:06:31.32,Default,,0,0,0,,Khóa mở rồi mà. Đến tủ 1147 đi.
Dialogue: 0,1:06:31.40,1:06:32.36,Default,,0,0,0,,Vâng!
Dialogue: 0,1:06:33.36,1:06:34.19,Default,,0,0,0,,Tôi mở đây!
Dialogue: 0,1:06:38.36,1:06:39.95,Default,,0,0,0,,Anh làm được rồi, tiến sĩ!
Dialogue: 0,1:06:40.62,1:06:42.16,Default,,0,0,0,,Ở đây hết cả.
Dialogue: 0,1:06:43.20,1:06:44.41,Default,,0,0,0,,Ừ.
Dialogue: 0,1:06:45.20,1:06:46.71,Default,,0,0,0,,Cuối cùng tôi cũng lấy lại được.
Dialogue: 0,1:06:48.37,1:06:50.88,Default,,0,0,0,,Thiết bị này và dữ liệu nghiên cứu\Nlà những thứ duy nhất
Dialogue: 0,1:06:50.96,1:06:52.75,Default,,0,0,0,,tôi sẽ không giao cho bất cứ ai.
Dialogue: 0,1:06:52.84,1:06:56.55,Default,,0,0,0,,- Tôi sẽ không bao giờ bỏ cuộc.\N- Mọi thứ đều theo đúng kế hoạch nhỉ?
Dialogue: 0,1:06:57.13,1:06:59.26,Default,,0,0,0,,Có vẻ như bọn villain\Ncũng đang rất suôn sẻ.
Dialogue: 0,1:06:59.76,1:07:00.76,Default,,0,0,0,,Cảm ơn anh.
Dialogue: 0,1:07:00.84,1:07:03.56,Default,,0,0,0,,Nhờ có sự dàn xếp của anh đấy, Sam.
Dialogue: 0,1:07:03.64,1:07:04.60,Default,,0,0,0,,Bố…
Dialogue: 0,1:07:04.68,1:07:05.64,Default,,0,0,0,,- Hả?\N- Hả?
Dialogue: 0,1:07:07.39,1:07:09.02,Default,,0,0,0,,Melissa…
Dialogue: 0,1:07:09.52,1:07:11.48,Default,,0,0,0,,Cô Melissa! Cô làm gì ở đây vậy?
Dialogue: 0,1:07:12.90,1:07:14.86,Default,,0,0,0,,Bố nói "dàn xếp" là sao ạ?
Dialogue: 0,1:07:17.32,1:07:20.82,Default,,0,0,0,,Bố, đừng nói với con bố là\Nngười đứng sau vụ này nhé?
Dialogue: 0,1:07:23.08,1:07:25.24,Default,,0,0,0,,Để có được thiết bị đó ư?
Dialogue: 0,1:07:26.95,1:07:28.62,Default,,0,0,0,,{\an8}Có thật không bố?
Dialogue: 0,1:07:33.54,1:07:34.38,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,1:07:36.26,1:07:38.34,Default,,0,0,0,,Tại sao? Để làm gì?
Dialogue: 0,1:07:38.42,1:07:41.30,Default,,0,0,0,,Tiến sĩ chỉ đang cố lấy lại\Nthứ đã bị cướp mất thôi.
Dialogue: 0,1:07:41.80,1:07:45.64,Default,,0,0,0,,Chính là phát minh đột phá\Ncó thể khuếch đại Kosei bằng máy móc này.
Dialogue: 0,1:07:46.85,1:07:48.18,Default,,0,0,0,,Khuếch đại Kosei ư?
Dialogue: 0,1:07:48.27,1:07:49.10,Default,,0,0,0,,Phải.
Dialogue: 0,1:07:49.69,1:07:51.48,Default,,0,0,0,,Nó vẫn trong giai đoạn thử nghiệm,
Dialogue: 0,1:07:51.56,1:07:54.44,Default,,0,0,0,,nhưng với thiết bị này,\NKosei có thể được khuếch đại
Dialogue: 0,1:07:54.52,1:07:58.28,Default,,0,0,0,,mà không ảnh hưởng đến cơ thể\Nnhư các loại chất cấm.
Dialogue: 0,1:07:59.61,1:08:03.12,Default,,0,0,0,,{\i1}Nhưng phía tài trợ\Nđã tịch thu phát minh này{\i0}
Dialogue: 0,1:08:03.20,1:08:04.74,Default,,0,0,0,,{\i1}và dữ liệu nghiên cứu của nó.{\i0}
Dialogue: 0,1:08:05.37,1:08:07.79,Default,,0,0,0,,{\i1}Chính nghiên cứu cũng bị đình chỉ.{\i0}
Dialogue: 0,1:08:09.79,1:08:12.08,Default,,0,0,0,,{\i1}Nếu phát minh này được thế giới biết đến,{\i0}
Dialogue: 0,1:08:12.17,1:08:14.96,Default,,0,0,0,,{\i1}cấu trúc xã hội siêu nhân\Nsẽ thay đổi một cách chóng mặt.{\i0}
Dialogue: 0,1:08:15.04,1:08:19.30,Default,,0,0,0,,{\i1}Tiến sĩ sợ rằng chính phủ các nước\Nsẽ gây áp lực cho mình.{\i0}
Dialogue: 0,1:08:20.72,1:08:22.30,Default,,0,0,0,,{\i1}Thế nên tiến sĩ mới…{\i0}
Dialogue: 0,1:08:24.09,1:08:25.47,Default,,0,0,0,,Tôi có thể lấy lại nghiên cứu?
Dialogue: 0,1:08:26.18,1:08:27.01,Default,,0,0,0,,Bằng cách nào?
Dialogue: 0,1:08:27.89,1:08:30.89,Default,,0,0,0,,Chúng ta sẽ thuê bọn villain đánh cắp nó\Nthay vì tự mình ra tay.
Dialogue: 0,1:08:33.23,1:08:34.69,Default,,0,0,0,,Đừng ngu ngốc thế.
Dialogue: 0,1:08:34.77,1:08:37.57,Default,,0,0,0,,Không bắt buộc phải là villain thật.
Dialogue: 0,1:08:37.65,1:08:38.48,Default,,0,0,0,,Hả?
Dialogue: 0,1:08:38.57,1:08:43.03,Default,,0,0,0,,Chúng ta có thể thuê người giả làm villain\Nvà bảo họ đánh cắp nó.
Dialogue: 0,1:08:43.11,1:08:46.78,Default,,0,0,0,,Tất nhiên là họ sẽ không làm hại ai.
Dialogue: 0,1:08:46.87,1:08:50.45,Default,,0,0,0,,Sau khi xong việc, chúng ta có thể\Ntiếp tục nghiên cứu ở nơi khác.
Dialogue: 0,1:08:51.54,1:08:52.75,Default,,0,0,0,,Nhưng…
Dialogue: 0,1:08:52.83,1:08:55.83,Default,,0,0,0,,{\an8}Tôi chỉ muốn\Nlấy lại nghiên cứu bằng mọi giá.
Dialogue: 0,1:08:56.46,1:08:58.46,Default,,0,0,0,,Anh không cảm thấy vậy à, tiến sĩ?
Dialogue: 0,1:09:00.59,1:09:02.30,Default,,0,0,0,,{\i1}Tôi đã chuẩn bị xong cả rồi.{\i0}
Dialogue: 0,1:09:02.38,1:09:04.51,Default,,0,0,0,,{\i1}Ta sẽ hành động\Nvào hôm tiền khai mạc I-Expo,{\i0}
Dialogue: 0,1:09:04.59,1:09:06.51,Default,,0,0,0,,{\i1}ngày tòa tháp đóng cửa.{\i0}
Dialogue: 0,1:09:06.60,1:09:08.72,Default,,0,0,0,,{\i1}Kế hoạch sẽ bắt đầu khi bữa tiệc mở màn,{\i0}
Dialogue: 0,1:09:08.81,1:09:12.39,Default,,0,0,0,,{\i1}cũng là thời điểm duy nhất\Ncác bên có liên quan tụ họp lại.{\i0}
Dialogue: 0,1:09:12.89,1:09:14.90,Default,,0,0,0,,{\i1}Chúng ta sẽ đứng đằng sau giật dây,{\i0}
Dialogue: 0,1:09:14.98,1:09:18.11,Default,,0,0,0,,{\i1}và giả vờ như bọn villain\Nđã chiếm lấy I-Island.{\i0}
Dialogue: 0,1:09:21.57,1:09:22.40,Default,,0,0,0,,Tiến sĩ…
Dialogue: 0,1:09:26.99,1:09:28.28,Default,,0,0,0,,Không thể nào…
Dialogue: 0,1:09:29.20,1:09:30.83,Default,,0,0,0,,Bố đang nói dối phải không?
Dialogue: 0,1:09:32.00,1:09:33.96,Default,,0,0,0,,- Nói với con đây là nói dối đi!\N- Không hề.
Dialogue: 0,1:09:36.21,1:09:37.92,Default,,0,0,0,,Chuyện này quá vô lý.
Dialogue: 0,1:09:39.42,1:09:43.13,Default,,0,0,0,,{\an8}Người bố mà con biết tuyệt đối\Nsẽ không bao giờ làm như thế!
Dialogue: 0,1:09:43.22,1:09:45.88,Default,,0,0,0,,Tại sao thế? Sao bố có thể chứ?
Dialogue: 0,1:09:48.47,1:09:50.06,Default,,0,0,0,,Là vì All Might.
Dialogue: 0,1:09:50.14,1:09:51.26,Default,,0,0,0,,- Hả?\N- Hả?
Dialogue: 0,1:09:51.77,1:09:56.14,Default,,0,0,0,,Hẳn là hai đứa không biết,\Nnhưng Kosei của cậu ấy đang dần biến mất.
Dialogue: 0,1:09:58.36,1:10:01.73,Default,,0,0,0,,Nhưng nếu cậu ấy dùng thiết bị của bố\Nthì Kosei có thể trở lại bình thường.
Dialogue: 0,1:10:02.40,1:10:05.78,Default,,0,0,0,,Không, nó có thể\Nkhiến cậu ấy mạnh hơn cả lúc trước.
Dialogue: 0,1:10:06.95,1:10:08.70,Default,,0,0,0,,{\i1}Anh hùng hạng nhất…{\i0}
Dialogue: 0,1:10:08.78,1:10:10.45,Default,,0,0,0,,{\i1}Biểu tượng Hòa bình…{\i0}
Dialogue: 0,1:10:10.53,1:10:13.08,Default,,0,0,0,,{\i1}Cậu ấy có thể lấy lại hào quang của mình.{\i0}
Dialogue: 0,1:10:14.00,1:10:17.54,Default,,0,0,0,,Cậu ấy sẽ lại có thể\Ncứu được nhiều người hơn nữa!
Dialogue: 0,1:10:18.63,1:10:22.30,Default,,0,0,0,,{\i1}Vì All Might đã truyền\NOne For All cho mình…{\i0}
Dialogue: 0,1:10:22.80,1:10:27.18,Default,,0,0,0,,{\i1}tiến sĩ phải chịu đau khổ vì toàn bộ\Nsức mạnh của All Might đang dần biến mất.{\i0}
Dialogue: 0,1:10:27.26,1:10:31.30,Default,,0,0,0,,Bố xin con.\NHãy giao thiết bị này cho All Might.
Dialogue: 0,1:10:31.39,1:10:33.18,Default,,0,0,0,,Không còn thời gian để làm lại nó nữa!
Dialogue: 0,1:10:33.27,1:10:36.18,Default,,0,0,0,,Sau khi cậu ấy nhận được,\Nbị trừng phạt thế nào bố cũng chịu.
Dialogue: 0,1:10:36.27,1:10:38.10,Default,,0,0,0,,- Bố đã sẵn sàng…\N- Họ đã liều mạng đấy!
Dialogue: 0,1:10:38.94,1:10:43.11,Default,,0,0,0,,Bố nghĩ Deku và các bạn học của cậu ấy
Dialogue: 0,1:10:43.19,1:10:46.11,Default,,0,0,0,,{\an8}phải trải qua những gì để cứu con tin hả?
Dialogue: 0,1:10:48.86,1:10:50.03,Default,,0,0,0,,Ý con là sao?
Dialogue: 0,1:10:50.95,1:10:51.91,Default,,0,0,0,,Lũ villain là giả.
Dialogue: 0,1:10:52.70,1:10:54.37,Default,,0,0,0,,Đây chỉ là một trò bịp thôi mà!
Dialogue: 0,1:10:54.45,1:10:57.29,Default,,0,0,0,,{\an8}Tất nhiên đều là giả cả.
Dialogue: 0,1:10:57.91,1:11:00.50,Default,,0,0,0,,Giả vờ làm villain giả ấy mà.
Dialogue: 0,1:11:00.58,1:11:01.54,Default,,0,0,0,,Tên đó…
Dialogue: 0,1:11:07.26,1:11:08.47,Default,,0,0,0,,Deku!
Dialogue: 0,1:11:09.93,1:11:12.14,Default,,0,0,0,,{\i1}Một Kosei khiến hắn\Ncó thể điều khiển kim loại ư?{\i0}
Dialogue: 0,1:11:12.72,1:11:14.47,Default,,0,0,0,,Đừng chống cự làm gì.
Dialogue: 0,1:11:14.97,1:11:16.56,Default,,0,0,0,,Sam, thiết bị đó đâu?
Dialogue: 0,1:11:17.14,1:11:18.81,Default,,0,0,0,,Ở đây!
Dialogue: 0,1:11:18.89,1:11:19.73,Default,,0,0,0,,Sam?
Dialogue: 0,1:11:21.65,1:11:24.57,Default,,0,0,0,,Đừng nói là anh định đưa thiết bị\Ncho bọn villain ngay từ đầu?
Dialogue: 0,1:11:24.65,1:11:27.36,Default,,0,0,0,,Chính anh đã lừa tôi.
Dialogue: 0,1:11:27.44,1:11:29.95,Default,,0,0,0,,Tôi đã phục vụ anh lâu như vậy,
Dialogue: 0,1:11:30.03,1:11:32.20,Default,,0,0,0,,nhưng anh lại dễ dàng\Nđể nghiên cứu bị đình chỉ,
Dialogue: 0,1:11:32.28,1:11:35.54,Default,,0,0,0,,và danh dự cũng như tiếng tăm\Nmà đáng lẽ chúng ta được hưởng
Dialogue: 0,1:11:35.62,1:11:38.33,Default,,0,0,0,,đều biến thành bọt biển…
Dialogue: 0,1:11:38.83,1:11:42.71,Default,,0,0,0,,Nếu không kiếm được chút tiền\Nthì chỉ lãng phí thời gian của tôi thôi!
Dialogue: 0,1:11:43.75,1:11:45.67,Default,,0,0,0,,Đây là phần thưởng ta đã hứa với ngươi.
Dialogue: 0,1:11:47.09,1:11:48.22,Default,,0,0,0,,Chú Sam!
Dialogue: 0,1:11:48.80,1:11:50.01,Default,,0,0,0,,Tại sao?
Dialogue: 0,1:11:50.09,1:11:51.59,Default,,0,0,0,,Đây đâu phải những gì anh đã hứa!
Dialogue: 0,1:11:51.68,1:11:52.97,Default,,0,0,0,,Lời hứa gì?
Dialogue: 0,1:11:53.47,1:11:54.81,Default,,0,0,0,,Ta không nhớ.
Dialogue: 0,1:11:54.89,1:11:57.14,Default,,0,0,0,,Đây là phần thưởng của ngươi.
Dialogue: 0,1:12:03.90,1:12:05.40,Default,,0,0,0,,Tiến sĩ, tại sao…
Dialogue: 0,1:12:05.48,1:12:06.73,Default,,0,0,0,,Chạy đi!
Dialogue: 0,1:12:06.82,1:12:07.78,Default,,0,0,0,,Bố!
Dialogue: 0,1:12:07.86,1:12:09.32,Default,,0,0,0,,Đừng đến đây!
Dialogue: 0,1:12:11.20,1:12:12.32,Default,,0,0,0,,Melissa!
Dialogue: 0,1:12:16.62,1:12:19.00,Default,,0,0,0,,Sau từng ấy chuyện\Nmà vẫn muốn chơi trò anh hùng à?
Dialogue: 0,1:12:19.08,1:12:20.12,Default,,0,0,0,,Vô ích thôi.
Dialogue: 0,1:12:20.79,1:12:24.58,Default,,0,0,0,,Dù lý do của ngươi là gì,\Nngươi cũng đã vấy bẩn tay mình.
Dialogue: 0,1:12:25.21,1:12:28.30,Default,,0,0,0,,Dù bọn ta là thật hay giả,
Dialogue: 0,1:12:28.38,1:12:30.59,Default,,0,0,0,,tội ác ngươi đã gây ra\Nsẽ không bao giờ biến mất.
Dialogue: 0,1:12:31.59,1:12:33.51,Default,,0,0,0,,Ngươi cũng như bọn ta thôi.
Dialogue: 0,1:12:34.26,1:12:39.43,Default,,0,0,0,,Ngươi không thể làm nhà khoa học\Ncũng như tiếp tục nghiên cứu được nữa.
Dialogue: 0,1:12:40.23,1:12:42.89,Default,,0,0,0,,{\an8}Ngươi sẽ chỉ chìm vào bóng tối\Nnhư villain bọn ta thôi.
Dialogue: 0,1:12:45.65,1:12:51.24,Default,,0,0,0,,Việc duy nhất ngươi có thể làm bây giờ\Nlà sản xuất thiết bị này với số lượng lớn.
Dialogue: 0,1:12:52.90,1:12:54.61,Default,,0,0,0,,Này, đưa hắn đi đi.
Dialogue: 0,1:12:54.70,1:12:56.28,Default,,0,0,0,,- Trả cho tôi!\N- Cái gì?
Dialogue: 0,1:12:56.87,1:13:00.16,Default,,0,0,0,,Trả bố lại cho tôi!
Dialogue: 0,1:13:00.25,1:13:01.62,Default,,0,0,0,,Phải rồi.
Dialogue: 0,1:13:02.12,1:13:04.83,Default,,0,0,0,,Ta phải diệt trừ hết\Nnhững gì gần gũi với tiến sĩ.
Dialogue: 0,1:13:04.92,1:13:07.50,Default,,0,0,0,,Dừng lại!
Dialogue: 0,1:13:08.30,1:13:10.80,Default,,0,0,0,,{\i1}Smash!{\i0}
Dialogue: 0,1:13:14.13,1:13:15.14,Default,,0,0,0,,{\i1}Chị Melissa!{\i0}
Dialogue: 0,1:13:16.72,1:13:18.81,Default,,0,0,0,,{\i1}Em sẽ cứu tiến sĩ và chú Sam!{\i0}
Dialogue: 0,1:13:21.14,1:13:22.60,Default,,0,0,0,,{\i1}Thế nên hãy đi tìm mọi người đi!{\i0}
Dialogue: 0,1:13:27.27,1:13:29.73,Default,,0,0,0,,- Đuổi theo nó! Đừng để nó thoát!\N- Vâng!
Dialogue: 0,1:13:38.62,1:13:40.12,Default,,0,0,0,,Đừng hòng qua đây!
Dialogue: 0,1:13:41.62,1:13:43.96,Default,,0,0,0,,Đừng có mà thể hiện!
Dialogue: 0,1:13:46.25,1:13:47.38,Default,,0,0,0,,{\i1}Mọi người…{\i0}
Dialogue: 0,1:13:48.08,1:13:48.96,Default,,0,0,0,,{\i1}Mọi người…{\i0}
Dialogue: 0,1:13:49.04,1:13:51.09,Default,,0,0,0,,{\i1}All Might…{\i0}
Dialogue: 0,1:13:51.17,1:13:52.46,Default,,0,0,0,,{\i1}Bố…{\i0}
Dialogue: 0,1:13:53.63,1:13:55.59,Default,,0,0,0,,{\i1}Chắc chắn mình sẽ…{\i0}
Dialogue: 0,1:13:55.68,1:13:57.30,Default,,0,0,0,,{\i1}Chắc chắn mình sẽ…{\i0}
Dialogue: 0,1:13:57.39,1:13:58.80,Default,,0,0,0,,{\i1}cứu được mọi người!{\i0}
Dialogue: 0,1:14:00.68,1:14:01.81,Default,,0,0,0,,BÌNH THƯỜNG
Dialogue: 0,1:14:05.39,1:14:08.44,Default,,0,0,0,,Chết tiệt.\NNó khôi phục hệ thống an ninh rồi à?
Dialogue: 0,1:14:11.23,1:14:12.28,Default,,0,0,0,,Có chuyện gì vậy?
Dialogue: 0,1:14:12.36,1:14:13.57,Default,,0,0,0,,Chúng dừng lại rồi?
Dialogue: 0,1:14:13.65,1:14:16.40,Default,,0,0,0,,Midoriya và chị Melissa thành công rồi à?
Dialogue: 0,1:14:17.07,1:14:20.41,Default,,0,0,0,,{\i1}Hệ thống an ninh của I-Island\Nhiện đang hoạt động bình thường.{\i0}
Dialogue: 0,1:14:20.49,1:14:23.41,Default,,0,0,0,,- Bình thường lại rồi.\N- Điện thoại có sóng rồi!
Dialogue: 0,1:14:24.54,1:14:25.87,Default,,0,0,0,,Bỗng dưng thế này là sao?
Dialogue: 0,1:14:25.96,1:14:28.12,Default,,0,0,0,,- Có chuyện gì vậy?\N- Chết tiệt!
Dialogue: 0,1:14:30.21,1:14:32.63,Default,,0,0,0,,{\i1}Là do mấy đứa làm đúng không?{\i0}
Dialogue: 0,1:14:34.80,1:14:36.68,Default,,0,0,0,,- Trực thăng đâu?\N- Đã hạ cánh rồi.
Dialogue: 0,1:14:37.26,1:14:39.43,Default,,0,0,0,,- Ra khỏi đây thôi.\N- Vâng.
Dialogue: 0,1:14:49.56,1:14:51.61,Default,,0,0,0,,{\i1}Cử động đi, Midoriya Izuku!{\i0}
Dialogue: 0,1:14:52.27,1:14:54.53,Default,,0,0,0,,{\i1}Mày ở đây để làm gì cơ chứ?{\i0}
Dialogue: 0,1:14:55.78,1:14:59.11,Default,,0,0,0,,{\i1}Mình sẽ làm hết những gì có thể\Nvào lúc này.{\i0}
Dialogue: 0,1:15:01.99,1:15:03.37,Default,,0,0,0,,{\i1}Sao vậy, Melissa?{\i0}
Dialogue: 0,1:15:03.45,1:15:06.29,Default,,0,0,0,,Chú Might,\Nbố cháu bị lũ villain bắt đi rồi,
Dialogue: 0,1:15:07.00,1:15:08.21,Default,,0,0,0,,Deku đang đuổi theo chúng!
Dialogue: 0,1:15:08.29,1:15:09.58,Default,,0,0,0,,Không sao đâu.
Dialogue: 0,1:15:10.75,1:15:12.00,Default,,0,0,0,,Chú sẽ đuổi theo!
Dialogue: 0,1:15:17.01,1:15:18.51,Default,,0,0,0,,Đại ca, những người khác thì sao?
Dialogue: 0,1:15:18.59,1:15:21.72,Default,,0,0,0,,Phải rời đi trước khi\Nhệ thống an ninh khôi phục hoàn toàn.
Dialogue: 0,1:15:21.80,1:15:22.76,Default,,0,0,0,,- Vâng!\N- Vâng!
Dialogue: 0,1:15:23.47,1:15:26.06,Default,,0,0,0,,Giết tôi đi…
Dialogue: 0,1:15:26.14,1:15:29.10,Default,,0,0,0,,Ta sẽ chỉ phạm thêm vài tội nữa thôi.
Dialogue: 0,1:15:29.98,1:15:32.23,Default,,0,0,0,,Sau đó, ta sẽ biến ước mơ của ngươi\Nthành sự thật.
Dialogue: 0,1:15:37.15,1:15:37.99,Default,,0,0,0,,Khoan đã!
Dialogue: 0,1:15:40.20,1:15:42.87,Default,,0,0,0,,- Trả tiến sĩ lại đây!\N- Hả?
Dialogue: 0,1:15:42.95,1:15:44.28,Default,,0,0,0,,Ra là thế.
Dialogue: 0,1:15:44.78,1:15:48.00,Default,,0,0,0,,Ngươi đến đây để giành lại\Ntên tội phạm này à?
Dialogue: 0,1:15:48.08,1:15:49.58,Default,,0,0,0,,Không!
Dialogue: 0,1:15:49.66,1:15:53.59,Default,,0,0,0,,Tôi đến đây để cứu tiến sĩ!
Dialogue: 0,1:15:53.67,1:15:55.80,Default,,0,0,0,,Cứu một tên tội phạm ư?
Dialogue: 0,1:15:57.42,1:15:59.92,Default,,0,0,0,,Tôi sẽ cứu tất cả mọi người!
Dialogue: 0,1:16:00.01,1:16:04.51,Default,,0,0,0,,- Tôi cũng sẽ cứu tiến sĩ!\N- Ngươi đang nói gì vậy?
Dialogue: 0,1:16:04.60,1:16:07.81,Default,,0,0,0,,Im đi! Anh hùng là thế đấy!
Dialogue: 0,1:16:07.89,1:16:10.52,Default,,0,0,0,,{\an8}Họ cứu những ai đang gặp nạn!
Dialogue: 0,1:16:10.60,1:16:11.94,Default,,0,0,0,,Bằng cách nào?
Dialogue: 0,1:16:13.86,1:16:15.23,Default,,0,0,0,,Đừng lo cho chú.
Dialogue: 0,1:16:15.32,1:16:16.15,Default,,0,0,0,,Tránh ra đi!
Dialogue: 0,1:16:18.15,1:16:21.91,Default,,0,0,0,,Hừ! Làm anh hùng bất tiện thật đấy.
Dialogue: 0,1:16:21.99,1:16:25.20,Default,,0,0,0,,{\an8}Ta chẳng làm gì mấy\Nmà ngươi đã không thể cử động rồi.
Dialogue: 0,1:16:35.13,1:16:38.00,Default,,0,0,0,,Dù có thế nào thì đó cũng không phải\Nmột cách sống thông mình.
Dialogue: 0,1:16:39.71,1:16:41.42,Default,,0,0,0,,- Đưa bọn tao đi!\N- Rõ!
Dialogue: 0,1:16:52.44,1:16:56.48,Default,,0,0,0,,{\i1}Hắn không thể kích hoạt Kosei\Nnếu mình không chạm vào tay hắn!{\i0}
Dialogue: 0,1:17:02.53,1:17:03.57,Default,,0,0,0,,Cháu…
Dialogue: 0,1:17:07.62,1:17:08.91,Default,,0,0,0,,Deku!
Dialogue: 0,1:17:11.04,1:17:12.46,Default,,0,0,0,,Tiến sĩ!
Dialogue: 0,1:17:12.96,1:17:13.79,Default,,0,0,0,,Dừng lại đi.
Dialogue: 0,1:17:13.87,1:17:15.38,Default,,0,0,0,,Midoriya, tránh ra đi!
Dialogue: 0,1:17:15.46,1:17:16.71,Default,,0,0,0,,Chị Melissa…
Dialogue: 0,1:17:17.38,1:17:19.38,Default,,0,0,0,,{\i1}Chị Melissa đang đợi chú đấy!{\i0}
Dialogue: 0,1:17:21.51,1:17:24.09,Default,,0,0,0,,{\an8}Ngươi chẳng phải là anh hùng gì cả,
Dialogue: 0,1:17:24.72,1:17:25.84,Default,,0,0,0,,mà là một kẻ ngốc.
Dialogue: 0,1:17:28.47,1:17:30.81,Default,,0,0,0,,{\i1}Đừng bỏ cuộc. Động não đi!{\i0}
Dialogue: 0,1:17:30.89,1:17:33.14,Default,,0,0,0,,{\i1}Giờ thì sao? Phải làm gì đây?{\i0}
Dialogue: 0,1:17:34.48,1:17:35.81,Default,,0,0,0,,Deku!
Dialogue: 0,1:17:37.69,1:17:38.98,Default,,0,0,0,,Ngủ ngon nhé!
Dialogue: 0,1:17:42.24,1:17:43.36,Default,,0,0,0,,Chết tiệt!
Dialogue: 0,1:17:44.28,1:17:45.91,Default,,0,0,0,,- Chết tiệt!\N- Deku!
Dialogue: 0,1:17:45.99,1:17:47.37,Default,,0,0,0,,Trả chú ấy lại đây!
Dialogue: 0,1:17:47.45,1:17:49.70,Default,,0,0,0,,Trả tiến sĩ lại đây!
Dialogue: 0,1:17:49.78,1:17:52.08,Default,,0,0,0,,Chết tiệt!
Dialogue: 0,1:17:52.16,1:17:55.50,Default,,0,0,0,,{\an8}Cháu nên cười vào những lúc thế này,\Nnhóc Midoriya!
Dialogue: 0,1:18:03.26,1:18:05.18,Default,,0,0,0,,Không sao rồi.
Dialogue: 0,1:18:05.26,1:18:06.68,Default,,0,0,0,,Tại sao ư?
Dialogue: 0,1:18:09.10,1:18:10.68,Default,,0,0,0,,Vì đã có tôi ở đây!
Dialogue: 0,1:18:12.52,1:18:13.60,Default,,0,0,0,,All Might…
Dialogue: 0,1:18:13.68,1:18:18.52,Default,,0,0,0,,{\an8}Ngươi sẽ phải trả bạn thân của ta lại,\Ntên villain kia!
Dialogue: 0,1:18:27.86,1:18:28.87,Default,,0,0,0,,Bố…
Dialogue: 0,1:18:29.74,1:18:30.58,Default,,0,0,0,,Bố!
Dialogue: 0,1:18:32.87,1:18:34.54,Default,,0,0,0,,Melissa…
Dialogue: 0,1:18:35.25,1:18:36.62,Default,,0,0,0,,Không sao rồi.
Dialogue: 0,1:18:37.71,1:18:39.29,Default,,0,0,0,,Tốt quá rồi.
Dialogue: 0,1:18:41.50,1:18:42.63,Default,,0,0,0,,All Might.
Dialogue: 0,1:18:43.59,1:18:44.46,Default,,0,0,0,,Tớ…
Dialogue: 0,1:18:46.55,1:18:47.93,Default,,0,0,0,,All Might!
Dialogue: 0,1:18:50.64,1:18:52.31,Default,,0,0,0,,- Bố!\N- Tiến sĩ!
Dialogue: 0,1:18:55.31,1:18:56.35,Default,,0,0,0,,Tên Sam đáng chết đó.
Dialogue: 0,1:18:56.43,1:18:59.40,Default,,0,0,0,,Hắn nói rằng\NKosei của All Might đang suy yếu
Dialogue: 0,1:18:59.48,1:19:03.19,Default,,0,0,0,,và hắn không còn mạnh như trước nữa.
Dialogue: 0,1:19:03.27,1:19:04.53,Default,,0,0,0,,Tên đó…
Dialogue: 0,1:19:04.61,1:19:05.61,Default,,0,0,0,,Thiết bị của tiến sĩ…
Dialogue: 0,1:19:07.78,1:19:10.32,Default,,0,0,0,,Chết tiệt! Sắp hết thời gian rồi…
Dialogue: 0,1:19:11.62,1:19:13.95,Default,,0,0,0,,Ngươi không biết\Nkhi nào thì nên bỏ cuộc nhỉ?
Dialogue: 0,1:19:14.45,1:19:19.33,Default,,0,0,0,,Texas Smash!
Dialogue: 0,1:19:20.75,1:19:22.79,Default,,0,0,0,,- Cái gì?\N- All Might!
Dialogue: 0,1:19:22.88,1:19:24.30,Default,,0,0,0,,{\an8}Gì thế này?
Dialogue: 0,1:19:43.36,1:19:46.28,Default,,0,0,0,,Không hổ là phát minh của David Shield.
Dialogue: 0,1:19:47.03,1:19:50.03,Default,,0,0,0,,Ta có thể nhận thấy\NKosei của mình đang tái sinh.
Dialogue: 0,1:19:51.36,1:19:54.95,Default,,0,0,0,,Hay đấy. Thiết bị này rất tuyệt!
Dialogue: 0,1:19:55.58,1:19:57.70,Default,,0,0,0,,Đây là phát minh của Dave…
Dialogue: 0,1:19:58.54,1:20:01.21,Default,,0,0,0,,Đó là sức mạnh của thiết bị mà bố tạo ra…
Dialogue: 0,1:20:03.71,1:20:05.46,Default,,0,0,0,,Giờ thì…
Dialogue: 0,1:20:05.55,1:20:08.88,Default,,0,0,0,,để tăng giá trị của thiết bị này,
Dialogue: 0,1:20:08.97,1:20:13.47,Default,,0,0,0,,hay chúng ta biểu diễn\Ncách nó hạ gục All Might nhé?
Dialogue: 0,1:20:27.32,1:20:28.44,Default,,0,0,0,,{\i1}All Might…{\i0}
Dialogue: 0,1:20:29.07,1:20:30.65,Default,,0,0,0,,{\i1}Đúng như mình nghĩ…{\i0}
Dialogue: 0,1:20:31.28,1:20:32.86,Default,,0,0,0,,{\i1}Chú ấy đến giới hạn rồi!{\i0}
Dialogue: 0,1:20:39.95,1:20:40.96,Default,,0,0,0,,Chú Might!
Dialogue: 0,1:20:46.38,1:20:49.13,Default,,0,0,0,,Mau bị đè bẹp đi!
Dialogue: 0,1:20:49.21,1:20:50.88,Default,,0,0,0,,{\an8}All Might!
Dialogue: 0,1:20:52.63,1:20:53.55,Default,,0,0,0,,Cái gì?
Dialogue: 0,1:20:53.63,1:20:55.72,Default,,0,0,0,,Đi chết đi!
Dialogue: 0,1:21:01.39,1:21:06.36,Default,,0,0,0,,Sao ông có thể để tên trùm cuối vớ vẩn này\Nđánh bại hả, All Might?
Dialogue: 0,1:21:06.94,1:21:08.23,Default,,0,0,0,,Nhóc Bakugo!
Dialogue: 0,1:21:08.32,1:21:10.61,Default,,0,0,0,,Hãy tận dụng cơ hội này\Nđể hạ gục villain đi…
Dialogue: 0,1:21:11.24,1:21:13.32,Default,,0,0,0,,Todoroki! Các cậu!
Dialogue: 0,1:21:13.40,1:21:16.24,Default,,0,0,0,,Bọn tớ sẽ giải quyết mấy khối kim loại!
Dialogue: 0,1:21:16.32,1:21:18.83,Default,,0,0,0,,- Yaoyorozu, cậu lo chuyện ở đây nhé!\N- Được!
Dialogue: 0,1:21:18.91,1:21:23.21,Default,,0,0,0,,Học sinh của ta đang rất nhiệt huyết,
Dialogue: 0,1:21:23.29,1:21:26.67,Default,,0,0,0,,ta không thể bị cái giới hạn ngu ngốc này\Nlàm vướng chân được!
Dialogue: 0,1:21:27.21,1:21:31.71,Default,,0,0,0,,Ta phải vượt qua giới hạn của mình,\Nvà vượt xa hơn thế nữa!
Dialogue: 0,1:21:32.84,1:21:33.67,Default,,0,0,0,,Đúng vậy!
Dialogue: 0,1:21:33.76,1:21:36.59,Default,,0,0,0,,Plus Ultra!
Dialogue: 0,1:21:52.82,1:21:56.70,Default,,0,0,0,,{\an8}Carolina Smash!
Dialogue: 0,1:22:00.79,1:22:04.04,Default,,0,0,0,,Chuẩn bị đi, villain!
Dialogue: 0,1:22:06.58,1:22:08.04,Default,,0,0,0,,Ngươi chỉ có thế này thôi…
Dialogue: 0,1:22:10.55,1:22:12.84,Default,,0,0,0,,Ta chuẩn bị ư?
Dialogue: 0,1:22:12.92,1:22:14.22,Default,,0,0,0,,Ngươi chuẩn bị thì đúng hơn…
Dialogue: 0,1:22:15.68,1:22:17.39,Default,,0,0,0,,All Might.
Dialogue: 0,1:22:17.97,1:22:20.10,Default,,0,0,0,,{\i1}Sức mạnh gì thế này?{\i0}
Dialogue: 0,1:22:26.23,1:22:27.65,Default,,0,0,0,,All Might!
Dialogue: 0,1:22:28.81,1:22:29.65,Default,,0,0,0,,Deku!
Dialogue: 0,1:22:29.73,1:22:32.07,Default,,0,0,0,,Chết tiệt!
Dialogue: 0,1:22:36.32,1:22:39.66,Default,,0,0,0,,{\i1}Sức mạnh này giúp tăng cường cơ bắp…{\i0}
Dialogue: 0,1:22:39.74,1:22:41.74,Default,,0,0,0,,{\i1}Hắn có nhiều Kosei…{\i0}
Dialogue: 0,1:22:41.83,1:22:43.66,Default,,0,0,0,,{\an8}Đừng nói là…
Dialogue: 0,1:22:43.74,1:22:45.25,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,1:22:45.33,1:22:50.13,Default,,0,0,0,,Khi bọn ta lên kế hoạch đánh chiếm,\Nta đã nhận được điện thoại của người đó.
Dialogue: 0,1:22:51.34,1:22:54.05,Default,,0,0,0,,{\i1}Hắn nói rằng hắn muốn hợp tác với ta.{\i0}
Dialogue: 0,1:22:54.13,1:22:56.30,Default,,0,0,0,,Khi ta hỏi lý do,
Dialogue: 0,1:22:56.88,1:22:59.18,Default,,0,0,0,,hắn nói thế này…
Dialogue: 0,1:22:59.68,1:23:03.93,Default,,0,0,0,,Nếu bạn thân của All Might muốn\Nnhúng tay vào chuyện xấu,
Dialogue: 0,1:23:04.02,1:23:06.81,Default,,0,0,0,,vậy thì ta phải giúp rồi.
Dialogue: 0,1:23:07.39,1:23:09.85,Default,,0,0,0,,{\i1}Thật đáng tiếc là\Nta sẽ không thể thấy được{\i0}
Dialogue: 0,1:23:09.94,1:23:14.36,Default,,0,0,0,,{\i1}gương mặt đau đớn của All Might\Nkhi hắn phát hiện ra.{\i0}
Dialogue: 0,1:23:15.86,1:23:18.32,Default,,0,0,0,,{\an8}All For One!
Dialogue: 0,1:23:18.82,1:23:21.95,Default,,0,0,0,,Cuối cùng ngươi cũng ngừng cười rồi à?
Dialogue: 0,1:23:22.03,1:23:24.45,Default,,0,0,0,,Không!
Dialogue: 0,1:23:33.13,1:23:35.00,Default,,0,0,0,,- All Might!\N- All Might!
Dialogue: 0,1:23:35.09,1:23:36.09,Default,,0,0,0,,Chú…
Dialogue: 0,1:23:36.17,1:23:38.84,Default,,0,0,0,,Vĩnh biệt, All Might!
Dialogue: 0,1:23:42.05,1:23:44.64,Default,,0,0,0,,Chú Might!
Dialogue: 0,1:23:47.56,1:23:49.64,Default,,0,0,0,,{\i1}Detroit…{\i0}
Dialogue: 0,1:23:50.90,1:23:54.61,Default,,0,0,0,,{\i1}Smash!{\i0}
Dialogue: 0,1:23:57.53,1:23:58.61,Default,,0,0,0,,Thằng nhóc đó!
Dialogue: 0,1:24:08.33,1:24:09.41,Default,,0,0,0,,Nhóc Midoriya!
Dialogue: 0,1:24:09.50,1:24:11.54,Default,,0,0,0,,Với cơ thể này sao cháu có thể…
Dialogue: 0,1:24:11.62,1:24:13.04,Default,,0,0,0,,Liều lĩnh quá đấy!
Dialogue: 0,1:24:13.54,1:24:14.67,Default,,0,0,0,,Nhưng…
Dialogue: 0,1:24:15.59,1:24:18.80,Default,,0,0,0,,anh hùng phải cứu những người gặp nạn!
Dialogue: 0,1:24:24.39,1:24:25.22,Default,,0,0,0,,Cảm ơn nhé.
Dialogue: 0,1:24:25.72,1:24:29.18,Default,,0,0,0,,Đúng là ta vừa gặp nạn đấy.
Dialogue: 0,1:24:29.27,1:24:32.19,Default,,0,0,0,,Giúp ta một tay nhé, nhóc Midoriya.
Dialogue: 0,1:24:32.27,1:24:33.52,Default,,0,0,0,,Vâng!
Dialogue: 0,1:24:42.57,1:24:43.86,Default,,0,0,0,,- Đi thôi!\N- Vâng!
Dialogue: 0,1:24:45.99,1:24:49.58,Default,,0,0,0,,Thằng nhãi vô dụng!
Dialogue: 0,1:24:50.08,1:24:51.58,Default,,0,0,0,,Ngươi chẳng là gì ngoài rác rưởi!
Dialogue: 0,1:24:52.08,1:24:55.50,Default,,0,0,0,,{\an8}Ngươi không hề biết lúc nào nên bỏ cuộc!
Dialogue: 0,1:24:55.59,1:24:58.84,Default,,0,0,0,,Kẻ đó là mày mới đúng!
Dialogue: 0,1:25:04.55,1:25:06.60,Default,,0,0,0,,Đừng hòng đắc ý!
Dialogue: 0,1:25:12.10,1:25:13.89,Default,,0,0,0,,Tránh ra!
Dialogue: 0,1:26:02.78,1:26:03.82,Default,,0,0,0,,Đau quá.
Dialogue: 0,1:26:07.28,1:26:09.12,Default,,0,0,0,,Cái gì thế?
Dialogue: 0,1:26:10.37,1:26:13.33,Default,,0,0,0,,{\i1}Vượt qua những khủng hoảng trước mắt mình…{\i0}
Dialogue: 0,1:26:14.00,1:26:16.33,Default,,0,0,0,,{\i1}…với tất cả những gì mình có…{\i0}
Dialogue: 0,1:26:16.42,1:26:18.29,Default,,0,0,0,,{\i1}…và cứu người…{\i0}
Dialogue: 0,1:26:18.38,1:26:20.34,Default,,0,0,0,,{\i1}…với tất cả những gì mình có…{\i0}
Dialogue: 0,1:26:20.42,1:26:22.09,Default,,0,0,0,,{\i1}…chính là thứ tạo nên…{\i0}
Dialogue: 0,1:26:22.59,1:26:24.09,Default,,0,0,0,,{\i1}…một anh hùng!{\i0}
Dialogue: 0,1:26:24.17,1:26:28.34,Default,,0,0,0,,Ta sẽ nghiền nát các ngươi\Ncùng với tòa tháp này!
Dialogue: 0,1:26:28.43,1:26:33.89,Default,,0,0,0,,{\i1}- Double Detroit…\N- Double Detroit…{\i0}
Dialogue: 0,1:26:33.97,1:26:37.56,Default,,0,0,0,,{\i1}- Smash!\N- Smash!{\i0}
Dialogue: 0,1:27:00.84,1:27:02.50,Default,,0,0,0,,Tiến lên!
Dialogue: 0,1:27:02.59,1:27:03.75,Default,,0,0,0,,- All Might!\N- All Might!
Dialogue: 0,1:27:03.84,1:27:05.05,Default,,0,0,0,,- Midoriya!\N- Midoriya!
Dialogue: 0,1:27:05.13,1:27:07.01,Default,,0,0,0,,- Hạ hắn đi!\N- Hạ hắn đi!
Dialogue: 0,1:27:10.43,1:27:11.76,Default,,0,0,0,,Hơn…
Dialogue: 0,1:27:11.85,1:27:13.51,Default,,0,0,0,,…thế nữa!
Dialogue: 0,1:27:14.85,1:27:16.18,Default,,0,0,0,,- Plus…\N- Plus…
Dialogue: 0,1:27:16.27,1:27:19.85,Default,,0,0,0,,- Ultra!\N- Ultra!
Dialogue: 0,1:28:01.73,1:28:02.81,Default,,0,0,0,,Họ hạ được hắn chưa?
Dialogue: 0,1:28:03.98,1:28:05.23,Default,,0,0,0,,Họ làm được rồi!
Dialogue: 0,1:28:05.73,1:28:08.07,Default,,0,0,0,,Họ đã kết liễu tên villain!
Dialogue: 0,1:28:08.15,1:28:09.78,Default,,0,0,0,,- Phải!\N- Được rồi!
Dialogue: 0,1:28:09.86,1:28:12.28,Default,,0,0,0,,- Họ làm được rồi!\N- Kết thúc rồi!
Dialogue: 0,1:28:12.36,1:28:14.83,Default,,0,0,0,,- Phải! Kết thúc rồi!\N- Phải!
Dialogue: 0,1:28:31.18,1:28:32.01,Default,,0,0,0,,Deku!
Dialogue: 0,1:28:33.18,1:28:34.35,Default,,0,0,0,,Chị Melissa!
Dialogue: 0,1:28:34.43,1:28:35.55,Default,,0,0,0,,Tốt quá rồi!
Dialogue: 0,1:28:36.81,1:28:37.64,Default,,0,0,0,,Dave.
Dialogue: 0,1:28:38.56,1:28:39.56,Default,,0,0,0,,Dave!
Dialogue: 0,1:28:42.14,1:28:44.36,Default,,0,0,0,,All Might…
Dialogue: 0,1:28:45.65,1:28:48.11,Default,,0,0,0,,Tớ đến cứu cậu đây, Dave.
Dialogue: 0,1:28:48.82,1:28:49.86,Default,,0,0,0,,Cảm ơn cậu.
Dialogue: 0,1:28:50.49,1:28:52.24,Default,,0,0,0,,Bố!
Dialogue: 0,1:28:52.32,1:28:53.91,Default,,0,0,0,,All Might!
Dialogue: 0,1:28:54.70,1:28:58.16,Default,,0,0,0,,Cậu nên cảm ơn Melissa\Nvà nhóc Midoriya mới đúng.
Dialogue: 0,1:28:58.83,1:29:00.16,Default,,0,0,0,,Tốt quá rồi.
Dialogue: 0,1:29:01.41,1:29:02.50,Default,,0,0,0,,Thật đấy.
Dialogue: 0,1:29:03.62,1:29:04.79,Default,,0,0,0,,Cảm ơn em nhé.
Dialogue: 0,1:29:04.88,1:29:08.50,Default,,0,0,0,,Nhờ có các em\Nmà chúng ta có thể cứu được mọi người.
Dialogue: 0,1:29:09.17,1:29:10.51,Default,,0,0,0,,Nhờ có cả chị Melissa nữa.
Dialogue: 0,1:29:11.05,1:29:16.01,Default,,0,0,0,,Em đã được cứu rất nhiều lần\Nnhờ Full Gauntlet của chị.
Dialogue: 0,1:29:17.05,1:29:18.39,Default,,0,0,0,,Cảm ơn chị rất nhiều!
Dialogue: 0,1:29:20.10,1:29:21.31,Default,,0,0,0,,Deku…
Dialogue: 0,1:29:21.85,1:29:24.52,Default,,0,0,0,,Nhưng mà em xin lỗi! Em làm vỡ mất rồi…
Dialogue: 0,1:29:25.56,1:29:26.61,Default,,0,0,0,,Đừng bận tâm.
Dialogue: 0,1:29:27.11,1:29:28.98,Default,,0,0,0,,Deku!
Dialogue: 0,1:29:29.07,1:29:30.78,Default,,0,0,0,,- Chị Melissa!\N- Cậu làm được rồi!
Dialogue: 0,1:29:30.86,1:29:32.82,Default,,0,0,0,,Hai người không sao chứ?
Dialogue: 0,1:29:32.90,1:29:36.57,Default,,0,0,0,,Bọn tớ vẫn ổn!\NAll Might và tiến sĩ cũng không sao cả!
Dialogue: 0,1:29:36.66,1:29:39.24,Default,,0,0,0,,Các em không sao chứ?
Dialogue: 0,1:29:39.33,1:29:41.16,Default,,0,0,0,,- Không sao hết ạ!\N- Không sao hết ạ!
Dialogue: 0,1:29:41.95,1:29:44.87,Default,,0,0,0,,Melissa kể tớ nghe hết rồi.
Dialogue: 0,1:29:47.13,1:29:48.21,Default,,0,0,0,,Tớ…
Dialogue: 0,1:29:49.46,1:29:51.34,Default,,0,0,0,,Tớ sợ mất đi hào quang của cậu…
Dialogue: 0,1:29:52.21,1:29:55.05,Default,,0,0,0,,Tớ sợ rằng nền hòa bình\Ncậu dựng nên sẽ sụp đổ.
Dialogue: 0,1:29:56.93,1:30:01.06,Default,,0,0,0,,Nhưng ý tưởng của tớ và thiết bị đó
Dialogue: 0,1:30:01.56,1:30:04.35,Default,,0,0,0,,cũng chỉ là cách\Nđể giữ nguyên hiện trạng mà thôi.
Dialogue: 0,1:30:06.60,1:30:10.40,Default,,0,0,0,,Dù tương lai và hy vọng\Nđang ở ngay trước mắt tớ,
Dialogue: 0,1:30:10.48,1:30:12.69,Default,,0,0,0,,tớ lại không hề phát hiện ra.
Dialogue: 0,1:30:14.49,1:30:18.28,Default,,0,0,0,,Cũng như Melissa đang nối bước tớ,
Dialogue: 0,1:30:18.95,1:30:19.95,Default,,0,0,0,,cậu có Midoriya Izuku.
Dialogue: 0,1:30:21.12,1:30:24.25,Default,,0,0,0,,Cậu bé là người kế nhiệm của cậu à?
Dialogue: 0,1:30:25.71,1:30:27.58,Default,,0,0,0,,Nhóc ấy vẫn chưa sẵn sàng đâu.
Dialogue: 0,1:30:28.75,1:30:33.88,Default,,0,0,0,,Nhưng nhóc ấy có tiềm năng\Ntỏa sáng hơn bất cứ anh hùng nào khác.
Dialogue: 0,1:30:37.64,1:30:39.76,Default,,0,0,0,,Tớ cũng thấy rồi, Toshi.
Dialogue: 0,1:30:41.64,1:30:43.31,Default,,0,0,0,,Một hào quang giống như của cậu…
Dialogue: 0,1:30:45.43,1:30:48.48,Default,,0,0,0,,Hào quang của một anh hùng.
Dialogue: 0,1:34:22.15,1:34:26.07,Default,,0,0,0,,{\an8}BỆNH VIỆN
Dialogue: 0,1:35:40.44,1:35:45.44,Default,,0,0,0,,{\an8}Biên dịch: Lâm Nguyệt Quỳnh

Dialogue: 0,0:00:00.00,0:00:00.00,Default,,0,0,0,,Hắc xì!
Dialogue: 0,0:00:00.00,0:00:00.00,Default,,0,0,0,,Dừng lại!
Dialogue: 0,0:00:00.00,0:00:00.00,Default,,0,0,0,,Á!
Dialogue: 0,0:01:23.46,0:01:27.46,Default,,0,0,0,,NETHD.ORG\NTRÂN TRỌNG GIỚI THIỆU
Dialogue: 0,0:01:28.79,0:01:30.79,Default,,0,0,0,,{\i1}Những năm cuối triều Thương,{\i0}
Dialogue: 0,0:01:30.88,0:01:33.17,Default,,0,0,0,,{\i1}Trụ Vương ngu xuẩn vô đạo.{\i0}
Dialogue: 0,0:01:33.25,0:01:35.75,Default,,0,0,0,,{\i1}Cửu Vĩ hồ yêu thừa loạn tác quái{\i0}
Dialogue: 0,0:01:35.83,0:01:37.66,Default,,0,0,0,,{\i1}hóa thành vương phi Đát Kỷ{\i0}
Dialogue: 0,0:01:37.75,0:01:39.29,Default,,0,0,0,,{\i1}chi phối triều chính.{\i0}
Dialogue: 0,0:01:39.38,0:01:40.71,Default,,0,0,0,,{\i1}Lãnh đạo Hồ tộc{\i0}
Dialogue: 0,0:01:40.79,0:01:42.29,Default,,0,0,0,,{\i1}họa loạn thế gian,{\i0}
Dialogue: 0,0:01:43.67,0:01:45.46,Default,,0,0,0,,{\i1}chúng sinh khốn khổ.{\i0}
Dialogue: 0,0:01:45.54,0:01:47.12,Default,,0,0,0,,{\i1}Trời cao thương tình{\i0}
Dialogue: 0,0:01:47.83,0:01:50.37,Default,,0,0,0,,{\i1}lệnh cho chưởng giáo Tịnh Hư cung, Sư Tôn{\i0}
Dialogue: 0,0:01:50.46,0:01:52.29,Default,,0,0,0,,{\i1}trợ Tây Chu khởi nghĩa.{\i0}
Dialogue: 0,0:01:53.33,0:01:56.29,Default,,0,0,0,,{\i1}Một trận chiến cuốn theo cả tam giới.{\i0}
Dialogue: 0,0:02:12.04,0:02:14.62,Default,,0,0,0,,{\i1}Tịnh Hư cung phái đại đệ tử Khương Tử Nha{\i0}
Dialogue: 0,0:02:14.71,0:02:16.29,Default,,0,0,0,,{\i1}xuống núi trừ yêu,{\i0}
Dialogue: 0,0:02:16.38,0:02:17.94,Default,,0,0,0,,{\i1}tróc nã Cửu Vĩ.{\i0}
Dialogue: 0,0:02:21.88,0:02:23.96,Default,,0,0,0,,Ái phi!
Dialogue: 0,0:02:24.04,0:02:25.62,Default,,0,0,0,,Nàng ở đâu?\NÁi phi!
Dialogue: 0,0:02:28.00,0:02:29.00,Default,,0,0,0,,Cửu Vĩ!
Dialogue: 0,0:02:29.08,0:02:30.18,Default,,0,0,0,,Không được càn rỡ!
Dialogue: 0,0:02:32.67,0:02:34.38,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:02:44.33,0:02:45.46,Default,,0,0,0,,Huyễn thuật của hồ yêu
Dialogue: 0,0:02:46.29,0:02:47.50,Default,,0,0,0,,không việc gì phải sợ.
Dialogue: 0,0:02:49.42,0:02:51.38,Default,,0,0,0,,Chúng ta khác đường chung đích,
Dialogue: 0,0:02:52.25,0:02:54.96,Default,,0,0,0,,cần gì phải tranh thắng thua?
Dialogue: 0,0:02:57.92,0:03:02.17,Default,,0,0,0,,Người đời\Nrồi cũng sẽ thần phục Hồ tộc ta.
Dialogue: 0,0:03:05.25,0:03:07.21,Default,,0,0,0,,Trấn Ma bảo ấn ở đây!
Dialogue: 0,0:03:07.29,0:03:09.01,Default,,0,0,0,,Tà ma tam giới,
Dialogue: 0,0:03:09.08,0:03:10.41,Default,,0,0,0,,kẻ nghịch tất diệt.
Dialogue: 0,0:03:37.50,0:03:49.79,Default,,0,0,0,,KHƯƠNG TỬ NHA
Dialogue: 0,0:03:50.92,0:03:54.88,Default,,0,0,0,,MƯỜI NĂM SAU
Dialogue: 0,0:03:55.54,0:03:57.04,Default,,0,0,0,,{\i1}Trận chiến đã kết thúc.{\i0}
Dialogue: 0,0:03:57.54,0:03:59.50,Default,,0,0,0,,{\i1}Tịnh Hư cung có công diệt Trụ{\i0}
Dialogue: 0,0:03:59.58,0:04:01.21,Default,,0,0,0,,{\i1}được thế gian tôn sùng.{\i0}
Dialogue: 0,0:04:01.92,0:04:04.21,Default,,0,0,0,,{\i1}Sư Tôn phụng mệnh nắm giữ bảng Phong Thần,{\i0}
Dialogue: 0,0:04:04.29,0:04:07.37,Default,,0,0,0,,{\i1}sắc phong chư thần phù hộ chúng sinh.{\i0}
Dialogue: 0,0:04:07.88,0:04:11.21,Default,,0,0,0,,{\i1}Triều Chu thành lập, phục hồi tổn thất.{\i0}
Dialogue: 0,0:04:11.29,0:04:14.46,Default,,0,0,0,,{\i1}Tội dân triều Thương bị giam\Ntrong chiến trường cổ Bắc Hải{\i0}
Dialogue: 0,0:04:15.67,0:04:17.30,Default,,0,0,0,,{\i1}không được ra ngoài.{\i0}
Dialogue: 0,0:04:30.21,0:04:32.79,Default,,0,0,0,,{\i1}Khương Tử Nha công lao nổi trội,{\i0}
Dialogue: 0,0:04:32.88,0:04:34.42,Default,,0,0,0,,{\i1}người đời kính ngưỡng.{\i0}
Dialogue: 0,0:04:35.04,0:04:37.83,Default,,0,0,0,,{\i1}Vốn được kỳ vọng sẽ lãnh đạo chư thần.{\i0}
Dialogue: 0,0:04:38.92,0:04:40.02,Default,,0,0,0,,{\i1}Song...{\i0}
Dialogue: 0,0:05:09.96,0:05:11.34,Default,,0,0,0,,Ngươi nhìn đi...
Dialogue: 0,0:05:11.42,0:05:13.05,Default,,0,0,0,,Ta đã bảo đó là tảng đá kỳ cục mà.
Dialogue: 0,0:05:14.13,0:05:16.38,Default,,0,0,0,,Ở đâu ra tảng đá như vậy?
Dialogue: 0,0:05:16.46,0:05:17.56,Default,,0,0,0,,Chúng ta qua xem thử.
Dialogue: 0,0:05:18.13,0:05:19.23,Default,,0,0,0,,Đứng im!
Dialogue: 0,0:05:20.58,0:05:21.68,Default,,0,0,0,,Các ngươi thì hiểu cái gì?
Dialogue: 0,0:05:28.54,0:05:29.79,Default,,0,0,0,,Tảng đá đó
Dialogue: 0,0:05:29.88,0:05:31.26,Default,,0,0,0,,là thần tiên trên trời.
Dialogue: 0,0:05:31.33,0:05:32.54,Default,,0,0,0,,Thần tiên?
Dialogue: 0,0:05:32.63,0:05:33.76,Default,,0,0,0,,Đúng vậy!
Dialogue: 0,0:05:33.83,0:05:35.25,Default,,0,0,0,,Sau trận đại chiến,
Dialogue: 0,0:05:35.33,0:05:38.04,Default,,0,0,0,,{\i1}Tịnh Hư cung phụng mệnh thả thang trời.{\i0}
Dialogue: 0,0:05:38.13,0:05:41.17,Default,,0,0,0,,{\i1}Từ đó, người đời đã có con đường lên trời.{\i0}
Dialogue: 0,0:05:41.25,0:05:42.54,Default,,0,0,0,,{\i1}Mà Khương Tử Nha{\i0}
Dialogue: 0,0:05:42.63,0:05:45.51,Default,,0,0,0,,{\i1}chính là người đầu tiên\Nbước lên thang trời,{\i0}
Dialogue: 0,0:05:46.04,0:05:48.04,Default,,0,0,0,,{\i1}nhất chiến phong thần.{\i0}
Dialogue: 0,0:06:11.54,0:06:16.37,Default,,0,0,0,,TỊNH HƯ CUNG, TRẢM YÊU ĐÀI
Dialogue: 0,0:06:31.42,0:06:34.05,Default,,0,0,0,,THÂN CÔNG BÁO
Dialogue: 0,0:06:38.58,0:06:41.04,Default,,0,0,0,,Đại đệ tử Khương Tử Nha về núi phục mệnh!
Dialogue: 0,0:06:42.08,0:06:43.54,Default,,0,0,0,,{\i1}Khương Tử Nha.{\i0}
Dialogue: 0,0:06:44.08,0:06:46.04,Default,,0,0,0,,{\i1}Hồ yêu loạn thế,{\i0}
Dialogue: 0,0:06:45.17,0:06:51.30,Default,,0,0,0,,THẬP NHỊ KIM TÔN
Dialogue: 0,0:06:46.13,0:06:48.13,Default,,0,0,0,,{\i1}đạo trời khó dung.{\i0}
Dialogue: 0,0:06:48.21,0:06:51.67,Default,,0,0,0,,{\i1}Hôm nay lệnh cho ngươi\Ntự tay chém Cửu Vĩ,{\i0}
Dialogue: 0,0:06:51.75,0:06:54.29,Default,,0,0,0,,{\i1}răn đe tam giới.{\i0}
Dialogue: 0,0:06:54.38,0:06:56.21,Default,,0,0,0,,{\i1}Từ nay về sau,{\i0}
Dialogue: 0,0:06:56.29,0:06:59.25,Default,,0,0,0,,{\i1}ngươi sẽ là người lãnh đạo chúng thần.{\i0}
Dialogue: 0,0:06:59.33,0:07:02.37,Default,,0,0,0,,{\i1}Mong ngươi dẫn dắt đệ tử Tịnh Hư cung{\i0}
Dialogue: 0,0:07:02.46,0:07:04.25,Default,,0,0,0,,{\i1}giữ gìn thiên đạo,{\i0}
Dialogue: 0,0:07:04.33,0:07:06.46,Default,,0,0,0,,{\i1}bảo vệ chúng sinh.{\i0}
Dialogue: 0,0:07:06.54,0:07:07.64,Default,,0,0,0,,Đệ tử tuân lệnh!
Dialogue: 0,0:07:50.50,0:07:52.42,Default,,0,0,0,,{\i1}Giết!\NGiết ả!{\i0}
Dialogue: 0,0:07:53.13,0:07:55.76,Default,,0,0,0,,{\i1}Giết ả!{\i0}
Dialogue: 0,0:07:55.83,0:07:56.93,Default,,0,0,0,,{\i1}Giết!{\i0}
Dialogue: 0,0:07:59.54,0:08:04.12,Default,,0,0,0,,{\i1}Giết ả!{\i0}
Dialogue: 0,0:08:04.21,0:08:08.42,Default,,0,0,0,,{\i1}Giết ả!{\i0}
Dialogue: 0,0:08:08.50,0:08:12.83,Default,,0,0,0,,{\i1}Giết ả!{\i0}
Dialogue: 0,0:08:12.92,0:08:17.63,Default,,0,0,0,,{\i1}Giết ả!{\i0}
Dialogue: 0,0:08:17.71,0:08:21.92,Default,,0,0,0,,{\i1}Giết ả!{\i0}
Dialogue: 0,0:08:33.04,0:08:34.14,Default,,0,0,0,,Cửu Vĩ,
Dialogue: 0,0:08:34.21,0:08:35.46,Default,,0,0,0,,thu lại huyễn thuật của ngươi đi.
Dialogue: 0,0:08:36.00,0:08:37.17,Default,,0,0,0,,Mệnh trời đã định,
Dialogue: 0,0:08:37.25,0:08:38.46,Default,,0,0,0,,mọi chuyện kết thúc rồi.
Dialogue: 0,0:08:41.54,0:08:43.33,Default,,0,0,0,,Khương Tử Nha,
Dialogue: 0,0:08:43.42,0:08:46.96,Default,,0,0,0,,Sư Tôn ngươi dựa vào đâu\Nmà muốn thay mặt thiên mệnh?
Dialogue: 0,0:08:47.04,0:08:49.21,Default,,0,0,0,,Sư Tôn cứu vớt chúng sinh thế gian.
Dialogue: 0,0:08:49.29,0:08:51.46,Default,,0,0,0,,Cứu vớt chúng sinh?
Dialogue: 0,0:08:55.79,0:08:57.42,Default,,0,0,0,,Vậy thì sao...
Dialogue: 0,0:08:57.50,0:09:00.50,Default,,0,0,0,,lại không chịu cứu nó?
Dialogue: 0,0:09:03.38,0:09:04.34,Default,,0,0,0,,Đó là ai?
Dialogue: 0,0:09:04.42,0:09:07.88,Default,,0,0,0,,Xem ra ngươi không biết gì\Nvề sự thật cả.
Dialogue: 0,0:09:09.13,0:09:09.96,Default,,0,0,0,,Nó là...
Dialogue: 0,0:09:10.04,0:09:11.25,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,0:09:12.21,0:09:15.88,Default,,0,0,0,,{\i1}Yêu nghiệt mê hoặc lòng người,\Ntrừ chậm ắt sinh biến.{\i0}
Dialogue: 0,0:09:15.96,0:09:17.40,Default,,0,0,0,,{\i1}Chém ngay!{\i0}
Dialogue: 0,0:09:19.71,0:09:21.42,Default,,0,0,0,,Yêu hồ, chịu chết đi!
Dialogue: 0,0:09:49.04,0:09:50.14,Default,,0,0,0,,Cẩn thận!
Dialogue: 0,0:09:51.25,0:09:52.45,Default,,0,0,0,,{\i1}Là huyễn thuật của hồ yêu!{\i0}
Dialogue: 0,0:10:02.17,0:10:03.80,Default,,0,0,0,,Giữ vững Đạo tâm,\Nchế ngự huyễn thuật.
Dialogue: 0,0:10:15.13,0:10:15.96,Default,,0,0,0,,Khương Tử Nha,
Dialogue: 0,0:10:16.04,0:10:17.54,Default,,0,0,0,,mau cởi chiếc mặt nạ này ra!
Dialogue: 0,0:10:20.21,0:10:21.31,Default,,0,0,0,,Hửm?
Dialogue: 0,0:10:23.71,0:10:26.13,Default,,0,0,0,,Làm trái thiên đạo,\Nhôm nay ngươi ắt phải bị diệt.
Dialogue: 0,0:10:28.29,0:10:30.08,Default,,0,0,0,,Giết ta rồi,
Dialogue: 0,0:10:30.17,0:10:33.05,Default,,0,0,0,,nó cũng sẽ chết.
Dialogue: 0,0:10:40.42,0:10:41.67,Default,,0,0,0,,Cứu ta...
Dialogue: 0,0:10:46.92,0:10:49.13,Default,,0,0,0,,Sư Tôn! Trong cơ thể Cửu Vĩ\Ncòn có một nguyên thần vô tội.
Dialogue: 0,0:10:54.29,0:10:55.42,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:11:09.42,0:11:10.52,Default,,0,0,0,,Thả con bé ra!
Dialogue: 0,0:11:31.88,0:11:32.98,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:11:33.04,0:11:34.21,Default,,0,0,0,,Đã bao nhiêu năm rồi?
Dialogue: 0,0:11:34.29,0:11:35.12,Default,,0,0,0,,Lại thất bại nữa!
Dialogue: 0,0:11:35.21,0:11:36.96,Default,,0,0,0,,Lúc nào chúng ta mới được\Nquay về Tịnh Hư cung đây?
Dialogue: 0,0:11:43.38,0:11:45.05,Default,,0,0,0,,{\i1}Cởi bỏ phong ấn{\i0}
Dialogue: 0,0:11:45.13,0:11:47.05,Default,,0,0,0,,{\i1}nên chịu tội gì?{\i0}
Dialogue: 0,0:11:47.13,0:11:49.30,Default,,0,0,0,,Trong cơ thể Cửu Vĩ,\Nđệ tử đã thấy một nguyên thần vô tội.
Dialogue: 0,0:11:49.38,0:11:50.38,Default,,0,0,0,,{\i1}Im miệng!{\i0}
Dialogue: 0,0:11:50.46,0:11:53.25,Default,,0,0,0,,{\i1}Cái ngươi nhìn thấy\Nlà ảo ảnh của hồ yêu.{\i0}
Dialogue: 0,0:11:53.33,0:11:54.21,Default,,0,0,0,,Ảo ảnh?
Dialogue: 0,0:11:54.29,0:11:57.25,Default,,0,0,0,,{\i1}Một khi Cửu Vĩ trở lại nhân gian,{\i0}
Dialogue: 0,0:11:57.33,0:12:00.75,Default,,0,0,0,,{\i1}đại chiến Phong Thần sẽ đổ sông đổ bể.{\i0}
Dialogue: 0,0:12:00.83,0:12:03.91,Default,,0,0,0,,{\i1}Cũng may Cửu Vĩ đã bị diệt.{\i0}
Dialogue: 0,0:12:04.42,0:12:07.71,Default,,0,0,0,,{\i1}Nhưng Tịnh Hư cung không thể\Ngiữ ngươi lại nữa.{\i0}
Dialogue: 0,0:12:10.38,0:12:12.51,Default,,0,0,0,,Nhưng đệ tử đã tận mắt...
Dialogue: 0,0:12:12.58,0:12:13.68,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,0:12:13.17,0:12:16.75,Default,,0,0,0,,SƯ TÔN
Dialogue: 0,0:12:14.54,0:12:15.64,Default,,0,0,0,,Sư Tôn!
Dialogue: 0,0:12:16.46,0:12:19.04,Default,,0,0,0,,{\i1}Vi sư phong ấn thần lực của con.{\i0}
Dialogue: 0,0:12:19.13,0:12:21.42,Default,,0,0,0,,{\i1}Con phải đến Bắc Hải tu luyện.{\i0}
Dialogue: 0,0:12:21.50,0:12:24.50,Default,,0,0,0,,{\i1}Khi nào con không còn\Nchịu ảo ảnh của hồ yêu nữa,{\i0}
Dialogue: 0,0:12:25.21,0:12:28.13,Default,,0,0,0,,{\i1}thì có thể trở về Tịnh Hư cung.{\i0}
Dialogue: 0,0:12:32.29,0:12:33.96,Default,,0,0,0,,Phải đi làm việc khổ sai gì đó.
Dialogue: 0,0:12:34.83,0:12:36.46,Default,,0,0,0,,Bây giờ người người được phong thần.
Dialogue: 0,0:12:36.54,0:12:38.83,Default,,0,0,0,,Cả Trụ Vương cũng được\Nlàm thần Nhân Duyên.
Dialogue: 0,0:12:38.92,0:12:40.05,Default,,0,0,0,,Ta còn ở đây
Dialogue: 0,0:12:40.13,0:12:41.46,Default,,0,0,0,,làm bảo mẫu!
Dialogue: 0,0:12:42.54,0:12:43.71,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:12:43.79,0:12:44.62,Default,,0,0,0,,Ngươi...
Dialogue: 0,0:12:44.71,0:12:46.55,Default,,0,0,0,,Hết thần lực rồi\Nmà vẫn còn ở đây ăn vụng
Dialogue: 0,0:12:47.04,0:12:48.21,Default,,0,0,0,,Ngươi dám cắn cả ta sao?
Dialogue: 0,0:12:48.88,0:12:51.01,Default,,0,0,0,,Hôm nay đừng ai hòng ăn cơm!
Dialogue: 0,0:12:54.33,0:12:55.46,Default,,0,0,0,,Lưỡi câu thẳng mà cũng cắn được,
Dialogue: 0,0:12:56.08,0:12:57.83,Default,,0,0,0,,huynh có thấy con cá này\Nngu giống huynh không?
Dialogue: 0,0:12:58.42,0:12:59.75,Default,,0,0,0,,Kẹt ở nơi tồi tàn này,
Dialogue: 0,0:13:00.75,0:13:01.92,Default,,0,0,0,,chịu giày vò...
Dialogue: 0,0:13:05.63,0:13:06.73,Default,,0,0,0,,Huynh nói xem!
Dialogue: 0,0:13:06.79,0:13:08.08,Default,,0,0,0,,Cả cá huynh cũng thả đi được,
Dialogue: 0,0:13:08.17,0:13:09.59,Default,,0,0,0,,mà không thể buông tha cho bản thân sao?
Dialogue: 0,0:13:12.58,0:13:13.68,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:13:16.38,0:13:17.48,Default,,0,0,0,,Ta...
Dialogue: 0,0:13:17.54,0:13:19.46,Default,,0,0,0,,Huynh có tin ta bẻ\NĐả Thần tiên của huynh không?
Dialogue: 0,0:13:21.33,0:13:22.62,Default,,0,0,0,,Ta bẻ thật đấy!
Dialogue: 0,0:13:24.96,0:13:25.84,Default,,0,0,0,,Huynh đứng lại!
Dialogue: 0,0:13:25.92,0:13:27.64,Default,,0,0,0,,Huynh có còn muốn\Nvề Tịnh Hư cung nữa không?
Dialogue: 0,0:13:29.38,0:13:30.21,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:13:30.29,0:13:31.75,Default,,0,0,0,,Đó chỉ là ảo ảnh thôi!
Dialogue: 0,0:13:35.46,0:13:37.21,Default,,0,0,0,,Chỉ là bị Cửu Vĩ lừa thôi mà...
Dialogue: 0,0:13:37.29,0:13:38.12,Default,,0,0,0,,Nói trắng ra
Dialogue: 0,0:13:38.21,0:13:39.96,Default,,0,0,0,,không có kẻ hở\Nlàm sao ruồi đẻ trứng được.
Dialogue: 0,0:13:40.04,0:13:40.87,Default,,0,0,0,,Đương nhiên rồi.
Dialogue: 0,0:13:40.96,0:13:43.38,Default,,0,0,0,,- Cũng không phải nói huynh là trứng ruồi, chỉ là...\N- Đệ về đi!
Dialogue: 0,0:13:44.88,0:13:46.38,Default,,0,0,0,,Mười năm đã đủ lâu rồi.
Dialogue: 0,0:13:47.25,0:13:48.38,Default,,0,0,0,,Người bị đày là ta,
Dialogue: 0,0:13:49.42,0:13:50.71,Default,,0,0,0,,đệ không nên ở lại đây.
Dialogue: 0,0:14:09.79,0:14:10.92,Default,,0,0,0,,Không biết phấn đấu!
Dialogue: 0,0:14:12.00,0:14:13.54,Default,,0,0,0,,Nếu không phải Sư Tôn phái ta xuống,
Dialogue: 0,0:14:14.13,0:14:15.46,Default,,0,0,0,,ta đã đi lâu rồi.
Dialogue: 0,0:14:21.08,0:14:22.18,Default,,0,0,0,,Tứ Bất Tướng?
Dialogue: 0,0:14:56.96,0:15:01.00,Default,,0,0,0,,DIỆT TRỪ HỒ YÊU
Dialogue: 0,0:15:01.04,0:15:05.75,Default,,0,0,0,,BẮC HẢI
Dialogue: 0,0:15:30.46,0:15:32.50,Default,,0,0,0,,Một con mắt. Hai cái miệng.
Dialogue: 0,0:15:32.58,0:15:33.46,Default,,0,0,0,,Ba cái tay.
Dialogue: 0,0:15:33.54,0:15:34.64,Default,,0,0,0,,Bốn cái chân.
Dialogue: 0,0:15:35.21,0:15:36.13,Default,,0,0,0,,Ngươi thua rồi!
Dialogue: 0,0:15:36.21,0:15:38.00,Default,,0,0,0,,Ngươi xem ta có mấy chân?
Dialogue: 0,0:15:38.08,0:15:39.18,Default,,0,0,0,,Uống mau!
Dialogue: 0,0:15:41.25,0:15:42.54,Default,,0,0,0,,Một đám ngu xuẩn!
Dialogue: 0,0:15:45.13,0:15:46.80,Default,,0,0,0,,Là thằng nhãi nhà ngươi à?
Dialogue: 0,0:15:46.88,0:15:47.80,Default,,0,0,0,,- Đứng lại!\N- Mẹ!
Dialogue: 0,0:15:47.88,0:15:49.05,Default,,0,0,0,,Có người ăn hiếp con!
Dialogue: 0,0:15:49.13,0:15:51.01,Default,,0,0,0,,Thằng trời đánh nào\Ndám ức hiếp con của bà đây?
Dialogue: 0,0:15:51.08,0:15:52.33,Default,,0,0,0,,Coi ta có cắt ngươi làm tám mảnh không?!
Dialogue: 0,0:15:52.42,0:15:53.59,Default,,0,0,0,,Ngươi muốn cắt ai?
Dialogue: 0,0:15:59.67,0:16:01.05,Default,,0,0,0,,Nóng! Nóng! Nóng! Nóng! Nóng!
Dialogue: 0,0:16:01.13,0:16:02.42,Default,,0,0,0,,Nóng chết mất!
Dialogue: 0,0:16:04.63,0:16:05.63,Default,,0,0,0,,Yêu quái thối tha,
Dialogue: 0,0:16:05.71,0:16:06.88,Default,,0,0,0,,muốn kiếm chuyện à?
Dialogue: 0,0:16:06.96,0:16:08.46,Default,,0,0,0,,Cầm vũ khí lên!
Dialogue: 0,0:16:10.17,0:16:11.17,Default,,0,0,0,,Nếu không nhờ chạy nhanh,
Dialogue: 0,0:16:11.25,0:16:13.17,Default,,0,0,0,,năm đó ngươi đã bị\Nhồ yêu nhắm rượu rồi.
Dialogue: 0,0:16:13.21,0:16:14.67,Default,,0,0,0,,Hồ yêu chỉ là đồ ăn thối.
Dialogue: 0,0:16:14.75,0:16:16.42,Default,,0,0,0,,Không thì cái chân ngươi đâu rồi?
Dialogue: 0,0:16:16.50,0:16:17.60,Default,,0,0,0,,Hôm nay ta phải...
Dialogue: 0,0:16:22.96,0:16:24.79,Default,,0,0,0,,Khương Tử Nha?
Dialogue: 0,0:16:24.88,0:16:26.55,Default,,0,0,0,,Hắn cũng đến nơi này nữa à?
Dialogue: 0,0:16:26.63,0:16:27.59,Default,,0,0,0,,Xui xẻo!
Dialogue: 0,0:16:27.67,0:16:29.05,Default,,0,0,0,,Hắn lại còn mặt mũi tới đây?
Dialogue: 0,0:16:50.29,0:16:51.50,Default,,0,0,0,,Chao ôi!
Dialogue: 0,0:16:51.58,0:16:54.54,Default,,0,0,0,,Đây không phải đại thần tiên\Nđã hại chúng ta bị nhốt ở đây sao?
Dialogue: 0,0:16:57.29,0:16:58.39,Default,,0,0,0,,Thứ lỗi!
Dialogue: 0,0:16:59.42,0:17:01.46,Default,,0,0,0,,Chúng ta không ra được,\Nngươi cũng không ra được.
Dialogue: 0,0:17:01.54,0:17:03.08,Default,,0,0,0,,Tịnh Hư cung và Bắc Hải
Dialogue: 0,0:17:03.17,0:17:05.34,Default,,0,0,0,,đều không hoan nghênh\Ncái loại bại hoại tam giới như ngươi!
Dialogue: 0,0:17:05.42,0:17:06.52,Default,,0,0,0,,Nói đúng lắm!
Dialogue: 0,0:17:06.58,0:17:07.58,Default,,0,0,0,,Bại hoại tam giới!
Dialogue: 0,0:17:07.67,0:17:12.84,Default,,0,0,0,,Bại hoại!
Dialogue: 0,0:17:12.92,0:17:14.75,Default,,0,0,0,,{\i1}Đánh thắng trận thì có ích gì?{\i0}
Dialogue: 0,0:17:14.83,0:17:16.87,Default,,0,0,0,,{\i1}Không phải vẫn bị giam\Nchung với chúng ta sao?{\i0}
Dialogue: 0,0:17:16.92,0:17:17.88,Default,,0,0,0,,Chủ quán,
Dialogue: 0,0:17:17.96,0:17:19.42,Default,,0,0,0,,xin hỏi gạo bán thế nào?
Dialogue: 0,0:17:22.25,0:17:24.04,Default,,0,0,0,,Khương Tử Nha?
Dialogue: 0,0:17:24.54,0:17:26.87,Default,,0,0,0,,Đúng là khách quý!
Dialogue: 0,0:17:27.46,0:17:29.79,Default,,0,0,0,,Quy định ở Bắc Hải\Nlấy vật đổi vật.
Dialogue: 0,0:17:31.21,0:17:33.96,Default,,0,0,0,,Ngươi... ngươi định đổi...\NĐả... Đả Thần tiên?
Dialogue: 0,0:17:34.04,0:17:35.14,Default,,0,0,0,,Là cái này.
Dialogue: 0,0:17:38.21,0:17:39.92,Default,,0,0,0,,Vân văn Côn Luân?
Dialogue: 0,0:17:41.92,0:17:45.00,Default,,0,0,0,,Trước đó không phải đều là\Ncon báo của nhà ngươi tới sao?
Dialogue: 0,0:17:45.08,0:17:47.25,Default,,0,0,0,,Vừa rồi hắn mới để lại\Nchiếc hoa tai này,
Dialogue: 0,0:17:47.33,0:17:49.96,Default,,0,0,0,,còn lòng đau khóc hu hu nữa.
Dialogue: 0,0:17:52.33,0:17:54.37,Default,,0,0,0,,Sao hả? Cãi nhau à?
Dialogue: 0,0:17:54.46,0:17:56.29,Default,,0,0,0,,Đừng để trong lòng,
Dialogue: 0,0:18:00.46,0:18:02.71,Default,,0,0,0,,chuyện ở đời\Ntám chín phần mười đều không như ý.
Dialogue: 0,0:18:03.50,0:18:05.38,Default,,0,0,0,,Hoặc là nghĩ thoáng ra.
Dialogue: 0,0:18:05.46,0:18:08.46,Default,,0,0,0,,Hoặc là... Chấp nhận.
Dialogue: 0,0:18:10.75,0:18:12.63,Default,,0,0,0,,Ngươi chấp nhận rồi...
Dialogue: 0,0:18:12.71,0:18:15.92,Default,,0,0,0,,thì sẽ được về\Nlàm thần tiên tiên thôi ấy mà.
Dialogue: 0,0:18:16.00,0:18:18.79,Default,,0,0,0,,{\i1}Thứ ngươi nhìn thấy\Nlà ảo ảnh của hồ yêu.{\i0}
Dialogue: 0,0:18:25.83,0:18:28.46,Default,,0,0,0,,Ai đó? Gây ồn ào như vậy?
Dialogue: 0,0:18:39.46,0:18:40.56,Default,,0,0,0,,Tránh ra!
Dialogue: 0,0:18:41.13,0:18:42.96,Default,,0,0,0,,Kẻ lạ mặt!
Dialogue: 0,0:18:43.04,0:18:45.21,Default,,0,0,0,,Không biết cô nương đến đây kiếm người...
Dialogue: 0,0:18:45.29,0:18:46.75,Default,,0,0,0,,hay kiếm chuyện đây?
Dialogue: 0,0:18:49.79,0:18:50.89,Default,,0,0,0,,Tìm cái chết!
Dialogue: 0,0:18:51.50,0:18:52.63,Default,,0,0,0,,Oắt con này!
Dialogue: 0,0:18:54.63,0:18:55.46,Default,,0,0,0,,Ui da!
Dialogue: 0,0:18:55.54,0:18:57.04,Default,,0,0,0,,Nóng! Nóng! Nóng!
Dialogue: 0,0:18:57.13,0:18:58.05,Default,,0,0,0,,{\i1}Đánh hay lắm!{\i0}
Dialogue: 0,0:18:58.13,0:18:59.38,Default,,0,0,0,,{\i1}Cô nương thật lợi hại!{\i0}
Dialogue: 0,0:19:13.04,0:19:14.87,Default,,0,0,0,,Chư vị! Ta muốn hỏi thăm một nơi.
Dialogue: 0,0:19:15.42,0:19:16.25,Default,,0,0,0,,Là con bé đó!
Dialogue: 0,0:19:16.33,0:19:19.00,Default,,0,0,0,,Ồ, không biết cô nương muốn\Nhỏi thăm nơi nào?
Dialogue: 0,0:19:20.00,0:19:21.50,Default,,0,0,0,,Nơi ta muốn tìm
Dialogue: 0,0:19:22.42,0:19:24.34,Default,,0,0,0,,mọc đầy hoa màu đen.
Dialogue: 0,0:19:24.42,0:19:25.71,Default,,0,0,0,,Hắc hoa?
Dialogue: 0,0:19:25.79,0:19:27.58,Default,,0,0,0,,{\i1}Hoa màu đen?{\i0}
Dialogue: 0,0:19:27.67,0:19:28.84,Default,,0,0,0,,{\i1}Ngươi từng nghe chưa?{\i0}
Dialogue: 0,0:19:28.92,0:19:30.42,Default,,0,0,0,,Cả tam giới
Dialogue: 0,0:19:30.50,0:19:34.00,Default,,0,0,0,,chỉ có một nơi mọc đầy Hắc hoa.
Dialogue: 0,0:19:35.13,0:19:39.21,Default,,0,0,0,,Đó là U Đô sơn.
Dialogue: 0,0:19:39.92,0:19:41.09,Default,,0,0,0,,Ngươi biết nơi đó à?
Dialogue: 0,0:19:41.17,0:19:42.00,Default,,0,0,0,,U Đô sơn ở đâu?
Dialogue: 0,0:19:42.08,0:19:43.18,Default,,0,0,0,,Cách đây bao xa?
Dialogue: 0,0:19:44.50,0:19:46.00,Default,,0,0,0,,Yên tâm đi,\Nta hiểu quy định ở đây.
Dialogue: 0,0:19:47.29,0:19:50.42,Default,,0,0,0,,U Đô sơn vốn là ngọn núi tiên.
Dialogue: 0,0:19:50.50,0:19:53.75,Default,,0,0,0,,Trụ Vương đã được\Nphong làm thần Nhân Duyên tại nơi đó.
Dialogue: 0,0:19:53.83,0:19:55.96,Default,,0,0,0,,Nhưng hai năm nay,
Dialogue: 0,0:19:56.04,0:19:57.79,Default,,0,0,0,,liên tục có chuyện kỳ quái.
Dialogue: 0,0:19:57.88,0:20:00.46,Default,,0,0,0,,Hắc hoa nở khắp núi đồi
Dialogue: 0,0:20:00.54,0:20:04.17,Default,,0,0,0,,không tìm được cả đường vào núi.
Dialogue: 0,0:20:04.25,0:20:07.29,Default,,0,0,0,,Cơ mà... gặp được ta\Nthì xem như cô may mắn rồi.
Dialogue: 0,0:20:07.38,0:20:08.67,Default,,0,0,0,,Ta có bản đồ.
Dialogue: 0,0:20:08.75,0:20:10.71,Default,,0,0,0,,Đương nhiên...\NKhông chỉ có bản đồ thôi,
Dialogue: 0,0:20:10.79,0:20:13.46,Default,,0,0,0,,kì trân dị thú, tín vật dân tộc,\Nsách dạy kỹ năng sống, ...
Dialogue: 0,0:20:13.54,0:20:15.37,Default,,0,0,0,,Chỗ của ta đều có hết.
Dialogue: 0,0:20:16.38,0:20:17.92,Default,,0,0,0,,Cô nhìn đi!\NCô nhìn đi!
Dialogue: 0,0:20:18.00,0:20:20.92,Default,,0,0,0,,Ngọn núi màu đen này\Nchính là U Đô sơn.
Dialogue: 0,0:20:21.00,0:20:22.75,Default,,0,0,0,,- Để ta xem nào!\N- Từ từ!
Dialogue: 0,0:20:23.25,0:20:24.42,Default,,0,0,0,,Vài ngày trước,
Dialogue: 0,0:20:24.50,0:20:27.88,Default,,0,0,0,,vừa có một đội săn hồ ly\Nmuốn đổi lấy bản đồ này của ta.
Dialogue: 0,0:20:28.63,0:20:31.21,Default,,0,0,0,,Nghe bọn họ bảo U Đô sơn có
Dialogue: 0,0:20:31.79,0:20:33.04,Default,,0,0,0,,hồ yêu hoành hành.
Dialogue: 0,0:20:33.13,0:20:34.23,Default,,0,0,0,,{\i1}Hồ yêu?{\i0}
Dialogue: 0,0:20:34.63,0:20:35.73,Default,,0,0,0,,{\i1}Chẳng phải hồ yêu đã chết hết rồi sao?{\i0}
Dialogue: 0,0:20:35.75,0:20:37.00,Default,,0,0,0,,{\i1}Hồ yêu xuất thế!{\i0}
Dialogue: 0,0:20:37.08,0:20:38.96,Default,,0,0,0,,{\i1}Tam giới lại sắp đại loạn rồi!{\i0}
Dialogue: 0,0:20:39.04,0:20:40.92,Default,,0,0,0,,Một tiểu cô nương như cô
Dialogue: 0,0:20:41.00,0:20:43.33,Default,,0,0,0,,lại muốn đi đến chỗ đấy.
Dialogue: 0,0:20:43.42,0:20:45.05,Default,,0,0,0,,Lẽ nào cô...
Dialogue: 0,0:20:45.13,0:20:46.30,Default,,0,0,0,,Ngươi không cần quan tâm.
Dialogue: 0,0:20:46.38,0:20:48.21,Default,,0,0,0,,Ta chỉ hỏi ngươi\Ncó đổi bản đồ này hay không thôi?
Dialogue: 0,0:20:48.29,0:20:49.79,Default,,0,0,0,,Quả nhiên rất thẳng thắn!
Dialogue: 0,0:20:49.88,0:20:51.67,Default,,0,0,0,,Có điều quy tắc ở chỗ chúng tôi
Dialogue: 0,0:20:51.75,0:20:54.25,Default,,0,0,0,,là trước khi giao dịch,\Nphải kiểm hàng.
Dialogue: 0,0:20:59.17,0:21:00.34,Default,,0,0,0,,Khương Tử Nha, ngươi...
Dialogue: 0,0:21:00.42,0:21:02.06,Default,,0,0,0,,Sao các ngươi lại dùng\Nthủ đoạn đê tiện thế này?
Dialogue: 0,0:21:02.88,0:21:03.98,Default,,0,0,0,,Đá vụn?
Dialogue: 0,0:21:04.79,0:21:06.33,Default,,0,0,0,,Đồ giả?
Dialogue: 0,0:21:06.42,0:21:07.52,Default,,0,0,0,,Lo chuyện bao đồng!
Dialogue: 0,0:21:12.79,0:21:13.71,Default,,0,0,0,,Tứ Bất Tướng.
Dialogue: 0,0:21:13.79,0:21:14.89,Default,,0,0,0,,Bắt lấy nó!
Dialogue: 0,0:21:15.42,0:21:16.52,Default,,0,0,0,,Ui da!
Dialogue: 0,0:21:17.38,0:21:18.48,Default,,0,0,0,,Ấy da!
Dialogue: 0,0:21:20.96,0:21:22.54,Default,,0,0,0,,Tịnh Hư thần thạch.
Dialogue: 0,0:21:35.54,0:21:36.42,Default,,0,0,0,,Khoan đã!
Dialogue: 0,0:21:36.50,0:21:39.54,Default,,0,0,0,,Đó là rượu tiên\Nta cất giữ nhiều năm trời đó.
Dialogue: 0,0:21:39.63,0:21:41.46,Default,,0,0,0,,Nếu đã quý như thế thì...
Dialogue: 0,0:21:41.54,0:21:42.64,Default,,0,0,0,,Chia sẻ đi!
Dialogue: 0,0:21:45.46,0:21:46.42,Default,,0,0,0,,Rượu tiên!
Dialogue: 0,0:21:46.50,0:21:47.38,Default,,0,0,0,,Là rượu tiên!
Dialogue: 0,0:21:47.46,0:21:49.88,Default,,0,0,0,,Mau bắt lấy con nhỏ đó cho ta!
Dialogue: 0,0:21:49.96,0:21:51.67,Default,,0,0,0,,Ta sẽ có thưởng hậu hĩnh.
Dialogue: 0,0:21:53.58,0:21:54.68,Default,,0,0,0,,Bản đồ!
Dialogue: 0,0:21:57.83,0:22:00.54,Default,,0,0,0,,{\i1}- Cướp rượu!\N- Cướp rượu!{\i0}
Dialogue: 0,0:22:01.83,0:22:02.83,Default,,0,0,0,,Cún ngoan!
Dialogue: 0,0:22:02.92,0:22:04.17,Default,,0,0,0,,Đưa bản đồ cho ta.
Dialogue: 0,0:22:05.08,0:22:06.58,Default,,0,0,0,,Đừng ăn mà! Mau nhả ra!
Dialogue: 0,0:22:06.67,0:22:08.13,Default,,0,0,0,,Nhả ra mau!
Dialogue: 0,0:22:08.21,0:22:10.84,Default,,0,0,0,,{\i1}Lên mau, chết cũng được!{\i0}
Dialogue: 0,0:22:10.92,0:22:11.75,Default,,0,0,0,,Đừng đả thương người!
Dialogue: 0,0:22:11.83,0:22:12.93,Default,,0,0,0,,Ái, ngươi...
Dialogue: 0,0:22:13.75,0:22:16.08,Default,,0,0,0,,{\i1}Việc gì cũng phải tự làm thế này?{\i0}
Dialogue: 0,0:22:25.08,0:22:26.75,Default,,0,0,0,,Hồ... hồ...
Dialogue: 0,0:22:40.50,0:22:42.50,Default,,0,0,0,,Hồ yêu?
Dialogue: 0,0:22:43.50,0:22:46.13,Default,,0,0,0,,Mau bắt ả giao cho Tịnh Hư cung,\Nchúng ta có thể ra ngoài rồi.
Dialogue: 0,0:22:46.21,0:22:47.34,Default,,0,0,0,,Đừng ai hòng tranh giành với mẹ con ta!
Dialogue: 0,0:22:47.42,0:22:49.46,Default,,0,0,0,,Nhẹ tay thôi,\Ngiữ cái đầu lại cho Tịnh Hư cung.
Dialogue: 0,0:22:54.54,0:22:55.64,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:22:56.17,0:22:57.63,Default,,0,0,0,,- Ở trên kia!\N- Mau mau mau!
Dialogue: 0,0:22:57.71,0:22:58.96,Default,,0,0,0,,- Ả ở đó!\N- Bắt lấy ả!
Dialogue: 0,0:23:05.71,0:23:06.81,Default,,0,0,0,,Mau đuổi theo, đuổi theo.
Dialogue: 0,0:23:07.33,0:23:08.43,Default,,0,0,0,,Mau lên!
Dialogue: 0,0:23:15.04,0:23:16.83,Default,,0,0,0,,Khương Tử Nha, huynh đi đâu đó?
Dialogue: 0,0:23:17.88,0:23:18.84,Default,,0,0,0,,Đứng lại!
Dialogue: 0,0:23:18.92,0:23:20.02,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:23:21.33,0:23:22.43,Default,,0,0,0,,Rốt cuộc ngươi là...
Dialogue: 0,0:23:30.04,0:23:31.00,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,0:23:31.08,0:23:33.96,Default,,0,0,0,,{\i1}Phía trước là ranh giới Bắc Hải,\Nranh giới Bắc Hải đấy!{\i0}
Dialogue: 0,0:23:46.21,0:23:47.63,Default,,0,0,0,,Cũng may ta đuổi kịp.
Dialogue: 0,0:23:47.71,0:23:49.25,Default,,0,0,0,,Nếu huynh vượt qua ranh giới này,
Dialogue: 0,0:23:49.33,0:23:50.83,Default,,0,0,0,,nghĩa là phản bội Tịnh Hư cung đấy.
Dialogue: 0,0:23:52.58,0:23:54.16,Default,,0,0,0,,Trước đó ta hơi nặng lời,
Dialogue: 0,0:23:54.25,0:23:55.88,Default,,0,0,0,,nhưng đó cũng là muốn tốt cho huynh.
Dialogue: 0,0:23:55.96,0:23:57.09,Default,,0,0,0,,Huynh mau đi theo ta.
Dialogue: 0,0:23:57.17,0:23:58.34,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:24:00.54,0:24:03.00,Default,,0,0,0,,Chỉ có làm rõ\Nnăm ấy đã nhìn thấy cái gì,
Dialogue: 0,0:24:03.08,0:24:04.71,Default,,0,0,0,,ta mới có thể quay về Tịnh Hư cung.
Dialogue: 0,0:24:05.33,0:24:06.43,Default,,0,0,0,,Đấy chẳng phải là ảo ảnh sao?
Dialogue: 0,0:24:06.46,0:24:07.71,Default,,0,0,0,,Huynh bĩnh tĩnh lại!
Dialogue: 0,0:24:09.58,0:24:11.96,Default,,0,0,0,,Huynh ấy đói hoa mắt rồi,\Nhay là mình bị mắng đến ngu luôn nhỉ?
Dialogue: 0,0:24:12.96,0:24:13.88,Default,,0,0,0,,Thôi kệ!
Dialogue: 0,0:24:13.96,0:24:15.88,Default,,0,0,0,,Hôm nay cho dù có vượt ranh giới\Ncũng phải bắt...
Dialogue: 0,0:24:19.17,0:24:20.75,Default,,0,0,0,,Khương Tử Nha dám vượt ranh giới.
Dialogue: 0,0:24:20.83,0:24:22.00,Default,,0,0,0,,Hắn điên rồi ư?
Dialogue: 0,0:24:23.42,0:24:25.46,Default,,0,0,0,,Tuyết lớn như thế,\Nlôi hắn về trước đã.
Dialogue: 0,0:24:26.58,0:24:28.58,Default,,0,0,0,,Kết cấu với hồ yêu,\Nxem ta trị ngươi thế nào!
Dialogue: 0,0:25:13.17,0:25:14.27,Default,,0,0,0,,Xuống đây!
Dialogue: 0,0:25:17.21,0:25:18.31,Default,,0,0,0,,Rốt cuộc ngươi là ai?
Dialogue: 0,0:25:18.83,0:25:19.93,Default,,0,0,0,,Đưa bản đồ cho ta!
Dialogue: 0,0:25:26.63,0:25:27.59,Default,,0,0,0,,Nói!
Dialogue: 0,0:25:27.67,0:25:29.38,Default,,0,0,0,,Ngươi và Cửu Vĩ có quan hệ gì?
Dialogue: 0,0:25:32.33,0:25:34.58,Default,,0,0,0,,Thân tiên Khương Tử Nha đỉnh đỉnh đại danh
Dialogue: 0,0:25:35.21,0:25:37.46,Default,,0,0,0,,bây giờ lại bắt nạt một thiếu nữ vô tội.
Dialogue: 0,0:25:37.54,0:25:39.30,Default,,0,0,0,,- Bớt làm trò này đi!\N- Thần tiên giết người kìa!
Dialogue: 0,0:25:39.33,0:25:40.43,Default,,0,0,0,,Ngươi...
Dialogue: 0,0:25:43.21,0:25:46.71,Default,,0,0,0,,Thần tiên mấy người\Nđều bất chấp lý lẽ vậy à?
Dialogue: 0,0:25:47.42,0:25:49.09,Default,,0,0,0,,Ngươi có quan hệ với Cửu Vĩ.
Dialogue: 0,0:25:49.17,0:25:50.96,Default,,0,0,0,,Không phải là ta.
Dialogue: 0,0:25:51.04,0:25:52.14,Default,,0,0,0,,Vậy đây là gì?
Dialogue: 0,0:25:53.00,0:25:54.10,Default,,0,0,0,,Không liên quan đến ông.
Dialogue: 0,0:25:54.17,0:25:55.63,Default,,0,0,0,,Chiếc vòng đỏ của Cửu Vĩ,
Dialogue: 0,0:25:55.71,0:25:57.25,Default,,0,0,0,,vì sao lại ở trên chân của ngươi?
Dialogue: 0,0:25:57.33,0:25:58.50,Default,,0,0,0,,Của Cửu Vĩ?
Dialogue: 0,0:26:01.42,0:26:02.52,Default,,0,0,0,,Đây là...
Dialogue: 0,0:26:02.96,0:26:04.17,Default,,0,0,0,,chiếc vòng của ta!
Dialogue: 0,0:26:05.29,0:26:06.39,Default,,0,0,0,,Đứng lại!
Dialogue: 0,0:26:13.71,0:26:14.81,Default,,0,0,0,,Cửu Vĩ.
Dialogue: 0,0:26:38.29,0:26:39.97,Default,,0,0,0,,Nhiếp quỷ phục túy, phong tức hỏa khởi!
Dialogue: 0,0:26:54.25,0:26:55.63,Default,,0,0,0,,Những thứ đó là gì?
Dialogue: 0,0:26:55.71,0:26:56.84,Default,,0,0,0,,Vì sao lại bắt ngươi?
Dialogue: 0,0:26:58.33,0:26:59.46,Default,,0,0,0,,Sao ta biết được?
Dialogue: 0,0:27:00.17,0:27:01.34,Default,,0,0,0,,Lúc tỉnh dậy,
Dialogue: 0,0:27:01.92,0:27:03.84,Default,,0,0,0,,bọn chúng cứ luôn quấn lấy ta.
Dialogue: 0,0:27:03.92,0:27:05.34,Default,,0,0,0,,Bay giờ càng ngày càng nhiều.
Dialogue: 0,0:27:06.83,0:27:09.71,Default,,0,0,0,,Chuyện trước kia\Nta đã không nhớ nổi nữa.
Dialogue: 0,0:27:10.29,0:27:11.96,Default,,0,0,0,,Chỉ có chiếc vòng này
Dialogue: 0,0:27:12.04,0:27:14.42,Default,,0,0,0,,có thể khiến ta nhìn thấy\Nnơi mọc đầy hắc hoa.
Dialogue: 0,0:27:21.29,0:27:22.39,Default,,0,0,0,,U Đô sơn?
Dialogue: 0,0:27:23.04,0:27:24.29,Default,,0,0,0,,Ta nhìn thấy
Dialogue: 0,0:27:24.38,0:27:25.76,Default,,0,0,0,,cha đang đợi ta ở đó.
Dialogue: 0,0:27:26.46,0:27:27.63,Default,,0,0,0,,Ông ấy có thể chứng minh
Dialogue: 0,0:27:27.71,0:27:28.96,Default,,0,0,0,,ta không phải hồ yêu.
Dialogue: 0,0:27:31.08,0:27:33.00,Default,,0,0,0,,{\i1}Yêu nghiệt mê hoặc lòng người{\i0}
Dialogue: 0,0:27:33.71,0:27:36.03,Default,,0,0,0,,Ngươi không được đi đâu hết!\NBây giờ theo ta về Tịnh Hư cung ngay!
Dialogue: 0,0:27:36.08,0:27:37.00,Default,,0,0,0,,Ta không đi!
Dialogue: 0,0:27:37.08,0:27:38.50,Default,,0,0,0,,Ta phải đi tìm cha ta.
Dialogue: 0,0:27:40.29,0:27:42.67,Default,,0,0,0,,{\i1}Sư Tôn cứu vớt chúng sinh thế gian.{\i0}
Dialogue: 0,0:27:42.75,0:27:45.96,Default,,0,0,0,,{\i1}Xem ra ngươi không biết gì về sự thật cả.{\i0}
Dialogue: 0,0:27:47.13,0:27:50.59,Default,,0,0,0,,{\i1}Khi nào con không còn\Nchịu ảo ảnh của hồ yêu nữa,{\i0}
Dialogue: 0,0:27:50.67,0:27:53.80,Default,,0,0,0,,{\i1}thì có thể trở về Tịnh Hư cung.{\i0}
Dialogue: 0,0:28:04.71,0:28:05.81,Default,,0,0,0,,Rốt cuộc ngươi...
Dialogue: 0,0:28:06.29,0:28:07.39,Default,,0,0,0,,Ngươi là...
Dialogue: 0,0:28:17.46,0:28:18.79,Default,,0,0,0,,U Đô sơn?
Dialogue: 0,0:28:25.79,0:28:26.89,Default,,0,0,0,,Đi thôi!
Dialogue: 0,0:28:27.38,0:28:28.48,Default,,0,0,0,,Đi đâu?
Dialogue: 0,0:28:28.92,0:28:30.02,Default,,0,0,0,,Đi U Đô sơn!
Dialogue: 0,0:28:30.63,0:28:32.34,Default,,0,0,0,,Đi cùng ông á?
Dialogue: 0,0:28:34.29,0:28:35.12,Default,,0,0,0,,Sao hả?
Dialogue: 0,0:28:35.21,0:28:36.31,Default,,0,0,0,,Không dám đi à?
Dialogue: 0,0:28:37.33,0:28:38.21,Default,,0,0,0,,Không dám?
Dialogue: 0,0:28:38.29,0:28:40.21,Default,,0,0,0,,Xì, có gì mà không dám chứ?
Dialogue: 0,0:28:40.29,0:28:42.75,Default,,0,0,0,,Hơn nữa, tấm bản đồ đó là của ta.
Dialogue: 0,0:28:43.58,0:28:45.79,Default,,0,0,0,,Ấy, ông chờ ta với.
Dialogue: 0,0:29:02.38,0:29:03.54,Default,,0,0,0,,Vết thương vẫn chưa lành sao?
Dialogue: 0,0:29:04.13,0:29:05.67,Default,,0,0,0,,Có muốn dùng thử...
Dialogue: 0,0:29:05.75,0:29:07.25,Default,,0,0,0,,bài thuốc gia truyền của ta không?
Dialogue: 0,0:29:08.50,0:29:11.17,Default,,0,0,0,,Chỉ cần ông cho ta mượn\Nbản đồ xem một chút.
Dialogue: 0,0:29:12.50,0:29:14.42,Default,,0,0,0,,Ôi dào, đều là người mình cả mà!
Dialogue: 0,0:29:14.50,0:29:16.08,Default,,0,0,0,,Từ khi nào ta và ngươi\Ntrở thành người mình thế?
Dialogue: 0,0:29:16.17,0:29:17.80,Default,,0,0,0,,Trước lạ sau quen.
Dialogue: 0,0:29:17.88,0:29:20.01,Default,,0,0,0,,Trò chuyện vài câu\Nchẳng phải được rồi ư?
Dialogue: 0,0:29:20.08,0:29:21.18,Default,,0,0,0,,Được thôi.
Dialogue: 0,0:29:21.25,0:29:22.79,Default,,0,0,0,,- Vậy nói từ chuyện của ngươi đi.\N- Hả?
Dialogue: 0,0:29:22.88,0:29:24.38,Default,,0,0,0,,Nếu ngươi thật sự là một người phàm,
Dialogue: 0,0:29:25.00,0:29:27.10,Default,,0,0,0,,làm sao vào được cấm địa Bắc Hải?
Dialogue: 0,0:29:27.13,0:29:28.23,Default,,0,0,0,,Thần...
Dialogue: 0,0:29:29.00,0:29:30.42,Default,,0,0,0,,Thần tiên mấy ông
Dialogue: 0,0:29:30.50,0:29:32.13,Default,,0,0,0,,đều trò chuyện kiểu này hả?
Dialogue: 0,0:29:33.25,0:29:34.96,Default,,0,0,0,,Ta còn chưa hỏi đấy.
Dialogue: 0,0:29:35.04,0:29:36.83,Default,,0,0,0,,Ông ăn mặc bần hàn thế này
Dialogue: 0,0:29:36.92,0:29:38.80,Default,,0,0,0,,cũng muốn đi bắt hồ ly...
Dialogue: 0,0:29:38.88,0:29:40.51,Default,,0,0,0,,đổi tiền thưởng à?
Dialogue: 0,0:29:47.13,0:29:48.67,Default,,0,0,0,,Quả nhiên là chiêu trò của hồ yêu.
Dialogue: 0,0:29:49.92,0:29:51.02,Default,,0,0,0,,Đợi một chút!
Dialogue: 0,0:29:52.33,0:29:53.16,Default,,0,0,0,,Mau dừng lại!
Dialogue: 0,0:29:53.25,0:29:54.13,Default,,0,0,0,,Được rồi, được rồi.
Dialogue: 0,0:29:54.21,0:29:55.31,Default,,0,0,0,,Đừng quậy!
Dialogue: 0,0:30:01.25,0:30:02.08,Default,,0,0,0,,Thần tiên thối!
Dialogue: 0,0:30:02.17,0:30:03.27,Default,,0,0,0,,Có gì ghê gớm đâu chứ?
Dialogue: 0,0:30:05.00,0:30:06.10,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:30:06.79,0:30:07.89,Default,,0,0,0,,Tứ Bất Tướng?
Dialogue: 0,0:30:09.21,0:30:12.25,Default,,0,0,0,,Con chó ngốc này\Nlà thần thú trong truyền thuyết đó hả?
Dialogue: 0,0:30:12.92,0:30:15.96,Default,,0,0,0,,Chẳng phải nó cao lớn uy mãnh,\Nphong độ hiên ngang hay sao?
Dialogue: 0,0:30:17.75,0:30:18.85,Default,,0,0,0,,Có một nhúm vậy thôi hả?
Dialogue: 0,0:30:23.29,0:30:24.92,Default,,0,0,0,,Để xuống núi cùng với ta,
Dialogue: 0,0:30:25.00,0:30:26.63,Default,,0,0,0,,nó từ bỏ sự phù hộ\Ncủa Tịnh Hư cung.
Dialogue: 0,0:30:28.92,0:30:31.34,Default,,0,0,0,,Ta thấy phải là không sống nổi ở trên trời
Dialogue: 0,0:30:31.42,0:30:33.55,Default,,0,0,0,,muốn đến phàm giới huyênh hoang thôi.
Dialogue: 0,0:30:35.79,0:30:36.89,Default,,0,0,0,,Trả cho ta!
Dialogue: 0,0:30:37.46,0:30:38.34,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:30:38.42,0:30:40.30,Default,,0,0,0,,Khương Tử Nha!\NMau trông chừng con chó thối của ông!
Dialogue: 0,0:30:42.50,0:30:44.63,Default,,0,0,0,,Chó thối!\NTa phải thẻo cái miệng của ngươi!
Dialogue: 0,0:30:46.42,0:30:47.59,Default,,0,0,0,,Ngươi quay lại cho ta!
Dialogue: 0,0:30:52.46,0:30:56.09,Default,,0,0,0,,THƯƠNG
Dialogue: 0,0:31:10.75,0:31:12.46,Default,,0,0,0,,Ngươi quay lại đây!
Dialogue: 0,0:31:12.17,0:31:15.96,Default,,0,0,0,,DI TÍCH ĐẠI VŨ
Dialogue: 0,0:31:14.75,0:31:15.85,Default,,0,0,0,,Đứng yên!
Dialogue: 0,0:31:16.13,0:31:17.26,Default,,0,0,0,,Bánh của ta!
Dialogue: 0,0:31:18.25,0:31:19.35,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:31:20.17,0:31:22.05,Default,,0,0,0,,Chó chết!
Dialogue: 0,0:31:23.29,0:31:25.08,Default,,0,0,0,,Ngươi đợi đấy!
Dialogue: 0,0:31:25.17,0:31:26.92,Default,,0,0,0,,Thần tiên cái gì?
Dialogue: 0,0:31:27.00,0:31:28.10,Default,,0,0,0,,Một đám trộm vặt thì có.
Dialogue: 0,0:31:28.17,0:31:29.46,Default,,0,0,0,,Hết trộm bản đồ
Dialogue: 0,0:31:33.88,0:31:34.98,Default,,0,0,0,,lại trộm bánh.
Dialogue: 0,0:31:38.13,0:31:40.05,Default,,0,0,0,,Chẳng phải là chuông gió xương thôi ư?
Dialogue: 0,0:31:40.13,0:31:41.34,Default,,0,0,0,,Thứ này ở đâu chẳng có.
Dialogue: 0,0:31:41.96,0:31:45.13,Default,,0,0,0,,Nói gì mà...\NGió thổi xương vang,
Dialogue: 0,0:31:45.21,0:31:47.21,Default,,0,0,0,,an ủi vong hồn.
Dialogue: 0,0:31:48.13,0:31:49.46,Default,,0,0,0,,Người đã chết ngắc...
Dialogue: 0,0:31:49.54,0:31:50.75,Default,,0,0,0,,an ủi thì được đếch gì?
Dialogue: 0,0:31:54.38,0:31:56.59,Default,,0,0,0,,Chuông gió gửi gắm\Nnỗi nhớ của người thân
Dialogue: 0,0:31:57.08,0:31:58.50,Default,,0,0,0,,để vong hồn an nghỉ.
Dialogue: 0,0:31:59.17,0:32:01.80,Default,,0,0,0,,Cũng để người còn sống cảm thấy an ủi.
Dialogue: 0,0:32:03.42,0:32:05.13,Default,,0,0,0,,Diệt hết hồ yêu
Dialogue: 0,0:32:05.21,0:32:06.84,Default,,0,0,0,,chiến tranh đã kết thúc rồi.
Dialogue: 0,0:32:07.63,0:32:09.05,Default,,0,0,0,,Mọi thứ trên thế gian
Dialogue: 0,0:32:09.79,0:32:11.17,Default,,0,0,0,,sẽ trở nên tốt đẹp hơn.
Dialogue: 0,0:32:16.54,0:32:18.62,Default,,0,0,0,,Cũng không biết là ngu thật\Nhay ngu giả nữa.
Dialogue: 0,0:32:43.42,0:32:45.42,Default,,0,0,0,,Thứ thuốc mê này
Dialogue: 0,0:32:45.50,0:32:46.67,Default,,0,0,0,,cũng mạnh dữ.
Dialogue: 0,0:32:49.38,0:32:51.63,Default,,0,0,0,,Không biết cái tên cố chấp kia\Nchạy đi đâu rồi.
Dialogue: 0,0:32:54.13,0:32:55.23,Default,,0,0,0,,Huynh ấy bị thương rồi.
Dialogue: 0,0:32:56.42,0:32:57.88,Default,,0,0,0,,Đúng là không để Báo gia bớt lo mà.
Dialogue: 0,0:33:00.13,0:33:02.09,Default,,0,0,0,,"Gò đất Bất Tử"\N"Danh tiếng lan xa"
Dialogue: 0,0:33:02.17,0:33:03.55,Default,,0,0,0,,"Trên có cây Bất Tử"
Dialogue: 0,0:33:03.63,0:33:04.73,Default,,0,0,0,,"Hoa Bất Tử"
Dialogue: 0,0:33:06.00,0:33:08.04,Default,,0,0,0,,"Suối Bất Tử"
Dialogue: 0,0:33:13.29,0:33:14.46,Default,,0,0,0,,Không hiểu chứ gì?
Dialogue: 0,0:33:14.54,0:33:15.92,Default,,0,0,0,,Đại chiến diễn ra lâu như vậy...
Dialogue: 0,0:33:16.00,0:33:17.33,Default,,0,0,0,,chỗ này đã thay đổi rồi.
Dialogue: 0,0:33:17.42,0:33:18.92,Default,,0,0,0,,Nếu như ông say nắng
Dialogue: 0,0:33:19.00,0:33:20.42,Default,,0,0,0,,thì để ta coi giúp ông cho.
Dialogue: 0,0:33:20.50,0:33:21.60,Default,,0,0,0,,Không cần cô nương nhọc lòng.
Dialogue: 0,0:33:23.88,0:33:25.13,Default,,0,0,0,,Ăn... ăn hết bánh rồi!
Dialogue: 0,0:33:33.42,0:33:34.52,Default,,0,0,0,,{\i1}Cắt lỗ tai của ả!{\i0}
Dialogue: 0,0:33:51.67,0:33:52.67,Default,,0,0,0,,Nước...
Dialogue: 0,0:33:52.75,0:33:53.85,Default,,0,0,0,,Ta muốn nước!
Dialogue: 0,0:33:54.33,0:33:55.43,Default,,0,0,0,,Cố lên!
Dialogue: 0,0:33:56.75,0:33:59.63,Default,,0,0,0,,Bản thân ông còn lo chưa xong\Ncòn muốn quan tâm gã?
Dialogue: 0,0:34:01.83,0:34:03.29,Default,,0,0,0,,Sinh tử trước mắt,
Dialogue: 0,0:34:03.38,0:34:04.48,Default,,0,0,0,,không thể thấy chết không cứu.
Dialogue: 0,0:34:06.38,0:34:07.30,Default,,0,0,0,,Thả ta ra!
Dialogue: 0,0:34:07.38,0:34:09.51,Default,,0,0,0,,Ông nghĩ... ông đang cứu ai đấy?
Dialogue: 0,0:34:18.33,0:34:19.54,Default,,0,0,0,,Đám thợ săn hồ ly này
Dialogue: 0,0:34:19.63,0:34:21.30,Default,,0,0,0,,đều muốn dùng ta đổi tiền thưởng.
Dialogue: 0,0:34:23.00,0:34:25.08,Default,,0,0,0,,Lúc bọn họ cắt tai của ta,
Dialogue: 0,0:34:25.17,0:34:26.80,Default,,0,0,0,,có ai đến cứu ta không?
Dialogue: 0,0:34:26.88,0:34:27.71,Default,,0,0,0,,Dừng tay!
Dialogue: 0,0:34:27.79,0:34:29.96,Default,,0,0,0,,Đừng lạm sát người vô tội!\NCòn bảo ngươi là người à?
Dialogue: 0,0:34:30.04,0:34:31.33,Default,,0,0,0,,Đó là hắn đáng đời!
Dialogue: 0,0:34:31.42,0:34:33.42,Default,,0,0,0,,Giả làm người tốt,\Nta không rảnh chơi chung.
Dialogue: 0,0:34:36.29,0:34:37.39,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:34:37.75,0:34:40.00,Default,,0,0,0,,Tĩnh tâm an thần,\Nđừng để những thứ này thâm nhập đầu óc.
Dialogue: 0,0:34:40.54,0:34:43.25,Default,,0,0,0,,Ngũ Đế tư nghênh, tinh quái vong hình,
Dialogue: 0,0:34:43.33,0:34:45.16,Default,,0,0,0,,cấp cấp như luật lệnh!
Dialogue: 0,0:34:46.00,0:34:47.33,Default,,0,0,0,,Hồ ly là của ta!
Dialogue: 0,0:34:47.42,0:34:48.55,Default,,0,0,0,,Là của ta!
Dialogue: 0,0:34:49.38,0:34:53.51,Default,,0,0,0,,U Đô sơn có hồ ly.
Dialogue: 0,0:35:02.04,0:35:03.17,Default,,0,0,0,,Hồ yêu...
Dialogue: 0,0:35:05.13,0:35:07.13,Default,,0,0,0,,Lại là hồ yêu!
Dialogue: 0,0:35:10.67,0:35:12.30,Default,,0,0,0,,U Đô sơn!
Dialogue: 0,0:35:15.58,0:35:18.50,Default,,0,0,0,,Một đại thần tiên đến từ trên trời
Dialogue: 0,0:35:18.58,0:35:21.71,Default,,0,0,0,,làm mất bản đồ, trơ mắt ếch.
Dialogue: 0,0:35:28.79,0:35:30.08,Default,,0,0,0,,Mấy con quạ thối!
Dialogue: 0,0:35:30.17,0:35:32.25,Default,,0,0,0,,Không ra oai cho các ngươi xem,
Dialogue: 0,0:35:32.33,0:35:34.12,Default,,0,0,0,,thì không biết sự lợi hại\Ncủa bản cô nương.
Dialogue: 0,0:35:52.38,0:35:53.48,Default,,0,0,0,,{\i1}Là máu!{\i0}
Dialogue: 0,0:35:57.00,0:35:58.21,Default,,0,0,0,,Đây lại là...
Dialogue: 0,0:36:08.67,0:36:10.92,Default,,0,0,0,,Quái vật gì nữa?
Dialogue: 0,0:36:17.83,0:36:18.93,Default,,0,0,0,,Cứu mạng!
Dialogue: 0,0:36:19.42,0:36:20.52,Default,,0,0,0,,Tránh ra!
Dialogue: 0,0:36:40.29,0:36:41.42,Default,,0,0,0,,Đưa Tứ Bất Tướng đi trước!
Dialogue: 0,0:36:42.29,0:36:43.39,Default,,0,0,0,,Còn đợi gì nữa?
Dialogue: 0,0:36:43.42,0:36:44.52,Default,,0,0,0,,Đi mau!
Dialogue: 0,0:36:59.13,0:37:00.23,Default,,0,0,0,,Quay lại!
Dialogue: 0,0:37:05.54,0:37:07.00,Default,,0,0,0,,Hai tên ngu ngốc.
Dialogue: 0,0:37:11.92,0:37:13.13,Default,,0,0,0,,Ai bảo ngươi quay lại?
Dialogue: 0,0:37:22.50,0:37:23.60,Default,,0,0,0,,Chó ngốc!
Dialogue: 0,0:37:33.50,0:37:34.60,Default,,0,0,0,,Sao cả ngươi cũng...
Dialogue: 0,0:37:35.08,0:37:36.96,Default,,0,0,0,,- Mau đưa Đả Thần tiên cho ta.\N- Ta cũng muốn lắm.
Dialogue: 0,0:37:37.04,0:37:38.17,Default,,0,0,0,,Cẩn thận!
Dialogue: 0,0:37:38.25,0:37:39.35,Default,,0,0,0,,Bản đồ!
Dialogue: 0,0:37:52.25,0:37:53.35,Default,,0,0,0,,Là âm thanh này.
Dialogue: 0,0:37:54.33,0:37:55.43,Default,,0,0,0,,Đưa chuông gió cho nó.
Dialogue: 0,0:38:09.63,0:38:11.30,Default,,0,0,0,,{\i1}Về nhà!{\i0}
Dialogue: 0,0:38:11.38,0:38:13.92,Default,,0,0,0,,{\i1}Ta muốn về nhà!{\i0}
Dialogue: 0,0:38:26.25,0:38:27.71,Default,,0,0,0,,Ở trong đó...
Dialogue: 0,0:38:27.79,0:38:28.89,Default,,0,0,0,,là người?
Dialogue: 0,0:38:30.42,0:38:31.52,Default,,0,0,0,,Là oan hồn.
Dialogue: 0,0:38:37.54,0:38:39.29,Default,,0,0,0,,Rời nhà xa xứ,
Dialogue: 0,0:38:39.38,0:38:40.80,Default,,0,0,0,,tử chiến sa trường.
Dialogue: 0,0:38:41.75,0:38:43.54,Default,,0,0,0,,Thi hài của những binh sĩ này
Dialogue: 0,0:38:43.63,0:38:45.26,Default,,0,0,0,,không có đất chôn.
Dialogue: 0,0:38:45.33,0:38:47.25,Default,,0,0,0,,Vong hồn không tìm được\Nnhà và người thân
Dialogue: 0,0:38:48.04,0:38:49.52,Default,,0,0,0,,biến thành oán hồn.
Dialogue: 0,0:38:50.38,0:38:52.05,Default,,0,0,0,,Không tìm được nhà
Dialogue: 0,0:38:52.13,0:38:53.26,Default,,0,0,0,,và người thân?
Dialogue: 0,0:38:58.42,0:38:59.88,Default,,0,0,0,,Là trận đại chiến đó.
Dialogue: 0,0:39:02.29,0:39:05.67,Default,,0,0,0,,{\i1}Nói gì mà gió thổi xương vang,{\i0}
Dialogue: 0,0:39:05.75,0:39:07.42,Default,,0,0,0,,{\i1}an ủi vong hồn?{\i0}
Dialogue: 0,0:39:14.67,0:39:16.63,Default,,0,0,0,,Bọn họ sẽ bị giam ở đây mãi mãi à?
Dialogue: 0,0:39:18.13,0:39:19.23,Default,,0,0,0,,Trừ khi...
Dialogue: 0,0:39:21.00,0:39:22.96,Default,,0,0,0,,Truyền thuyết nói có loại dị thú
Dialogue: 0,0:39:23.00,0:39:24.13,Default,,0,0,0,,tên là Huyền Điểu.
Dialogue: 0,0:39:24.67,0:39:26.71,Default,,0,0,0,,Sẽ bị cảm động bởi tâm niệm chân thành
Dialogue: 0,0:39:26.79,0:39:28.29,Default,,0,0,0,,giáng lâm thế gian
Dialogue: 0,0:39:28.38,0:39:30.17,Default,,0,0,0,,dẫn độ oán hồn.
Dialogue: 0,0:39:30.25,0:39:31.42,Default,,0,0,0,,Chỉ là
Dialogue: 0,0:39:31.50,0:39:32.88,Default,,0,0,0,,đã nhiều năm rồi không thấy.
Dialogue: 0,0:39:34.88,0:39:36.92,Default,,0,0,0,,- {\i1}Tiểu Cửu!{\i0}\N- {\i1}Cha!{\i0}
Dialogue: 0,0:39:41.13,0:39:42.23,Default,,0,0,0,,Ngươi...
Dialogue: 0,0:39:43.67,0:39:45.84,Default,,0,0,0,,Ta còn có nhà và người thân.
Dialogue: 0,0:39:47.25,0:39:50.42,Default,,0,0,0,,Ta nhất định...\Nphải tìm được cha.
Dialogue: 0,0:39:59.04,0:40:00.58,Default,,0,0,0,,- Băng bó vết thương đi.\N- Không còn bản đồ,
Dialogue: 0,0:40:00.67,0:40:02.30,Default,,0,0,0,,làm sao để đi đến U Đô sơn đây?
Dialogue: 0,0:40:21.25,0:40:22.35,Default,,0,0,0,,Con chó ngốc này!
Dialogue: 0,0:41:53.13,0:41:54.80,Default,,0,0,0,,Mau qua đây!
Dialogue: 0,0:41:56.50,0:41:57.60,Default,,0,0,0,,Ngươi...
Dialogue: 0,0:41:57.67,0:41:58.88,Default,,0,0,0,,Ngươi muốn làm gì?
Dialogue: 0,0:42:02.79,0:42:03.71,Default,,0,0,0,,{\i1}Hồ yêu!{\i0}
Dialogue: 0,0:42:03.79,0:42:04.89,Default,,0,0,0,,{\i1}Đứng lại!{\i0}
Dialogue: 0,0:42:04.96,0:42:06.42,Default,,0,0,0,,{\i1}Mau bắt ả lại!{\i0}
Dialogue: 0,0:42:08.71,0:42:09.54,Default,,0,0,0,,{\i1}Đưa dao qua đây!{\i0}
Dialogue: 0,0:42:09.63,0:42:10.91,Default,,0,0,0,,{\i1}Cắt tai hồ yêu đi lĩnh thưởng.{\i0}
Dialogue: 0,0:42:15.75,0:42:16.85,Default,,0,0,0,,{\i1}Cắt tai ả!{\i0}
Dialogue: 0,0:42:26.63,0:42:27.75,Default,,0,0,0,,Mơ thấy ác mộng à?
Dialogue: 0,0:42:29.67,0:42:31.99,Default,,0,0,0,,Trẻ con không được chép miệng,\Nngười lớn ăn cơm không được run đùi.
Dialogue: 0,0:42:34.50,0:42:36.42,Default,,0,0,0,,Khẩu quyết thần tiên này của ông
Dialogue: 0,0:42:36.50,0:42:37.92,Default,,0,0,0,,cũng hợp hoàn cảnh đấy.
Dialogue: 0,0:42:40.25,0:42:42.00,Default,,0,0,0,,Đây không phải khẩu quyết thần tiên gì cả.
Dialogue: 0,0:42:45.63,0:42:47.21,Default,,0,0,0,,Đại chiến năm đó,
Dialogue: 0,0:42:47.29,0:42:49.42,Default,,0,0,0,,sĩ binh ở nhân giới\Nlúc ăn cơm sẽ hát như vậy.
Dialogue: 0,0:42:49.50,0:42:50.60,Default,,0,0,0,,Đại chiến?
Dialogue: 0,0:42:50.67,0:42:52.00,Default,,0,0,0,,Nó cũng từng đánh với Cửu Vĩ à?
Dialogue: 0,0:42:54.17,0:42:55.67,Default,,0,0,0,,Lần trước đến đây,
Dialogue: 0,0:42:56.25,0:42:57.75,Default,,0,0,0,,nơi này còn là biển.
Dialogue: 0,0:42:59.04,0:43:01.29,Default,,0,0,0,,Tứ Bất Tướng sải bước qua nước biển
Dialogue: 0,0:43:01.38,0:43:03.01,Default,,0,0,0,,xông thẳng vào trận địch.
Dialogue: 0,0:43:04.13,0:43:05.71,Default,,0,0,0,,Giết chết Cửu Vĩ
Dialogue: 0,0:43:05.79,0:43:07.50,Default,,0,0,0,,là có thể cứu được chúng sinh.
Dialogue: 0,0:43:08.83,0:43:10.00,Default,,0,0,0,,Lúc đó
Dialogue: 0,0:43:10.08,0:43:11.62,Default,,0,0,0,,bọn ta đều tin như vậy.
Dialogue: 0,0:43:13.04,0:43:14.92,Default,,0,0,0,,Nhưng thế gian này hiện tại,
Dialogue: 0,0:43:15.00,0:43:17.17,Default,,0,0,0,,lại trở nên hoàng tàn, đổ nát hơn.
Dialogue: 0,0:43:17.88,0:43:19.46,Default,,0,0,0,,Nơi này còn đỡ đó.
Dialogue: 0,0:43:19.54,0:43:21.75,Default,,0,0,0,,Chỗ của ta còn thảm hơn ở đây nữa.
Dialogue: 0,0:43:25.38,0:43:27.01,Default,,0,0,0,,Hầy, đành chịu vậy.
Dialogue: 0,0:43:28.13,0:43:29.23,Default,,0,0,0,,Đại thần tiên,
Dialogue: 0,0:43:29.29,0:43:31.29,Default,,0,0,0,,nếu ông còn băn khoăn
Dialogue: 0,0:43:31.38,0:43:33.80,Default,,0,0,0,,vậy thì giúp ta kiếm U Đô sơn đi!
Dialogue: 0,0:43:34.63,0:43:36.38,Default,,0,0,0,,Ngươi tin rằng cha ngươi ở đó thật à?
Dialogue: 0,0:43:41.17,0:43:42.80,Default,,0,0,0,,Từ lúc tỉnh lại trong đống đổ nát,
Dialogue: 0,0:43:43.88,0:43:45.52,Default,,0,0,0,,ta đã không còn nhớ được gì rồi.
Dialogue: 0,0:43:46.63,0:43:48.51,Default,,0,0,0,,Nhưng luôn mơ thấy cùng một giấc mơ.
Dialogue: 0,0:43:50.42,0:43:51.52,Default,,0,0,0,,Trong mơ
Dialogue: 0,0:43:52.29,0:43:54.17,Default,,0,0,0,,cha bện cho ta một con búp bê.
Dialogue: 0,0:43:55.88,0:43:57.17,Default,,0,0,0,,Trên con búp bê,
Dialogue: 0,0:43:57.25,0:43:58.63,Default,,0,0,0,,có buộc một hòn đó nhỏ.
Dialogue: 0,0:43:59.25,0:44:00.46,Default,,0,0,0,,Gió vừa thổi
Dialogue: 0,0:44:00.54,0:44:01.64,Default,,0,0,0,,thì sẽ kêu.
Dialogue: 0,0:44:03.25,0:44:04.75,Default,,0,0,0,,Giống tiếng phong linh vậy.
Dialogue: 0,0:44:06.25,0:44:07.92,Default,,0,0,0,,Âm thanh đó rất thật.
Dialogue: 0,0:44:13.33,0:44:14.87,Default,,0,0,0,,Mỗi lần ta lẻn ra ngoài,
Dialogue: 0,0:44:15.67,0:44:17.38,Default,,0,0,0,,cha sẽ treo nó ở cửa.
Dialogue: 0,0:44:20.50,0:44:22.00,Default,,0,0,0,,Trời tối gió nổi lên
Dialogue: 0,0:44:23.38,0:44:25.05,Default,,0,0,0,,là lúc ta trở về nhà.
Dialogue: 0,0:44:30.13,0:44:31.23,Default,,0,0,0,,"Tiểu Cửu!"
Dialogue: 0,0:44:32.17,0:44:33.89,Default,,0,0,0,,Trong mơ, cha gọi ta như vậy đó.
Dialogue: 0,0:44:36.38,0:44:38.26,Default,,0,0,0,,Nơi mọc đầy hắc hoa
Dialogue: 0,0:44:39.75,0:44:40.88,Default,,0,0,0,,là nơi ông ấy đang ở.
Dialogue: 0,0:44:53.50,0:44:54.46,Default,,0,0,0,,Đó không phải mơ,
Dialogue: 0,0:44:54.54,0:44:55.64,Default,,0,0,0,,đó là thật!
Dialogue: 0,0:44:57.00,0:44:58.75,Default,,0,0,0,,Là hy vọng để ta sống tiếp.
Dialogue: 0,0:45:02.17,0:45:03.38,Default,,0,0,0,,Ta phải tìm được ông ấy.
Dialogue: 0,0:45:19.92,0:45:21.05,Default,,0,0,0,,Là Huyền Điểu.
Dialogue: 0,0:46:21.13,0:46:22.76,Default,,0,0,0,,Huyền Điểu mang oán hồn đi,
Dialogue: 0,0:46:24.13,0:46:25.63,Default,,0,0,0,,cũng đem hy vọng đến.
Dialogue: 0,0:46:49.17,0:46:50.27,Default,,0,0,0,,Đi thôi!
Dialogue: 0,0:46:51.67,0:46:52.77,Default,,0,0,0,,Trên bản đồ nói
Dialogue: 0,0:46:53.25,0:46:54.79,Default,,0,0,0,,điểm cuối của ngân hà
Dialogue: 0,0:46:54.88,0:46:56.51,Default,,0,0,0,,sẽ là phương hướng đến U Đô sơn.
Dialogue: 0,0:47:00.38,0:47:01.67,Default,,0,0,0,,Mình với nhau cả,
Dialogue: 0,0:47:01.75,0:47:03.00,Default,,0,0,0,,ăn no rồi hẵng đi.
Dialogue: 0,0:47:04.00,0:47:05.50,Default,,0,0,0,,Trí nhớ của thần tiên này cũng tốt nhỉ.
Dialogue: 0,0:47:10.13,0:47:11.23,Default,,0,0,0,,Ai da, đừng ngửi nữa
Dialogue: 0,0:47:11.25,0:47:12.42,Default,,0,0,0,,hết bánh thật rồi mà.
Dialogue: 0,0:47:14.50,0:47:16.63,Default,,0,0,0,,Trả cho ta! Đó là cái cuối rồi đó!
Dialogue: 0,0:47:19.71,0:47:20.81,Default,,0,0,0,,Đừng chạy!
Dialogue: 0,0:47:24.50,0:47:29.13,Default,,0,0,0,,HẮC THỦY
Dialogue: 0,0:47:28.88,0:47:31.38,Default,,0,0,0,,"Thấy rừng đá, đi ngược dòng Hắc Thủy"
Dialogue: 0,0:47:32.04,0:47:34.54,Default,,0,0,0,,Lối vào U Đô sơn ở phía trước.
Dialogue: 0,0:47:35.75,0:47:37.21,Default,,0,0,0,,Có phải ông nhớ nhầm rồi không?
Dialogue: 0,0:47:37.29,0:47:39.00,Default,,0,0,0,,Nơi này chỉ có đống đá vụn thôi,
Dialogue: 0,0:47:39.63,0:47:41.05,Default,,0,0,0,,chẳng có gì cả.
Dialogue: 0,0:47:41.96,0:47:43.63,Default,,0,0,0,,Không phải không có,
Dialogue: 0,0:47:43.71,0:47:44.84,Default,,0,0,0,,là nhìn không thấy.
Dialogue: 0,0:47:48.08,0:47:49.18,Default,,0,0,0,,Hắc hoa?
Dialogue: 0,0:47:49.58,0:47:50.79,Default,,0,0,0,,Đúng là hướng này rồi.
Dialogue: 0,0:47:52.13,0:47:53.23,Default,,0,0,0,,Cẩn thận!
Dialogue: 0,0:47:55.58,0:47:58.33,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:48:01.88,0:48:04.05,Default,,0,0,0,,Cuối cùng cũng bắt kịp huynh rồi.
Dialogue: 0,0:48:04.13,0:48:06.84,Default,,0,0,0,,Huynh tự ý xuất giới\Nđáng chịu tội gì?
Dialogue: 0,0:48:08.75,0:48:10.83,Default,,0,0,0,,- Đệ cũng vậy mà?\N- Ta...
Dialogue: 0,0:48:10.92,0:48:12.50,Default,,0,0,0,,Ta đến để trông chừng huynh
Dialogue: 0,0:48:12.58,0:48:14.54,Default,,0,0,0,,để huynh không bị\Nhồ ly tinh này lừa gạt thôi mà.
Dialogue: 0,0:48:14.63,0:48:16.31,Default,,0,0,0,,- Huynh mau theo ta về!\N- Ta chưa về được.
Dialogue: 0,0:48:17.67,0:48:19.80,Default,,0,0,0,,Ê! Con báo ngốc đó\Nsắp xù lông lên kìa!
Dialogue: 0,0:48:21.67,0:48:22.77,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:48:24.42,0:48:25.52,Default,,0,0,0,,Vết sẹo này...
Dialogue: 0,0:48:26.04,0:48:27.58,Default,,0,0,0,,Nhìn xem bộ dạng hiện giờ của huynh
Dialogue: 0,0:48:27.67,0:48:29.07,Default,,0,0,0,,thần lực suy yếu đến mức này.
Dialogue: 0,0:48:29.13,0:48:30.46,Default,,0,0,0,,Không lẽ huynh muốn\Nchết ở đây luôn sao?
Dialogue: 0,0:48:33.33,0:48:34.58,Default,,0,0,0,,Đợi làm rõ mọi việc
Dialogue: 0,0:48:35.75,0:48:37.29,Default,,0,0,0,,ta sẽ cùng đệ về Bắc Hải.
Dialogue: 0,0:48:37.38,0:48:38.48,Default,,0,0,0,,Việc gì chứ?
Dialogue: 0,0:48:38.96,0:48:41.42,Default,,0,0,0,,Năm đó ta nhìn thấy con bé\Ntrong cơ thể Cửu Vĩ.
Dialogue: 0,0:48:41.50,0:48:42.92,Default,,0,0,0,,Ta phải làm rõ con bé là ai.
Dialogue: 0,0:48:45.58,0:48:46.91,Default,,0,0,0,,Nó chính là...
Dialogue: 0,0:48:47.00,0:48:47.92,Default,,0,0,0,,Ta...
Dialogue: 0,0:48:48.00,0:48:49.58,Default,,0,0,0,,- Nói, rốt cuộc ngươi là ai?\N- Thân Công Báo!
Dialogue: 0,0:48:50.46,0:48:51.84,Default,,0,0,0,,Ma quỷ mê hoặc.
Dialogue: 0,0:48:51.92,0:48:53.34,Default,,0,0,0,,Bỏ ra! Liên quan gì đến ông?
Dialogue: 0,0:48:53.42,0:48:54.84,Default,,0,0,0,,Còn cãi bướng?
Dialogue: 0,0:48:54.92,0:48:56.96,Default,,0,0,0,,Bây giờ ta sẽ khiến ngươi hiện nguyên hình!
Dialogue: 0,0:49:02.00,0:49:03.10,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:49:04.88,0:49:06.01,Default,,0,0,0,,Muốn chạy à?
Dialogue: 0,0:49:06.58,0:49:07.50,Default,,0,0,0,,Đứng lại cho ta!
Dialogue: 0,0:49:07.58,0:49:08.68,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:49:09.38,0:49:10.48,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:49:11.08,0:49:12.18,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:49:13.75,0:49:14.85,Default,,0,0,0,,Tứ Bất...
Dialogue: 0,0:49:21.92,0:49:23.30,Default,,0,0,0,,{\i1}Ở đây này!{\i0}
Dialogue: 0,0:49:26.25,0:49:27.75,Default,,0,0,0,,{\i1}Ở đây này!{\i0}
Dialogue: 0,0:49:28.79,0:49:29.89,Default,,0,0,0,,Cha!
Dialogue: 0,0:49:30.21,0:49:31.31,Default,,0,0,0,,Là người sao?
Dialogue: 0,0:49:33.13,0:49:34.23,Default,,0,0,0,,Cha!
Dialogue: 0,0:49:43.38,0:49:44.59,Default,,0,0,0,,Đại chiến năm đó...
Dialogue: 0,0:49:44.67,0:49:46.79,Default,,0,0,0,,biết bao nhiêu người vô tội\Nphải chết trong tay hồ yêu.
Dialogue: 0,0:49:47.67,0:49:49.67,Default,,0,0,0,,Rốt cuộc huynh đang làm gì vậy?
Dialogue: 0,0:49:52.79,0:49:54.62,Default,,0,0,0,,- Việc này còn uẩn khúc.\N- Nhảm nhí!
Dialogue: 0,0:49:54.71,0:49:56.21,Default,,0,0,0,,Huynh hết thuốc chữa rồi!
Dialogue: 0,0:49:56.92,0:49:58.46,Default,,0,0,0,,{\i1}Trốn không thoát{\i0}
Dialogue: 0,0:50:06.63,0:50:08.30,Default,,0,0,0,,{\i1}chính là mệnh.{\i0}
Dialogue: 0,0:50:08.38,0:50:09.76,Default,,0,0,0,,{\i1}Cắt không đứt{\i0}
Dialogue: 0,0:50:10.58,0:50:12.58,Default,,0,0,0,,{\i1}mới gọi là yêu!{\i0}
Dialogue: 0,0:50:18.96,0:50:20.13,Default,,0,0,0,,Cha!
Dialogue: 0,0:50:20.21,0:50:21.34,Default,,0,0,0,,Cha?
Dialogue: 0,0:50:22.79,0:50:25.33,Default,,0,0,0,,TRỤ VƯƠNG
Dialogue: 0,0:50:23.13,0:50:25.05,Default,,0,0,0,,Ta không phải cha ngươi.
Dialogue: 0,0:50:26.21,0:50:27.13,Default,,0,0,0,,Ngươi là ai?
Dialogue: 0,0:50:27.21,0:50:28.96,Default,,0,0,0,,Ta là ai ư?
Dialogue: 0,0:50:33.25,0:50:36.00,Default,,0,0,0,,Bổn vương là người chưởng quản\Nnhân duyên thế gian,
Dialogue: 0,0:50:36.63,0:50:38.92,Default,,0,0,0,,Thần Nhân Duyên.
Dialogue: 0,0:50:41.33,0:50:42.62,Default,,0,0,0,,Ta thấy ngươi là tên biến thái thì có!
Dialogue: 0,0:50:42.71,0:50:43.84,Default,,0,0,0,,Mau tháo dây ra!
Dialogue: 0,0:50:44.83,0:50:46.12,Default,,0,0,0,,Được thôi.
Dialogue: 0,0:50:54.75,0:50:57.96,Default,,0,0,0,,- Gương mặt nhỏ này thật quen mắt.\N- Tránh ra!
Dialogue: 0,0:51:01.71,0:51:03.09,Default,,0,0,0,,Túc Mệnh tỏa?
Dialogue: 0,0:51:03.71,0:51:04.84,Default,,0,0,0,,Ngươi cũng biết chiếc vòng này à?
Dialogue: 0,0:51:07.83,0:51:09.71,Default,,0,0,0,,Đương nhiên ta biết rồi.
Dialogue: 0,0:51:12.58,0:51:14.08,Default,,0,0,0,,Nó không phải chiếc vòng,
Dialogue: 0,0:51:14.67,0:51:16.96,Default,,0,0,0,,mà là một cái khóa.
Dialogue: 0,0:51:17.04,0:51:17.96,Default,,0,0,0,,Khóa?
Dialogue: 0,0:51:18.04,0:51:21.37,Default,,0,0,0,,Nó có thể khóa\Nhai nguyên thần lại với nhau.
Dialogue: 0,0:51:21.46,0:51:23.09,Default,,0,0,0,,Sống cùng sống,
Dialogue: 0,0:51:23.17,0:51:24.96,Default,,0,0,0,,chết cùng chết.
Dialogue: 0,0:51:26.17,0:51:28.09,Default,,0,0,0,,Trên người ái phi của ta
Dialogue: 0,0:51:28.17,0:51:30.38,Default,,0,0,0,,cũng có cái khóa này.
Dialogue: 0,0:51:30.46,0:51:31.46,Default,,0,0,0,,Thì ra...
Dialogue: 0,0:51:31.54,0:51:33.96,Default,,0,0,0,,người liên kết với nàng ấy
Dialogue: 0,0:51:34.04,0:51:35.37,Default,,0,0,0,,chính là ngươi!
Dialogue: 0,0:51:35.46,0:51:37.50,Default,,0,0,0,,Chỉ cần khiến khóa hiện hình
Dialogue: 0,0:51:37.58,0:51:39.87,Default,,0,0,0,,thì có thể tìm được ái phi.
Dialogue: 0,0:51:45.92,0:51:48.13,Default,,0,0,0,,Hiện hình đi!
Dialogue: 0,0:51:54.46,0:51:55.63,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,0:51:55.71,0:51:56.63,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:51:56.71,0:51:57.59,Default,,0,0,0,,Đợi ta với!
Dialogue: 0,0:51:57.67,0:51:58.59,Default,,0,0,0,,Ái phi!
Dialogue: 0,0:51:58.67,0:52:00.05,Default,,0,0,0,,Ái phi, nàng ở đâu?
Dialogue: 0,0:52:04.33,0:52:05.83,Default,,0,0,0,,Đồ lì lợm!
Dialogue: 0,0:52:05.92,0:52:07.92,Default,,0,0,0,,Chuyện hồ yêu còn chưa rút kinh nghiệm sao?
Dialogue: 0,0:52:09.71,0:52:10.96,Default,,0,0,0,,Ngươi là con hồ yêu đó?
Dialogue: 0,0:52:11.04,0:52:13.21,Default,,0,0,0,,Là ngươi hại ái phi của ta.
Dialogue: 0,0:52:16.92,0:52:18.02,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,0:52:25.13,0:52:26.55,Default,,0,0,0,,Cẩn thận! Là huyễn thuật.
Dialogue: 0,0:53:07.88,0:53:09.76,Default,,0,0,0,,- Cửu...\N- Huyễn thuật của Cửu Vĩ!
Dialogue: 0,0:53:14.46,0:53:15.56,Default,,0,0,0,,Con bé là...
Dialogue: 0,0:53:16.13,0:53:17.88,Default,,0,0,0,,{\i1}Vì sao?{\i0}
Dialogue: 0,0:53:19.13,0:53:21.46,Default,,0,0,0,,{\i1}Vì sao lại hại ái phi của ta?{\i0}
Dialogue: 0,0:53:21.54,0:53:23.25,Default,,0,0,0,,{\i1}Hủy vương triều của ta?{\i0}
Dialogue: 0,0:53:23.96,0:53:26.00,Default,,0,0,0,,{\i1}Giết người moi tim,{\i0}
Dialogue: 0,0:53:26.08,0:53:27.58,Default,,0,0,0,,{\i1}cưa chân xem tủy,{\i0}
Dialogue: 0,0:53:28.63,0:53:30.26,Default,,0,0,0,,{\i1}đều do hồ yêu này.{\i0}
Dialogue: 0,0:53:32.13,0:53:33.23,Default,,0,0,0,,{\i1}Giết ả đi!{\i0}
Dialogue: 0,0:53:34.46,0:53:35.75,Default,,0,0,0,,{\i1}Giết ả đi!{\i0}
Dialogue: 0,0:53:37.13,0:53:38.23,Default,,0,0,0,,{\i1}Giết ả đi!{\i0}
Dialogue: 0,0:53:39.63,0:53:41.26,Default,,0,0,0,,{\i1}Giết ả đi!{\i0}
Dialogue: 0,0:53:50.38,0:53:51.48,Default,,0,0,0,,{\i1}Cha ta...{\i0}
Dialogue: 0,0:53:51.88,0:53:53.51,Default,,0,0,0,,{\i1}Ông ấy ở U Đô sơn đợi ta.{\i0}
Dialogue: 0,0:53:55.00,0:53:57.13,Default,,0,0,0,,Không có ai đợi ngươi\Nở U Đô sơn cả!
Dialogue: 0,0:53:59.79,0:54:01.33,Default,,0,0,0,,Ta phải đi tìm cha của ta.
Dialogue: 0,0:54:09.63,0:54:10.73,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,0:54:12.21,0:54:13.31,Default,,0,0,0,,Đứng lại!
Dialogue: 0,0:54:14.50,0:54:16.04,Default,,0,0,0,,Giết ả đi!
Dialogue: 0,0:54:16.13,0:54:17.23,Default,,0,0,0,,Giết ả đi!
Dialogue: 0,0:54:18.00,0:54:19.10,Default,,0,0,0,,Không thể giết!
Dialogue: 0,0:54:19.13,0:54:21.01,Default,,0,0,0,,Ả chết rồi...\Nái phi cũng sẽ chết theo.
Dialogue: 0,0:54:29.42,0:54:33.05,Default,,0,0,0,,U ĐÔ SƠN
Dialogue: 0,0:54:54.75,0:54:56.42,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,0:54:57.17,0:54:59.30,Default,,0,0,0,,Đã lâu không gặp!
Dialogue: 0,0:55:02.46,0:55:03.67,Default,,0,0,0,,Cửu Vĩ?
Dialogue: 0,0:55:03.75,0:55:04.63,Default,,0,0,0,,Chẳng...
Dialogue: 0,0:55:04.71,0:55:06.17,Default,,0,0,0,,Chẳng phải ngươi bị\Nthánh khí chém rồi sao?
Dialogue: 0,0:55:06.25,0:55:09.71,Default,,0,0,0,,Không phải Khương Tử Nha đã cứu ta rồi à?
Dialogue: 0,0:55:10.75,0:55:11.85,Default,,0,0,0,,{\i1}Thả con bé ra!{\i0}
Dialogue: 0,0:55:12.83,0:55:13.93,Default,,0,0,0,,Thả con bé ra!
Dialogue: 0,0:55:30.58,0:55:33.54,Default,,0,0,0,,Đúng là thánh khí\Nđã chém thân xác của ta.
Dialogue: 0,0:55:34.17,0:55:36.67,Default,,0,0,0,,Nhưng chỉ cần\Nnguyên thần thoát được một kiếp
Dialogue: 0,0:55:37.25,0:55:39.42,Default,,0,0,0,,thì yêu lực có thể tạo dựng lại thân xác.
Dialogue: 0,0:55:40.25,0:55:41.17,Default,,0,0,0,,Hồ ly thối!
Dialogue: 0,0:55:41.25,0:55:43.58,Default,,0,0,0,,Trước mặt Báo gia còn dám ba hoa?
Dialogue: 0,0:55:46.17,0:55:49.09,Default,,0,0,0,,Con mèo nhỏ ở đâu ra vậy?
Dialogue: 0,0:55:49.17,0:55:50.27,Default,,0,0,0,,Coi chừng!
Dialogue: 0,0:55:50.92,0:55:53.17,Default,,0,0,0,,Mấy năm không gặp,
Dialogue: 0,0:55:53.25,0:55:54.35,Default,,0,0,0,,Khương Tử Nha
Dialogue: 0,0:55:54.42,0:55:57.38,Default,,0,0,0,,không ngờ ngươi lại sa sút đến mức này.
Dialogue: 0,0:55:58.29,0:56:01.58,Default,,0,0,0,,Ta càng nhìn càng thấy đau lòng.
Dialogue: 0,0:56:02.25,0:56:04.25,Default,,0,0,0,,Ngươi nói xem có phải không,
Dialogue: 0,0:56:04.33,0:56:07.00,Default,,0,0,0,,- Tiểu Cửu!\N- À...
Dialogue: 0,0:56:07.83,0:56:09.41,Default,,0,0,0,,Nên gọi ngươi
Dialogue: 0,0:56:09.50,0:56:12.25,Default,,0,0,0,,là Tô Đát Kỷ.
Dialogue: 0,0:56:12.33,0:56:13.91,Default,,0,0,0,,Ái phi!
Dialogue: 0,0:56:14.00,0:56:15.42,Default,,0,0,0,,Nó...
Dialogue: 0,0:56:15.50,0:56:17.71,Default,,0,0,0,,chính là con gái Tô gia.
Dialogue: 0,0:56:18.38,0:56:21.01,Default,,0,0,0,,Cô gái loài người vốn dĩ
Dialogue: 0,0:56:22.38,0:56:24.55,Default,,0,0,0,,phải lấy Trụ Vương.
Dialogue: 0,0:56:28.88,0:56:29.98,Default,,0,0,0,,Cha...
Dialogue: 0,0:56:30.63,0:56:31.88,Default,,0,0,0,,Cha ở đâu?
Dialogue: 0,0:56:34.63,0:56:36.09,Default,,0,0,0,,Cha của ngươi...
Dialogue: 0,0:56:37.13,0:56:39.63,Default,,0,0,0,,ngươi nghĩ cho kĩ xem.
Dialogue: 0,0:56:50.63,0:56:51.73,Default,,0,0,0,,Cha!
Dialogue: 0,0:56:53.00,0:56:54.10,Default,,0,0,0,,Cha!
Dialogue: 0,0:56:55.71,0:56:56.81,Default,,0,0,0,,Xuất phát!
Dialogue: 0,0:56:59.75,0:57:02.25,Default,,0,0,0,,Trên xe ngựa mà ngươi xuất giá,
Dialogue: 0,0:57:03.13,0:57:05.55,Default,,0,0,0,,ta đã hóa thành ngươi
Dialogue: 0,0:57:05.63,0:57:08.96,Default,,0,0,0,,thành vương phi triều Thương.
Dialogue: 0,0:57:10.25,0:57:11.35,Default,,0,0,0,,Còn ngươi...
Dialogue: 0,0:57:12.08,0:57:13.87,Default,,0,0,0,,thì bị trói buộc cùng chiếc khóa này,
Dialogue: 0,0:57:13.96,0:57:16.21,Default,,0,0,0,,vĩnh viễn vùi trong thân ta.
Dialogue: 0,0:57:18.88,0:57:20.51,Default,,0,0,0,,Nhưng không ngờ,
Dialogue: 0,0:57:20.58,0:57:22.37,Default,,0,0,0,,nguyên thần của ngươi bỏ trốn.
Dialogue: 0,0:57:22.46,0:57:24.13,Default,,0,0,0,,Còn phân chia yêu lực của ta
Dialogue: 0,0:57:24.21,0:57:26.13,Default,,0,0,0,,tạo dựng lại thân xác năm xưa.
Dialogue: 0,0:57:26.88,0:57:28.30,Default,,0,0,0,,Bây giờ
Dialogue: 0,0:57:28.38,0:57:29.63,Default,,0,0,0,,phải quay lại rồi!
Dialogue: 0,0:57:31.33,0:57:32.43,Default,,0,0,0,,Dừng tay!
Dialogue: 0,0:57:40.96,0:57:42.06,Default,,0,0,0,,Con mèo chết tiệt!
Dialogue: 0,0:58:27.13,0:58:28.38,Default,,0,0,0,,Sư Tôn?
Dialogue: 0,0:58:30.58,0:58:32.62,Default,,0,0,0,,Khóa của Sư Tôn ngươi.
Dialogue: 0,0:58:33.13,0:58:35.59,Default,,0,0,0,,Các ngươi làm sao chém đứt được?
Dialogue: 0,0:58:39.21,0:58:40.31,Default,,0,0,0,,Yêu lực...
Dialogue: 0,0:58:41.38,0:58:42.59,Default,,0,0,0,,Yêu lực!
Dialogue: 0,0:58:44.46,0:58:46.63,Default,,0,0,0,,Không hay rồi!\NPháp lực của mụ điên này khôi phục rồi.
Dialogue: 0,0:58:47.83,0:58:48.93,Default,,0,0,0,,Sư Tôn...
Dialogue: 0,0:58:50.38,0:58:51.51,Default,,0,0,0,,Sự thật...
Dialogue: 0,0:58:51.58,0:58:53.25,Default,,0,0,0,,- Rốt cuộc sự thật...\N- Khương Tử Nha!
Dialogue: 0,0:58:53.33,0:58:54.66,Default,,0,0,0,,Ả muốn quẩy nhiễu tâm trí huynh.
Dialogue: 0,0:58:54.75,0:58:56.17,Default,,0,0,0,,Đừng nghe ả nói bậy bạ!
Dialogue: 0,0:58:56.25,0:58:58.25,Default,,0,0,0,,Nói bậy bạ?
Dialogue: 0,0:58:58.92,0:59:01.21,Default,,0,0,0,,Mở miệng ra là thiên đạo, chúng sinh.
Dialogue: 0,0:59:03.46,0:59:05.38,Default,,0,0,0,,Ngươi thật sự biết
Dialogue: 0,0:59:05.46,0:59:08.13,Default,,0,0,0,,- Sư Tôn ngươi đã làm những gì sao,\N- Thân Công Báo?
Dialogue: 0,0:59:11.63,0:59:13.05,Default,,0,0,0,,Nhưng mà...
Dialogue: 0,0:59:13.13,0:59:16.55,Default,,0,0,0,,Nếu không có sự thiện lương\Ncủa Khương Tử Nha ngươi,
Dialogue: 0,0:59:17.21,0:59:20.67,Default,,0,0,0,,ta làm sao còn có cơ hội trở mình?
Dialogue: 0,0:59:20.75,0:59:22.92,Default,,0,0,0,,Đáng thương cho ngươi\Nlà người trong cuộc
Dialogue: 0,0:59:23.00,0:59:25.13,Default,,0,0,0,,không nhìn rõ sự thật.
Dialogue: 0,0:59:28.83,0:59:29.66,Default,,0,0,0,,Thả họ ra!
Dialogue: 0,0:59:29.75,0:59:31.38,Default,,0,0,0,,Nhóc con,
Dialogue: 0,0:59:31.46,0:59:33.67,Default,,0,0,0,,ta sẽ không giết ngươi đâu.
Dialogue: 0,0:59:33.75,0:59:34.85,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,0:59:36.46,0:59:37.56,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,0:59:43.08,0:59:44.18,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,0:59:55.63,0:59:56.73,Default,,0,0,0,,Ẩn!
Dialogue: 0,0:59:57.38,0:59:58.63,Default,,0,0,0,,- Khóa...\N- Chạy mau!
Dialogue: 0,0:59:59.88,1:00:01.51,Default,,0,0,0,,Đừng hòng chạy!
Dialogue: 0,1:00:03.75,1:00:04.85,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,1:00:10.50,1:00:12.04,Default,,0,0,0,,Chỉ cần vẫn còn khóa,
Dialogue: 0,1:00:12.13,1:00:13.59,Default,,0,0,0,,ngươi sẽ không cứu được nó đâu!
Dialogue: 0,1:00:14.08,1:00:16.50,Default,,0,0,0,,Hồ tộc sẽ hùng mạnh trở lại!
Dialogue: 0,1:00:16.58,1:00:20.08,Default,,0,0,0,,Không một ai trong các ngươi thoát được!
Dialogue: 0,1:00:50.88,1:00:52.05,Default,,0,0,0,,Khương Tử Nha...
Dialogue: 0,1:00:53.38,1:00:54.51,Default,,0,0,0,,Ông nói đúng,
Dialogue: 0,1:00:55.67,1:00:58.88,Default,,0,0,0,,U Đô sơn... không có ai đợi ta cả.
Dialogue: 0,1:01:09.17,1:01:10.38,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,1:01:10.46,1:01:11.56,Default,,0,0,0,,Cẩn thận!
Dialogue: 0,1:01:12.58,1:01:13.68,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,1:01:22.00,1:01:23.10,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,1:01:25.50,1:01:26.60,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,1:01:30.88,1:01:34.80,Default,,0,0,0,,Tam bộ sinh thần, bát cảnh kỷ minh,\Ncấp cấp như luật lệnh!
Dialogue: 0,1:01:34.88,1:01:38.42,Default,,0,0,0,,Tam bộ sinh thần, bát cảnh kỷ minh,\Ncấp cấp như luật lệnh!
Dialogue: 0,1:01:38.50,1:01:39.60,Default,,0,0,0,,Tam bộ sinh thần, bát cảnh ...
Dialogue: 0,1:01:44.88,1:01:45.98,Default,,0,0,0,,Cố gắng lên!
Dialogue: 0,1:02:05.50,1:02:06.60,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,1:02:23.25,1:02:24.38,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,1:03:14.13,1:03:15.26,Default,,0,0,0,,Tứ Bất Tướng!
Dialogue: 0,1:04:16.88,1:04:19.13,Default,,0,0,0,,Các ngươi ở đâu?
Dialogue: 0,1:04:23.50,1:04:25.67,Default,,0,0,0,,Cuối cùng cũng nghe thấy rồi.
Dialogue: 0,1:04:26.38,1:04:27.67,Default,,0,0,0,,Ái phi!
Dialogue: 0,1:04:30.04,1:04:34.92,Default,,0,0,0,,Cả Trụ Vương ngươi\Ncũng được làm tiểu thần tiên.
Dialogue: 0,1:04:35.00,1:04:36.10,Default,,0,0,0,,Trả lại...
Dialogue: 0,1:04:36.50,1:04:37.92,Default,,0,0,0,,ái phi cho ta!
Dialogue: 0,1:04:38.50,1:04:41.63,Default,,0,0,0,,Vương triều và ái phi của ngươi...
Dialogue: 0,1:04:43.38,1:04:44.76,Default,,0,0,0,,đều chết rồi!
Dialogue: 0,1:05:00.13,1:05:01.23,Default,,0,0,0,,Nhiều năm nay,
Dialogue: 0,1:05:01.75,1:05:03.95,Default,,0,0,0,,tam giới đều nói\Nhuynh bị ảo ảnh mê hoặc.
Dialogue: 0,1:05:04.63,1:05:05.73,Default,,0,0,0,,Đến cuối cùng,
Dialogue: 0,1:05:06.50,1:05:07.88,Default,,0,0,0,,là do bọn ta mù mắt.
Dialogue: 0,1:05:11.50,1:05:13.58,Default,,0,0,0,,Tịnh Hư cung sớm muộn gì\Ncũng diệt trừ Cửu Vĩ.
Dialogue: 0,1:05:14.25,1:05:15.79,Default,,0,0,0,,Nếu không sớm chặt đứt Túc Mệnh tỏa,
Dialogue: 0,1:05:17.88,1:05:19.13,Default,,0,0,0,,con bé và Cửu Vĩ
Dialogue: 0,1:05:20.00,1:05:21.13,Default,,0,0,0,,sẽ cùng chết.
Dialogue: 0,1:05:25.13,1:05:26.26,Default,,0,0,0,,Nhưng cái khóa này...
Dialogue: 0,1:05:28.54,1:05:30.42,Default,,0,0,0,,Nếu khóa này thật sự\Ndo Sư Tôn buộc lại...
Dialogue: 0,1:05:31.50,1:05:32.60,Default,,0,0,0,,Vậy thì từ nay về sau,
Dialogue: 0,1:05:33.63,1:05:35.05,Default,,0,0,0,,chúng ta rốt cuộc phải tin vào điều gì?
Dialogue: 0,1:05:39.63,1:05:41.26,Default,,0,0,0,,Ta phải đi gặp Sư Tôn,
Dialogue: 0,1:05:50.75,1:05:52.29,Default,,0,0,0,,hỏi rõ sự tình.
Dialogue: 0,1:05:52.38,1:05:53.92,Default,,0,0,0,,Để mở Túc Mệnh tỏa này.
Dialogue: 0,1:05:58.79,1:05:59.89,Default,,0,0,0,,Tật!
Dialogue: 0,1:06:11.63,1:06:13.67,Default,,0,0,0,,Bọn họ sẽ không\Nthả thang trời xuống cho huynh đâu.
Dialogue: 0,1:06:16.75,1:06:18.83,Default,,0,0,0,,Khoan đã! Sẽ hủy hoại tu hành của đệ.
Dialogue: 0,1:06:18.92,1:06:20.50,Default,,0,0,0,,Đây không phải chuyện\Ncủa một mình huynh.
Dialogue: 0,1:06:24.25,1:06:25.08,Default,,0,0,0,,Đi đi!
Dialogue: 0,1:06:25.17,1:06:26.50,Default,,0,0,0,,Hãy cho thế gian này\Nmột lời giải đáp.
Dialogue: 0,1:06:58.75,1:07:00.00,Default,,0,0,0,,Đệ tử cầu kiến Sư Tôn!
Dialogue: 0,1:07:01.42,1:07:03.00,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,1:07:06.75,1:07:09.33,Default,,0,0,0,,{\i1}Sư Tôn lệnh cho ngươi\Nhạ giới suy ngẫm.{\i0}
Dialogue: 0,1:07:09.42,1:07:12.92,Default,,0,0,0,,{\i1}Ngươi phạm quy phá giới,\Nthì đáng tội gì?{\i0}
Dialogue: 0,1:07:13.00,1:07:14.50,Default,,0,0,0,,Đệ tử tình nguyện chịu phạt.
Dialogue: 0,1:07:14.58,1:07:15.91,Default,,0,0,0,,Nhưng Cửu Vĩ vẫn chưa bị tiêu diệt,
Dialogue: 0,1:07:16.00,1:07:17.71,Default,,0,0,0,,nguyên thần liên kết với người vô tội.
Dialogue: 0,1:07:18.25,1:07:21.00,Default,,0,0,0,,Xin hỏi Sư Tôn, rốt cuộc sự tình hôm đó\Nlà như thế nào?
Dialogue: 0,1:07:21.54,1:07:22.67,Default,,0,0,0,,{\i1}Kẻ mất tư cách như ngươi,{\i0}
Dialogue: 0,1:07:22.75,1:07:23.88,Default,,0,0,0,,{\i1}không có mặt mũi nào gặp Sư Tôn{\i0}
Dialogue: 0,1:07:27.63,1:07:29.34,Default,,0,0,0,,Nếu Sư Tôn không có gì muốn che giấu
Dialogue: 0,1:07:29.42,1:07:30.80,Default,,0,0,0,,sao lại sợ đệ tử truy hỏi?
Dialogue: 0,1:07:30.88,1:07:31.98,Default,,0,0,0,,{\i1}To gan!{\i0}
Dialogue: 0,1:07:34.38,1:07:36.67,Default,,0,0,0,,Túc Mệnh tỏa có phải\Ndo người buộc không?
Dialogue: 0,1:07:36.75,1:07:38.50,Default,,0,0,0,,Phải chăng Cửu Vĩ có liên quan đến người?
Dialogue: 0,1:07:43.63,1:07:46.05,Default,,0,0,0,,Hy sinh người vô tội
Dialogue: 0,1:07:46.13,1:07:48.05,Default,,0,0,0,,đây chính là thiên đạo sao?
Dialogue: 0,1:08:24.92,1:08:26.02,Default,,0,0,0,,Sư Tôn!
Dialogue: 0,1:08:28.75,1:08:30.00,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,1:08:30.88,1:08:33.05,Default,,0,0,0,,{\i1}Những việc Tịnh Hư cung làm{\i0}
Dialogue: 0,1:08:33.13,1:08:35.80,Default,,0,0,0,,{\i1}đều là để bảo vệ chúng sinh.{\i0}
Dialogue: 0,1:08:37.50,1:08:38.60,Default,,0,0,0,,Đệ tử không hiểu!
Dialogue: 0,1:08:38.63,1:08:41.05,Default,,0,0,0,,Nếu vậy tại sao Sư Tôn lại nói\Nthứ đệ tử nhìn thấy là ảo giác?
Dialogue: 0,1:08:42.75,1:08:43.96,Default,,0,0,0,,{\i1}Xem ra...{\i0}
Dialogue: 0,1:08:44.04,1:08:46.29,Default,,0,0,0,,{\i1}con vẫn chưa ngộ ra.{\i0}
Dialogue: 0,1:08:49.00,1:08:50.79,Default,,0,0,0,,{\i1}Lòng mang thương cảm{\i0}
Dialogue: 0,1:08:50.88,1:08:52.71,Default,,0,0,0,,{\i1}mới hiểu chúng sinh.{\i0}
Dialogue: 0,1:08:52.79,1:08:55.00,Default,,0,0,0,,{\i1}Con có một trái tim thánh thiện,{\i0}
Dialogue: 0,1:08:55.88,1:08:58.92,Default,,0,0,0,,{\i1}nhưng vi sư lệnh cho con\Nđích thân chém Cửu Vĩ.{\i0}
Dialogue: 0,1:08:59.00,1:09:00.79,Default,,0,0,0,,{\i1}Thứ cần chém, không phải cái ác{\i0}
Dialogue: 0,1:09:00.88,1:09:02.01,Default,,0,0,0,,{\i1}mà là thiện.{\i0}
Dialogue: 0,1:09:02.75,1:09:03.88,Default,,0,0,0,,Diệt thiện?
Dialogue: 0,1:09:05.38,1:09:06.51,Default,,0,0,0,,Thiện...
Dialogue: 0,1:09:07.25,1:09:08.79,Default,,0,0,0,,lẽ nào là sai?
Dialogue: 0,1:09:08.88,1:09:10.92,Default,,0,0,0,,{\i1}Thiện vốn không sai.{\i0}
Dialogue: 0,1:09:11.00,1:09:12.75,Default,,0,0,0,,{\i1}Nhưng đối với một vị thần mà nói,{\i0}
Dialogue: 0,1:09:13.50,1:09:16.42,Default,,0,0,0,,{\i1}chỉ thiện đối với một người trước mặt{\i0}
Dialogue: 0,1:09:16.50,1:09:18.13,Default,,0,0,0,,{\i1}thì đó chính là tội lỗi.{\i0}
Dialogue: 0,1:09:20.54,1:09:23.67,Default,,0,0,0,,{\i1}Hy sinh một người để cứu chúng sinh.{\i0}
Dialogue: 0,1:09:23.75,1:09:27.25,Default,,0,0,0,,{\i1}Đó chính là số mệnh của thần.{\i0}
Dialogue: 0,1:09:31.17,1:09:35.17,Default,,0,0,0,,{\i1}Khương Tử Nha lập tức về Bắc Hải tu luyện{\i0}
Dialogue: 0,1:09:35.25,1:09:38.08,Default,,0,0,0,,{\i1}đến khi diệt xong ảo tưởng trong lòng{\i0}
Dialogue: 0,1:09:38.17,1:09:41.55,Default,,0,0,0,,{\i1}thì có thể quay lại lãnh đạo chúng thần{\i0}
Dialogue: 0,1:09:41.63,1:09:44.88,Default,,0,0,0,,{\i1}tiếp tục bảo vệ chúng sinh tam giới.{\i0}
Dialogue: 0,1:09:47.63,1:09:48.88,Default,,0,0,0,,Không cứu được một người...
Dialogue: 0,1:09:50.54,1:09:52.37,Default,,0,0,0,,thì làm sao cứu được chúng sinh?
Dialogue: 0,1:09:53.54,1:09:55.04,Default,,0,0,0,,{\i1}Khương Thượng! Ngươi thật to gan!{\i0}
Dialogue: 0,1:09:55.13,1:09:57.92,Default,,0,0,0,,{\i1}Một người với chúng sinh,\Nbên nhẹ bên nặng.{\i0}
Dialogue: 0,1:10:01.54,1:10:03.12,Default,,0,0,0,,Một người như giọt sương,
Dialogue: 0,1:10:03.96,1:10:05.80,Default,,0,0,0,,chúng sinh như biển cả.
Dialogue: 0,1:10:06.67,1:10:08.55,Default,,0,0,0,,Nhưng trong mắt đệ tử
Dialogue: 0,1:10:08.63,1:10:09.73,Default,,0,0,0,,Tiểu Cửu,
Dialogue: 0,1:10:09.75,1:10:10.85,Default,,0,0,0,,một người,
Dialogue: 0,1:10:10.92,1:10:12.02,Default,,0,0,0,,hay chúng sinh
Dialogue: 0,1:10:12.67,1:10:14.00,Default,,0,0,0,,cũng không có gì khác biệt.
Dialogue: 0,1:10:16.50,1:10:18.04,Default,,0,0,0,,Trận chiến năm đó,
Dialogue: 0,1:10:18.13,1:10:19.42,Default,,0,0,0,,họ đã mất đi người thân,
Dialogue: 0,1:10:20.13,1:10:21.42,Default,,0,0,0,,không còn nhà để về.
Dialogue: 0,1:10:23.75,1:10:25.25,Default,,0,0,0,,Con tự thấy hổ thẹn,
Dialogue: 0,1:10:26.50,1:10:28.54,Default,,0,0,0,,từ nay không vào bảng Phong Thần nữa,
Dialogue: 0,1:10:29.38,1:10:30.88,Default,,0,0,0,,trở về làm người phàm.
Dialogue: 0,1:10:32.00,1:10:33.10,Default,,0,0,0,,Chỉ cần có thể
Dialogue: 0,1:10:33.63,1:10:35.63,Default,,0,0,0,,cứu lấy mỗi một người ngay trước mắt.
Dialogue: 0,1:10:38.50,1:10:39.67,Default,,0,0,0,,{\i1}Con...{\i0}
Dialogue: 0,1:10:40.50,1:10:42.25,Default,,0,0,0,,{\i1}Nghĩ kĩ chưa?{\i0}
Dialogue: 0,1:10:49.50,1:10:52.21,Default,,0,0,0,,{\i1}Một khi Túc Mệnh tỏa đã nối liền{\i0}
Dialogue: 0,1:10:52.29,1:10:55.12,Default,,0,0,0,,{\i1}thì không còn cách nào giải được.{\i0}
Dialogue: 0,1:10:56.75,1:10:58.38,Default,,0,0,0,,{\i1}Chỉ có chuyển thế{\i0}
Dialogue: 0,1:10:59.25,1:11:01.17,Default,,0,0,0,,{\i1}mới được giải thoát.{\i0}
Dialogue: 0,1:11:04.50,1:11:06.67,Default,,0,0,0,,{\i1}Một khi Đát Kỷ vào Quy Khư{\i0}
Dialogue: 0,1:11:07.50,1:11:09.88,Default,,0,0,0,,{\i1}thì có thể chuyển thế trùng sinh.{\i0}
Dialogue: 0,1:11:12.75,1:11:14.17,Default,,0,0,0,,Đa tạ Sư Tôn!
Dialogue: 0,1:11:39.00,1:11:40.21,Default,,0,0,0,,Đây chính là Quy Khư?
Dialogue: 0,1:12:18.63,1:12:19.46,Default,,0,0,0,,Là Cửu Vĩ.
Dialogue: 0,1:12:19.54,1:12:20.64,Default,,0,0,0,,Đi mau!
Dialogue: 0,1:12:26.42,1:12:27.55,Default,,0,0,0,,Hai người đi trước đi!
Dialogue: 0,1:12:29.50,1:12:30.60,Default,,0,0,0,,Đệ...
Dialogue: 0,1:12:36.04,1:12:37.29,Default,,0,0,0,,- Thân...\N- Khương Tử Nha!
Dialogue: 0,1:12:38.75,1:12:39.88,Default,,0,0,0,,Huynh nhất định phải cứu được con bé!
Dialogue: 0,1:12:41.63,1:12:42.73,Default,,0,0,0,,Mọi việc xong xuôi
Dialogue: 0,1:12:43.17,1:12:44.30,Default,,0,0,0,,chúng ta quay về Bắc Hải.
Dialogue: 0,1:12:46.67,1:12:47.77,Default,,0,0,0,,Được!
Dialogue: 0,1:13:56.21,1:13:57.59,Default,,0,0,0,,{\i1}Thật sự không làm thần nữa sao?{\i0}
Dialogue: 0,1:13:58.13,1:13:59.23,Default,,0,0,0,,{\i1}Không làm nữa.{\i0}
Dialogue: 0,1:13:59.50,1:14:00.60,Default,,0,0,0,,{\i1}Về phần đệ,{\i0}
Dialogue: 0,1:14:00.67,1:14:02.73,Default,,0,0,0,,{\i1}đã theo ta đến Bắc Hải suốt mười năm{\i0}
Dialogue: 0,1:14:02.75,1:14:05.63,Default,,0,0,0,,{\i1}- cũng nên đến lúc phục mệnh Sư Tôn rồi.\N- Hầy! Phục cái khỉ gì?{\i0}
Dialogue: 0,1:14:06.17,1:14:08.50,Default,,0,0,0,,{\i1}Thật ra là ta lén Sư Tôn xuống đây{\i0}
Dialogue: 0,1:14:13.92,1:14:15.05,Default,,0,0,0,,{\i1}Khương Tử Nha,{\i0}
Dialogue: 0,1:14:15.54,1:14:16.96,Default,,0,0,0,,{\i1}năm đó ta thật sự ngưỡng mộ huynh.{\i0}
Dialogue: 0,1:14:17.79,1:14:19.58,Default,,0,0,0,,{\i1}Được Sư Tôn xem trọng,{\i0}
Dialogue: 0,1:14:19.67,1:14:20.96,Default,,0,0,0,,{\i1}người người kính ngưỡng.{\i0}
Dialogue: 0,1:14:24.00,1:14:25.58,Default,,0,0,0,,{\i1}Nhưng bây giờ ta mới hiểu,{\i0}
Dialogue: 0,1:14:31.04,1:14:32.42,Default,,0,0,0,,{\i1}huynh vẫn luôn là huynh,{\i0}
Dialogue: 0,1:14:33.29,1:14:35.25,Default,,0,0,0,,{\i1}là anh hùng mà ta ngưỡng mộ.{\i0}
Dialogue: 0,1:14:39.54,1:14:40.64,Default,,0,0,0,,Lần này...
Dialogue: 0,1:14:41.54,1:14:42.67,Default,,0,0,0,,đến lượt ta rồi!
Dialogue: 0,1:15:24.63,1:15:26.21,Default,,0,0,0,,Khương Tử Nha.
Dialogue: 0,1:15:26.29,1:15:27.39,Default,,0,0,0,,Ngươi tỉnh rồi à?
Dialogue: 0,1:15:28.42,1:15:29.80,Default,,0,0,0,,Chúng ta đang đi đâu vậy?
Dialogue: 0,1:15:30.92,1:15:32.46,Default,,0,0,0,,Cởi bỏ Túc Mệnh tỏa,
Dialogue: 0,1:15:32.54,1:15:33.79,Default,,0,0,0,,chuyển thế trùng sinh.
Dialogue: 0,1:15:34.79,1:15:36.46,Default,,0,0,0,,Chuyển thế?
Dialogue: 0,1:15:41.17,1:15:42.30,Default,,0,0,0,,Hình như ta...
Dialogue: 0,1:15:43.50,1:15:45.79,Default,,0,0,0,,lại quay về lúc ban đầu\Nkhi mới tỉnh dậy,
Dialogue: 0,1:15:46.50,1:15:47.75,Default,,0,0,0,,không có gì cả.
Dialogue: 0,1:15:49.79,1:15:50.89,Default,,0,0,0,,Mở mắt ra...
Dialogue: 0,1:15:51.42,1:15:52.84,Default,,0,0,0,,khắp nơi đều là đống đổ nát.
Dialogue: 0,1:15:54.42,1:15:55.75,Default,,0,0,0,,Những người ta gặp
Dialogue: 0,1:15:56.79,1:15:58.33,Default,,0,0,0,,đều muốn cắt tai ta.
Dialogue: 0,1:15:59.67,1:16:01.34,Default,,0,0,0,,Ta nói với họ
Dialogue: 0,1:16:01.42,1:16:02.92,Default,,0,0,0,,ta không phải hồ yêu
Dialogue: 0,1:16:05.54,1:16:07.83,Default,,0,0,0,,nhưng không một ai tin ta.
Dialogue: 0,1:16:10.79,1:16:11.89,Default,,0,0,0,,Kiếp sau
Dialogue: 0,1:16:12.75,1:16:14.63,Default,,0,0,0,,ngươi sẽ giống\Nnhư một đứa trẻ bình thường.
Dialogue: 0,1:16:15.67,1:16:16.77,Default,,0,0,0,,Có nhà,
Dialogue: 0,1:16:17.42,1:16:18.52,Default,,0,0,0,,có bạn bè,
Dialogue: 0,1:16:19.63,1:16:21.80,Default,,0,0,0,,có một người cha thật sự yêu thương ngươi.
Dialogue: 0,1:16:26.13,1:16:27.42,Default,,0,0,0,,Thần tiên
Dialogue: 0,1:16:27.92,1:16:29.34,Default,,0,0,0,,không gạt người chứ?
Dialogue: 0,1:16:30.88,1:16:32.17,Default,,0,0,0,,Ta không gạt ngươi.
Dialogue: 0,1:17:14.21,1:17:20.75,Default,,0,0,0,,QUY KHƯ
Dialogue: 0,1:17:38.92,1:17:40.02,Default,,0,0,0,,Đi nhé!
Dialogue: 0,1:18:34.88,1:18:35.98,Default,,0,0,0,,Khương Tử Nha!
Dialogue: 0,1:18:36.92,1:18:38.02,Default,,0,0,0,,Cảm ơn!
Dialogue: 0,1:19:18.33,1:19:19.50,Default,,0,0,0,,Thân Công Báo...
Dialogue: 0,1:19:23.50,1:19:24.88,Default,,0,0,0,,Cửu Vĩ!!!
Dialogue: 0,1:19:53.33,1:19:55.33,Default,,0,0,0,,Túc Mệnh tỏa đã giải,
Dialogue: 0,1:19:55.42,1:19:57.38,Default,,0,0,0,,con bé đã không còn\Ndính dáng đến ngươi.
Dialogue: 0,1:20:16.88,1:20:18.21,Default,,0,0,0,,Hãy để...
Dialogue: 0,1:20:18.29,1:20:19.54,Default,,0,0,0,,Hãy để con bé chuyển thế đi!
Dialogue: 0,1:20:38.63,1:20:40.21,Default,,0,0,0,,Chuyển thế?
Dialogue: 0,1:20:41.17,1:20:42.30,Default,,0,0,0,,Ngươi điên rồi!
Dialogue: 0,1:20:53.88,1:20:56.21,Default,,0,0,0,,Chuyển thế gì chứ?
Dialogue: 0,1:20:59.08,1:21:00.18,Default,,0,0,0,,Dừng tay!
Dialogue: 0,1:21:01.04,1:21:03.92,Default,,0,0,0,,Người đời vốn chẳng hề hay biết
Dialogue: 0,1:21:04.00,1:21:06.21,Default,,0,0,0,,Sư Tôn ngươi đã làm chuyện gì.
Dialogue: 0,1:21:15.92,1:21:18.46,Default,,0,0,0,,Ra đây đi!\NHỡi Hồ tộc của ta!
Dialogue: 0,1:21:22.42,1:21:23.88,Default,,0,0,0,,Mới uống hai cốc
Dialogue: 0,1:21:23.96,1:21:26.17,Default,,0,0,0,,sao mắt trái ta\Ncứ nháy không ngớt thế này?
Dialogue: 0,1:21:26.25,1:21:28.08,Default,,0,0,0,,Nháy mắt trái là điềm báo tài lộc...
Dialogue: 0,1:21:56.42,1:21:59.00,Default,,0,0,0,,{\i1}Thả ta ra ngoài!{\i0}
Dialogue: 0,1:22:00.54,1:22:02.54,Default,,0,0,0,,Đây chính là
Dialogue: 0,1:22:02.63,1:22:07.59,Default,,0,0,0,,hàng ngàn hàng vạn\Ntử hồ tử tôn của ta.
Dialogue: 0,1:22:08.54,1:22:09.87,Default,,0,0,0,,Sao lại là oán hồn?
Dialogue: 0,1:22:10.63,1:22:11.96,Default,,0,0,0,,Năm đó,
Dialogue: 0,1:22:12.04,1:22:13.92,Default,,0,0,0,,hắn hứa rằng
Dialogue: 0,1:22:14.00,1:22:17.17,Default,,0,0,0,,có thể giúp Cửu Vĩ ta\Nphong thần lên trời.
Dialogue: 0,1:22:18.42,1:22:19.92,Default,,0,0,0,,Mà cái giá...
Dialogue: 0,1:22:20.00,1:22:23.58,Default,,0,0,0,,chính là để hồ tộc ta\Nliên kết với con người qua cái khóa này.
Dialogue: 0,1:22:23.67,1:22:25.43,Default,,0,0,0,,Giúp hắn phát động đại chiến,
Dialogue: 0,1:22:25.50,1:22:27.33,Default,,0,0,0,,nhất thống tam giới.
Dialogue: 0,1:22:31.88,1:22:33.34,Default,,0,0,0,,Ai ngờ...
Dialogue: 0,1:22:33.42,1:22:35.75,Default,,0,0,0,,Sau khi sự thành,
Dialogue: 0,1:22:36.38,1:22:38.34,Default,,0,0,0,,để che giấu sự thật,
Dialogue: 0,1:22:39.33,1:22:42.25,Default,,0,0,0,,hắn đã giết hết hồ tộc.
Dialogue: 0,1:22:43.13,1:22:45.71,Default,,0,0,0,,Chôn dưới vực thẳm không đáy,
Dialogue: 0,1:22:46.54,1:22:50.08,Default,,0,0,0,,muôn kiếp không được siêu sinh.
Dialogue: 0,1:22:53.58,1:22:58.33,Default,,0,0,0,,Hồ tộc cũng là\Nvật hy sinh trong cuộc đại chiến năm đó.
Dialogue: 0,1:22:59.17,1:23:00.96,Default,,0,0,0,,Là ta!
Dialogue: 0,1:23:01.04,1:23:03.33,Default,,0,0,0,,Bởi vì lòng tham của bản thân
Dialogue: 0,1:23:03.42,1:23:05.50,Default,,0,0,0,,hại chết chúng nó.
Dialogue: 0,1:23:10.54,1:23:11.46,Default,,0,0,0,,Cửu Vĩ...
Dialogue: 0,1:23:11.54,1:23:13.25,Default,,0,0,0,,Hôm nay...
Dialogue: 0,1:23:13.33,1:23:17.91,Default,,0,0,0,,ta bắt Sư Tôn ngươi\Nphải nếm trải nỗi đau của hồ tộc.
Dialogue: 0,1:23:18.58,1:23:20.58,Default,,0,0,0,,Để tam giới nhìn rõ
Dialogue: 0,1:23:20.67,1:23:24.59,Default,,0,0,0,,bộ mặt thật sự của bọn họ.
Dialogue: 0,1:23:35.21,1:23:36.31,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,1:24:22.17,1:24:24.05,Default,,0,0,0,,{\i1}Giết con bé!{\i0}
Dialogue: 0,1:24:25.83,1:24:28.16,Default,,0,0,0,,{\i1}Thả ta ra ngoài!{\i0}
Dialogue: 0,1:24:28.25,1:24:29.42,Default,,0,0,0,,{\i1}Giết con bé!{\i0}
Dialogue: 0,1:24:32.13,1:24:33.96,Default,,0,0,0,,{\i1}Gỡ bỏ khóa này!{\i0}
Dialogue: 0,1:24:36.13,1:24:37.34,Default,,0,0,0,,{\i1}Giết con bé!{\i0}
Dialogue: 0,1:24:38.88,1:24:39.98,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,1:24:41.71,1:24:42.81,Default,,0,0,0,,{\i1}Giết con bé!{\i0}
Dialogue: 0,1:24:57.58,1:24:58.71,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,1:25:10.71,1:25:11.81,Default,,0,0,0,,Sư Tôn!
Dialogue: 0,1:25:14.71,1:25:16.07,Default,,0,0,0,,Xin Sư Tôn nghĩ lại!
Dialogue: 0,1:25:17.88,1:25:19.55,Default,,0,0,0,,Tất thảy
Dialogue: 0,1:25:19.63,1:25:22.01,Default,,0,0,0,,đều vì chúng sinh!
Dialogue: 0,1:26:00.38,1:26:02.21,Default,,0,0,0,,Tiểu... Cửu!
Dialogue: 0,1:26:37.33,1:26:38.33,Default,,0,0,0,,{\i1}Đi đi!{\i0}
Dialogue: 0,1:26:38.42,1:26:40.38,Default,,0,0,0,,{\i1}Cứu rỗi mỗi một người trước mắt huynh.{\i0}
Dialogue: 0,1:26:43.00,1:26:45.79,Default,,0,0,0,,Chỉ cần Túc Mệnh tỏa vẫn còn
Dialogue: 0,1:26:45.88,1:26:48.26,Default,,0,0,0,,thì ngươi không cứu được ai cả.
Dialogue: 0,1:26:48.88,1:26:50.21,Default,,0,0,0,,Thế gian...
Dialogue: 0,1:26:50.29,1:26:53.12,Default,,0,0,0,,vẫn sẽ có hồ tộc kế tiếp.
Dialogue: 0,1:27:09.88,1:27:10.98,Default,,0,0,0,,{\i1}Kiếp sau{\i0}
Dialogue: 0,1:27:11.96,1:27:14.17,Default,,0,0,0,,{\i1}ta sẽ có một người cha\Nyêu thương mình thật sao?{\i0}
Dialogue: 0,1:27:28.92,1:27:30.25,Default,,0,0,0,,{\i1}Là thần,{\i0}
Dialogue: 0,1:27:30.88,1:27:32.17,Default,,0,0,0,,{\i1}hay là yêu,{\i0}
Dialogue: 0,1:27:33.00,1:27:34.29,Default,,0,0,0,,{\i1}là người,{\i0}
Dialogue: 0,1:27:36.00,1:27:37.38,Default,,0,0,0,,{\i1}cũng không thể khinh thường,{\i0}
Dialogue: 0,1:27:38.17,1:27:39.67,Default,,0,0,0,,{\i1}không thể thao túng,{\i0}
Dialogue: 0,1:27:41.38,1:27:42.80,Default,,0,0,0,,{\i1}không thể không cứu.{\i0}
Dialogue: 0,1:27:54.83,1:27:57.54,Default,,0,0,0,,{\i1}Khóa đã cởi!{\i0}
Dialogue: 0,1:28:01.63,1:28:04.21,Default,,0,0,0,,{\i1}Cha, mẹ!{\i0}
Dialogue: 0,1:28:04.83,1:28:06.21,Default,,0,0,0,,{\i1}Có thể về nhà rồi!{\i0}
Dialogue: 0,1:28:23.33,1:28:25.04,Default,,0,0,0,,{\i1}Cứu ta với!{\i0}
Dialogue: 0,1:28:25.13,1:28:26.26,Default,,0,0,0,,{\i1}Dẫn ta đi!{\i0}
Dialogue: 0,1:28:59.21,1:29:00.54,Default,,0,0,0,,Tiểu Cửu!
Dialogue: 0,1:29:08.88,1:29:10.13,Default,,0,0,0,,Khương Tử Nha...
Dialogue: 0,1:29:39.92,1:29:41.34,Default,,0,0,0,,Đây là kiếp sau sao?
Dialogue: 0,1:31:22.29,1:31:23.39,Default,,0,0,0,,{\i1}Thần tiên{\i0}
Dialogue: 0,1:31:24.88,1:31:26.26,Default,,0,0,0,,{\i1}không lừa người chứ?{\i0}
Dialogue: 0,1:31:43.33,1:31:46.16,Default,,0,0,0,,{\i1}Đại đệ tử Khương Tử Nha.{\i0}
Dialogue: 0,1:31:46.25,1:31:48.75,Default,,0,0,0,,{\i1}Nay dẫn dụ hồ yêu hiện thế{\i0}
Dialogue: 0,1:31:48.83,1:31:51.08,Default,,0,0,0,,{\i1}có công diệt trừ.{\i0}
Dialogue: 0,1:31:51.17,1:31:52.63,Default,,0,0,0,,{\i1}Trở về{\i0}
Dialogue: 0,1:31:52.71,1:31:54.50,Default,,0,0,0,,{\i1}lãnh đạo chúng thần.{\i0}
Dialogue: 0,1:32:14.58,1:32:17.29,Default,,0,0,0,,Vẫn còn một xiềng xích cuối cùng.
Dialogue: 0,1:32:52.17,1:32:55.05,Default,,0,0,0,,Đệ tử Khương Tử Nha!\NNguyện đi theo Sư Tôn,
Dialogue: 0,1:32:55.13,1:32:56.55,Default,,0,0,0,,buông bỏ mọi thứ.
Dialogue: 0,1:32:56.63,1:32:58.96,Default,,0,0,0,,Trở thành một vị thần,\Nbảo vệ chúng sinh.
Dialogue: 0,1:33:01.58,1:33:03.50,Default,,0,0,0,,Dùng cách của riêng mình
Dialogue: 0,1:33:03.58,1:33:04.79,Default,,0,0,0,,để trở thành một
Dialogue: 0,1:33:05.58,1:33:06.79,Default,,0,0,0,,vị thần thực thụ.
Dialogue: 0,1:33:10.83,1:33:13.10,Default,,0,0,0,,{\i1}Thái Công ở đây{\i0}
Dialogue: 0,1:33:13.13,1:33:15.17,Default,,0,0,0,,{\i1}chư thần lùi bước.{\i0}
Dialogue: 0,1:33:17.83,1:33:19.29,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,1:33:19.38,1:33:21.01,Default,,0,0,0,,{\i1}Mệnh trời đã định.{\i0}
Dialogue: 0,1:33:21.08,1:33:23.12,Default,,0,0,0,,{\i1}Quỳ xuống phong thần.{\i0}
Dialogue: 0,1:33:27.33,1:33:28.43,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,1:33:32.71,1:33:33.81,Default,,0,0,0,,{\i1}To gan!{\i0}
Dialogue: 0,1:33:39.58,1:33:41.16,Default,,0,0,0,,{\i1}Kiếp nạn chúng sinh{\i0}
Dialogue: 0,1:33:41.88,1:33:43.80,Default,,0,0,0,,{\i1}tội do hồ tộc.{\i0}
Dialogue: 0,1:33:48.21,1:33:49.71,Default,,0,0,0,,{\i1}Khương Tử Nha!{\i0}
Dialogue: 0,1:33:51.21,1:33:52.92,Default,,0,0,0,,{\i1}Đây chính là sự sắp đặt{\i0}
Dialogue: 0,1:33:53.00,1:33:54.92,Default,,0,0,0,,{\i1}tốt nhất ta dành cho thế gian.{\i0}
Dialogue: 0,1:34:07.96,1:34:09.79,Default,,0,0,0,,Nguyện thế gian
Dialogue: 0,1:34:09.88,1:34:11.64,Default,,0,0,0,,không còn loạn lạc chia ly.
Dialogue: 0,1:34:13.25,1:34:15.00,Default,,0,0,0,,Nguyện chúng sinh
Dialogue: 0,1:34:15.08,1:34:16.87,Default,,0,0,0,,không còn nghe lời dối trá.
Dialogue: 0,1:34:18.33,1:34:19.66,Default,,0,0,0,,Nguyện thiên hạ...
Dialogue: 0,1:34:24.38,1:34:26.59,Default,,0,0,0,,Không còn bất công!
Dialogue: 0,1:34:28.63,1:34:29.88,Default,,0,0,0,,{\i1}Ngươi điên rồi!{\i0}
Dialogue: 0,1:34:29.96,1:34:33.04,Default,,0,0,0,,{\i1}Nếu thang trời gãy\Nthế gian sẽ chẳng còn được phù hộ.{\i0}
Dialogue: 0,1:34:49.38,1:34:50.48,Default,,0,0,0,,{\i1}Khương Tử Nha...{\i0}
Dialogue: 0,1:35:32.58,1:35:34.79,Default,,0,0,0,,Khương Tử Nha đập nát thang trời rồi.
Dialogue: 0,1:35:35.71,1:35:38.54,Default,,0,0,0,,Sư Tôn cũng phải chịu\Nsự trừng phạt của trời cao.
Dialogue: 0,1:35:40.46,1:35:42.29,Default,,0,0,0,,Huyền Điểu giáng thế
Dialogue: 0,1:35:42.38,1:35:43.66,Default,,0,0,0,,dẫn độ oán hồn.
Dialogue: 0,1:35:45.21,1:35:46.75,Default,,0,0,0,,Tất cả mọi thứ trên thế gian
Dialogue: 0,1:35:47.33,1:35:49.71,Default,,0,0,0,,lại có được thái bình và hy vọng.
Dialogue: 0,1:35:50.58,1:35:51.46,Default,,0,0,0,,Sau này con
Dialogue: 0,1:35:51.54,1:35:53.87,Default,,0,0,0,,cũng muốn trở thành một đại anh hùng\Ngiống như Khương Tử Nha
Dialogue: 0,1:35:54.63,1:35:55.34,Default,,0,0,0,,Sư phụ
Dialogue: 0,1:35:55.33,1:35:57.87,Default,,0,0,0,,người nói con Huyền Điểu này\Ntừ đâu tới nhỉ?
Dialogue: 0,1:35:58.75,1:36:00.08,Default,,0,0,0,,Một nơi
Dialogue: 0,1:36:00.08,1:36:02.83,Default,,0,0,0,,còn cao hơn Tịnh Hư cung rất nhiều lần.
Dialogue: 0,1:36:03.38,1:36:04.88,Default,,0,0,0,,Người ngự ở đó
Dialogue: 0,1:36:05.21,1:36:07.79,Default,,0,0,0,,là sư phụ của sư phụ của sư phụ con đấy.
Dialogue: 0,1:36:08.46,1:36:09.56,Default,,0,0,0,,{\i1}Òa!{\i0}
Dialogue: 0,1:36:10.08,1:36:13.08,Default,,0,0,0,,{\i1}Sư phụ của sư phụ là Sư Tôn.{\i0}
Dialogue: 0,1:36:13.17,1:36:15.71,Default,,0,0,0,,{\i1}Vậy thì sư phụ của Sư Tôn là{\i0}
Dialogue: 0,1:36:15.79,1:36:16.62,Default,,0,0,0,,{\i1}Sư Tổ!{\i0}
Dialogue: 0,1:36:17.83,1:36:20.54,Default,,0,0,0,,{\i1}Không được gọi bừa tên húy của Sư Tổ{\i0}
Dialogue: 0,1:36:20.63,1:36:22.76,Default,,0,0,0,,{\i1}Đó là đấng thế tôn của chín tầng trời,{\i0}
Dialogue: 0,1:36:22.83,1:36:25.79,Default,,0,0,0,,{\i1}thần tiên dẫn Huyền Điểu giáng thế.{\i0}
Dialogue: 0,1:36:25.88,1:36:27.51,Default,,0,0,0,,{\i1}Khương Tử Nha này cũng được phết!{\i0}
Dialogue: 0,1:36:27.58,1:36:29.66,Default,,0,0,0,,{\i1}Đến cả Sư Tổ cũng ra mặt giúp hắn.{\i0}
Dialogue: 0,1:36:29.75,1:36:31.38,Default,,0,0,0,,{\i1}Thật không hổ là huynh đệ tốt của ta.{\i0}
Dialogue: 0,1:36:31.46,1:36:32.92,Default,,0,0,0,,{\i1}Huynh đệ của ngươi?{\i0}
Dialogue: 0,1:36:33.00,1:36:34.25,Default,,0,0,0,,{\i1}Cả Bắc Hải này{\i0}
Dialogue: 0,1:36:34.33,1:36:35.75,Default,,0,0,0,,{\i1}cũng chỉ có ta với Tử Nha{\i0}

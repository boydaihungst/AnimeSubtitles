Dialogue: 0,0:00:02.45,0:00:05.24,Default,,0,0,0,,[Quỷ thần hiểm ác, quái vật xấu xa,]
Dialogue: 0,0:00:05.32,0:00:08.66,Default,,0,0,0,,[các sứ giả trên trời, những vị anh hùng\Nđã và đang thay đổi thế giới.]
Dialogue: 0,0:00:08.74,0:00:09.95,Default,,0,0,0,,[Họ có thể vay mượn sức mạnh]
Dialogue: 0,0:00:10.03,0:00:13.41,Default,,0,0,0,,[từ những thế lực phi thường tồn tại\Nngoài vốn hiểu biết của con người.]
Dialogue: 0,0:00:13.49,0:00:14.95,Default,,0,0,0,,Đó chính là
Dialogue: 0,0:00:15.03,0:00:16.95,Default,,0,0,0,,ý nghĩa của "Giải đấu Chúa tể Học đường".
Dialogue: 0,0:00:30.95,0:00:32.53,Default,,0,0,0,,[Chúa Tể Học Đường]
Dialogue: 0,0:01:47.95,0:01:52.12,Default,,0,0,0,,[bản ngã/sức mạnh]
Dialogue: 0,0:01:52.95,0:01:57.07,Default,,0,0,0,,Vòng Quốc gia Giải đấu Chúa tể Học đường\Ncuối cùng đã bắt đầu!
Dialogue: 0,0:01:57.16,0:01:58.62,Default,,0,0,0,,Trận đấu đầu tiên của Bảng A
Dialogue: 0,0:01:58.70,0:02:01.66,Default,,0,0,0,,[là giữa đội tỉnh Chungcheongbuk\Nvà đội Seoul!]
Dialogue: 0,0:02:02.82,0:02:04.70,Default,,0,0,0,,[Trận đấu này sẽ diễn ra\Ntheo thể thức đấu đội.]
Dialogue: 0,0:02:04.78,0:02:08.91,Default,,0,0,0,,[Do đó, họ phải trải qua ba trận đấu.]
Dialogue: 0,0:02:09.28,0:02:12.95,Default,,0,0,0,,Đội nào thắng hai trận trước\Nsẽ giành chiến thắng!
Dialogue: 0,0:02:13.57,0:02:15.74,Default,,0,0,0,,Được rồi! Trận đầu tiên để tôi.
Dialogue: 0,0:02:15.82,0:02:17.28,Default,,0,0,0,,Cẩn thận đấy!
Dialogue: 0,0:02:17.74,0:02:20.74,Default,,0,0,0,,Có thể họ sẽ dùng Tá Lực\Nmà chúng ta đã nghe qua.
Dialogue: 0,0:02:21.37,0:02:24.78,Default,,0,0,0,,Chẳng sao. Tôi cũng có kỹ năng mới mà.
Dialogue: 0,0:02:26.12,0:02:27.95,Default,,0,0,0,,Cảm ơn sự kiên nhẫn của mọi người!
Dialogue: 0,0:02:28.03,0:02:29.16,Default,,0,0,0,,Trong trận đấu đầu, ta có..
Dialogue: 0,0:02:29.24,0:02:31.82,Default,,0,0,0,,Jin Mo Ri đấu với Nah Gi Dong!
Dialogue: 0,0:02:31.91,0:02:33.70,Default,,0,0,0,,Sẵn sàng, chiến đấu!
Dialogue: 0,0:02:33.78,0:02:36.32,Default,,0,0,0,,Xem như cậu xui xẻo\Nkhi phải đấu với tôi đi.
Dialogue: 0,0:02:36.41,0:02:38.57,Default,,0,0,0,,Xem tôi hạ gục cậu trong nháy mắt này.
Dialogue: 0,0:02:39.28,0:02:43.99,Default,,0,0,0,,Giờ tôi sẽ cho anh thấy chiêu mới của tôi!
Dialogue: 0,0:02:46.87,0:02:49.07,Default,,0,0,0,,Những huyệt đạo này\Nsẽ giải phóng sức mạnh tiềm tàng.
Dialogue: 0,0:02:50.07,0:02:54.28,Default,,0,0,0,,Giờ thì những đòn tấn công của tôi\Nsẽ mạnh hơn rất nhiều lần.
Dialogue: 0,0:02:57.20,0:02:59.95,Default,,0,0,0,,Đó là tuyệt kỹ ta đã chỉ cho cậu ta.
Dialogue: 0,0:03:00.03,0:03:03.41,Default,,0,0,0,,Taekwondo của Tae Jin lỗi thời rồi.
Dialogue: 0,0:03:03.49,0:03:05.62,Default,,0,0,0,,Được rồi, tôi tới đây!
Dialogue: 0,0:03:09.32,0:03:11.12,Default,,0,0,0,,Không cử động được!
Dialogue: 0,0:03:11.20,0:03:13.20,Default,,0,0,0,,Mình ấn sai huyệt đạo rồi!
Dialogue: 0,0:03:21.24,0:03:23.03,Default,,0,0,0,,Người chiến thắng là Nah Gi Dong!
Dialogue: 0,0:03:24.16,0:03:25.53,Default,,0,0,0,,Thắng rồi!
Dialogue: 0,0:03:27.07,0:03:29.91,Default,,0,0,0,,Chắc dùng Taekwondo sẽ tốt hơn.
Dialogue: 0,0:03:30.57,0:03:31.95,Default,,0,0,0,,Pum Gwang, anh có thấy không?
Dialogue: 0,0:03:32.03,0:03:33.45,Default,,0,0,0,,Anh có thấy cách đánh của em không?
Dialogue: 0,0:03:33.53,0:03:35.03,Default,,0,0,0,,Làm tốt lắm!
Dialogue: 0,0:03:35.12,0:03:37.95,Default,,0,0,0,,Đừng ngủ quên trên chiến thắng\Nđể rồi lơ là cảnh giác.
Dialogue: 0,0:03:38.03,0:03:40.49,Default,,0,0,0,,[Tiếp theo là trận đấu thứ hai!]
Dialogue: 0,0:03:40.57,0:03:42.82,Default,,0,0,0,,Nếu ta thua thêm một trận thì sẽ bị loại.
Dialogue: 0,0:03:42.91,0:03:44.49,Default,,0,0,0,,Chúng ta phải thắng.
Dialogue: 0,0:03:45.03,0:03:47.87,Default,,0,0,0,,[Cậu mà thua như tên ngốc đó\Nthì tôi không tha cho đâu!]
Dialogue: 0,0:03:48.91,0:03:49.87,Default,,0,0,0,,[Trận đấu thứ hai sẽ là]
Dialogue: 0,0:03:49.95,0:03:51.82,Default,,0,0,0,,[Han Dae Wi đấu với Jin Pum Gwang!]
Dialogue: 0,0:03:52.49,0:03:54.87,Default,,0,0,0,,[Jin Pum Gwang, 38 tuổi,]
Dialogue: 0,0:03:54.95,0:03:57.66,Default,,0,0,0,,[là đấu sĩ lớn tuổi nhất của giải đấu!]
Dialogue: 0,0:03:58.70,0:04:01.20,Default,,0,0,0,,Anh ấy 38 tuổi rồi mà vẫn học cấp ba ư?
Dialogue: 0,0:04:01.28,0:04:02.37,Default,,0,0,0,,Thật vậy sao?
Dialogue: 0,0:04:02.45,0:04:05.78,Default,,0,0,0,,Nếu có lòng học hỏi,\Ntuổi tác đâu phải là vấn đề.
Dialogue: 0,0:04:10.03,0:04:11.45,Default,,0,0,0,,[Tuyệt thật đấy.]
Dialogue: 0,0:04:11.53,0:04:15.16,Default,,0,0,0,,[Giám khảo Q đang run rẩy dưới chân ta.]
Dialogue: 0,0:04:15.24,0:04:18.20,Default,,0,0,0,,Gương mặt tội nghiệp của hắn\Nkhiến ta buồn cười!
Dialogue: 0,0:04:18.28,0:04:19.37,Default,,0,0,0,,Cả tên đeo kính râm nữa.
Dialogue: 0,0:04:19.45,0:04:22.07,Default,,0,0,0,,Vẻ mặt thống khổ của hắn\Ntrước lúc chết thật tuyệt mỹ!
Dialogue: 0,0:04:22.16,0:04:25.78,Default,,0,0,0,,Hắn còn van xin ta rằng:\N"Xin hãy tha mạng cho họ."
Dialogue: 0,0:04:26.32,0:04:29.99,Default,,0,0,0,,Nên ta cho hắn toại nguyện,
Dialogue: 0,0:04:30.07,0:04:33.03,Default,,0,0,0,,và tiễn vợ con hắn lên đường cùng luôn.
Dialogue: 0,0:04:54.24,0:04:55.87,Default,,0,0,0,,Này, tên kia.
Dialogue: 0,0:04:57.66,0:05:00.07,Default,,0,0,0,,Im đi có được không?
Dialogue: 0,0:05:08.28,0:05:10.41,Default,,0,0,0,,Lảm nhảm ồn ào quá.
Dialogue: 0,0:05:13.28,0:05:14.20,Default,,0,0,0,,Chết đi.
Dialogue: 0,0:05:26.24,0:05:29.07,Default,,0,0,0,,Tầng Lớp Lao Động: Ném Túi Cơm!
Dialogue: 0,0:05:31.66,0:05:34.49,Default,,0,0,0,,Tầng Lớp Lao Động: Đóng Đinh Dài!
Dialogue: 0,0:05:35.28,0:05:36.12,Default,,0,0,0,,Dae Wi!
Dialogue: 0,0:05:37.32,0:05:39.45,Default,,0,0,0,,Mình sẽ dứt điểm bằng chiêu này!
Dialogue: 0,0:05:41.57,0:05:42.57,Default,,0,0,0,,Huyền Vũ!
Dialogue: 0,0:05:45.41,0:05:46.57,Default,,0,0,0,,Chu Tước!
Dialogue: 0,0:05:47.62,0:05:48.99,Default,,0,0,0,,Bạch Hổ!
Dialogue: 0,0:05:51.95,0:05:52.99,Default,,0,0,0,,Anh Pum Gwang!
Dialogue: 0,0:05:53.07,0:05:55.74,Default,,0,0,0,,Không sao. Anh ấy không thua đâu.
Dialogue: 0,0:05:56.28,0:05:59.95,Default,,0,0,0,,Anh ấy là người bất luận có thế nào\Ncũng sẽ không từ bỏ.
Dialogue: 0,0:06:00.57,0:06:03.03,Default,,0,0,0,,[Đúng vậy. Hồi ấy cũng thế.]
Dialogue: 0,0:06:07.28,0:06:10.91,Default,,0,0,0,,[Người đó nói sẽ xin nhập học cấp ba.]
Dialogue: 0,0:06:10.99,0:06:12.28,Default,,0,0,0,,[Gì chứ? Ở tuổi đó sao?]
Dialogue: 0,0:06:12.37,0:06:14.95,Default,,0,0,0,,Ừ, anh ta đã rớt ba lần rồi.
Dialogue: 0,0:06:15.03,0:06:17.45,Default,,0,0,0,,Chẳng biết đang nghĩ gì nữa,
Dialogue: 0,0:06:17.53,0:06:19.66,Default,,0,0,0,,nhưng xấu hổ thật đấy.
Dialogue: 0,0:06:19.74,0:06:20.66,Default,,0,0,0,,Đúng vậy!
Dialogue: 0,0:06:23.87,0:06:25.66,Default,,0,0,0,,Mấy cô hẳn còn là học sinh.
Dialogue: 0,0:06:26.24,0:06:28.87,Default,,0,0,0,,Hút thuốc vào là sớm về chầu trời đấy.
Dialogue: 0,0:06:29.53,0:06:30.74,Default,,0,0,0,,Vậy sao?
Dialogue: 0,0:06:31.12,0:06:32.87,Default,,0,0,0,,Bị sao thế? Cái thứ vênh váo.
Dialogue: 0,0:06:34.32,0:06:35.24,Default,,0,0,0,,Trời ạ.
Dialogue: 0,0:06:36.95,0:06:37.91,Default,,0,0,0,,Anh Pum Gwang,
Dialogue: 0,0:06:38.99,0:06:41.20,Default,,0,0,0,,sách tham khảo anh nhờ em mua đây.
Dialogue: 0,0:06:41.28,0:06:43.24,Default,,0,0,0,,Cảm ơn nhiều nhé, Gi Dong.
Dialogue: 0,0:06:44.70,0:06:46.45,Default,,0,0,0,,Em mới là người phải cảm ơn anh.
Dialogue: 0,0:06:46.53,0:06:47.78,Default,,0,0,0,,Nếu không gặp được anh,
Dialogue: 0,0:06:47.87,0:06:51.20,Default,,0,0,0,,em vẫn sẽ sống cuộc đời đánh lộn tối ngày.
Dialogue: 0,0:06:51.99,0:06:54.07,Default,,0,0,0,,[- Lảm nhảm gì vậy chứ?]\N[- Phiền chết đi được.]
Dialogue: 0,0:06:54.16,0:06:55.24,Default,,0,0,0,,[Chắc não chỉ toàn cơ bắp.]
Dialogue: 0,0:06:55.32,0:06:57.99,Default,,0,0,0,,[Chắc đó là kiểu mọi người hay gọi là\Nthất bại của tạo hóa.]
Dialogue: 0,0:06:58.74,0:07:01.24,Default,,0,0,0,,[Mình nhất định không được trở thành\Nmột người như vậy.]
Dialogue: 0,0:07:01.32,0:07:02.99,Default,,0,0,0,,[Tuyệt đối không.]
Dialogue: 0,0:07:06.07,0:07:08.78,Default,,0,0,0,,[Hai người đó cũng vào\Nchung kết năm ngoái, phải không?]
Dialogue: 0,0:07:08.87,0:07:11.24,Default,,0,0,0,,[Nghe nói họ đấu với nhau\Nnhiều lần trước đây rồi,]
Dialogue: 0,0:07:11.32,0:07:13.32,Default,,0,0,0,,[nhưng người thắng luôn là...]
Dialogue: 0,0:07:13.41,0:07:14.32,Default,,0,0,0,,[Mình có thể thắng!]
Dialogue: 0,0:07:16.24,0:07:18.32,Default,,0,0,0,,Người chiến thắng là Kim Na Young!
Dialogue: 0,0:07:19.82,0:07:24.32,Default,,0,0,0,,- Tuyệt lắm!\N- Cậu thắng rồi!
Dialogue: 0,0:07:41.07,0:07:43.07,Default,,0,0,0,,Xem cô đã làm gì kìa.
Dialogue: 0,0:07:43.16,0:07:44.20,Default,,0,0,0,,Không sao chứ?
Dialogue: 0,0:07:44.78,0:07:46.87,Default,,0,0,0,,Tên tôi là Pum Gwang.
Dialogue: 0,0:07:47.16,0:07:48.95,Default,,0,0,0,,Tôi có biết anh.
Dialogue: 0,0:07:49.03,0:07:50.70,Default,,0,0,0,,Mọi người đều bàn tán về anh.
Dialogue: 0,0:07:53.57,0:07:55.16,Default,,0,0,0,,Chắc chẳng có gì tốt đẹp cả.
Dialogue: 0,0:07:55.70,0:07:57.66,Default,,0,0,0,,Tại sao thế?
Dialogue: 0,0:07:57.74,0:08:00.03,Default,,0,0,0,,Sao anh vẫn còn cười được?
Dialogue: 0,0:08:00.57,0:08:02.62,Default,,0,0,0,,Chẳng phải\Nanh đã thất bại nhiều lần rồi sao?
Dialogue: 0,0:08:03.07,0:08:04.74,Default,,0,0,0,,Mọi người đều cười nhạo, bảo anh bất tài.
Dialogue: 0,0:08:04.82,0:08:06.28,Default,,0,0,0,,Sao anh chưa từ bỏ?
Dialogue: 0,0:08:06.70,0:08:09.03,Default,,0,0,0,,[Vì tôi không muốn từ bỏ bản thân.]
Dialogue: 0,0:08:10.12,0:08:12.32,Default,,0,0,0,,Tôi học không giỏi.
Dialogue: 0,0:08:12.41,0:08:15.57,Default,,0,0,0,,Tôi từng học cấp ba,\Nnhưng lại bỏ giữa chừng.
Dialogue: 0,0:08:16.07,0:08:17.95,Default,,0,0,0,,Sau đó, tôi đã thử mọi việc
Dialogue: 0,0:08:18.03,0:08:19.99,Default,,0,0,0,,[nhưng lúc nào cũng bỏ dở.]
Dialogue: 0,0:08:20.07,0:08:21.57,Default,,0,0,0,,[Lý do rất đơn giản.]
Dialogue: 0,0:08:22.07,0:08:25.03,Default,,0,0,0,,[Vì tôi luôn từ bỏ bản thân,]
Dialogue: 0,0:08:25.91,0:08:27.99,Default,,0,0,0,,[cho rằng mình không làm được.]
Dialogue: 0,0:08:29.91,0:08:31.37,Default,,0,0,0,,Nên tôi đã quyết định.
Dialogue: 0,0:08:31.78,0:08:35.62,Default,,0,0,0,,Tôi nhất quyết không từ bỏ bản thân nữa.
Dialogue: 0,0:08:37.95,0:08:39.82,Default,,0,0,0,,Cô cũng không nên thế.
Dialogue: 0,0:08:39.91,0:08:42.62,Default,,0,0,0,,Miễn là không từ bỏ, nguyện ước sẽ thành.
Dialogue: 0,0:08:56.74,0:08:58.49,Default,,0,0,0,,[Đúng là ngốc.]
Dialogue: 0,0:09:00.87,0:09:03.82,Default,,0,0,0,,[Nhưng kẻ ngốc thật sự là...]
Dialogue: 0,0:09:10.03,0:09:13.41,Default,,0,0,0,,Đáp án cho câu đó là K bằng một phần hai.
Dialogue: 0,0:09:14.16,0:09:15.20,Default,,0,0,0,,Cô đang...
Dialogue: 0,0:09:15.70,0:09:17.37,Default,,0,0,0,,Đề dễ thế này mà anh đã chật vật rồi.
Dialogue: 0,0:09:17.45,0:09:19.24,Default,,0,0,0,,Anh có chắc mình ổn không đấy?
Dialogue: 0,0:09:20.03,0:09:21.99,Default,,0,0,0,,Tôi cũng sẽ sớm xin nhập học cấp ba.
Dialogue: 0,0:09:22.07,0:09:23.49,Default,,0,0,0,,Tôi giúp anh học nhé?
Dialogue: 0,0:09:23.91,0:09:25.28,Default,,0,0,0,,Thật không?
Dialogue: 0,0:09:25.37,0:09:26.87,Default,,0,0,0,,Như vậy sẽ giúp ích cho tôi nhiều lắm!
Dialogue: 0,0:09:30.07,0:09:33.37,Default,,0,0,0,,[Người đó nhất định sẽ không thua đâu.]
Dialogue: 0,0:09:39.70,0:09:40.74,Default,,0,0,0,,Tá Lực?
Dialogue: 0,0:09:41.66,0:09:43.07,Default,,0,0,0,,Đỉnh quá!
Dialogue: 0,0:09:49.45,0:09:50.99,Default,,0,0,0,,Tôi có một ước mơ.
Dialogue: 0,0:09:51.66,0:09:57.03,Default,,0,0,0,,Tôi muốn giúp những đứa trẻ khao khát\Nđược đi học mà không có khả năng.
Dialogue: 0,0:09:57.62,0:10:00.41,Default,,0,0,0,,Tôi muốn đạt được ước mơ đó\Nthông qua giải đấu này.
Dialogue: 0,0:10:06.07,0:10:08.78,Default,,0,0,0,,Tôi thì muốn trở nên mạnh mẽ hơn.
Dialogue: 0,0:10:08.87,0:10:10.66,Default,,0,0,0,,Nếu vượt qua được anh,
Dialogue: 0,0:10:11.53,0:10:13.87,Default,,0,0,0,,tôi cảm thấy\Nmình có thể tiến về phía trước một bước.
Dialogue: 0,0:10:14.74,0:10:15.87,Default,,0,0,0,,Xông lên đi.
Dialogue: 0,0:10:18.95,0:10:21.74,Default,,0,0,0,,Nghiền nát đối thủ nào, Lôi Thần Chùy!
Dialogue: 0,0:10:26.28,0:10:27.70,Default,,0,0,0,,Mình không thể...
Dialogue: 0,0:10:27.78,0:10:29.95,Default,,0,0,0,,- Dae Wi, cố lên!\N[- ...thua ở đây được.]
Dialogue: 0,0:10:30.03,0:10:32.62,Default,,0,0,0,,- Dae Wi!\N[- Mình không thể thua trước mặt họ.]
Dialogue: 0,0:10:32.95,0:10:36.16,Default,,0,0,0,,Đệ Tứ Hình: Thanh Long!
Dialogue: 0,0:10:48.32,0:10:50.37,Default,,0,0,0,,Người chiến thắng là Han Dae Wi!
Dialogue: 0,0:10:51.28,0:10:52.45,Default,,0,0,0,,Xuất sắc!
Dialogue: 0,0:11:16.57,0:11:17.87,Default,,0,0,0,,Vô ích thôi.
Dialogue: 0,0:11:18.57,0:11:21.87,Default,,0,0,0,,Đây là Tá Lực của ta.
Dialogue: 0,0:11:21.95,0:11:24.99,Default,,0,0,0,,Chừng nào còn sở hữu sức mạnh này\Nthì ta có thể tái sinh không ngừng.
Dialogue: 0,0:11:25.07,0:11:28.41,Default,,0,0,0,,[Ngươi có làm gì cũng vô ích thôi.]
Dialogue: 0,0:11:29.82,0:11:30.91,Default,,0,0,0,,Ai quan tâm chứ?
Dialogue: 0,0:11:51.41,0:11:53.99,Default,,0,0,0,,[Các ngươi chui từ đâu ra thế?]
Dialogue: 0,0:12:05.53,0:12:07.49,Default,,0,0,0,,Sao chúng mày đứa nào đứa nấy
Dialogue: 0,0:12:08.32,0:12:10.03,Default,,0,0,0,,vào nhà người khác
Dialogue: 0,0:12:12.78,0:12:14.20,Default,,0,0,0,,mà không cởi giày hả?
Dialogue: 0,0:12:15.41,0:12:18.24,Default,,0,0,0,,Đức Cha Axley, Đức Mẹ Saturn!
Dialogue: 0,0:12:18.32,0:12:20.87,Default,,0,0,0,,Drake, sao ngươi lại ở đây?
Dialogue: 0,0:12:20.95,0:12:24.45,Default,,0,0,0,,Bọn ta mới là người được lệnh\Nthủ tiêu ban giám khảo.
Dialogue: 0,0:12:25.28,0:12:27.66,Default,,0,0,0,,Ta cũng là thành viên của Nox.
Dialogue: 0,0:12:27.74,0:12:30.24,Default,,0,0,0,,Ta cũng có khả năng hoàn thành nhiệm vụ!
Dialogue: 0,0:12:30.32,0:12:32.66,Default,,0,0,0,,Sang thế giới bên kia mà tán dóc.
Dialogue: 0,0:12:34.53,0:12:36.49,Default,,0,0,0,,Thiếu kiên nhẫn quá đấy.
Dialogue: 0,0:12:38.12,0:12:41.37,Default,,0,0,0,,Quốc Bảo, Kiếm Durandal!
Dialogue: 0,0:13:03.53,0:13:05.91,Default,,0,0,0,,Nhận lấy đòn trừng phạt của Chúa đi!
Dialogue: 0,0:13:10.82,0:13:12.82,Default,,0,0,0,,Cảm nhận được anh gỡ bỏ\Nvòng giới hạn sức mạnh,
Dialogue: 0,0:13:12.91,0:13:14.66,Default,,0,0,0,,nên tôi ghé qua xem anh thế nào.
Dialogue: 0,0:13:15.16,0:13:17.91,Default,,0,0,0,,[Quả nhiên ở đây xôm tụ thật.]
Dialogue: 0,0:13:17.99,0:13:20.45,Default,,0,0,0,,Tôi tự lo liệu được.
Dialogue: 0,0:13:22.37,0:13:23.99,Default,,0,0,0,,Anh sẽ bị trừ ba tháng lương
Dialogue: 0,0:13:24.07,0:13:25.78,Default,,0,0,0,,nếu tự ý gỡ vòng giới hạn sức mạnh ra đấy.
Dialogue: 0,0:13:26.45,0:13:28.62,Default,,0,0,0,,Cùng với vụ việc lần trước...
Dialogue: 0,0:13:28.70,0:13:30.07,Default,,0,0,0,,tổng cộng sẽ là sáu tháng lương.
Dialogue: 0,0:13:30.87,0:13:32.82,Default,,0,0,0,,Tận... sáu tháng lương ư?
Dialogue: 0,0:13:34.24,0:13:36.66,Default,,0,0,0,,Tiếp theo sẽ là trận đấu cuối cùng
Dialogue: 0,0:13:36.74,0:13:37.82,Default,,0,0,0,,giữa hai đấu sĩ...
Dialogue: 0,0:13:37.91,0:13:40.37,Default,,0,0,0,,[Yoo Mi Ra và Jang Jang Mi.]
Dialogue: 0,0:13:40.45,0:13:42.07,Default,,0,0,0,,[Cả hai đội đều đang có 1 thắng, 1 thua.]
Dialogue: 0,0:13:42.49,0:13:44.37,Default,,0,0,0,,[Vì vậy, đội nào thắng trận này]
Dialogue: 0,0:13:44.45,0:13:46.87,Default,,0,0,0,,[sẽ được bước tiếp vào vòng sau.]
Dialogue: 0,0:13:46.95,0:13:48.41,Default,,0,0,0,,[Xin mời các đấu sĩ vào vị trí!]
Dialogue: 0,0:13:48.49,0:13:50.53,Default,,0,0,0,,Chuẩn bị, bắt đầu!
Dialogue: 0,0:14:21.82,0:14:23.28,Default,,0,0,0,,[Ngay từ đầu mình đã biết]
Dialogue: 0,0:14:23.37,0:14:25.12,Default,,0,0,0,,[kiếm pháp của mình không địch lại cô ta.]
Dialogue: 0,0:14:25.20,0:14:26.57,Default,,0,0,0,,[Jang Mi!]
Dialogue: 0,0:14:26.66,0:14:28.45,Default,,0,0,0,,[- Nhưng...]\N- Cố lên!
Dialogue: 0,0:14:28.53,0:14:30.07,Default,,0,0,0,,[...trận này...]
Dialogue: 0,0:14:31.12,0:14:32.32,Default,,0,0,0,,[mình không thể thua được.]
Dialogue: 0,0:14:44.57,0:14:47.16,Default,,0,0,0,,Đây là Tá Lực của tôi!
Dialogue: 0,0:14:47.24,0:14:48.53,Default,,0,0,0,,Bastard!
Dialogue: 0,0:15:20.57,0:15:22.99,Default,,0,0,0,,Nhai cho kỹ vào, Chang Sik.
Dialogue: 0,0:15:28.57,0:15:31.37,Default,,0,0,0,,Tên giòi bọ kia!
Dialogue: 0,0:15:31.45,0:15:32.99,Default,,0,0,0,,[Ta sẽ bẻ tay ngươi]
Dialogue: 0,0:15:33.07,0:15:35.20,Default,,0,0,0,,[rồi dùng nó để chọc vào mũi ngươi luôn!]
Dialogue: 0,0:15:35.87,0:15:39.41,Default,,0,0,0,,Xem ra vốn từ của ngươi\Ncũng đặc sắc thật đấy.
Dialogue: 0,0:15:47.74,0:15:49.28,Default,,0,0,0,,Đến lúc hạ màn rồi.
Dialogue: 0,0:16:08.28,0:16:09.20,Default,,0,0,0,,Ngon quá!
Dialogue: 0,0:16:09.28,0:16:11.57,Default,,0,0,0,,Đừng tranh bữa trưa của người khác chứ!
Dialogue: 0,0:16:11.74,0:16:15.07,Default,,0,0,0,,Thật là... Chẳng lẽ\Nhai cậu không lo cho đồng đội mình à?
Dialogue: 0,0:16:15.16,0:16:16.62,Default,,0,0,0,,Cô ấy không sao đâu.
Dialogue: 0,0:16:16.70,0:16:18.20,Default,,0,0,0,,Chỉ cần nhìn ánh mắt cô ấy là biết.
Dialogue: 0,0:16:18.28,0:16:20.91,Default,,0,0,0,,Bọn tôi mà lo lắng\Nthì lát nữa sẽ bị cô ấy cho một trận mất.
Dialogue: 0,0:16:25.32,0:16:27.91,Default,,0,0,0,,[Hãy tự nhiên... và tự do...]
Dialogue: 0,0:16:30.95,0:16:34.45,Default,,0,0,0,,[Mình muốn thắng.\NSau đó, mình sẽ tiến lên trước một bước]
Dialogue: 0,0:16:34.53,0:16:36.74,Default,,0,0,0,,[và truyền tinh thần đó\Ncho thanh kiếm của mình.]
Dialogue: 0,0:16:41.24,0:16:43.82,Default,,0,0,0,,Kiếm Pháp Minh Nguyệt, Nhị Thập Tam Thức:
Dialogue: 0,0:16:44.62,0:16:46.20,Default,,0,0,0,,Phong Liễu!
Dialogue: 0,0:16:53.49,0:16:58.49,Default,,0,0,0,,Và người chiến thắng là... Yoo Mi Ra!
Dialogue: 0,0:17:04.49,0:17:05.99,Default,,0,0,0,,Đồ khốn!
Dialogue: 0,0:17:06.07,0:17:08.87,Default,,0,0,0,,Đồ quỷ tha ma bắt! Về chầu Diêm Vương đi!
Dialogue: 0,0:17:25.82,0:17:29.12,Default,,0,0,0,,Ta sẽ dâng tế ngươi lên\Ncho vong hồn Shim Bong Sa!
Dialogue: 0,0:17:29.20,0:17:30.74,Default,,0,0,0,,Đi chết đi!
Dialogue: 0,0:17:49.28,0:17:51.41,Default,,0,0,0,,Chết tiệt! Chúng chạy thoát rồi!
Dialogue: 0,0:17:54.57,0:17:56.70,Default,,0,0,0,,Đừng lờ ta đi như vậy chứ!
Dialogue: 0,0:17:56.78,0:18:00.32,Default,,0,0,0,,Ta sẽ là người kết liễu các ngươi!
Dialogue: 0,0:18:18.62,0:18:21.32,Default,,0,0,0,,Yoo Mi Ra!
Dialogue: 0,0:18:21.41,0:18:22.66,Default,,0,0,0,,Xấu hổ quá, đừng hét lên nữa!
Dialogue: 0,0:18:22.74,0:18:23.91,Default,,0,0,0,,[Giờ thì...]
Dialogue: 0,0:18:25.66,0:18:27.16,Default,,0,0,0,,[mình không thể đánh bại cô ấy, nhỉ?]
Dialogue: 0,0:18:34.49,0:18:35.70,Default,,0,0,0,,Cô tỉnh rồi à?
Dialogue: 0,0:18:39.70,0:18:42.74,Default,,0,0,0,,Xin lỗi, tôi đã không thể\Nđánh bại được cô ta.
Dialogue: 0,0:18:42.82,0:18:44.66,Default,,0,0,0,,[Vẫn chưa kết thúc đâu.]
Dialogue: 0,0:18:44.74,0:18:45.91,Default,,0,0,0,,Chỉ cần không từ bỏ,
Dialogue: 0,0:18:45.99,0:18:48.16,Default,,0,0,0,,ta vẫn có thể thử lại\Nbao nhiêu lần cũng được mà.
Dialogue: 0,0:18:52.74,0:18:53.87,Default,,0,0,0,,Cậu định véo má tôi mãi à?
Dialogue: 0,0:18:53.95,0:18:55.62,Default,,0,0,0,,Nãy giờ cậu véo tôi hơi lâu rồi đấy!
Dialogue: 0,0:19:02.62,0:19:05.20,Default,,0,0,0,,Tình hình có vẻ nghiêm trọng rồi đây.
Dialogue: 0,0:19:06.91,0:19:08.07,Default,,0,0,0,,Nox...
Dialogue: 0,0:19:11.12,0:19:12.99,Default,,0,0,0,,[Giám khảo O vừa báo tin.]
Dialogue: 0,0:19:13.53,0:19:16.28,Default,,0,0,0,,[Có vẻ như Nox đã tấn công Giám khảo Q.]
Dialogue: 0,0:19:16.99,0:19:18.70,Default,,0,0,0,,Đáng lẽ\Nngười ngoài không cách nào biết được
Dialogue: 0,0:19:18.78,0:19:21.53,Default,,0,0,0,,nơi ở của các giám khảo.
Dialogue: 0,0:19:22.28,0:19:23.99,Default,,0,0,0,,[Trong chúng ta, hẳn là có kẻ phản bội.]
Dialogue: 0,0:19:24.57,0:19:27.78,Default,,0,0,0,,Một con rận dơ bẩn đang lợi dụng\Nđể hút máu một con hổ...
Dialogue: 0,0:19:27.87,0:19:30.57,Default,,0,0,0,,Còn nữa, anh xem cái này đi.
Dialogue: 0,0:19:32.53,0:19:34.16,Default,,0,0,0,,[Sau khi kiểm tra,]
Dialogue: 0,0:19:34.24,0:19:36.74,Default,,0,0,0,,[nó đã được xác nhận là của Jin Tae Jin.]
Dialogue: 0,0:19:38.03,0:19:42.07,Default,,0,0,0,,[Tôi e là\Nông ấy đã bị đưa đến nơi khác rồi.]
Dialogue: 0,0:19:43.24,0:19:45.78,Default,,0,0,0,,Sau đây là trận đấu đầu tiên của Bảng B!
Dialogue: 0,0:19:45.87,0:19:49.03,Default,,0,0,0,,[Đội của tỉnh Gyeongsang Nam\Nsẽ đấu với tỉnh Jeolla Bắc!]
Dialogue: 0,0:19:49.12,0:19:49.95,Default,,0,0,0,,[Đây sẽ là trận đấu]
Dialogue: 0,0:19:50.03,0:19:52.07,Default,,0,0,0,,[giữa Jegal Taek và Jeon Ju Gok.]
Dialogue: 0,0:19:53.07,0:19:55.28,Default,,0,0,0,,Tôi từng thấy người đó trên tạp chí rồi.
Dialogue: 0,0:19:55.78,0:19:57.99,Default,,0,0,0,,[Theo như tôi nhớ\Nthì cậu ta đến từ Tập đoàn Six Sung.]
Dialogue: 0,0:19:58.07,0:20:01.32,Default,,0,0,0,,[À, cậu ta là người thừa kế\Ncủa một tập đoàn hàng đầu.]
Dialogue: 0,0:20:01.87,0:20:04.03,Default,,0,0,0,,Chuẩn bị, bắt đầu!
Dialogue: 0,0:20:04.62,0:20:07.20,Default,,0,0,0,,[Thái độ của anh có thế nào\Ntôi cũng chẳng quan tâm.]
Dialogue: 0,0:20:07.95,0:20:11.53,Default,,0,0,0,,Tôi sẽ chỉ làm những việc\Nmình cần phải làm thôi.
Dialogue: 0,0:20:11.62,0:20:14.74,Default,,0,0,0,,Đô, Rê, Mi, Fa...
Dialogue: 0,0:20:14.82,0:20:16.07,Default,,0,0,0,,Hỏa Cầu!
Dialogue: 0,0:20:37.82,0:20:39.16,Default,,0,0,0,,Dừng lại ngay!
Dialogue: 0,0:20:49.32,0:20:51.70,Default,,0,0,0,,Tránh ra đi, đồ phiền phức!
Dialogue: 0,0:20:57.95,0:20:59.07,Default,,0,0,0,,Không được!
Dialogue: 0,0:20:59.16,0:21:01.78,Default,,0,0,0,,Lần này mà can thiệp nữa\Nlà cậu sẽ bị truất quyền thi đấu đấy.
Dialogue: 0,0:21:02.37,0:21:05.24,Default,,0,0,0,,Giờ cậu có mục đích chiến đấu rồi mà,\Nđúng không?
Dialogue: 0,0:21:05.62,0:21:07.16,Default,,0,0,0,,Vậy còn cậu ấy thì sao?
Dialogue: 0,0:21:12.53,0:21:13.53,Default,,0,0,0,,Dừng lại!
Dialogue: 0,0:21:15.03,0:21:16.87,Default,,0,0,0,,Thú vị ghê.
Dialogue: 0,0:21:19.57,0:21:20.82,Default,,0,0,0,,Đủ rồi đấy!
Dialogue: 0,0:21:21.66,0:21:24.03,Default,,0,0,0,,Tôi sẽ không bỏ qua\Ncho hành động của các cậu nữa.
Dialogue: 0,0:21:27.28,0:21:28.91,Default,,0,0,0,,Lôi Kích...
Dialogue: 0,0:21:36.16,0:21:38.32,Default,,0,0,0,,Tôi gánh vác kỳ vọng của cả gia tộc
Dialogue: 0,0:21:38.45,0:21:40.70,Default,,0,0,0,,và của ông tôi trên vai\Nmà đứng trong đấu trường này.
Dialogue: 0,0:21:41.32,0:21:43.57,Default,,0,0,0,,Làm sao tôi có thể
Dialogue: 0,0:21:43.66,0:21:45.82,Default,,0,0,0,,thua một kẻ như anh chứ?
Dialogue: 0,0:21:52.62,0:21:55.49,Default,,0,0,0,,Vẫn còn yếu lắm.

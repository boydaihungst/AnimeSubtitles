Dialogue: 0,0:01:32.79,0:01:34.26,[Sub_Phụ],,0000,0000,0000,,Làm sao bây giờ?
Dialogue: 0,0:01:34.26,0:01:36.68,[Sub_Phụ],,0000,0000,0000,,Ta hết đường leo lên rồi...
Dialogue: 0,0:01:36.68,0:01:37.82,[Sub_Phụ],,0000,0000,0000,,Đã có kế gì chưa?
Dialogue: 0,0:01:38.40,0:01:39.28,[Black_Lagoon],,0000,0000,0000,,Đang nghĩ nó đây.
Dialogue: 0,0:01:39.56,0:01:40.75,[Sub_Phụ],,0000,0000,0000,,Ah! Thế cơ à?
Dialogue: 0,0:01:44.23,0:01:44.93,[Black_Lagoon],,0000,0000,0000,,Ouch!
Dialogue: 0,0:01:49.91,0:01:51.97,[Hội_Tam_Hoàng],,0000,0000,0000,,Cuộc chơi bây giờ mới chính thức bắt đầu!
Dialogue: 0,0:01:52.38,0:01:53.96,[Black_Lagoon],,0000,0000,0000,,Đến rồi đó!
Dialogue: 0,0:01:59.45,0:02:00.19,[Black_Lagoon],,0000,0000,0000,,Có trúng không?
Dialogue: 0,0:02:00.19,0:02:01.31,[Neutralist],,0000,0000,0000,,Không biết được.
Dialogue: 0,0:02:01.45,0:02:02.35,[Black_Lagoon],,0000,0000,0000,,Cẩn thận đó.
Dialogue: 0,0:02:02.79,0:02:03.41,[Neutralist],,0000,0000,0000,,Ờ.
Dialogue: 0,0:02:08.79,0:02:10.64,[Hội_Tam_Hoàng],,0000,0000,0000,,Dính rồi nhé! Ngay chóc luôn!
Dialogue: 0,0:02:14.50,0:02:15.03,[Hội_Tam_Hoàng],,0000,0000,0000,,Ô!
Dialogue: 0,0:02:15.27,0:02:17.52,[Hội_Tam_Hoàng],,0000,0000,0000,,Thế mà cứ tưởng bắt được cô em chứ.
Dialogue: 0,0:02:17.52,0:02:20.19,[Hội_Tam_Hoàng],,0000,0000,0000,,Nghe giọng ư ử này mười phần là dính bà sơ rồi.
Dialogue: 0,0:02:20.83,0:02:22.78,[Hội_Tam_Hoàng],,0000,0000,0000,,Đến cô đấy, Sawyer!
Dialogue: 0,0:02:23.72,0:02:24.83,[Black_Lagoon],,0000,0000,0000,,Bà mẹ nó!
Dialogue: 0,0:02:25.64,0:02:26.68,[Black_Lagoon],,0000,0000,0000,,Đệt!
Dialogue: 0,0:02:29.24,0:02:30.06,[Neutralist],,0000,0000,0000,,Revy...
Dialogue: 0,0:02:30.06,0:02:32.09,[Neutralist],,0000,0000,0000,,Mụ mổ gia súc này chỉ mồi nhử thôi.
Dialogue: 0,0:02:32.09,0:02:33.40,[Neutralist],,0000,0000,0000,,Chủ công là ả kia.
Dialogue: 0,0:02:33.40,0:02:34.21,[Neutralist],,0000,0000,0000,,Nhanh đi bắt ả đi.
Dialogue: 0,0:02:34.38,0:02:37.86,[Black_Lagoon],,0000,0000,0000,,Dĩ nhiên rồi. Ở lại giúp bà cũng đâu được xu cắc nào chứ.
Dialogue: 0,0:02:38.12,0:02:38.94,[Black_Lagoon],,0000,0000,0000,,Đi nhé.
Dialogue: 0,0:02:38.94,0:02:39.76,[Neutralist],,0000,0000,0000,,Chó nó.
Dialogue: 0,0:02:40.12,0:02:41.03,[Black_Lagoon],,0000,0000,0000,,Cố lên nhé.
Dialogue: 0,0:02:41.03,0:02:41.72,[Neutralist],,0000,0000,0000,,Ờ.
Dialogue: 0,0:02:45.69,0:02:48.01,[Hội_Tam_Hoàng],,0000,0000,0000,,Chiến thuật nghi binh mĩ mãn phải ha bà sơ con tin.
Dialogue: 0,0:02:49.13,0:02:51.52,[Hội_Tam_Hoàng],,0000,0000,0000,,Này, nghe rõ chưa, con mụ kia!
Dialogue: 0,0:02:52.10,0:02:54.17,[Hội_Tam_Hoàng],,0000,0000,0000,,Vứt súng rồi lòi mặt ra đi!
Dialogue: 0,0:02:54.17,0:02:56.79,[Hội_Tam_Hoàng],,0000,0000,0000,,Mụ sơ này mà tỏi là cả hai chết chùm đó!
Dialogue: 0,0:02:56.79,0:02:57.84,[Hội_Tam_Hoàng],,0000,0000,0000,,Thế thì không hay đâu!
Dialogue: 0,0:02:57.84,0:02:58.42,[Neutralist],,0000,0000,0000,,Revy!
Dialogue: 0,0:02:58.42,0:02:59.59,[Neutralist],,0000,0000,0000,,Đừng lo cho tôi...
Dialogue: 0,0:02:59.59,0:03:01.13,[Neutralist],,0000,0000,0000,,Bắn ả đi!
Dialogue: 0,0:03:02.91,0:03:04.87,[Hội_Tam_Hoàng],,0000,0000,0000,,Quả cảm đó, bà sơ à.
Dialogue: 0,0:03:04.87,0:03:06.61,[Hội_Tam_Hoàng],,0000,0000,0000,,Tiếc là ả kia biến đi rồi.
Dialogue: 0,0:03:06.78,0:03:07.48,[Hội_Tam_Hoàng],,0000,0000,0000,,Giờ thì...
Dialogue: 0,0:03:07.48,0:03:08.02,[Neutralist],,0000,0000,0000,,Hey!
Dialogue: 0,0:03:08.02,0:03:08.33,[Neutralist][00],,0000,0000,0000,,Hey!
Dialogue: 0,0:03:08.33,0:03:08.92,[Neutralist],,0000,0000,0000,,Hey!
Dialogue: 0,0:03:08.92,0:03:10.31,[Neutralist],,0000,0000,0000,,Chờ! Chờ chút!
Dialogue: 0,0:03:10.31,0:03:11.55,[Neutralist],,0000,0000,0000,,Chờ thêm tí đi.
Dialogue: 0,0:03:11.55,0:03:13.46,[Neutralist],,0000,0000,0000,,Ít ra cũng cho người ta trăn trối vài câu chứ.
Dialogue: 0,0:03:13.72,0:03:15.50,[Hội_Tam_Hoàng],,0000,0000,0000,,Đây không ngu thế đâu.
Dialogue: 0,0:03:15.50,0:03:17.05,[Hội_Tam_Hoàng],,0000,0000,0000,,Đừng có mơ mà câu giờ.
Dialogue: 0,0:03:17.51,0:03:20.40,[Neutralist],,0000,0000,0000,,Đi giết một bà sơ là mang tội xuống địa ngục đó.
Dialogue: 0,0:03:20.40,0:03:21.92,[Hội_Tam_Hoàng],,0000,0000,0000,,Đây sống vô thần...
Dialogue: 0,0:03:21.92,0:03:23.71,[Hội_Tam_Hoàng],,0000,0000,0000,,Mấy thứ mê tín đó đây đéo quan tâm.
Dialogue: 0,0:03:24.13,0:03:25.64,[Hội_Tam_Hoàng],,0000,0000,0000,,Vĩnh biệt nhé, bà sơ!
Dialogue: 0,0:03:26.80,0:03:27.77,[Neutralist],,0000,0000,0000,,Đồ óc lợn.
Dialogue: 0,0:03:27.77,0:03:29.96,[Neutralist],,0000,0000,0000,,Chuyến tàu đưa mụ xuống địa phủ đến nhanh hơn mụ nghĩ đấy.
Dialogue: 0,0:03:29.96,0:03:31.53,[Neutralist],,0000,0000,0000,,Quay đằng sau mà coi!
Dialogue: 0,0:03:38.35,0:03:39.33,[Sub_Phụ],,0000,0000,0000,,Đến kìa!
Dialogue: 0,0:03:39.33,0:03:39.68,[Sub_Phụ][00],,0000,0000,0000,,Đến kìa!
Dialogue: 0,0:03:39.68,0:03:39.94,[Sub_Phụ],,0000,0000,0000,,Đến kìa!
Dialogue: 0,0:03:39.94,0:03:40.69,[Sub_Phụ][00],,0000,0000,0000,,Đến kìa!
Dialogue: 0,0:03:41.23,0:03:42.75,[Sub_Phụ],,0000,0000,0000,,Mau làm gì đó đi chứ!
Dialogue: 0,0:03:42.83,0:03:43.86,[Black_Lagoon],,0000,0000,0000,,Chờ đã, chờ đã.
Dialogue: 0,0:03:43.86,0:03:44.83,[Black_Lagoon],,0000,0000,0000,,Lúc này không được hoảng loạn.
Dialogue: 0,0:03:44.83,0:03:46.23,[Sub_Phụ],,0000,0000,0000,,Không được hoảng loạn á?
Dialogue: 0,0:03:46.23,0:03:47.53,[Sub_Phụ],,0000,0000,0000,,Có bị ngu không thế!?
Dialogue: 0,0:03:48.03,0:03:50.12,[Black_Lagoon],,0000,0000,0000,,Vậy! Chứ bảo phải làm gì bây giờ?
Dialogue: 0,0:03:50.74,0:03:52.45,[Sub_Phụ],,0000,0000,0000,,Cái hố bẫy á?
Dialogue: 0,0:03:52.45,0:03:53.39,[Sub_Phụ],,0000,0000,0000,,Không ngu là gì!
Dialogue: 0,0:03:53.39,0:03:54.68,[Black_Lagoon],,0000,0000,0000,,Đừng có chửi bới!
Dialogue: 0,0:03:54.68,0:03:58.67,[Black_Lagoon],,0000,0000,0000,,Mấy thanh xà gỗ nâng nóc nhà gần sụm hết cả rồi.
Dialogue: 0,0:04:00.30,0:04:01.67,[Neutralist],,0000,0000,0000,,Bắt...
Dialogue: 0,0:04:01.67,0:04:02.68,[Neutralist],,0000,0000,0000,,Được... rồi...
Dialogue: 0,0:04:02.68,0:04:03.17,[Neutralist],,0000,0000,0000,,Nhé.
Dialogue: 0,0:04:06.18,0:04:08.00,[Black_Lagoon],,0000,0000,0000,,Gần đến cảng rồi...
Dialogue: 0,0:04:08.00,0:04:08.97,[Black_Lagoon],,0000,0000,0000,,Chút nữa là thấy tận mắt thôi.
Dialogue: 0,0:04:09.32,0:04:10.08,[Black_Lagoon],,0000,0000,0000,,Thế?
Dialogue: 0,0:04:10.08,0:04:10.80,[Black_Lagoon],,0000,0000,0000,,Ngóng thấy gì chưa?
Dialogue: 0,0:04:11.21,0:04:12.28,[Black_Lagoon],,0000,0000,0000,,Vẫn chưa...
Dialogue: 0,0:04:12.28,0:04:15.03,[Black_Lagoon],,0000,0000,0000,,Mới thấy bức tượng Phật với ánh đèn thành phố thôi.
Dialogue: 0,0:04:15.89,0:04:16.79,[Black_Lagoon],,0000,0000,0000,,Hả, gì đây...
Dialogue: 0,0:04:16.79,0:04:17.82,[Black_Lagoon],,0000,0000,0000,,Con mẹ nó!
Dialogue: 0,0:04:18.17,0:04:19.37,[Black_Lagoon],,0000,0000,0000,,{\i1}Đệt nó!
Dialogue: 0,0:04:19.37,0:04:20.54,[Black_Lagoon],,0000,0000,0000,,{\i1}Fucking wonderful luôn!{\i0}
Dialogue: 0,0:04:20.76,0:04:21.91,[Black_Lagoon],,0000,0000,0000,,Sao thế, Dutch?
Dialogue: 0,0:04:21.92,0:04:24.18,[Black_Lagoon],,0000,0000,0000,,Roanapur dính bom nguyên tử à?
Dialogue: 0,0:04:24.58,0:04:27.10,[Black_Lagoon],,0000,0000,0000,,Còn khủng hơn thế nhiều. Địt mẹ nó!
Dialogue: 0,0:04:27.36,0:04:30.12,[Black_Lagoon],,0000,0000,0000,,Mà không chỉ mỗi bên ta là bị thiệt hại đâu.
Dialogue: 0,0:04:30.87,0:04:35.52,[Black_Lagoon],,0000,0000,0000,,Có thằng chó điên nào phóng hỏa hết cảng tàu của bố rồi!
Dialogue: 0,0:04:50.72,0:04:51.67,[Black_Lagoon],,0000,0000,0000,,Đau quá~!
Dialogue: 0,0:04:51.67,0:04:52.95,[Black_Lagoon],,0000,0000,0000,,Mẹ nó!
Dialogue: 0,0:04:53.31,0:04:56.02,[Black_Lagoon],,0000,0000,0000,,Eda, con mụ thối thây! Vừa quay đi một phát là đã trở mặt là sao.
Dialogue: 0,0:04:55.90,0:04:56.69,[Neutralist],,0000,0000,0000,,Xin lỗi ha.
Dialogue: 0,0:04:56.69,0:04:57.93,[Black_Lagoon],,0000,0000,0000,,Đâm trúng bà rồi này!
Dialogue: 0,0:04:57.93,0:04:59.03,[Neutralist],,0000,0000,0000,,Biết rồi mà.
Dialogue: 0,0:04:59.03,0:05:00.40,[Black_Lagoon],,0000,0000,0000,,Tiên sư mụ.
Dialogue: 0,0:05:01.06,0:05:03.53,[Neutralist],,0000,0000,0000,,Mà không có đây là mồi nhử ả thì có mà bắn đến sáng cũng chả trúng nhá.
Dialogue: 0,0:05:04.07,0:05:05.86,[Neutralist],,0000,0000,0000,,Mà việc đó giờ không quan trọng.
Dialogue: 0,0:05:05.86,0:05:07.61,[Neutralist],,0000,0000,0000,,Ở trển tình hình sao rồi?
Dialogue: 0,0:05:08.04,0:05:10.04,[Black_Lagoon],,0000,0000,0000,,Thang lên hỏng bố nó rồi. Chắc ả ma nữ đó cắt đây mà.
Dialogue: 0,0:05:10.04,0:05:11.95,[Black_Lagoon],,0000,0000,0000,,Mẹ, đau thế!
Dialogue: 0,0:05:12.18,0:05:14.37,[Sub_Phụ],,0000,0000,0000,,Cái quái gì thế này?
Dialogue: 0,0:05:14.37,0:05:16.74,[Sub_Phụ],,0000,0000,0000,,Có ai bảo ông mở dạ tiệc lửa Valencian đâu hả?
Dialogue: 0,0:05:17.11,0:05:18.01,[Sub_Phụ],,0000,0000,0000,,Torch!
Dialogue: 0,0:05:18.01,0:05:22.22,[Sub_Phụ],,0000,0000,0000,,Tôi yêu cầu ông bắt con ả Jane, chứ không phải đốt trui cả khu này!
Dialogue: 0,0:05:22.49,0:05:24.45,[Neutralist],,0000,0000,0000,,Chúng ở trên mái nhà ấy.
Dialogue: 0,0:05:25.72,0:05:28.29,[Neutralist],,0000,0000,0000,,Lửa cháy to quá, đâu lên trên đó được.
Dialogue: 0,0:05:29.89,0:05:32.73,[Black_Lagoon],,0000,0000,0000,,Đúng rồi. Bước vào chỗ đó đi.
Dialogue: 0,0:05:33.16,0:05:34.79,[Black_Lagoon],,0000,0000,0000,,Tiến thẳng vào đây đi.
Dialogue: 0,0:05:36.57,0:05:38.73,[Sub_Phụ],,0000,0000,0000,,Sao trông ả phấn khích thế nhỉ.
Dialogue: 0,0:05:42.34,0:05:45.37,[Neutralist],,0000,0000,0000,,Thường... thì... ta... chỉ... chặt... ra... mấy... khúc...
Dialogue: 0,0:05:45.37,0:05:46.82,[Neutralist],,0000,0000,0000,,Thế... là... xong.
Dialogue: 0,0:05:47.76,0:05:51.52,[Neutralist],,0000,0000,0000,,Chơi... trò... thiệt... vui... ghê...
Dialogue: 0,0:05:51.52,0:05:52.92,[Neutralist],,0000,0000,0000,,Lâu... lâu... mới... có... dịp.
Dialogue: 0,0:05:53.92,0:05:56.67,[Neutralist],,0000,0000,0000,,Nên... ta... sẽ... không... sát... sinh... hôm... nay.
Dialogue: 0,0:05:56.67,0:05:59.96,[Sub_Phụ],,0000,0000,0000,,Làm gì đi chứ?\NẢ ấy bắt đầu lảm nhảm rồi kìa!
Dialogue: 0,0:06:00.17,0:06:01.27,[Neutralist],,0000,0000,0000,,Nhưng... ta...
Dialogue: 0,0:06:01.27,0:06:02.46,[Neutralist],,0000,0000,0000,,Chỉ... xin... các... người...
Dialogue: 0,0:06:02.46,0:06:04.59,[Neutralist],,0000,0000,0000,,Một... đôi...
Dialogue: 0,0:06:04.59,0:06:07.14,[Neutralist],,0000,0000,0000,,Tay... chân... thôi.
Dialogue: 0,0:06:08.65,0:06:11.75,[Neutralist],,0000,0000,0000,,Ta... là... dân... lau... dọn...
Dialogue: 0,0:06:11.75,0:06:13.10,[Neutralist],,0000,0000,0000,,Luôn... đi...
Dialogue: 0,0:06:13.10,0:06:14.80,[Neutralist],,0000,0000,0000,,Xử... lý... mọi... thứ... còn... sót... lại.
Dialogue: 0,0:06:15.50,0:06:17.89,[Neutralist],,0000,0000,0000,,Nhưng... hôm... nay... thì... khác.
Dialogue: 0,0:06:17.89,0:06:20.73,[Neutralist],,0000,0000,0000,,Những... kẻ... khác... đều... gục... xác... dưới... chân...  ta.
Dialogue: 0,0:06:21.17,0:06:23.39,[Neutralist],,0000,0000,0000,,Ta... là... quân... Tiên... Phong.
Dialogue: 0,0:06:23.39,0:06:25.71,[Neutralist],,0000,0000,0000,,Vậy..., bắt... đầu... luôn... nhỉ?
Dialogue: 0,0:06:26.12,0:06:27.51,[Sub_Phụ],,0000,0000,0000,,Đều do anh cả đấy.
Dialogue: 0,0:06:27.51,0:06:28.22,[Black_Lagoon],,0000,0000,0000,,Chỗ nào hả?
Dialogue: 0,0:06:28.29,0:06:29.64,[Neutralist],,0000,0000,0000,,Không... sao... đâu...
Dialogue: 0,0:06:29.64,0:06:34.03,[Neutralist],,0000,0000,0000,,Cổ... họng... ta... bị... chẻ... đôi...
Dialogue: 0,0:06:34.62,0:06:36.39,[Neutralist],,0000,0000,0000,,Mà... vẫn... sống... đây.
Dialogue: 0,0:06:43.87,0:06:44.93,[Sub_Phụ],,0000,0000,0000,,Giờ thì sao!?
Dialogue: 0,0:06:45.00,0:06:47.05,[Black_Lagoon],,0000,0000,0000,,Lửa lan đến cụm dầu máy dưới cảng rồi!
Dialogue: 0,0:06:47.05,0:06:47.61,[Black_Lagoon],,0000,0000,0000,,Chết tiệt!
Dialogue: 0,0:06:48.91,0:06:51.03,[Sub_Phụ],,0000,0000,0000,,Ả vẫn cười được kìa!
Dialogue: 0,0:06:51.21,0:06:54.97,[Sub_Phụ],,0000,0000,0000,,Ả lại rên rỉ mấy câu kinh dị\Nđó rồi chặt phéng tôi ra cho xem!
Dialogue: 0,0:06:55.28,0:06:56.77,[Black_Lagoon],,0000,0000,0000,,Làm cái gì đi chứ!
Dialogue: 0,0:06:56.77,0:06:57.99,[Black_Lagoon],,0000,0000,0000,,Làm được mới lạ!
Dialogue: 0,0:07:03.65,0:07:04.45,[Sub_Phụ],,0000,0000,0000,,Gì thế?
Dialogue: 0,0:07:04.45,0:07:05.38,[Black_Lagoon],,0000,0000,0000,,Ai biết?
Dialogue: 0,0:07:08.56,0:07:10.02,[Sub_Phụ],,0000,0000,0000,,Thế là sao?
Dialogue: 0,0:07:10.02,0:07:10.85,[Sub_Phụ],,0000,0000,0000,,Giải thích đi coi!
Dialogue: 0,0:07:11.06,0:07:12.31,[Black_Lagoon],,0000,0000,0000,,Ai mà biết được!
Dialogue: 0,0:07:12.31,0:07:13.68,[Black_Lagoon],,0000,0000,0000,,Tôi cũng có hiểu cái quái gì đâu.
Dialogue: 0,0:07:14.90,0:07:16.14,[Sub_Phụ],,0000,0000,0000,,Ôi mẹ ơi!
Dialogue: 0,0:07:19.64,0:07:21.33,[Black_Lagoon],,0000,0000,0000,,Rock! Có sao không?
Dialogue: 0,0:07:21.33,0:07:22.62,[Black_Lagoon],,0000,0000,0000,,Ả ma nữ đi đâu mất tiêu rồi?
Dialogue: 0,0:07:22.89,0:07:24.20,[Black_Lagoon],,0000,0000,0000,,À, không sao...
Dialogue: 0,0:07:24.20,0:07:25.13,[Black_Lagoon],,0000,0000,0000,,Ở đây.
Dialogue: 0,0:07:28.04,0:07:30.28,[Black_Lagoon],,0000,0000,0000,,Tình thế gì mà quái gở thế này.
Dialogue: 0,0:07:30.28,0:07:31.36,[Black_Lagoon],,0000,0000,0000,,Làm thế nào hay vậy?
Dialogue: 0,0:07:31.59,0:07:33.63,[Black_Lagoon],,0000,0000,0000,,Tôi cũng chẳng rõ vì sao nữa.
Dialogue: 0,0:07:33.88,0:07:34.85,[Black_Lagoon],,0000,0000,0000,,Thật đó.
Dialogue: 0,0:07:34.85,0:07:36.98,[Sub_Phụ],,0000,0000,0000,,Ả làm rơi cái máy phát thanh...
Dialogue: 0,0:07:36.98,0:07:38.35,[Sub_Phụ],,0000,0000,0000,,Thế là tự kỉ một cục thế luôn đó.
Dialogue: 0,0:07:38.82,0:07:39.93,[Neutralist],,0000,0000,0000,,Ra là loại này à.
Dialogue: 0,0:07:39.93,0:07:41.85,[Neutralist],,0000,0000,0000,,Dân tự kỉ ngoại hạng đó.
Dialogue: 0,0:07:42.36,0:07:44.42,[Neutralist],,0000,0000,0000,,Loại này hồi trước tôi thấy một lần rồi.
Dialogue: 0,0:07:44.66,0:07:46.00,[Black_Lagoon],,0000,0000,0000,,Ra là thế à.
Dialogue: 0,0:07:46.00,0:07:46.57,[Black_Lagoon],,0000,0000,0000,,Này...
Dialogue: 0,0:07:46.57,0:07:47.75,[Black_Lagoon],,0000,0000,0000,,Ma nữ kia.
Dialogue: 0,0:07:47.75,0:07:50.06,[Black_Lagoon],,0000,0000,0000,,Ngu thế hả. Bon chen làm mấy \Nthứ mình không rành làm đệt gì hả.
Dialogue: 0,0:07:50.06,0:07:51.69,[Black_Lagoon],,0000,0000,0000,,Revy. Cô bị thương rồi.
Dialogue: 0,0:07:51.74,0:07:53.21,[Black_Lagoon],,0000,0000,0000,,Ờ! All right.
Dialogue: 0,0:07:53.21,0:07:54.29,[Black_Lagoon],,0000,0000,0000,,Trông thế chứ không sâu lắm.
Dialogue: 0,0:07:55.67,0:07:56.38,[Neutralist],,0000,0000,0000,,Hey!
Dialogue: 0,0:07:56.38,0:07:56.58,[Neutralist][00],,0000,0000,0000,,Hey!
Dialogue: 0,0:07:56.58,0:07:56.79,[Neutralist],,0000,0000,0000,,Hey!
Dialogue: 0,0:07:56.79,0:07:57.32,[Neutralist][00],,0000,0000,0000,,Hey!
Dialogue: 0,0:07:58.01,0:07:59.96,[Neutralist],,0000,0000,0000,,Có phải chỗ này đang sập xuống không?
Dialogue: 0,0:08:02.78,0:08:04.58,[Black_Lagoon],,0000,0000,0000,,Lửa cháy sập hết cảng của tôi rồi!
Dialogue: 0,0:08:04.98,0:08:06.65,[Black_Lagoon],,0000,0000,0000,,Có thấy Revy với những người khác không?
Dialogue: 0,0:08:06.65,0:08:07.95,[Black_Lagoon],,0000,0000,0000,,Không chắc lắm.
Dialogue: 0,0:08:08.12,0:08:11.47,[Black_Lagoon],,0000,0000,0000,,Nhưng ta mà nổ súng bây giờ là mụ phỡn tới luôn.
Dialogue: 0,0:08:11.47,0:08:12.61,[Black_Lagoon],,0000,0000,0000,,Nghề của mụ mà lị.
Dialogue: 0,0:08:12.89,0:08:14.00,[Black_Lagoon],,0000,0000,0000,,Benny!
Dialogue: 0,0:08:14.00,0:08:16.34,[Black_Lagoon],,0000,0000,0000,,Phóng hết tốc lực rồi ngoặt 180 độ ngay trước đầu cảng cho tôi!
Dialogue: 0,0:08:16.46,0:08:18.36,[Black_Lagoon],,0000,0000,0000,,{\i1}All right! Cập bến là nghề của chàng mà.{\i0}
Dialogue: 0,0:08:28.88,0:08:30.56,[Neutralist],,0000,0000,0000,,Ôi, ôi, ôi!
Dialogue: 0,0:08:30.56,0:08:31.49,[Sub_Phụ],,0000,0000,0000,,Cái gì thế này!?
Dialogue: 0,0:08:32.89,0:08:34.25,[Neutralist],,0000,0000,0000,,Ta đến...
Dialogue: 0,0:08:34.25,0:08:36.37,[Neutralist],,0000,0000,0000,,Để lấy mạng ngươi!
Dialogue: 0,0:08:36.37,0:08:38.53,[Neutralist],,0000,0000,0000,,Ta có nguyên nhân riêng của mình!
Dialogue: 0,0:08:39.38,0:08:41.40,[Neutralist],,0000,0000,0000,,Tên ta là Wizard!
Dialogue: 0,0:08:41.40,0:08:44.26,[Neutralist],,0000,0000,0000,,Lotton the Wizard!
Dialogue: 0,0:08:44.26,0:08:45.95,[Neutralist],,0000,0000,0000,,Dù ta với ngươi không thù hằn gì...
Dialogue: 0,0:08:59.40,0:09:00.19,[Black_Lagoon],,0000,0000,0000,,Mẹ nó!
Dialogue: 0,0:09:00.19,0:09:01.50,[Black_Lagoon],,0000,0000,0000,,Sập hết nhà ông của ông rồi.
Dialogue: 0,0:09:09.70,0:09:10.91,[Black_Lagoon],,0000,0000,0000,,Chết tiệt-
Dialogue: 0,0:09:13.77,0:09:14.63,[Black_Lagoon],,0000,0000,0000,,Thuyền kia rồi!
Dialogue: 0,0:09:14.68,0:09:16.10,[Neutralist],,0000,0000,0000,,Vừa hay đó.
Dialogue: 0,0:09:17.67,0:09:19.45,[Black_Lagoon],,0000,0000,0000,,Tốt. Đến nơi rồi.
Dialogue: 0,0:09:19.61,0:09:20.73,[Black_Lagoon],,0000,0000,0000,,Ra cầu tàu mau!
Dialogue: 0,0:09:22.62,0:09:25.18,[Sub_Phụ],,0000,0000,0000,,Chúng kìa! Là chúng đấy! Bắt lại mau!
Dialogue: 0,0:09:27.25,0:09:28.21,[Sub_Phụ],,0000,0000,0000,,Đứng lại!
Dialogue: 0,0:09:40.73,0:09:42.02,[Black_Lagoon],,0000,0000,0000,,Touchdown, Dutch!
Dialogue: 0,0:09:42.16,0:09:42.89,[Black_Lagoon],,0000,0000,0000,,Đi thôi, Benny!
Dialogue: 0,0:09:43.02,0:09:43.69,[Black_Lagoon],,0000,0000,0000,,Rõ rồi!
Dialogue: 0,0:09:43.70,0:09:45.07,[Sub_Phụ],,0000,0000,0000,,Đứng lại!
Dialogue: 0,0:09:49.17,0:09:49.96,[Black_Lagoon],,0000,0000,0000,,Tiên sư nó!
Dialogue: 0,0:09:49.96,0:09:51.59,[Black_Lagoon],,0000,0000,0000,,Có mấy thằng lên thuyền rồi!
Dialogue: 0,0:09:52.47,0:09:53.14,[Black_Lagoon],,0000,0000,0000,,Rock!
Dialogue: 0,0:09:53.14,0:09:54.56,[Black_Lagoon],,0000,0000,0000,,Đưa mụ kia vào trong đi!
Dialogue: 0,0:09:55.81,0:09:56.75,[Black_Lagoon],,0000,0000,0000,,Vào đây!
Dialogue: 0,0:10:08.35,0:10:09.09,[Hội_Tam_Hoàng],,0000,0000,0000,,Ô!
Dialogue: 0,0:10:09.09,0:10:11.54,[Hội_Tam_Hoàng],,0000,0000,0000,,Vẫn còn sống cơ à...
Dialogue: 0,0:10:11.54,0:10:13.02,[Hội_Tam_Hoàng],,0000,0000,0000,,Mạng anh lớn lắm đấy.
Dialogue: 0,0:10:14.19,0:10:15.07,[Neutralist],,0000,0000,0000,,Cô thấy hết à?
Dialogue: 0,0:10:15.07,0:10:16.66,[Hội_Tam_Hoàng],,0000,0000,0000,, Ờ, đại loại thế.
Dialogue: 0,0:10:17.83,0:10:18.72,[Neutralist],,0000,0000,0000,,Trông có ngầu không?
Dialogue: 0,0:10:20.56,0:10:23.13,[Hội_Tam_Hoàng],,0000,0000,0000,,Trông chả hơn ông chúa xứ thằng đần là mấy.
Dialogue: 0,0:10:23.99,0:10:27.02,[Hội_Tam_Hoàng],,0000,0000,0000,,Khuyên là anh bỏ cái kiểu chào đầu đó đi là vừa.
Dialogue: 0,0:10:29.52,0:10:30.95,[Neutralist],,0000,0000,0000,,Cô có sao không?
Dialogue: 0,0:10:32.34,0:10:35.39,[Hội_Tam_Hoàng],,0000,0000,0000,,Thế trông tôi còn đủ chuẩn\Nđi múa lân cho anh xem không?
Dialogue: 0,0:10:36.39,0:10:38.58,[Neutralist],,0000,0000,0000,,Lần sau...
Dialogue: 0,0:10:38.58,0:10:39.96,[Neutralist],,0000,0000,0000,,Nhớ phải luôn mang theo áo chống đạn đó.
Dialogue: 0,0:10:41.05,0:10:42.36,[Neutralist],,0000,0000,0000,,Trong mấy vụ này...
Dialogue: 0,0:10:42.36,0:10:43.12,[Neutralist],,0000,0000,0000,,Có lợi lắm.
Dialogue: 0,0:10:45.39,0:10:47.74,[Hội_Tam_Hoàng],,0000,0000,0000,,Vũ khí thượng đẳng là chỉ có công mà thôi.
Dialogue: 0,0:10:49.83,0:10:50.79,[Hội_Tam_Hoàng],,0000,0000,0000,,Với lại...
Dialogue: 0,0:10:50.79,0:10:52.40,[Hội_Tam_Hoàng],,0000,0000,0000,,Thương tích như thế này...
Dialogue: 0,0:10:52.40,0:10:54.23,[Hội_Tam_Hoàng],,0000,0000,0000,,Chắc cũng chẳng còn lần sau đâu.
Dialogue: 0,0:10:54.70,0:10:56.85,[Neutralist],,0000,0000,0000,,Để tôi đưa cô đến chỗ bác sĩ.
Dialogue: 0,0:10:58.44,0:11:01.68,[Neutralist],,0000,0000,0000,,Ai lại để phụ nữ...
Dialogue: 0,0:11:01.68,0:11:02.65,[Neutralist],,0000,0000,0000,,Chết trước mắt mình bao giờ.
Dialogue: 0,0:11:05.34,0:11:08.61,[Hội_Tam_Hoàng],,0000,0000,0000,,Anh muốn thành sát thủ thế này thì hơi khó đó.
Dialogue: 0,0:11:08.96,0:11:12.67,[Hội_Tam_Hoàng],,0000,0000,0000,,Bản chất của anh đáng ra phải đi thờ Chúa mới đúng.
Dialogue: 0,0:11:13.95,0:11:15.99,[Neutralist],,0000,0000,0000,,Ngón đó thì...
Dialogue: 0,0:11:15.99,0:11:16.81,[Neutralist],,0000,0000,0000,,Tôi vụng lắm.
Dialogue: 0,0:11:19.22,0:11:20.77,[Neutralist],,0000,0000,0000,,Ta đưa...
Dialogue: 0,0:11:20.77,0:11:22.20,[Neutralist],,0000,0000,0000,,Cô gái ấy đi chung nhé.
Dialogue: 0,0:11:24.45,0:11:26.25,[Hội_Tam_Hoàng],,0000,0000,0000,,Lại một người may mắn nữa.
Dialogue: 0,0:11:27.20,0:11:28.15,[Sub_Phụ],,0000,0000,0000,,Lobos!
Dialogue: 0,0:11:28.15,0:11:30.07,[Sub_Phụ],,0000,0000,0000,,Thằng óc heo Lobos chết xó nào rồi!?
Dialogue: 0,0:11:30.07,0:11:30.86,[Sub_Phụ],,0000,0000,0000,,Ngài gọi tôi ạ?
Dialogue: 0,0:11:30.86,0:11:31.63,[Sub_Phụ],,0000,0000,0000,,Russell đâu!?
Dialogue: 0,0:11:31.63,0:11:33.00,[Sub_Phụ],,0000,0000,0000,,Sao đến giờ mà nó chưa về!?
Dialogue: 0,0:11:33.00,0:11:36.14,[Sub_Phụ],,0000,0000,0000,,Ngài đừng đòi hỏi nhiều quá.\NÔng ta dính phiền phức ghê lắm.
Dialogue: 0,0:11:36.14,0:11:36.98,[Sub_Phụ],,0000,0000,0000,,Mẹ mày!
Dialogue: 0,0:11:37.19,0:11:41.22,[Sub_Phụ],,0000,0000,0000,,Chứ đến lúc Don Geralamo trát bùn lên đầy mặt tao thì mày tính sao hả?
Dialogue: 0,0:11:41.22,0:11:42.91,[Sub_Phụ],,0000,0000,0000,,Tôi cũng biết chứ ạ, có điều...
Dialogue: 0,0:11:42.91,0:11:43.84,[Sub_Phụ],,0000,0000,0000,,Chó chết!
Dialogue: 0,0:11:44.18,0:11:46.57,[Sub_Phụ],,0000,0000,0000,,Nếu thật mày hiểu được một từ trong lời tao nói...
Dialogue: 0,0:11:46.57,0:11:49.21,[Sub_Phụ],,0000,0000,0000,,Còn không mau triệu quân đội ở nhà đến đây cho tao!
Dialogue: 0,0:11:49.35,0:11:53.90,[Sub_Phụ],,0000,0000,0000,,Nhưng thế sẽ động chạm nặng đến quân Nga và Tàu ở thành phố này.
Dialogue: 0,0:11:53.90,0:11:55.76,[Sub_Phụ],,0000,0000,0000,,Chấp hết chúng nó!
Dialogue: 0,0:11:55.76,0:11:57.92,[Sub_Phụ],,0000,0000,0000,,Xách ngay điện thoại đây cho tao!
Dialogue: 0,0:11:58.10,0:11:59.40,[Sub_Phụ],,0000,0000,0000,,Tao gọi ngay bây giờ!
Dialogue: 0,0:11:59.40,0:12:01.27,[Sub_Phụ],,0000,0000,0000,,Xin ngài chờ chút ạ.
Dialogue: 0,0:12:07.66,0:12:10.07,[Sub_Phụ],,0000,0000,0000,,Có cửa ở đây này.
Dialogue: 0,0:12:10.07,0:12:11.08,[Sub_Phụ],,0000,0000,0000,,Tôi đi xuống dưới.
Dialogue: 0,0:12:11.08,0:12:12.80,[Sub_Phụ],,0000,0000,0000,,Còn lại lo lũ kia đi.
Dialogue: 0,0:12:13.31,0:12:14.24,[Sub_Phụ],,0000,0000,0000,,Rõ rồi.
Dialogue: 0,0:12:17.02,0:12:17.89,[Black_Lagoon],,0000,0000,0000,,Chết tiệt.
Dialogue: 0,0:12:17.89,0:12:19.29,[Black_Lagoon],,0000,0000,0000,,Tôi xuống phòng điều khiển đây.
Dialogue: 0,0:12:19.29,0:12:20.67,[Black_Lagoon],,0000,0000,0000,,Tình thế gấp lắm rồi, Dutch!
Dialogue: 0,0:12:21.09,0:12:23.09,[Black_Lagoon],,0000,0000,0000,,Cho lũ chúng chơi trò rodeo một phen.
Dialogue: 0,0:12:23.09,0:12:25.64,[Black_Lagoon],,0000,0000,0000,,Đến cả Tuff Hedeman còn phải ướt đấy.
Dialogue: 0,0:12:25.71,0:12:28.07,[Black_Lagoon],,0000,0000,0000,,Nghe ra trò đấy, Dutch.
Dialogue: 0,0:12:28.07,0:12:29.71,[Black_Lagoon],,0000,0000,0000,,Lúc nào hứng đến đỉnh hú đây một tiếng nhá.
Dialogue: 0,0:12:29.93,0:12:30.68,[Sub_Phụ],,0000,0000,0000,,Ui da.
Dialogue: 0,0:12:30.68,0:12:31.85,[Black_Lagoon],,0000,0000,0000,,Có sao không?
Dialogue: 0,0:12:31.85,0:12:33.19,[Black_Lagoon],,0000,0000,0000,,Xin lỗi vì đã đẩy cô.
Dialogue: 0,0:12:33.19,0:12:33.89,[Sub_Phụ],,0000,0000,0000,,Phải...
Dialogue: 0,0:12:33.89,0:12:35.59,[Sub_Phụ],,0000,0000,0000,,Nhờ anh mà nguyên cái mặt này đạp đất đó.
Dialogue: 0,0:12:36.10,0:12:40.00,[Black_Lagoon],,0000,0000,0000,,Thôi, để Dutch cầm lái được rồi.
Dialogue: 0,0:12:40.00,0:12:41.11,[Black_Lagoon],,0000,0000,0000,,Khách của ta đây à?
Dialogue: 0,0:12:41.83,0:12:43.52,[Black_Lagoon],,0000,0000,0000,,Chào mừng đến công ty Lagoon.
Dialogue: 0,0:12:43.52,0:12:45.24,[Black_Lagoon],,0000,0000,0000,,Cứ gọi tôi là Benny.
Dialogue: 0,0:12:45.78,0:12:47.36,[Sub_Phụ],,0000,0000,0000,,Janet Bhai.
Dialogue: 0,0:12:47.36,0:12:48.40,[Sub_Phụ],,0000,0000,0000,,Gọi Jane cho tiện.
Dialogue: 0,0:12:49.35,0:12:52.59,[Black_Lagoon],,0000,0000,0000,,Xem ra ta có vào vị khách không mời ở trên boong đó.
Dialogue: 0,0:12:53.14,0:12:56.22,[Black_Lagoon],,0000,0000,0000,,Vậy mời vào phòng tôi.
Dialogue: 0,0:12:56.22,0:12:57.61,[Black_Lagoon],,0000,0000,0000,,Thế thì an toàn hơn nhiều.
Dialogue: 0,0:12:58.08,0:12:58.84,[Black_Lagoon],,0000,0000,0000,,Ah!
Dialogue: 0,0:12:58.84,0:13:02.17,[Black_Lagoon],,0000,0000,0000,,Và phiền cô đừng động vào mấy ổ cứng này nhé.
Dialogue: 0,0:13:02.49,0:13:03.59,[Black_Lagoon],,0000,0000,0000,,Một cái...
Dialogue: 0,0:13:03.59,0:13:04.55,[Black_Lagoon],,0000,0000,0000,,Cũng không được.
Dialogue: 0,0:13:04.76,0:13:06.17,[Black_Lagoon],,0000,0000,0000,,Đến đoạn "khó nhằn" rồi đó.
Dialogue: 0,0:13:06.35,0:13:07.30,[Black_Lagoon],,0000,0000,0000,,Sẵn sàng chưa?
Dialogue: 0,0:13:07.36,0:13:08.17,[Black_Lagoon],,0000,0000,0000,,Chờ đã.
Dialogue: 0,0:13:08.32,0:13:09.48,[Black_Lagoon],,0000,0000,0000,,Eda. Bám chắc vào.
Dialogue: 0,0:13:09.90,0:13:11.00,[Neutralist],,0000,0000,0000,,Thế là sao hả?
Dialogue: 0,0:13:11.00,0:13:13.35,[Black_Lagoon],,0000,0000,0000,,All right, cho BÒ vào trận đi nào!
Dialogue: 0,0:13:18.10,0:13:20.01,[Black_Lagoon],,0000,0000,0000,,Thế nào hả, Eda!? Happy chớ!?
Dialogue: 0,0:13:20.01,0:13:21.33,[Neutralist],,0000,0000,0000,,Cái mẹ gì chứ!?
Dialogue: 0,0:13:27.99,0:13:29.24,[Sub_Phụ],,0000,0000,0000,,Đi chết đi!
Dialogue: 0,0:13:30.05,0:13:31.63,[Sub_Phụ],,0000,0000,0000,,Cho hỏi cái...
Dialogue: 0,0:13:31.63,0:13:34.01,[Sub_Phụ],,0000,0000,0000,,Thuyền này lúc nào cũng đi thế này à?
Dialogue: 0,0:13:34.36,0:13:36.33,[Black_Lagoon],,0000,0000,0000,,Chỉ có hôm nay thôi.
Dialogue: 0,0:13:36.57,0:13:39.56,[Sub_Phụ],,0000,0000,0000,,Nói thật, có lúc tôi thấy mấy anh làm ăn như đi giết khách hàng ấy chứ.
Dialogue: 0,0:13:40.47,0:13:41.48,[Black_Lagoon],,0000,0000,0000,,Khốn nạn!
Dialogue: 0,0:13:41.48,0:13:43.83,[Black_Lagoon],,0000,0000,0000,,Đừng có nứng quá độ thế chứ, đồ chó dại!
Dialogue: 0,0:13:45.27,0:13:46.56,[Sub_Phụ],,0000,0000,0000,,Bất cẩn quá đó, Revy...
Dialogue: 0,0:13:46.56,0:13:47.75,[Sub_Phụ],,0000,0000,0000,,Hết đạn rồi chứ gì?
Dialogue: 0,0:13:47.75,0:13:50.78,[Sub_Phụ],,0000,0000,0000,,Thế thì đây càng xử cô nhẹ nhàng hơn đó.
Dialogue: 0,0:13:50.27,0:13:51.38,[Black_Lagoon],,0000,0000,0000,,Revy. Cầm lấy.
Dialogue: 0,0:13:55.55,0:13:57.38,[Black_Lagoon],,0000,0000,0000,,Thấy quả rodeo vừa nãy thế nào hả?
Dialogue: 0,0:13:57.38,0:13:58.51,[Black_Lagoon],,0000,0000,0000,,{\i1}Ờ. Phê lắm đó.{\i0}
Dialogue: 0,0:13:59.42,0:14:00.40,[Neutralist],,0000,0000,0000,,Đừng có mà hâm...
Dialogue: 0,0:14:00.40,0:14:02.77,[Neutralist],,0000,0000,0000,,Có cho vàng đây cũng không làm trò đó lại đâu...
Dialogue: 0,0:14:02.77,0:14:03.68,[Neutralist],,0000,0000,0000,,Ớ!
Dialogue: 0,0:14:03.68,0:14:04.44,[Neutralist],,0000,0000,0000,,Revy?
Dialogue: 0,0:14:07.38,0:14:09.28,[Black_Lagoon],,0000,0000,0000,,Tên này cứ để tôi lo.
Dialogue: 0,0:14:09.63,0:14:12.38,[Black_Lagoon],,0000,0000,0000,,{\i1}Phát hiện có một tên nấp ở dưới phòng máy.{\i0}
Dialogue: 0,0:14:13.26,0:14:13.84,[Black_Lagoon],,0000,0000,0000,,Eda...
Dialogue: 0,0:14:13.84,0:14:15.39,[Black_Lagoon],,0000,0000,0000,,Mụ lo tên đó đi.
Dialogue: 0,0:14:15.44,0:14:16.23,[Neutralist],,0000,0000,0000,,Duyệt luôn...
Dialogue: 0,0:14:16.23,0:14:16.83,[Neutralist],,0000,0000,0000,,Đồng sự.
Dialogue: 0,0:14:17.17,0:14:19.17,[Sub_Phụ],,0000,0000,0000,,Chỗ quái gì thế này?
Dialogue: 0,0:14:19.17,0:14:20.95,[Sub_Phụ],,0000,0000,0000,,Jane! Mày đâu rồi!?
Dialogue: 0,0:14:21.08,0:14:23.23,[Sub_Phụ],,0000,0000,0000,,Bao lâu nữa là ta đến bến cảng cần đến vậy?
Dialogue: 0,0:14:23.31,0:14:25.24,[Black_Lagoon],,0000,0000,0000,,Khoảng tầm 8 tiếng gì đó.
Dialogue: 0,0:14:25.28,0:14:28.09,[Sub_Phụ],,0000,0000,0000,,All right! Còn dư dả thời gian.
Dialogue: 0,0:14:28.09,0:14:29.28,[Sub_Phụ],,0000,0000,0000,,Anh Nhật kia...
Dialogue: 0,0:14:29.28,0:14:31.15,[Sub_Phụ],,0000,0000,0000,,Lấy hộ tôi cái laptop trong túi cái.
Dialogue: 0,0:14:32.44,0:14:33.87,[Black_Lagoon],,0000,0000,0000,,Có cái túi nào đâu.
Dialogue: 0,0:14:34.52,0:14:35.49,[Sub_Phụ],,0000,0000,0000,,Không á?
Dialogue: 0,0:14:35.49,0:14:36.52,[Sub_Phụ],,0000,0000,0000,,Sao cơ?
Dialogue: 0,0:14:36.52,0:14:37.90,[Sub_Phụ],,0000,0000,0000,,Có bị ngu không hả!?
Dialogue: 0,0:14:37.90,0:14:39.09,[Sub_Phụ],,0000,0000,0000,,Sủa thế là sao hả!?
Dialogue: 0,0:14:39.09,0:14:40.17,[Black_Lagoon],,0000,0000,0000,,Biết bố nào được?
Dialogue: 0,0:14:40.17,0:14:42.34,[Black_Lagoon],,0000,0000,0000,,Nếu nó quan trọng đến thế sao không lo mà giữ bên người!?
Dialogue: 0,0:14:42.34,0:14:43.80,[Sub_Phụ],,0000,0000,0000,,Vẫn chưa hiểu à!
Dialogue: 0,0:14:43.80,0:14:44.52,[Black_Lagoon],,0000,0000,0000,,Hiểu gì!?
Dialogue: 0,0:14:44.85,0:14:46.84,[Black_Lagoon],,0000,0000,0000,,Đừng vận động mạnh nhá.
Dialogue: 0,0:14:46.84,0:14:47.98,[Black_Lagoon],,0000,0000,0000,,Hư hết máy móc bây giờ.
Dialogue: 0,0:14:48.27,0:14:50.54,[Sub_Phụ],,0000,0000,0000,,Không có đồ nghề thì khỏi có làm lụm gì hết!
Dialogue: 0,0:14:50.84,0:14:53.48,[Sub_Phụ],,0000,0000,0000,,Không có máy laptop thì sẽ không vào được mạng...
Dialogue: 0,0:14:54.31,0:14:55.58,[Sub_Phụ],,0000,0000,0000,,Máy tính á?
Dialogue: 0,0:14:56.96,0:14:58.79,[Sub_Phụ],,0000,0000,0000,,Jane! Đi đâu rồi!?
Dialogue: 0,0:14:58.79,0:15:00.12,[Sub_Phụ],,0000,0000,0000,,Lòi mặt ra đây!
Dialogue: 0,0:15:00.44,0:15:01.50,[Sub_Phụ],,0000,0000,0000,,Shit! Shit! Shit!
Dialogue: 0,0:15:02.11,0:15:04.58,[Sub_Phụ],,0000,0000,0000,,Không có đường nối lên phần trước à. Đùa với tao chắc.
Dialogue: 0,0:15:05.02,0:15:07.29,[Sub_Phụ],,0000,0000,0000,,Cho mày biết, thứ máy chó chết!
Dialogue: 0,0:15:07.29,0:15:09.29,[Sub_Phụ],,0000,0000,0000,,Xong vụ này...
Dialogue: 0,0:15:09.29,0:15:11.47,[Sub_Phụ],,0000,0000,0000,,Ta nốc đến chán chê thì thôi!
Dialogue: 0,0:15:11.96,0:15:16.93,[Sub_Phụ],,0000,0000,0000,,Chạy khắp Rawhide rồi chài vài em\Ngái hạng sang chơi đến buốt dái thì thôi!
Dialogue: 0,0:15:18.53,0:15:20.83,[Neutralist],,0000,0000,0000,,Trông "phấn khởi" quá hen.
Dialogue: 0,0:15:20.83,0:15:23.30,[Neutralist],,0000,0000,0000,,Cowboy có khác. Trông như ngựa giống ấy.
Dialogue: 0,0:15:23.67,0:15:24.88,[Neutralist],,0000,0000,0000,,Nhưng trước khi làm mấy trò đó...
Dialogue: 0,0:15:24.88,0:15:30.00,[Neutralist],,0000,0000,0000,,Sao không phone cho Chúa hỏi xem có cho\N mấy hòn bi của ông lên chầu giời trước được không đi.
Dialogue: 0,0:15:35.55,0:15:36.57,[Black_Lagoon],,0000,0000,0000,,Dutch...
Dialogue: 0,0:15:36.57,0:15:38.43,[Black_Lagoon],,0000,0000,0000,,Hắn bắn lửa khóa hết phạm vi của tôi rồi.
Dialogue: 0,0:15:38.62,0:15:39.45,[Black_Lagoon],,0000,0000,0000,,{\i1}Phiền thật ha.{\i0}
Dialogue: 0,0:15:39.55,0:15:41.00,[Black_Lagoon],,0000,0000,0000,,Làm thêm phát "bull ride" đi.
Dialogue: 0,0:15:41.00,0:15:41.98,[Black_Lagoon],,0000,0000,0000,,Làm được không?
Dialogue: 0,0:15:41.98,0:15:44.74,[Black_Lagoon],,0000,0000,0000,,Thằng heo này trông thế mà nhanh lắm.
Dialogue: 0,0:15:46.52,0:15:48.53,[Black_Lagoon],,0000,0000,0000,,Tôi ra hiệu là tống tiễn hắn đi luôn nhé.
Dialogue: 0,0:15:49.03,0:15:50.70,[Black_Lagoon],,0000,0000,0000,,{\i1}All right. Thế thì dễ ợt.
Dialogue: 0,0:15:50.70,0:15:51.50,[Black_Lagoon],,0000,0000,0000,,{\i1}Vậy còn cô tính sao?{\i0}
Dialogue: 0,0:15:52.30,0:15:55.46,[Black_Lagoon],,0000,0000,0000,,{\i1}Tùy thuộc vào độ căn của anh thôi...
Dialogue: 0,0:15:55.46,0:15:55.97,[Black_Lagoon],,0000,0000,0000,,{\i1}Đi đây!{\i0}
Dialogue: 0,0:15:58.74,0:16:00.62,[Sub_Phụ],,0000,0000,0000,,Đừng có ngáng đường tao, mụ chó sơ!
Dialogue: 0,0:16:00.62,0:16:02.57,[Sub_Phụ],,0000,0000,0000,,Tao ngán cù nhây với lũ khốn chúng mày lắm rồi!
Dialogue: 0,0:16:03.45,0:16:04.92,[Neutralist],,0000,0000,0000,,Kệ bố...
Dialogue: 0,0:16:04.92,0:16:06.73,[Neutralist],,0000,0000,0000,,Thứ nhà quê mày!
Dialogue: 0,0:16:06.73,0:16:09.05,[Neutralist],,0000,0000,0000,,Thế thì té khỏi tàu đi!
Dialogue: 0,0:16:09.05,0:16:10.74,[Sub_Phụ],,0000,0000,0000,,Họa điên mới thế!
Dialogue: 0,0:16:10.11,0:16:10.99,[Neutralist],,0000,0000,0000,,Câm mồm!
Dialogue: 0,0:16:10.99,0:16:12.92,[Sub_Phụ],,0000,0000,0000,,Hàng này là của tao!
Dialogue: 0,0:16:10.99,0:16:12.92,[Neutralist],,0000,0000,0000,,Hàng này là của tao!
Dialogue: 0,0:16:14.07,0:16:15.97,[Black_Lagoon][01],,0000,0000,0000,,Không có bản in tiền sao?
Dialogue: 0,0:16:14.07,0:16:15.97,[Black_Lagoon],,0000,0000,0000,,Không có bản in tiền sao?
Dialogue: 0,0:16:16.31,0:16:18.57,[Sub_Phụ],,0000,0000,0000,,Bản in đó vẫn trôi nổi trên mạng.
Dialogue: 0,0:16:18.99,0:16:22.07,[Sub_Phụ],,0000,0000,0000,,Công việc chính của bọn tôi là thu thập mọi dữ liệu kỹ thuật...
Dialogue: 0,0:16:22.43,0:16:25.12,[Sub_Phụ],,0000,0000,0000,,Rải khắp toàn cầu để cấu thành nó.
Dialogue: 0,0:16:25.34,0:16:28.97,[Sub_Phụ],,0000,0000,0000,,Nhưng do chúng giết mất kỹ thuật viên nên giờ tôi đành bó tay.
Dialogue: 0,0:16:29.70,0:16:32.78,[Sub_Phụ],,0000,0000,0000,,Mật mã thì cứ thay đổi theo ngày nữa chứ...
Dialogue: 0,0:16:32.78,0:16:33.71,[Sub_Phụ],,0000,0000,0000,,Bó tay thật rồi.
Dialogue: 0,0:16:34.11,0:16:37.38,[Black_Lagoon],,0000,0000,0000,,Nhà thờ Ripoff đã trả đủ phần cho bên tôi rồi, nên cũng chả sao, có điều...
Dialogue: 0,0:16:38.48,0:16:39.94,[Black_Lagoon],,0000,0000,0000,,Vấn đề là Eda kìa.
Dialogue: 0,0:16:40.25,0:16:42.65,[Black_Lagoon],,0000,0000,0000,,Mụ ấy mà biết cô không bản in đó trong tay...
Dialogue: 0,0:16:42.65,0:16:46.03,[Black_Lagoon],,0000,0000,0000,,Cái thân cô bị đóng đinh lên tường còn tởm hơn cả Chúa đấy.
Dialogue: 0,0:16:46.03,0:16:47.25,[Sub_Phụ],,0000,0000,0000,,Là đùa phải không?
Dialogue: 0,0:16:47.32,0:16:51.70,[Black_Lagoon],,0000,0000,0000,,Roanapur là nơi mà hiện thực hóa mọi trò đùa tàn bạo nhất trên đời đấy.
Dialogue: 0,0:16:51.92,0:16:52.48,[Black_Lagoon],,0000,0000,0000,,Đúng.
Dialogue: 0,0:16:52.66,0:16:53.86,[Sub_Phụ],,0000,0000,0000,,Nếu thế thì...
Dialogue: 0,0:16:53.86,0:16:56.10,[Sub_Phụ],,0000,0000,0000,,Xin anh cho tôi mượn máy của anh một chút thôi cũng được!?
Dialogue: 0,0:16:55.92,0:16:56.70,[Black_Lagoon],,0000,0000,0000,,Khỏi đi.
Dialogue: 0,0:16:56.74,0:16:58.17,[Sub_Phụ],,0000,0000,0000,,Sao không!?
Dialogue: 0,0:16:58.17,0:16:59.43,[Sub_Phụ],,0000,0000,0000,,Kiệt, kiệt, kiệt xỉn!
Dialogue: 0,0:16:59.43,0:17:01.09,[Black_Lagoon],,0000,0000,0000,,Nói sao thì tôi cũng không cho đâu.
Dialogue: 0,0:17:01.09,0:17:05.62,[Black_Lagoon],,0000,0000,0000,,Tôi thà đi tu dầu hải ly còn hơn để người khác đụng vào máy của mình.
Dialogue: 0,0:17:06.67,0:17:07.60,[Black_Lagoon],,0000,0000,0000,,Nhưng...
Dialogue: 0,0:17:07.60,0:17:09.98,[Black_Lagoon],,0000,0000,0000,,Nếu người dùng là tôi thì lại là chuyện khác.
Dialogue: 0,0:17:10.86,0:17:14.81,[Black_Lagoon],,0000,0000,0000,,Tôi khá là tò mò với\Nmấy trò của anh thợ bên cô đấy.
Dialogue: 0,0:17:15.71,0:17:17.85,[Black_Lagoon],,0000,0000,0000,,Nên đừng để tôi thất vọng đó.
Dialogue: 0,0:17:17.85,0:17:20.51,[Black_Lagoon],,0000,0000,0000,,Nếu đây chỉ là trò tiêu khiển của lũ ranh trung học...
Dialogue: 0,0:17:20.87,0:17:22.97,[Black_Lagoon],,0000,0000,0000,,Thì lo mà chuẩn bị xì tiền ra đi.
Dialogue: 0,0:17:26.11,0:17:27.32,[Neutralist],,0000,0000,0000,,Ôi chà chà...
Dialogue: 0,0:17:27.32,0:17:30.69,[Neutralist],,0000,0000,0000,,Chơi trò đuổi bắt với gái đẹp thiệt là vui ghê đi.
Dialogue: 0,0:17:30.86,0:17:33.18,[Neutralist],,0000,0000,0000,,Không may là tôi thấy thấm mệt rồi đó.
Dialogue: 0,0:17:33.68,0:17:35.25,[Neutralist],,0000,0000,0000,,Thế giờ muốn đầu hàng...
Dialogue: 0,0:17:35.25,0:17:37.20,[Neutralist],,0000,0000,0000,,Hay bị thiêu nào?
Dialogue: 0,0:17:38.46,0:17:40.18,[Neutralist],,0000,0000,0000,,Nhanh qua thôi...
Dialogue: 0,0:17:40.18,0:17:42.79,[Neutralist],,0000,0000,0000,,Mụ vợ tôi chịu không quá 5 phút nữa là.
Dialogue: 0,0:17:43.14,0:17:46.45,[Neutralist],,0000,0000,0000,,Mùi khét của da hóa thành than thiệt hấp dẫn lắm đó.
Dialogue: 0,0:17:47.61,0:17:50.28,[Neutralist],,0000,0000,0000,,Nào, cô em, trăng thanh gió mát thế này...
Dialogue: 0,0:17:50.28,0:17:52.50,[Neutralist],,0000,0000,0000,,Ăn tiệc nướng thì còn gì bằng...
Dialogue: 0,0:17:54.63,0:17:55.41,[Black_Lagoon],,0000,0000,0000,,Dutch!
Dialogue: 0,0:17:55.41,0:17:56.50,[Black_Lagoon],,0000,0000,0000,,Ride!
Dialogue: 0,0:18:01.89,0:18:03.47,[Black_Lagoon],,0000,0000,0000,,Đi làm rocketman nhá, baby!
Dialogue: 0,0:18:09.00,0:18:12.32,[Sub_Phụ],,0000,0000,0000,,Biết rồi đó. Quả thuyền\Nnày thiệt muốn giết tôi đây mà.
Dialogue: 0,0:18:12.32,0:18:13.98,[Black_Lagoon],,0000,0000,0000,,Không, lâu lâu mới thế thôi.
Dialogue: 0,0:18:13.98,0:18:14.75,[Sub_Phụ],,0000,0000,0000,,Thật á.
Dialogue: 0,0:18:15.11,0:18:17.60,[Sub_Phụ],,0000,0000,0000,,Vậy ít ra hãy để tôi cầm dữ liệu rồi hãy cho tôi chết.
Dialogue: 0,0:18:17.81,0:18:20.95,[Black_Lagoon],,0000,0000,0000,,Không phải mã IFSR, mã trao đổi cũng không nốt.
Dialogue: 0,0:18:20.95,0:18:23.57,[Black_Lagoon],,0000,0000,0000,,Chắc chắn chỉ có thể là mã GOST .
Dialogue: 0,0:18:23.78,0:18:28.08,[Black_Lagoon],,0000,0000,0000,,Có thể nên dùng buffer overflow, thế thì sẽ nhanh hơn.
Dialogue: 0,0:18:28.38,0:18:30.15,[Black_Lagoon],,0000,0000,0000,,Nhưng thế thì thiếu ý tứ quá.
Dialogue: 0,0:18:30.36,0:18:31.98,[Sub_Phụ],,0000,0000,0000,,Giờ còn quan tâm ý tứ ý cò gì nữa?
Dialogue: 0,0:18:32.30,0:18:33.46,[Black_Lagoon],,0000,0000,0000,,Không phải nóng.
Dialogue: 0,0:18:33.46,0:18:36.81,[Black_Lagoon],,0000,0000,0000,,Tôi đã cho chạy 6 đường truyền phá mã GOST rồi.
Dialogue: 0,0:18:37.18,0:18:42.46,[Black_Lagoon],,0000,0000,0000,,Một khi 1 đèn báo trên màn hình điểm, nó sẽ tự động tải về ngay.
Dialogue: 0,0:18:42.97,0:18:44.62,[Neutralist],,0000,0000,0000,,Xem ra bạn tôi...
Dialogue: 0,0:18:44.62,0:18:46.26,[Neutralist],,0000,0000,0000,,Đã xử đẹp gã lợn kia rồi.
Dialogue: 0,0:18:46.84,0:18:48.60,[Neutralist],,0000,0000,0000,,Chỉ còn mình ông thôi đó...
Dialogue: 0,0:18:48.60,0:18:49.40,[Neutralist],,0000,0000,0000,,Cowboy.
Dialogue: 0,0:18:50.54,0:18:53.81,[Neutralist],,0000,0000,0000,,Chống đối lại thì coi chừng toe đầu\N còn xác thì chìm dưới biển đấy.
Dialogue: 0,0:18:54.31,0:18:55.70,[Sub_Phụ],,0000,0000,0000,,Rồi mà. Rồi mà.
Dialogue: 0,0:18:55.98,0:18:57.59,[Sub_Phụ],,0000,0000,0000,,Khỉ nó. Chả vui tí nào cả.
Dialogue: 0,0:18:57.59,0:18:59.99,[Sub_Phụ],,0000,0000,0000,,Thế này thì tôi biết ăn nói với sếp thế nào đây?
Dialogue: 0,0:19:00.19,0:19:01.57,[Neutralist],,0000,0000,0000,,Chuyện đó khỏi phải lo.
Dialogue: 0,0:19:01.57,0:19:03.41,[Neutralist],,0000,0000,0000,,Vụ đó cứ để Lobos lo.
Dialogue: 0,0:19:04.09,0:19:06.58,[Neutralist],,0000,0000,0000,,Lão ta biết thừa luật rừng ở Roanapur là như thế nào.
Dialogue: 0,0:19:06.87,0:19:08.38,[Sub_Phụ],,0000,0000,0000,,Cái gã Lobos á?
Dialogue: 0,0:19:08.69,0:19:10.93,[Sub_Phụ],,0000,0000,0000,,Hắn mà đòi xử Elvis ư?
Dialogue: 0,0:19:11.86,0:19:13.80,[Sub_Phụ],,0000,0000,0000,,Hắn dám mới lạ...
Dialogue: 0,0:19:15.80,0:19:16.51,[Neutralist],,0000,0000,0000,,Thích gì hả?
Dialogue: 0,0:19:17.10,0:19:20.25,[Sub_Phụ],,0000,0000,0000,,Chờ một chút. Ta đã gặp nhau chưa nhỉ.
Dialogue: 0,0:19:20.25,0:19:21.27,[Sub_Phụ],,0000,0000,0000,,Florida chẳng hạn?
Dialogue: 0,0:19:21.27,0:19:22.59,[Sub_Phụ],,0000,0000,0000,,A, không phải...
Dialogue: 0,0:19:22.59,0:19:26.56,[Sub_Phụ],,0000,0000,0000,,Phải rồi là Dirty City mới đúng, Washington D.C. Chắc như đinh luôn!
Dialogue: 0,0:19:27.59,0:19:28.49,[Neutralist],,0000,0000,0000,,Thế à?
Dialogue: 0,0:19:28.49,0:19:30.61,[Neutralist],,0000,0000,0000,,Tôi không quen ông...
Dialogue: 0,0:19:30.61,0:19:32.19,[Neutralist],,0000,0000,0000,,Quê đây ở Alabama...
Dialogue: 0,0:19:32.19,0:19:33.86,[Neutralist],,0000,0000,0000,,Cả đời chưa đến D.C bao giờ.
Dialogue: 0,0:19:34.23,0:19:37.21,[Sub_Phụ],,0000,0000,0000,,Vậy chắc chắn là có một cô nàng giống hệt như cô mà.
Dialogue: 0,0:19:37.52,0:19:41.15,[Sub_Phụ],,0000,0000,0000,,Chỉ do cái kính râm với mớ đồ \Nlùng nhùng kia nên đến giờ tôi mới nhận ra.
Dialogue: 0,0:19:41.66,0:19:43.56,[Sub_Phụ],,0000,0000,0000,,Đệt, hồi nào ta?
Dialogue: 0,0:19:43.56,0:19:46.12,[Sub_Phụ],,0000,0000,0000,,Hình như hồi Don Geralamo ở D.C, rồi...
Dialogue: 0,0:19:46.12,0:19:48.19,[Sub_Phụ],,0000,0000,0000,,Là hắn. Chắc chắn là gã đó!
Dialogue: 0,0:19:48.19,0:19:48.57,[Neutralist],,0000,0000,0000,,Hey!
Dialogue: 0,0:19:49.37,0:19:51.05,[Neutralist],,0000,0000,0000,,Câm mõm được rồi đó, cowboy...
Dialogue: 0,0:19:51.05,0:19:52.97,[Neutralist],,0000,0000,0000,,Đã bảo là lão nhận nhầm người rồi.
Dialogue: 0,0:19:53.35,0:19:57.58,[Sub_Phụ],,0000,0000,0000,,Tôi nhớ ra rồi! Chính là tên thượng nghị sĩ Jack Bonham óc lợn đó!
Dialogue: 0,0:19:58.64,0:19:59.63,[Black_Lagoon],,0000,0000,0000,,Tally ho!
Dialogue: 0,0:19:59.63,0:20:01.10,[Black_Lagoon],,0000,0000,0000,,Quả đúng là mã GOST mà!
Dialogue: 0,0:20:01.41,0:20:04.73,[Black_Lagoon],,0000,0000,0000,,Anh thợ nhà cô cũng có nghề đó,\Nnhưng so ra vẫn chưa đến tầm đâu.
Dialogue: 0,0:20:05.00,0:20:07.48,[Black_Lagoon],,0000,0000,0000,,Tôi quả là cao tay cỡ Kevin Mitnick mà.
Dialogue: 0,0:20:07.48,0:20:08.17,[Black_Lagoon],,0000,0000,0000,,Yeah, yeah!
Dialogue: 0,0:20:08.17,0:20:09.42,[Black_Lagoon],,0000,0000,0000,,Đỉnh quá đi!
Dialogue: 0,0:20:10.02,0:20:10.76,[Black_Lagoon],,0000,0000,0000,,Thế thì.
Dialogue: 0,0:20:10.76,0:20:12.78,[Black_Lagoon],,0000,0000,0000,,Thắng cược rồi nhé.
Dialogue: 0,0:20:12.78,0:20:13.43,[Black_Lagoon],,0000,0000,0000,,Phần thưởng đâu?
Dialogue: 0,0:20:22.50,0:20:23.44,[Sub_Phụ],,0000,0000,0000,,Nè...
Dialogue: 0,0:20:23.44,0:20:24.72,[Sub_Phụ],,0000,0000,0000,,Anh yêu à...
Dialogue: 0,0:20:24.72,0:20:27.25,[Sub_Phụ],,0000,0000,0000,,Trông anh cute với sexy lắm đó.
Dialogue: 0,0:20:31.07,0:20:32.40,[Neutralist],,0000,0000,0000,,Cô nàng đó mặc...
Dialogue: 0,0:20:32.40,0:20:34.20,[Neutralist],,0000,0000,0000,,Đồng phục hải quân màu xanh phải không?
Dialogue: 0,0:20:34.65,0:20:39.28,[Neutralist],,0000,0000,0000,,Cô ta đến dự tiệc cùng viên thượng nghĩ sĩ và vài quan chức khác nữa.
Dialogue: 0,0:20:39.75,0:20:42.43,[Neutralist],,0000,0000,0000,,Tại một nhà hàng Pháp tên là Fontainebleau.
Dialogue: 0,0:20:42.65,0:20:45.20,[Sub_Phụ],,0000,0000,0000,,Chính thế! Đúng là Fontainebleau!
Dialogue: 0,0:20:45.20,0:20:47.99,[Sub_Phụ],,0000,0000,0000,,Nhưng sao cô lại đi cùng với lão thượng nghị sĩ đó?
Dialogue: 0,0:20:48.19,0:20:51.50,[Neutralist],,0000,0000,0000,,Lúc đó, cô ấy đang bàn luận với cấp trên của mình...
Dialogue: 0,0:20:52.10,0:20:59.14,[Neutralist],,0000,0000,0000,,Về việc ngoại giao với các nước trong khối EAEC cùng phe đối lập NAFTA...
Dialogue: 0,0:20:59.60,0:21:01.69,[Neutralist],,0000,0000,0000,,Và đối sách đối phó chúng.
Dialogue: 0,0:21:02.69,0:21:04.13,[Sub_Phụ],,0000,0000,0000,,Chẵng lẽ cô là...
Dialogue: 0,0:21:04.13,0:21:05.54,[Neutralist],,0000,0000,0000,,Ông nhận nhầm người rồi.
Dialogue: 0,0:21:05.74,0:21:07.59,[Neutralist],,0000,0000,0000,,Đó là một kẻ khác...
Dialogue: 0,0:21:07.59,0:21:10.33,[Neutralist],,0000,0000,0000,,Còn đây chỉ là một bà sơ cùi ở nhà thờ Ripoff mà thôi.
Dialogue: 0,0:21:10.98,0:21:14.26,[Neutralist],,0000,0000,0000,,Nhưng để đây bật mí cho ông một chuyện.
Dialogue: 0,0:21:14.91,0:21:17.25,[Neutralist],,0000,0000,0000,,Quê tôi không phải là ở Alabama...
Dialogue: 0,0:21:17.25,0:21:18.83,[Neutralist],,0000,0000,0000,,Gốc của con này là ở...
Dialogue: 0,0:21:18.83,0:21:20.69,[Neutralist],,0000,0000,0000,,Virginia, Langley.
Dialogue: 0,0:21:20.80,0:21:22.28,[Sub_Phụ],,0000,0000,0000,,Lũ chó CIA?!
Dialogue: 0,0:21:27.33,0:21:28.42,[Black_Lagoon],,0000,0000,0000,,Yo!
Dialogue: 0,0:21:28.42,0:21:30.09,[Black_Lagoon],,0000,0000,0000,,Làm ăn lâu la quá nhỉ.
Dialogue: 0,0:21:30.48,0:21:31.16,[Neutralist],,0000,0000,0000,,Ờ.
Dialogue: 0,0:21:31.51,0:21:33.31,[Black_Lagoon],,0000,0000,0000,,Xử đẹp hắn luôn chứ?
Dialogue: 0,0:21:37.97,0:21:39.23,[Black_Lagoon],,0000,0000,0000,,Gần về đến cảng rồi.
Dialogue: 0,0:21:39.23,0:21:40.69,[Black_Lagoon],,0000,0000,0000,,Thư giãn tí đi.
Dialogue: 0,0:21:40.85,0:21:42.07,[Neutralist],,0000,0000,0000,,Ờ~!
Dialogue: 0,0:21:42.07,0:21:46.52,[Neutralist],,0000,0000,0000,,Chúa sống trên giời còn chúng sinh phè phỡn dưới đất mà lị.

Dialogue: 0,0:01:30.00,0:01:35.01,Default,NTP,0,0,0,,{\an8}Tập 9:\NTế bào tuyến ngực
Dialogue: 0,0:01:32.55,0:01:34.01,Default,NTP,0,0,0,,"Tế bào tuyến ngực"
Dialogue: 0,0:01:43.85,0:01:45.60,Default,NTP,0,0,0,,{\i1}Bên trong cơ thể con người,{\i0}
Dialogue: 0,0:01:46.19,0:01:49.98,Default,NTP,0,0,0,,{\i1}có tới 37.2 nghìn tỉ tế bào{\i0}
Dialogue: 0,0:01:48.02,0:01:49.02,OnTop,NTP,0,0,0,,Sợi nhánh
Dialogue: 0,0:01:50.40,0:01:51.90,Default,NTP,0,0,0,,{\i1}đều đặn hàng ngày.{\i0}
Dialogue: 0,0:01:51.90,0:01:53.15,Default,NTP,0,0,0,,{\a6}Toàn thư kháng thể
Dialogue: 0,0:01:52.32,0:01:54.86,Default,NTP,0,0,0,,{\i1}24 giờ, 365 ngày.{\i0}
Dialogue: 0,0:01:55.49,0:01:57.49,Default,NTP,0,0,0,,{\i1}luôn làm việc chăm chỉ.{\i0}
Dialogue: 0,0:01:57.49,0:02:00.16,LeftSignM2,NTP,0,0,0,,Công trường đang xây dựng
Dialogue: 0,0:02:03.66,0:02:05.58,Default,NTP,0,0,0,,{\i1}Đây là bên trong cơ thể người.{\i0}
Dialogue: 0,0:02:07.37,0:02:10.29,Default,NTP,0,0,0,,{\i1}Chúng ta là những tế bào T bất bại!{\i0}
Dialogue: 0,0:02:10.54,0:02:13.63,Default,NTP,0,0,0,,{\i1}Không sợ hãi bất kì thứ gì!{\i0}
Dialogue: 0,0:02:13.88,0:02:17.01,Default,NTP,0,0,0,,{\i1}Chúng ta là những tế bào T bất bại!{\i0}
Dialogue: 0,0:02:16.80,0:02:20.30,Default,NTP,0,0,0,,{\a6}Chạy nhanh lên lũ chậm chạm kia!
Dialogue: 0,0:02:17.26,0:02:20.30,Default,NTP,0,0,0,,{\i1}Không sợ hãi bất kì thứ gì!{\i0}
Dialogue: 0,0:02:21.39,0:02:23.06,Default,NTP,0,0,0,,{\i1}Tế bào T độc{\i0}
Dialogue: 0,0:02:23.31,0:02:26.35,Default,NTP,0,0,0,,{\i1}Hoạt động theo lệnh của tế bào T hỗ trợ{\i0}
Dialogue: 0,0:02:26.81,0:02:30.69,Default,NTP,0,0,0,,{\i1}Phát hiện và tiêu diệt những dị thể như{\i0}
Dialogue: 0,0:02:30.94,0:02:33.15,Default,NTP,0,0,0,,{\i1}tế bào ung thư hoặc tế bào nhiễm vi rút{\i0}
Dialogue: 0,0:02:35.24,0:02:38.49,Default,NTP,0,0,0,,{\i1}Chúng ta là những tế bào T bất bại!{\i0}
Dialogue: 0,0:02:39.20,0:02:40.57,Default,NTP,0,0,0,,Rồi, xếp hàng đi!
Dialogue: 0,0:02:41.07,0:02:45.45,Default,NTP,0,0,0,,Tiếp theo là bài tập cực khổ rèn\N luyện thể lực và thực chiến!
Dialogue: 0,0:02:46.96,0:02:48.00,Default,NTP,0,0,0,,Tuân lệnh!
Dialogue: 0,0:02:48.83,0:02:52.17,Default,NTP,0,0,0,,1,867... 1,868...
Dialogue: 0,0:02:52.84,0:02:54.75,Default,NTP,0,0,0,,{\a6}2,465...
Dialogue: 0,0:02:55.17,0:02:56.88,Default,NTP,0,0,0,,{\a6}2,466...
Dialogue: 0,0:02:57.22,0:02:59.09,Default,NTP,0,0,0,,3,578...
Dialogue: 0,0:02:59.80,0:03:01.59,Default,NTP,0,0,0,,3,579...
Dialogue: 0,0:03:01.97,0:03:03.68,Default,NTP,0,0,0,,3,580!
Dialogue: 0,0:03:04.60,0:03:07.23,Default,NTP,0,0,0,,Nào, nói to nữa lên!
Dialogue: 0,0:03:07.23,0:03:09.56,Default,NTP,0,0,0,,Thế mà cũng nhận là tế bào T sao?
Dialogue: 0,0:03:09.56,0:03:10.77,Default,NTP,0,0,0,,Tuân lệnh!
Dialogue: 0,0:03:14.27,0:03:15.48,Default,NTP,0,0,0,,Chết này!
Dialogue: 0,0:03:17.32,0:03:18.74,Default,NTP,0,0,0,,Chết này!
Dialogue: 0,0:03:21.28,0:03:23.62,Default,NTP,0,0,0,,Mấy cậu toàn một lũ ẻo lả đấy à?
Dialogue: 0,0:03:23.87,0:03:26.20,Default,NTP,0,0,0,,Định đem tế bào T ra làm trò cười sao?
Dialogue: 0,0:03:26.20,0:03:27.83,Default,NTP,0,0,0,,Không ạ!
Dialogue: 0,0:03:27.83,0:03:30.83,Default,NTP,0,0,0,,Lũ thảm hại các cậu không đáng làm tế bào T!
Dialogue: 0,0:03:31.08,0:03:31.92,Default,NTP,0,0,0,,Tuân lệnh!
Dialogue: 0,0:03:31.92,0:03:33.54,Default,NTP,0,0,0,,Có biết tế bào T các cậu
Dialogue: 0,0:03:33.54,0:03:36.96,Default,NTP,0,0,0,,là phòng tuyến miễn dịch cuối cùng không hả?
Dialogue: 0,0:03:36.96,0:03:38.05,Default,NTP,0,0,0,,Có ạ!
Dialogue: 0,0:03:38.21,0:03:40.47,Default,NTP,0,0,0,,Phòng tuyết cuối cùng mà vứt \Nđi thì này thì làm ăn gì hả?
Dialogue: 0,0:03:40.72,0:03:42.97,Default,NTP,0,0,0,,Chúng bay liệu mà rèn giũa bản thân đi!
Dialogue: 0,0:03:42.97,0:03:47.06,Default,NTP,0,0,0,,{\a7}Kháng nguyên
Dialogue: 0,0:03:43.26,0:03:44.39,Default,NTP,0,0,0,,Tuân lệnh!
Dialogue: 0,0:03:44.39,0:03:46.06,Default,NTP,0,0,0,,Tôi đóng cửa lại nhé.
Dialogue: 0,0:03:46.06,0:03:47.81,Default,NTP,0,0,0,,Mấy cậu cứ sống như thế...
Dialogue: 0,0:03:47.81,0:03:48.85,Default,NTP,0,0,0,,Tuân lệnh!
Dialogue: 0,0:03:48.85,0:03:50.10,Default,NTP,0,0,0,,Nghĩ tôi là thằng ngốc à?
Dialogue: 0,0:03:50.94,0:03:53.65,Default,NTP,0,0,0,,Ây dà, nóng nảy quá nhỉ?
Dialogue: 0,0:03:54.02,0:03:56.44,Default,NTP,0,0,0,,Đến ngồi đây còn nghe thấy tiếng quát tháo.
Dialogue: 0,0:03:57.32,0:03:58.90,Default,NTP,0,0,0,,{\i1}Tế bào T hỗ trợ.{\i0}
Dialogue: 0,0:03:59.24,0:04:03.24,Default,NTP,0,0,0,,{\i1}Khi được báo tin có ngoại lai xâm\N nhập, bọn họ sẽ vào vai tư lệnh{\i0}
Dialogue: 0,0:04:03.49,0:04:06.53,Default,NTP,0,0,0,,{\i1}để đề ra chiến lược phù hợp và\N điều động tế bào T độc.{\i0}
Dialogue: 0,0:04:06.95,0:04:11.46,Default,NTP,0,0,0,,Làm hỏng cả buổi tiệc trà phải\N không tế bào T điều hòa-san?
Dialogue: 0,0:04:11.62,0:04:12.79,Default,NTP,0,0,0,,Thế à?
Dialogue: 0,0:04:13.08,0:04:15.79,Default,NTP,0,0,0,,Bây giờ cũng có phải tiệc trà\N đâu, tôi đang làm việc mà.
Dialogue: 0,0:04:16.21,0:04:18.05,Default,NTP,0,0,0,,{\i1}Tế bào T điều hòa.{\i0}
Dialogue: 0,0:04:18.30,0:04:20.17,Default,NTP,0,0,0,,{\i1}Ngăn không cho các tế bào T mất kiểm soát{\i0}
Dialogue: 0,0:04:20.42,0:04:23.30,Default,NTP,0,0,0,,{\i1}và điều hòa để không xảy ra\N bất thường trong miễn dịch.{\i0}
Dialogue: 0,0:04:25.26,0:04:26.47,Default,NTP,0,0,0,,Tiệc trà đúng là—
Dialogue: 0,0:04:35.61,0:04:36.77,Default,NTP,0,0,0,,Có sao không?
Dialogue: 0,0:04:37.57,0:04:39.23,Default,NTP,0,0,0,,Ấy, tư lệnh.
Dialogue: 0,0:04:39.53,0:04:41.61,Default,NTP,0,0,0,,Có vẻ tôi ném cậu ta hơi quá.
Dialogue: 0,0:04:41.95,0:04:45.03,Default,NTP,0,0,0,,Ném trúng vào tư lệnh T hỗ trợ luôn rồi!
Dialogue: 0,0:04:48.28,0:04:50.58,Default,NTP,0,0,0,,T độc-kun.
Dialogue: 0,0:04:51.58,0:04:53.75,Default,NTP,0,0,0,,Tôi có cái này cứ nghĩ mãi!
Dialogue: 0,0:04:54.00,0:04:57.13,Default,NTP,0,0,0,,Cậu ấy, làm việc bớt nóng chút có được không?
Dialogue: 0,0:04:57.42,0:05:00.13,Default,NTP,0,0,0,,Cậu chưa bao giờ nghĩ đến hiệu suất à?
Dialogue: 0,0:05:00.46,0:05:02.51,Default,NTP,0,0,0,,Tôi đây đang làm việc đấy.
Dialogue: 0,0:05:02.84,0:05:07.72,Default,NTP,0,0,0,,Phải chăm cho mấy cậu này sẵn sàng\N tấn công theo lệnh của cậu chứ.
Dialogue: 0,0:05:07.72,0:05:11.47,Default,NTP,0,0,0,,Ai sẽ gặp rắc rối khi lực lượng \Nchiến đấu toàn mấy tên yếu ớt hả?
Dialogue: 0,0:05:11.47,0:05:13.14,Default,NTP,0,0,0,,Chẳng phải là cậu sao?
Dialogue: 0,0:05:13.35,0:05:16.48,Default,NTP,0,0,0,,Thế tôi mới bảo cậu xem lại cách làm việc đi!
Dialogue: 0,0:05:16.73,0:05:18.48,Default,NTP,0,0,0,,Đừng có cứ ném người ta đi như thế!
Dialogue: 0,0:05:18.73,0:05:20.73,Default,NTP,0,0,0,,Vẫn còn lóng ngóng quá nhỉ!
Dialogue: 0,0:05:20.98,0:05:22.19,Default,NTP,0,0,0,,Nói cái gì vậy?
Dialogue: 0,0:05:22.19,0:05:25.28,Default,NTP,0,0,0,,Trong rèn luyện quan trọng nhất\N là rèn giũa trái tim họ mà!
Dialogue: 0,0:05:25.28,0:05:27.66,Default,NTP,0,0,0,,Cứ nhẹ nhàng mãi thì sao\N mà họ cứng cáp lên được?
Dialogue: 0,0:05:28.12,0:05:32.04,Default,NTP,0,0,0,,{\a6}Cậu chẳng biết tính toán gì hết cả!
Dialogue: 0,0:05:28.53,0:05:30.62,Default,NTP,0,0,0,,Có sao không, nghỉ ngơi chút đi.
Dialogue: 0,0:05:31.70,0:05:33.41,Default,NTP,0,0,0,,C-cái gì vậy?
Dialogue: 0,0:05:32.04,0:05:35.37,Default,NTP,0,0,0,,{\a6}Còn cậu chỉ biết ngồi chỉ tay năm ngón "Làm này, làm kia đi"!
Dialogue: 0,0:05:33.66,0:05:36.12,Default,NTP,0,0,0,,Bọn họ có quan hệ thế nào vậy?
Dialogue: 0,0:05:35.37,0:05:39.17,Default,NTP,0,0,0,,{\a6}Tôi mới là người biết chiến trường nó như thế nào!
Dialogue: 0,0:05:36.50,0:05:38.25,Default,NTP,0,0,0,,Không phải chỉ là cấp trên và cấp dưới thôi sao?
Dialogue: 0,0:05:39.17,0:05:40.84,Default,NTP,0,0,0,,{\a6}Tôi không phải chỉ biết ngồi chỉ tay năm ngón!
Dialogue: 0,0:05:42.09,0:05:43.01,Default,NTP,0,0,0,,Có muốn biết không?
Dialogue: 0,0:05:44.30,0:05:45.80,Default,NTP,0,0,0,,{\i1}Tế bào tua.{\i0}
Dialogue: 0,0:05:46.05,0:05:48.30,Default,NTP,0,0,0,,{\i1}Chỉ ra những vi khuẩn hoặc vi rút {\i0}
Dialogue: 0,0:05:48.55,0:05:52.35,Default,NTP,0,0,0,,{\i1}xâm nhập cơ thể như kháng nguyên{\i0}
Dialogue: 0,0:05:52.68,0:05:54.98,Default,NTP,0,0,0,,{\i1}và báo cho các tế bào khác của hệ miễn dịch.{\i0}
Dialogue: 0,0:05:55.23,0:05:57.98,Default,NTP,0,0,0,,{\i1}Cũng phần nào liên quan đến quá trình nuôi dưỡng tế bào T.{\i0}
Dialogue: 0,0:05:58.86,0:05:59.94,Default,NTP,0,0,0,,Nói cái gì hả?
Dialogue: 0,0:05:59.94,0:06:01.65,Default,NTP,0,0,0,,Có ý kiến gì?
Dialogue: 0,0:06:00.19,0:06:02.53,Default,NTP,0,0,0,,{\a9}Tế bào T
Dialogue: 0,0:06:00.86,0:06:02.15,Default,NTP,0,0,0,,{\a6}Bọn họ ấy...
Dialogue: 0,0:06:01.90,0:06:02.90,Default,NTP,0,0,0,,Cái gì hả?
Dialogue: 0,0:06:05.11,0:06:05.86,Default,NTP,0,0,0,,Không thể nào!
Dialogue: 0,0:06:06.36,0:06:09.16,Default,NTP,0,0,0,,Là bạn cùng lớp ở trường học tuyến ngực.
Dialogue: 0,0:06:11.24,0:06:13.83,Default,NTP,0,0,0,,Cậu đeo kính này là tư lệnh T hỗ trợ?
Dialogue: 0,0:06:14.16,0:06:15.25,Default,NTP,0,0,0,,Nhìn trông năng nổ quá!
Dialogue: 0,0:06:15.50,0:06:17.25,Default,NTP,0,0,0,,Anh T độc thì èo oặt quá!
Dialogue: 0,0:06:17.50,0:06:19.92,Default,NTP,0,0,0,,Lại còn khóc nữa, giống hệt bọn mình!
Dialogue: 0,0:06:20.42,0:06:24.13,Default,NTP,0,0,0,,Lại còn bị đánh bại nữa! Khác hẳn bây giờ luôn!
Dialogue: 0,0:06:22.88,0:06:28.55,Default,NTP,0,0,0,,{\a6}Tôi hơi bị ghen tị với công việc vô não chỉ\N phải huấn luyện người khác của cậu đấy!
Dialogue: 0,0:06:25.47,0:06:26.88,Default,NTP,0,0,0,,Nhìn đúng là như vậy nhỉ...
Dialogue: 0,0:06:27.22,0:06:30.01,Default,NTP,0,0,0,,Nhưng mà bọn họ ngày xưa ấy...
Dialogue: 0,0:06:28.55,0:06:30.01,Default,NTP,0,0,0,,{\a6}Nói gì hả?
Dialogue: 0,0:06:31.93,0:06:35.43,Default,NTP,0,0,0,,{\i1}- Chào mừng tới địa ngục, lũ nhãi con\N- Chúng ta là tế bào T học việc!{\i0}
Dialogue: 0,0:06:32.97,0:06:35.43,Onscreen,NTP,0,0,0,,{\a6}Tuyến ngực: Cơ quan bạch huyết\N phân loại và nuôi dưỡng tế bào T.
Dialogue: 0,0:06:35.98,0:06:36.85,Default,NTP,0,0,0,,Nghe đây!
Dialogue: 0,0:06:36.23,0:06:39.15,Default,NTP,0,0,0,,{\a6}{\i1}Không sợ hãi bất kì thứ gì!{\i0}
Dialogue: 0,0:06:37.10,0:06:41.90,Default,NTP,0,0,0,,Ở tuyến ngực này sẽ phân \Nloại ai có ích và ai vô dụng!
Dialogue: 0,0:06:39.40,0:06:41.90,Default,NTP,0,0,0,,{\a6}{\i1}Chúng ta là tế bào T học việc!{\i0}
Dialogue: 0,0:06:42.61,0:06:45.69,Default,NTP,0,0,0,,{\i1}Giết hết vi rút và vi khuẩn!{\i0}
Dialogue: 0,0:06:47.45,0:06:50.32,Default,NTP,0,0,0,,{\i1}Chúng ta là tế bào T học việc!{\i0}
Dialogue: 0,0:06:49.07,0:06:50.87,Default,NTP,0,0,0,,{\a9}Biểu mô
Dialogue: 0,0:06:50.57,0:06:53.62,Default,NTP,0,0,0,,{\i1}Không sợ hãi bất kì thứ gì!{\i0}
Dialogue: 0,0:06:53.87,0:06:56.83,Default,NTP,0,0,0,,{\i1}Chúng ta là tế bào T học việc!{\i0}
Dialogue: 0,0:06:57.08,0:07:00.29,Default,NTP,0,0,0,,{\i1}Giết hết vi rút và vi khuẩn!{\i0}
Dialogue: 0,0:07:01.33,0:07:02.67,Default,NTP,0,0,0,,Lũ ngốc kia,
Dialogue: 0,0:07:02.92,0:07:06.26,Default,NTP,0,0,0,,từ hôm nay sẽ rèn luyện rất cực khổ.\N Liệu mà chuẩn bị tinh thần đi!
Dialogue: 0,0:07:07.09,0:07:08.80,Default,NTP,0,0,0,,{\i1}Tế bào biểu mô tuyến ngực.{\i0}
Dialogue: 0,0:07:09.22,0:07:11.72,Default,NTP,0,0,0,,{\i1}Tế bào biểu mô hình thành tuyến ngực.{\i0}
Dialogue: 0,0:07:11.97,0:07:16.06,Default,NTP,0,0,0,,{\i1}Nuôi dưỡng tế bào bạch huyết\N và giúp phân loại tế bào T.{\i0}
Dialogue: 0,0:07:16.93,0:07:17.77,Default,NTP,0,0,0,,Nghe rõ chưa?
Dialogue: 0,0:07:18.02,0:07:19.14,Default,NTP,0,0,0,,Rõ!
Dialogue: 0,0:07:22.52,0:07:23.61,Default,NTP,0,0,0,,Ngay bây giờ,
Dialogue: 0,0:07:23.86,0:07:26.23,Default,NTP,0,0,0,,các cậu sẽ làm một bài kiểm tra!
Dialogue: 0,0:07:26.82,0:07:30.36,Default,NTP,0,0,0,,Xem xem có thể phản ứng với kháng\N nguyên từ kẻ địch bên ngoài không?
Dialogue: 0,0:07:30.78,0:07:33.95,Default,NTP,0,0,0,,Bất ngờ sẽ có những tấm \Nbảng dạng người xuất hiện!
Dialogue: 0,0:07:34.20,0:07:38.58,Default,NTP,0,0,0,,Hãy quyết định xem đó có phải kháng\N nguyên không và tấn công!
Dialogue: 0,0:07:39.08,0:07:41.71,Default,NTP,0,0,0,,Những ai nhầm lẫn tấn công tế bào thông thường,
Dialogue: 0,0:07:41.96,0:07:44.79,Default,NTP,0,0,0,,hoặc những ai không tiêu diệt được\N kháng nguyên nào trước khi tới đích,
Dialogue: 0,0:07:45.25,0:07:46.71,Default,NTP,0,0,0,,sẽ bị đánh trượt!
Dialogue: 0,0:07:46.96,0:07:47.76,Default,NTP,0,0,0,,Hiểu chưa?
Dialogue: 0,0:07:48.01,0:07:48.88,Default,NTP,0,0,0,,Rõ!
Dialogue: 0,0:07:49.67,0:07:54.22,Default,NTP,0,0,0,,Chỉ khoảng 2, 3 phần trăm còn\N trụ lại đến cuối cùng thôi!
Dialogue: 0,0:07:54.55,0:07:56.01,Default,NTP,0,0,0,,Nên là hết mình đi!
Dialogue: 0,0:07:56.01,0:07:56.85,Default,NTP,0,0,0,,Rõ!
Dialogue: 0,0:08:11.07,0:08:12.45,Default,NTP,0,0,0,,{\i1}Đó là...{\i0}
Dialogue: 0,0:08:13.07,0:08:15.74,Default,NTP,0,0,0,,{\i1}Có phải kháng nguyên không nhỉ?{\i0}
Dialogue: 0,0:08:16.08,0:08:17.08,Default,NTP,0,0,0,,{\i1}Hay lắm!{\i0}
Dialogue: 0,0:08:22.83,0:08:25.09,Default,NTP,0,0,0,,Khá lắm, được đấy!
Dialogue: 0,0:08:25.46,0:08:26.80,Default,NTP,0,0,0,,Ghê quá!
Dialogue: 0,0:08:27.05,0:08:28.05,Default,NTP,0,0,0,,Cậu ta...
Dialogue: 0,0:08:28.30,0:08:30.13,Default,NTP,0,0,0,,cắt đôi tấm gỗ luôn kìa!
Dialogue: 0,0:08:30.97,0:08:34.26,Default,NTP,0,0,0,,Này, tôi thấy trước mà!
Dialogue: 0,0:08:34.51,0:08:37.60,Default,NTP,0,0,0,,Với cả sao lại dẫm lên đầu người khác chứ?
Dialogue: 0,0:08:38.18,0:08:40.06,Default,NTP,0,0,0,,Ồ, xin lỗi nhé.
Dialogue: 0,0:08:40.39,0:08:42.19,Default,NTP,0,0,0,,Cho phép tôi dùng cậu làm cái bàn đạp.
Dialogue: 0,0:08:42.69,0:08:45.06,Default,NTP,0,0,0,,Vì đó là đường ngắn nhất mà.
Dialogue: 0,0:08:45.48,0:08:47.61,Default,NTP,0,0,0,,Nhưng mà nếu cậu không phản ứng nhanh hơn,
Dialogue: 0,0:08:47.86,0:08:51.07,Default,NTP,0,0,0,,thì khó mà trở thành tế bào T chững chạc được đó.
Dialogue: 0,0:08:51.78,0:08:52.70,Default,NTP,0,0,0,,Thằng kia...
Dialogue: 0,0:08:53.99,0:08:55.07,Default,NTP,0,0,0,,Đau!
Dialogue: 0,0:08:56.87,0:08:59.95,Default,NTP,0,0,0,,Đừng bắt tôi phải tiêu tốn\N năng lượng thừa thãi chứ?
Dialogue: 0,0:09:02.75,0:09:05.92,Default,NTP,0,0,0,,Búng vào trán thôi mà đau thế sao?
Dialogue: 0,0:09:06.42,0:09:09.71,Default,NTP,0,0,0,,Kệ cậu ta đi! Dính vào chỉ phí thời gian thôi!
Dialogue: 0,0:09:10.46,0:09:13.13,Default,NTP,0,0,0,,Này, đừng ngồi khóc lóc nữa, chạy đi!
Dialogue: 0,0:09:13.38,0:09:18.10,Default,NTP,0,0,0,,Thành viên đợt này chênh lệch thực lực quá nhỉ...
Dialogue: 0,0:09:26.52,0:09:29.52,Default,NTP,0,0,0,,Ai ngờ được chúng ta lại cùng phòng nhỉ?
Dialogue: 0,0:09:29.86,0:09:32.36,Default,NTP,0,0,0,,Vậy việc chia phòng không phải theo thực lực à.
Dialogue: 0,0:09:32.86,0:09:34.70,Default,NTP,0,0,0,,Thế nghĩa là sao hả?
Dialogue: 0,0:09:35.66,0:09:37.99,Default,NTP,0,0,0,,Đừng bắt tôi cái gì cũng phải nói ra chứ.
Dialogue: 0,0:09:38.24,0:09:40.45,Default,NTP,0,0,0,,Giường tầng thì tôi lấy tầng dưới đấy.
Dialogue: 0,0:09:41.37,0:09:42.79,Default,NTP,0,0,0,,Cho tôi giường trên à?
Dialogue: 0,0:09:43.04,0:09:45.54,Default,NTP,0,0,0,,Leo lên leo xuống tốn thời gian lắm.
Dialogue: 0,0:09:45.96,0:09:49.50,Default,NTP,0,0,0,,Nhưng thay vào đấy thì\N cậu nhớ hạn chế leo trèo thôi.
Dialogue: 0,0:09:49.75,0:09:52.05,Default,NTP,0,0,0,,Nếu không tôi không tập trung ngủ được.
Dialogue: 0,0:09:52.30,0:09:55.18,Default,NTP,0,0,0,,Cậu mà ngủ luôn dưới sàn thì tốt quá.
Dialogue: 0,0:09:55.51,0:09:59.93,Default,NTP,0,0,0,,Cái gì, cậu nghĩ người khác là cái gì hả?
Dialogue: 0,0:10:00.14,0:10:01.39,Default,NTP,0,0,0,,Cậu bị làm sao đấy?
Dialogue: 0,0:10:01.64,0:10:04.02,Default,NTP,0,0,0,,Cậu là dạng nói cho mà cũng không hiểu sao?
Dialogue: 0,0:10:04.98,0:10:06.90,Default,NTP,0,0,0,,Hai người rảnh rỗi quá nhỉ.
Dialogue: 0,0:10:06.90,0:10:08.40,Default,NTP,0,0,0,,Có việc gì thế?
Dialogue: 0,0:10:09.02,0:10:09.94,Default,NTP,0,0,0,,Không có gì.
Dialogue: 0,0:10:10.36,0:10:12.11,Default,NTP,0,0,0,,Tình cờ đi qua thôi.
Dialogue: 0,0:10:12.36,0:10:16.36,Default,NTP,0,0,0,,Cứ tưởng mấy cậu vừa bị đuổi nên gây gổ.
Dialogue: 0,0:10:16.36,0:10:18.91,Default,NTP,0,0,0,,Không phải, làm gì có chuyện đấy được chứ?
Dialogue: 0,0:10:19.16,0:10:20.08,Default,NTP,0,0,0,,Thế à?
Dialogue: 0,0:10:20.53,0:10:21.41,Default,NTP,0,0,0,,Mà này...
Dialogue: 0,0:10:22.41,0:10:27.33,Default,NTP,0,0,0,,Có vẻ 2 cậu giường bên này sau\N buổi hôm nay bị đuổi rồi đấy.
Dialogue: 0,0:10:27.67,0:10:29.13,Default,NTP,0,0,0,,Tôi cũng phải cố gắng đây.
Dialogue: 0,0:10:29.58,0:10:30.46,Default,NTP,0,0,0,,Chào nhé!
Dialogue: 0,0:10:34.46,0:10:37.97,Default,NTP,0,0,0,,{\i1}Những buổi rèn luyện cực khổ cứ thế tiếp tục.{\i0}
Dialogue: 0,0:10:48.23,0:10:50.06,Default,NTP,0,0,0,,{\i1}May mắn là họ đã chịu đựng được{\i0}
Dialogue: 0,0:10:50.36,0:10:52.27,Default,NTP,0,0,0,,{\i1}và tránh không bị đuổi.{\i0}
Dialogue: 0,0:11:00.28,0:11:03.95,Default,NTP,0,0,0,,Đằng kia, đừng có than thở nữa, đuổi đi bây giờ!
Dialogue: 0,0:11:04.70,0:11:07.29,Default,NTP,0,0,0,,Tuân lệnh!
Dialogue: 0,0:11:08.37,0:11:11.67,Default,NTP,0,0,0,,Ngoan cố quá nhỉ, kết quả vẫn sẽ như vậy thôi!
Dialogue: 0,0:11:12.38,0:11:14.80,Default,NTP,0,0,0,,Cậu không hợp làm tế bào T rồi.
Dialogue: 0,0:11:15.05,0:11:20.26,Default,NTP,0,0,0,,Chỉ những ai xuất sắc như\N mình mới trụ lại được thôi nhỉ?
Dialogue: 0,0:11:22.39,0:11:23.51,Default,NTP,0,0,0,,Đúng đó.
Dialogue: 0,0:11:24.22,0:11:26.89,Default,NTP,0,0,0,,Ai xong rồi về phòng đi!
Dialogue: 0,0:11:27.43,0:11:28.48,Default,NTP,0,0,0,,Mệt quá!
Dialogue: 0,0:11:28.48,0:11:29.85,Default,NTP,0,0,0,,Đau hết cả người!
Dialogue: 0,0:11:33.73,0:11:34.65,Default,NTP,0,0,0,,Chết tiệt!
Dialogue: 0,0:11:34.90,0:11:38.15,Default,NTP,0,0,0,,Tôi thì đang oằn mình ra còn \Ncậu thì suốt ngày uống trà thôi!
Dialogue: 0,0:11:39.07,0:11:40.03,Default,NTP,0,0,0,,Nói lắm thế nhỉ?
Dialogue: 0,0:11:40.28,0:11:42.41,Default,NTP,0,0,0,,Muốn búng thêm phát nữa không?
Dialogue: 0,0:11:53.71,0:11:55.42,Default,NTP,0,0,0,,Nào, làm thôi.
Dialogue: 0,0:12:02.30,0:12:04.14,Default,NTP,0,0,0,,{\i1}Hôm nay đến thế thôi nhỉ.{\i0}
Dialogue: 0,0:12:13.40,0:12:15.36,Default,NTP,0,0,0,,{\i1}Cậu ta làm gì vậy?{\i0}
Dialogue: 0,0:12:23.28,0:12:25.91,Default,NTP,0,0,0,,Cú đấm T độc, 42 phát liền!
Dialogue: 0,0:12:31.33,0:12:34.67,Default,NTP,0,0,0,,{\i1}Sau buổi tập hôm nay lẽ ra phải kiệt sức rồi chứ.{\i0}
Dialogue: 0,0:12:41.80,0:12:43.43,Default,NTP,0,0,0,,Miễn Dịch Đại Dung Giải Quyền!
Dialogue: 0,0:12:43.68,0:12:45.10,Default,NTP,0,0,0,,Tuyệt kĩ thể nội!
Dialogue: 0,0:12:45.35,0:12:48.85,Default,NTP,0,0,0,,Kháng Nguyên Đại Bộc Sát\NMộc Đoan Vi Trần Quyền!
Dialogue: 0,0:12:49.97,0:12:53.14,Default,NTP,0,0,0,,{\i1}Cái gì mà tuyệt kĩ chứ? Bị ngốc à?{\i0}
Dialogue: 0,0:12:53.44,0:12:54.44,Default,NTP,0,0,0,,{\i1}Thôi kệ.{\i0}
Dialogue: 0,0:12:54.69,0:12:56.44,Default,NTP,0,0,0,,{\i1}Chả liên quan đến mình.{\i0}
Dialogue: 0,0:13:12.54,0:13:14.67,Default,NTP,0,0,0,,Đúng là tuổi trẻ!
Dialogue: 0,0:13:16.21,0:13:19.21,Default,NTP,0,0,0,,Em cũng quan tâm tới 2 bạn ấy phải không?
Dialogue: 0,0:13:19.67,0:13:21.38,Default,NTP,0,0,0,,Không, cũng không hẳn.
Dialogue: 0,0:13:23.05,0:13:27.64,Default,NTP,0,0,0,,Giờ mà có chút ẩu đả vào\N nữa thì càng đúng là tuổi trẻ.
Dialogue: 0,0:13:27.89,0:13:29.35,Default,NTP,0,0,0,,Thế à?
Dialogue: 0,0:13:30.68,0:13:33.60,Default,NTP,0,0,0,,2 bạn đó mà trụ được đến cuối cùng thì thật là tốt nhỉ?
Dialogue: 0,0:13:40.57,0:13:43.99,Default,NTP,0,0,0,,{\a6}{\i1}Chúng ta là tế bào T học việc!{\i0}
Dialogue: 0,0:13:41.44,0:13:42.32,Default,NTP,0,0,0,,Hataraku...
Dialogue: 0,0:13:42.32,0:13:43.99,Default,NTP,0,0,0,,...Saibou!
Dialogue: 0,0:13:46.11,0:13:47.66,Default,NTP,0,0,0,,Hataraku Saibou!
Dialogue: 0,0:13:51.24,0:13:54.25,Default,NTP,0,0,0,,{\i1}Chúng ta là tế bào T học việc!{\i0}
Dialogue: 0,0:13:54.50,0:13:57.46,Default,NTP,0,0,0,,{\i1}Không sợ hãi bất kì thứ gì!{\i0}
Dialogue: 0,0:13:57.88,0:14:00.88,Default,NTP,0,0,0,,{\i1}Chúng ta là tế bào T học việc!{\i0}
Dialogue: 0,0:14:01.13,0:14:02.84,Default,NTP,0,0,0,,{\i1}Giết hết vi rút và vi khuẩn!{\i0}
Dialogue: 0,0:14:03.88,0:14:07.30,Default,NTP,0,0,0,,Hôm nay chúng ta sẽ có bài kiểm tra đột xuất!
Dialogue: 0,0:14:08.85,0:14:11.01,Default,NTP,0,0,0,,Đừng có hoảng hết cả lên vậy!
Dialogue: 0,0:14:11.26,0:14:14.18,Default,NTP,0,0,0,,Kẻ địch bên ngoài cũng không\Nbáo trước rồi mới đến đâu!
Dialogue: 0,0:14:14.43,0:14:16.81,Default,NTP,0,0,0,,Những ai lười biếng luyện tập
Dialogue: 0,0:14:17.06,0:14:19.19,Default,NTP,0,0,0,,sẽ bị đá đít!
Dialogue: 0,0:14:22.69,0:14:26.65,Default,NTP,0,0,0,,Bây giờ các cậu sẽ tham gia\N quá trình chọn lọc dương tính!
Dialogue: 0,0:14:27.28,0:14:28.95,Default,NTP,0,0,0,,Luật vẫn như mọi khi!
Dialogue: 0,0:14:29.28,0:14:33.41,Default,NTP,0,0,0,,Chỉ cần tấn công tấm gỗ kháng\N nguyên trước khi hết thời gian!
Dialogue: 0,0:14:33.87,0:14:38.17,Default,NTP,0,0,0,,Hầu hết sẽ bị loại vào hôm nay!
Dialogue: 0,0:14:41.75,0:14:42.84,Default,NTP,0,0,0,,Không phải lo lắng!
Dialogue: 0,0:14:43.55,0:14:45.34,Default,NTP,0,0,0,,Cứ làm như mọi khi thôi!
Dialogue: 0,0:14:47.59,0:14:48.80,Default,NTP,0,0,0,,{\i1}Bắt đầu!{\i0}
Dialogue: 0,0:14:49.55,0:14:51.81,Default,NTP,0,0,0,,Cứ như mọi khi thôi nhỉ?
Dialogue: 0,0:14:52.06,0:14:53.26,Default,NTP,0,0,0,,Được rồi!
Dialogue: 0,0:14:54.77,0:14:56.23,Default,NTP,0,0,0,,Cái đầu đó...
Dialogue: 0,0:14:56.69,0:14:59.23,Default,NTP,0,0,0,,Phát hiện mục tiêu, hay lắm!
Dialogue: 0,0:15:00.48,0:15:02.44,Default,NTP,0,0,0,,Thế này là mình đỗ rồi!
Dialogue: 0,0:15:03.98,0:15:05.61,Default,NTP,0,0,0,,Đây là?
Dialogue: 0,0:15:05.61,0:15:06.99,Default,NTP,0,0,0,,Bị loại!
Dialogue: 0,0:15:07.24,0:15:10.57,Default,NTP,0,0,0,,Đó chỉ là một tế bào thông thường cầm hòn đá thôi!
Dialogue: 0,0:15:11.49,0:15:16.71,Default,NTP,0,0,0,,Sao chẳng giống mọi khi gì cả!
Dialogue: 0,0:15:21.79,0:15:25.21,Default,NTP,0,0,0,,Cứ tưởng vi rút hóa ra chỉ là\N một chiếc mũ màu mè thôi!
Dialogue: 0,0:15:25.21,0:15:26.97,Default,NTP,0,0,0,,{\i1}Bị loại!{\i0}
Dialogue: 0,0:15:38.94,0:15:41.44,Default,NTP,0,0,0,,Khá lắm, đỗ rồi!
Dialogue: 0,0:15:41.94,0:15:42.86,Default,NTP,0,0,0,,{\i1}Ra vậy.{\i0}
Dialogue: 0,0:15:43.31,0:15:45.98,Default,NTP,0,0,0,,{\i1}Có vẻ bài kiểm tra lần này sẽ khó hơn mọi khi.{\i0}
Dialogue: 0,0:15:48.11,0:15:51.57,Default,NTP,0,0,0,,{\i1}Không, chỉ là tế bào thường đội mũ tránh nạn thôi!{\i0}
Dialogue: 0,0:15:52.45,0:15:53.58,Default,NTP,0,0,0,,Ở đây!
Dialogue: 0,0:16:00.46,0:16:02.50,Default,NTP,0,0,0,,Đến lượt tôi à?
Dialogue: 0,0:16:04.96,0:16:08.21,Default,NTP,0,0,0,,Lúc tấn công cậu có thói quen vô thức nhắm mắt lại.
Dialogue: 0,0:16:08.59,0:16:10.72,Default,NTP,0,0,0,,Thế nên đương nhiên là không trúng rồi.
Dialogue: 0,0:16:10.97,0:16:14.72,Default,NTP,0,0,0,,Không được rời mắt đối tượng!
Dialogue: 0,0:16:19.98,0:16:22.06,Default,NTP,0,0,0,,Cậu ta...
Dialogue: 0,0:16:22.44,0:16:23.44,Default,NTP,0,0,0,,{\i1}Tiếp theo!{\i0}
Dialogue: 0,0:16:23.44,0:16:24.77,Default,NTP,0,0,0,,Vâng!
Dialogue: 0,0:16:29.49,0:16:32.53,Default,NTP,0,0,0,,{\i1}Run đến mức người thấy khó chịu rồi!{\i0}
Dialogue: 0,0:16:34.78,0:16:36.49,Default,NTP,0,0,0,,Đây rồi!
Dialogue: 0,0:16:36.49,0:16:39.41,Default,NTP,0,0,0,,Lúc tấn công cậu có thói quen vô thức nhắm mắt lại.
Dialogue: 0,0:16:39.66,0:16:40.62,Default,NTP,0,0,0,,Không được rời mắt đối tượng!
Dialogue: 0,0:16:42.25,0:16:43.75,Default,NTP,0,0,0,,Nguy hiểm quá!
Dialogue: 0,0:16:44.08,0:16:46.71,Default,NTP,0,0,0,,Chỉ là tế bào thường cầm cái cắm kim thôi!
Dialogue: 0,0:16:47.09,0:16:48.80,Default,NTP,0,0,0,,Bộ đồ khâu vá sao?
Dialogue: 0,0:16:49.30,0:16:51.55,Default,NTP,0,0,0,,Không bị lừa rồi à!
Dialogue: 0,0:16:51.76,0:16:53.09,Default,NTP,0,0,0,,Vâng!
Dialogue: 0,0:16:56.14,0:16:58.81,Default,NTP,0,0,0,,Thật là, cứ làm mình lo.
Dialogue: 0,0:16:59.68,0:17:03.06,Default,NTP,0,0,0,,Không còn nhiều thời gian đâu, luôn phải cẩn trọng!
Dialogue: 0,0:17:06.40,0:17:07.90,Default,NTP,0,0,0,,Đằng sau kìa, đằng sau!
Dialogue: 0,0:17:08.98,0:17:09.86,Default,NTP,0,0,0,,Cái gì?
Dialogue: 0,0:17:11.44,0:17:12.78,Default,NTP,0,0,0,,Bên phải, bên phải!
Dialogue: 0,0:17:12.78,0:17:14.66,Default,NTP,0,0,0,,Bên phải là bên nào?
Dialogue: 0,0:17:14.91,0:17:17.28,Default,NTP,0,0,0,,Đồ ngốc đừng có cuống lên thế, bên trái!
Dialogue: 0,0:17:17.28,0:17:18.41,Default,NTP,0,0,0,,- Phải!\N- Trái!
Dialogue: 0,0:17:18.41,0:17:19.95,Default,NTP,0,0,0,,- Phải!\N- Đằng sau!
Dialogue: 0,0:17:19.95,0:17:22.25,Default,NTP,0,0,0,,- Phải!\N- Đằng sau á? Ở đâu cơ?
Dialogue: 0,0:17:23.25,0:17:24.75,Default,NTP,0,0,0,,- Phải!\N- Trái!
Dialogue: 0,0:17:24.75,0:17:25.96,Default,NTP,0,0,0,,- Đằng sau!\N- Phải!
Dialogue: 0,0:17:25.96,0:17:27.50,Default,NTP,0,0,0,,- Trái!\N- Đằng sau!
Dialogue: 0,0:17:27.50,0:17:30.71,Default,NTP,0,0,0,,Hết thời gian rồi! Ở đằng sau ấy, đằng sau!
Dialogue: 0,0:17:32.55,0:17:37.18,Default,NTP,0,0,0,,Kháng Nguyên Đại Bộc Sát\NMộc Đoan Vi Trần Quyền!
Dialogue: 0,0:17:38.60,0:17:40.81,Default,NTP,0,0,0,,Sao cậu lại biết?
Dialogue: 0,0:17:41.52,0:17:42.93,Default,NTP,0,0,0,,Đỗ rồi!
Dialogue: 0,0:17:43.14,0:17:44.39,Default,NTP,0,0,0,,Chúc mừng!
Dialogue: 0,0:18:04.46,0:18:05.67,Default,NTP,0,0,0,,Phải mừng mới đúng chứ.
Dialogue: 0,0:18:06.08,0:18:07.46,Default,NTP,0,0,0,,Đỗ rồi còn gì.
Dialogue: 0,0:18:11.09,0:18:12.46,Default,NTP,0,0,0,,Chỉ là ăn may thôi.
Dialogue: 0,0:18:13.05,0:18:14.67,Default,NTP,0,0,0,,Nói thật tôi không có tí tự tin nào
Dialogue: 0,0:18:15.55,0:18:18.18,Default,NTP,0,0,0,,trong chuyện làm một tế bào T.
Dialogue: 0,0:18:26.85,0:18:28.44,Default,NTP,0,0,0,,Cậu thì sướng rồi.
Dialogue: 0,0:18:28.69,0:18:29.94,Default,NTP,0,0,0,,Không phải chỉ là T độc bình thường
Dialogue: 0,0:18:30.19,0:18:33.94,Default,NTP,0,0,0,,cậu còn được làm T hỗ trợ hoặc T điều hòa cơ mà.
Dialogue: 0,0:18:35.74,0:18:36.70,Default,NTP,0,0,0,,Nếu vậy thì,
Dialogue: 0,0:18:37.57,0:18:40.24,Default,NTP,0,0,0,,vẫn chưa quá muộn để xin rút đâu.
Dialogue: 0,0:18:40.83,0:18:42.33,Default,NTP,0,0,0,,Không có chút tự tin nào mà?
Dialogue: 0,0:18:42.74,0:18:44.08,Default,NTP,0,0,0,,Thế thì bỏ thôi?
Dialogue: 0,0:18:45.29,0:18:46.92,Default,NTP,0,0,0,,Nói gì hả?
Dialogue: 0,0:18:47.17,0:18:48.29,Default,NTP,0,0,0,,Nói cho mà biết,
Dialogue: 0,0:18:48.54,0:18:52.30,Default,NTP,0,0,0,,không ai thương hại khi cậu\N than thở khóc lóc nhảm nhí như vậy đâu,
Dialogue: 0,0:18:52.30,0:18:53.30,Default,NTP,0,0,0,,cả thế giới này...
Dialogue: 0,0:18:53.67,0:18:56.68,Default,NTP,0,0,0,,Xã hội này không ngọt ngào như vậy đâu!
Dialogue: 0,0:18:57.51,0:18:59.26,Default,NTP,0,0,0,,"Nhảm nhí" sao?
Dialogue: 0,0:19:00.43,0:19:03.56,Default,NTP,0,0,0,,Những kẻ thiên tài bẩm sinh như cậu...
Dialogue: 0,0:19:05.23,0:19:07.56,Default,NTP,0,0,0,,làm sao mà hiểu được cảm giác của tôi chứ!
Dialogue: 0,0:19:07.56,0:19:09.02,Default,NTP,0,0,0,,Thằng ngốc này!
Dialogue: 0,0:19:18.36,0:19:20.45,Default,NTP,0,0,0,,Nếu đã chịu đối diện với điểm yếu của mình,
Dialogue: 0,0:19:20.70,0:19:22.58,Default,NTP,0,0,0,,thì liệu mà hạ quyết tâm đi chứ!
Dialogue: 0,0:19:23.12,0:19:24.49,Default,NTP,0,0,0,,Mở to mắt ra mà nhìn.
Dialogue: 0,0:19:25.41,0:19:27.41,Default,NTP,0,0,0,,Cậu chỉ còn cách phải cố gắng thôi!
Dialogue: 0,0:19:28.04,0:19:29.75,Default,NTP,0,0,0,,Dù trông có vẻ như chỉ là sự cố gắng thừa thãi,
Dialogue: 0,0:19:30.00,0:19:33.42,Default,NTP,0,0,0,,nhưng chẳng phải quyết tâm vươn lên chính\N là phẩm chất quý báu của cậu sao?
Dialogue: 0,0:19:37.88,0:19:39.97,Default,NTP,0,0,0,,Cậu...
Dialogue: 0,0:19:49.14,0:19:51.98,Default,NTP,0,0,0,,Hiểu rồi, cậu sẽ không thấy tôi than thở nữa!
Dialogue: 0,0:19:52.44,0:19:53.48,Default,NTP,0,0,0,,Ừ.
Dialogue: 0,0:19:54.07,0:19:55.90,Default,NTP,0,0,0,,Tôi sẽ mạnh mẽ lên.
Dialogue: 0,0:19:56.65,0:20:00.16,Default,NTP,0,0,0,,Tôi sẽ rèn luyện tới khi đánh bại\N mọi đối thủ bằng một cú đấm!
Dialogue: 0,0:20:00.82,0:20:04.08,Default,NTP,0,0,0,,Rèn luyện cơ bắp hơn nữa, cố gắng hơn nữa!
Dialogue: 0,0:20:05.79,0:20:11.21,Default,NTP,0,0,0,,Đúng vậy, tôi cũng đã biết\N mình nên làm gì sau này rồi.
Dialogue: 0,0:20:11.62,0:20:17.09,Default,NTP,0,0,0,,Có vẻ những kẻ vụng về như cậu cần ai đó chỉ dẫn.
Dialogue: 0,0:20:18.92,0:20:22.51,Default,NTP,0,0,0,,Cậu mà làm tư lệnh thì liệu mà nhẹ tay chút đi,
Dialogue: 0,0:20:22.76,0:20:24.47,Default,NTP,0,0,0,,không bọn cấp dưới sợ vãi ra quần mất.
Dialogue: 0,0:20:26.56,0:20:30.14,Default,NTP,0,0,0,,Không cần cậu nói tôi cũng biết!
Dialogue: 0,0:20:30.39,0:20:33.56,Default,NTP,0,0,0,,Chỉ cần thoải mái hơn chút là được phải\N không nào, để tôi làm cho mà xem.
Dialogue: 0,0:20:33.81,0:20:34.98,Default,NTP,0,0,0,,Cái gì tôi chẳng làm được!
Dialogue: 0,0:20:39.90,0:20:41.40,Default,NTP,0,0,0,,Hai tên ngốc này...
Dialogue: 0,0:20:50.37,0:20:52.29,Default,NTP,0,0,0,,Chuyện là như thế đó.
Dialogue: 0,0:20:54.25,0:20:55.25,Default,NTP,0,0,0,,Chết tiệt!
Dialogue: 0,0:20:55.50,0:20:58.76,Default,NTP,0,0,0,,Bọn tôi phải oằn người ra còn \Ncậu thì suốt ngày uống trà thôi!
Dialogue: 0,0:20:58.76,0:21:02.68,Default,NTP,0,0,0,,Thì công việc bàn giấy vất vả lắm!
Dialogue: 0,0:21:02.93,0:21:06.22,Default,NTP,0,0,0,,Suốt ngày càu nhàu thế? Đổi việc không?
Dialogue: 0,0:21:06.72,0:21:10.06,Default,NTP,0,0,0,,Hai người đó mà cũng có lúc vậy sao?
Dialogue: 0,0:21:10.81,0:21:13.19,Default,NTP,0,0,0,,Mấy cậu xem gì nãy giờ thế?
Dialogue: 0,0:21:17.07,0:21:17.82,Default,NTP,0,0,0,,Này!
Dialogue: 0,0:21:18.98,0:21:20.36,Default,NTP,0,0,0,,Ảnh gì thế?
Dialogue: 0,0:21:29.87,0:21:33.04,Default,NTP,0,0,0,,Đưa mấy tấm ảnh đó đây. Tôi phải đi đốt ngay!
Dialogue: 0,0:21:33.29,0:21:37.13,Default,NTP,0,0,0,,Cậu làm gì thế hả, tài liệu quan trọng đấy!
Dialogue: 0,0:21:37.13,0:21:40.55,Default,NTP,0,0,0,,Làm ơn đó, hãy đốt nó đi!
Dialogue: 0,0:21:46.47,0:21:47.30,Default,NTP,0,0,0,,Xin lỗi mà!
Dialogue: 0,0:21:48.26,0:21:51.27,Default,NTP,0,0,0,,{\i1}Đúng là 2 tên ngốc.{\i0}
Dialogue: 0,0:21:51.60,0:21:53.94,Default,NTP,0,0,0,,{\i1}Chẳng thay đổi gì từ hồi đó tới giờ.{\i0}
Dialogue: 0,0:23:27.11,0:23:28.20,Default,NTP,0,0,0,,Là người đeo mặt nạ kìa!
Dialogue: 0,0:23:29.61,0:23:31.28,Default,NTP,0,0,0,,Tập tiếp theo
Dialogue: 0,0:23:31.28,0:23:32.58,Default,NTP,0,0,0,,"Tụ cầu vàng"!
Dialogue: 0,0:23:33.83,0:23:34.95,Default,NTP,0,0,0,,Anh ấy bảo mọi người nhớ xem nhé.
Dialogue: 0,0:23:36.00,0:23:38.21,Default,NTP,0,0,0,,Chắc thế!
Dialogue: 0,0:23:36.00,0:23:40.00,Default,NTP,0,0,0,,{\an8}Tập 10:\NTụ cầu vàng

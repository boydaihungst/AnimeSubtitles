Dialogue: 0,0:00:02.16,0:00:07.96,Opviet,,0000,0000,0000,,Sự dễ thương không thể diễn đạt được bằng lời
Dialogue: 0,0:00:20.43,0:00:26.23,Opviet,,0000,0000,0000,,cuối cùng thì "những quả dâu mềm" đã tới
Dialogue: 0,0:00:26.23,0:00:31.28,Opviet,,0000,0000,0000,,những âm thanh phiền phức cũng đến theo
Dialogue: 0,0:00:32.44,0:00:38.19,Opviet,,0000,0000,0000,,cuối cùng thì "những quả dâu mềm" đã tới
Dialogue: 0,0:00:38.24,0:00:43.12,Opviet,,0000,0000,0000,,chúng ngập tràn hương vị ngọt ngào
Dialogue: 0,0:00:43.60,0:00:49.59,Opviet,,0000,0000,0000,,màu sắc không hợp cũng không vấn đề gì,
Dialogue: 0,0:00:49.61,0:00:53.01,Opviet,,0000,0000,0000,,mắc phải lỗi lầm là điều tự nhiên thôi,
Dialogue: 0,0:00:53.12,0:00:57.68,Opviet,,0000,0000,0000,,vậy nên hãy tha thứ cho bọn em
Dialogue: 0,0:00:59.01,0:01:02.18,Opviet,,0000,0000,0000,,đừng nổi giận, đừng nổi giận,
Dialogue: 0,0:01:02.31,0:01:04.94,Opviet,,0000,0000,0000,,hãy để bọn em làm theo ý thích!
Dialogue: 0,0:01:04.98,0:01:07.60,Opviet,,0000,0000,0000,,người lớn sẽ không hiểu được
Dialogue: 0,0:01:07.69,0:01:10.62,Opviet,,0000,0000,0000,,sự tuyệt vời, kỳ diệu, và hoàn hảo
Dialogue: 0,0:01:10.98,0:01:16.74,Opviet,,0000,0000,0000,,sự dễ thương không thể diễn đạt được bằng lời
Dialogue: 0,0:01:16.75,0:01:20.24,Opviet,,0000,0000,0000,,không có lý do gì ẩn chứa đằng sau những nụ cười
Dialogue: 0,0:01:20.28,0:01:24.45,Opviet,,0000,0000,0000,,những cô bé giống như dâu tây
Dialogue: 0,0:22:15.68,0:22:26.19,EDviệt,,0000,0000,0000,,Nếu tôi hồi tưởng lại giấc mơ đã quên lãng,
Dialogue: 0,0:22:27.17,0:22:37.10,EDviệt,,0000,0000,0000,,nụ cười của bạn sẽ vô tình băng qua tâm trí tôi
Dialogue: 0,0:22:37.85,0:22:43.91,EDviệt,,0000,0000,0000,,tôi không biết ngày mai sẽ ra sao
Dialogue: 0,0:22:43.91,0:22:49.88,EDviệt,,0000,0000,0000,,nè, hãy ghi nhớ những ngày chúng ta vui đùa bên nhau
Dialogue: 0,0:22:49.98,0:23:03.87,EDviệt,,0000,0000,0000,,con đường là dốc đứng , nhưng chúng ta vẫn chạy như không.
Dialogue: 0,0:23:03.87,0:23:07.07,EDviệt,,0000,0000,0000,,chạy vội vã, vội vã
Dialogue: 0,0:23:07.12,0:23:14.97,EDviệt,,0000,0000,0000,,lời thì thầm của bạn vào tai tôi, rủ tôi cùng nghịch bóng.
Dialogue: 0,0:23:15.24,0:23:23.54,EDviệt,,0000,0000,0000,,váy chúng tôi lắc lư theo hơi lạnh lướt qua đầu gối
Dialogue: 0,0:23:23.92,0:23:33.89,EDviệt,,0000,0000,0000,,Nè, giờ bạn đang ở đâu rồi!
Dialogue: 0,0:01:48.78,0:01:52.68,ichigo,,0000,0000,0000,,110 yên, 1, 2, 3 yên.
Dialogue: 0,0:01:54.51,0:01:55.91,ichigo,,0000,0000,0000,,Nè...
Dialogue: 0,0:01:56.35,0:01:58.51,ichigo,,0000,0000,0000,,có trò gì vui và lạ để chơi không?
Dialogue: 0,0:01:58.65,0:01:59.91,ichigo,,0000,0000,0000,,Tớ không biết.
Dialogue: 0,0:02:00.22,0:02:02.09,ichigo,,0000,0000,0000,,Thế này chán quá.
Dialogue: 0,0:02:16.80,0:02:17.13,ichigo,,0000,0000,0000,,Chi?
Dialogue: 0,0:02:17.13,0:02:18.20,ichigo,,0000,0000,0000,,Hử?
Dialogue: 0,0:02:19.01,0:02:19.47,ichigo,,0000,0000,0000,,Chìa khoá.
Dialogue: 0,0:02:19.47,0:02:20.70,ichigo,,0000,0000,0000,,Này.
Dialogue: 0,0:02:20.97,0:02:22.41,ichigo,,0000,0000,0000,,Chỉ mua thuốc lá thôi mà.
Dialogue: 0,0:02:22.48,0:02:24.50,ichigo,,0000,0000,0000,,Không có "chỉ" gì hết...
Dialogue: 0,0:02:25.98,0:02:28.54,ichigo,,0000,0000,0000,,Ờ, vậy chị không mua gì đâu, đưa 2000 yên đây.
Dialogue: 0,0:02:28.65,0:02:31.28,ichigo,,0000,0000,0000,,Trả em tiền chị mượn lần trước đã.
Dialogue: 0,0:02:40.66,0:02:43.89,ichigo,,0000,0000,0000,,Chi, em xài tiền trong heo đất vào việc riêng phải không?
Dialogue: 0,0:02:44.23,0:02:46.56,ichigo,,0000,0000,0000,,Là heo đất của em!
Dialogue: 0,0:02:46.63,0:02:48.90,ichigo,,0000,0000,0000,,Em dùng tiền để mua CD.
Dialogue: 0,0:02:50.10,0:02:51.83,ichigo,,0000,0000,0000,,Cơn khủng hoảng lại đến rồi.
Dialogue: 0,0:02:51.91,0:02:54.90,ichigo,,0000,0000,0000,,Đừng có cầm heo đất của người ta mà càu nhàu.
Dialogue: 0,0:02:55.41,0:02:56.57,ichigo,,0000,0000,0000,,Em mượn, em mượn!
Dialogue: 0,0:03:02.22,0:03:03.31,ichigo,,0000,0000,0000,,Này, này.
Dialogue: 0,0:03:09.69,0:03:12.35,ichigo,,0000,0000,0000,,Đồ bần cùng ngu ngốc!
Dialogue: 0,0:03:18.47,0:03:20.09,ichigo,,0000,0000,0000,,Vĩnh biệt nghèo nàn!
Dialogue: 0,0:03:20.63,0:03:22.00,ichigo,,0000,0000,0000,,Này, làm cái quái gì thế hả?
Dialogue: 0,0:03:31.65,0:03:34.71,ichigo,,0000,0000,0000,,Chả có việc làm thêm nào ngon lành.
Dialogue: 0,0:03:34.95,0:03:35.92,ichigo,,0000,0000,0000,,Onee-chan!
Dialogue: 0,0:03:36.18,0:03:37.15,ichigo,,0000,0000,0000,,Gì?
Dialogue: 0,0:03:37.22,0:03:38.81,ichigo,,0000,0000,0000,,Không có việc nào cho mười hai tuổi hoặc nhỏ hơn.
Dialogue: 0,0:03:38.89,0:03:39.85,ichigo,,0000,0000,0000,,Sao mà có được...
Dialogue: 0,0:03:42.16,0:03:43.68,ichigo,,0000,0000,0000,,Mấy đứa may lắm đấy.
Dialogue: 0,0:03:44.32,0:03:46.52,ichigo,,0000,0000,0000,,Mấy đứa chỉ việc tới trường và chơi đùa.
Dialogue: 0,0:03:46.69,0:03:49.03,ichigo,,0000,0000,0000,,Chị thậm chí còn tự trả tiền xăng.
Dialogue: 0,0:03:49.10,0:03:50.69,ichigo,,0000,0000,0000,,Ai lười hả?
Dialogue: 0,0:03:50.86,0:03:52.46,ichigo,,0000,0000,0000,,Có ai nói đâu.
Dialogue: 0,0:03:52.73,0:03:53.89,ichigo,,0000,0000,0000,,Này, cậu kia.
Dialogue: 0,0:03:54.53,0:03:56.26,ichigo,,0000,0000,0000,,Đi rót tớ ít trà.
Dialogue: 0,0:03:56.80,0:03:58.20,ichigo,,0000,0000,0000,,Đ-Được.
Dialogue: 0,0:04:02.61,0:04:04.20,ichigo,,0000,0000,0000,,Cậu ta đi thật.
Dialogue: 0,0:04:05.05,0:04:08.81,ichigo,,0000,0000,0000,,Chả có việc làm thêm nào giờ giấc linh động và trả công...
Dialogue: 0,0:04:08.88,0:04:10.47,ichigo,,0000,0000,0000,,khoảng 20,000 yên một giờ nhỉ.
Dialogue: 0,0:04:10.92,0:04:12.41,ichigo,,0000,0000,0000,,Có nè.
Dialogue: 0,0:04:12.49,0:04:14.15,ichigo,,0000,0000,0000,,Quán rượu Kannaduki.
Dialogue: 0,0:04:14.29,0:04:17.31,ichigo,,0000,0000,0000,,Đơn giản chỉ là nói chuyện với mấy gã đàn ông.
Dialogue: 0,0:04:20.09,0:04:21.15,ichigo,,0000,0000,0000,,Không làm hả?
Dialogue: 0,0:04:21.53,0:04:23.00,ichigo,,0000,0000,0000,,Chị đập em giờ.
Dialogue: 0,0:04:24.56,0:04:25.72,ichigo,,0000,0000,0000,,Russian...
Dialogue: 0,0:04:28.70,0:04:29.53,ichigo,,0000,0000,0000,,Của chị đây.
Dialogue: 0,0:04:29.60,0:04:31.50,ichigo,,0000,0000,0000,,À, cảm ơn em.
Dialogue: 0,0:04:33.64,0:04:35.40,ichigo,,0000,0000,0000,,Làm ca cuối tuần hả?
Dialogue: 0,0:04:35.64,0:04:37.08,ichigo,,0000,0000,0000,,Cuối tuần mình chả muốn làm việc.
Dialogue: 0,0:04:37.78,0:04:40.34,ichigo,,0000,0000,0000,,Chị ghét làm thêm thế à?
Dialogue: 0,0:04:40.68,0:04:42.91,ichigo,,0000,0000,0000,,Tớ hơi hứng thú với vụ này.
Dialogue: 0,0:04:43.22,0:04:46.91,ichigo,,0000,0000,0000,,Nếu sẽ làm, cậu muốn làm thêm loại việc nào?
Dialogue: 0,0:04:47.12,0:04:48.09,ichigo,,0000,0000,0000,,Hả?
Dialogue: 0,0:04:48.56,0:04:52.16,ichigo,,0000,0000,0000,,Tớ... Tớ không làm việc nhanh được...
Dialogue: 0,0:04:52.16,0:04:54.52,ichigo,,0000,0000,0000,,...vì thế tớ sẽ chọn việc nào làm chậm rãi thôi.
Dialogue: 0,0:04:55.10,0:04:56.06,ichigo,,0000,0000,0000,,Um...
Dialogue: 0,0:04:57.80,0:04:59.03,ichigo,,0000,0000,0000,,Chậm rãi...
Dialogue: 0,0:04:59.10,0:05:03.90,ichigo,,0000,0000,0000,,Khoai nướng đây... khoai nướng...
Dialogue: 0,0:05:04.57,0:05:08.03,ichigo,,0000,0000,0000,,Cảm ơn sự ủng hộ của các bạn.
Dialogue: 0,0:05:08.14,0:05:10.58,ichigo,,0000,0000,0000,,Chúng tôi sẽ đi chầm chậm.
Dialogue: 0,0:05:11.18,0:05:12.01,ichigo,,0000,0000,0000,,Giống không?
Dialogue: 0,0:05:12.08,0:05:13.60,ichigo,,0000,0000,0000,,Tớ không nghĩ thế.
Dialogue: 0,0:05:13.81,0:05:15.11,ichigo,,0000,0000,0000,,Vậy cậu thì sao?
Dialogue: 0,0:05:15.18,0:05:17.58,ichigo,,0000,0000,0000,,Ể? Để xem...
Dialogue: 0,0:05:18.25,0:05:20.34,ichigo,,0000,0000,0000,,Tớ sẽ đi phát tờ rơi hoặc tương tự thế là được.
Dialogue: 0,0:05:20.42,0:05:22.29,ichigo,,0000,0000,0000,,"Tương tự thế là được"?
Dialogue: 0,0:05:23.26,0:05:24.52,ichigo,,0000,0000,0000,,Đừng coi thường!
Dialogue: 0,0:05:24.59,0:05:27.46,ichigo,,0000,0000,0000,,Cậu không biết phát tờ rơi khó thế nào đâu.
Dialogue: 0,0:05:28.09,0:05:29.72,ichigo,,0000,0000,0000,,Thử tớ xem.
Dialogue: 0,0:05:38.71,0:05:40.23,ichigo,,0000,0000,0000,,Tôi không xem chỉ tay.
Dialogue: 0,0:06:23.71,0:06:24.80,ichigo,,0000,0000,0000,,Đỡ này!
Dialogue: 0,0:06:26.39,0:06:28.12,ichigo,,0000,0000,0000,,Đau quá!
Dialogue: 0,0:06:28.32,0:06:30.31,ichigo,,0000,0000,0000,,Hai đứa làm trò khỉ gì thế hả?
Dialogue: 0,0:06:30.39,0:06:32.19,ichigo,,0000,0000,0000,,Do Micchan đấy.
Dialogue: 0,0:06:32.26,0:06:33.59,ichigo,,0000,0000,0000,,Cả hai đứa đều tham gia.
Dialogue: 0,0:06:33.66,0:06:37.36,ichigo,,0000,0000,0000,,Nè, Ana-chan cậu muốn làm loại việc làm thêm nào?
Dialogue: 0,0:06:37.90,0:06:41.80,ichigo,,0000,0000,0000,,Vài ngày trước mình đã làm rồi, vì bố có nhờ mình giúp.
Dialogue: 0,0:06:42.60,0:06:44.50,ichigo,,0000,0000,0000,,Thật à? Việc gì thế?
Dialogue: 0,0:06:44.90,0:06:46.80,ichigo,,0000,0000,0000,,Lụm banh tennis.
Dialogue: 0,0:06:47.24,0:06:48.21,ichigo,,0000,0000,0000,,Ể...
Dialogue: 0,0:06:48.34,0:06:51.50,ichigo,,0000,0000,0000,,Nhìn thì đơn giản, nhưng cũng cần vài kỹ năng đấy.
Dialogue: 0,0:06:51.81,0:06:54.97,ichigo,,0000,0000,0000,,Ể... đúng là người Anh có khác.
Dialogue: 0,0:06:57.48,0:06:58.85,ichigo,,0000,0000,0000,,Lụm banh hả...
Dialogue: 0,0:06:59.02,0:06:59.99,ichigo,,0000,0000,0000,,Oh!
Dialogue: 0,0:07:00.05,0:07:01.02,ichigo,,0000,0000,0000,,Ô.
Dialogue: 0,0:07:06.39,0:07:09.36,ichigo,,0000,0000,0000,,Trước sau cũng không thể so với năng lực của hoang dã được.
Dialogue: 0,0:07:09.80,0:07:11.49,ichigo,,0000,0000,0000,,Nó đang ảo tưởng cái gì thế?
Dialogue: 0,0:07:11.60,0:07:12.53,ichigo,,0000,0000,0000,,Không biết được.
Dialogue: 0,0:07:12.67,0:07:13.63,ichigo,,0000,0000,0000,,Um...
Dialogue: 0,0:07:14.37,0:07:17.34,ichigo,,0000,0000,0000,,Vậy cậu thích làm loại việc nào, Miu-chan?
Dialogue: 0,0:07:17.87,0:07:18.86,ichigo,,0000,0000,0000,,Làm bác sĩ.
Dialogue: 0,0:07:18.94,0:07:21.17,ichigo,,0000,0000,0000,,Không, đó có phải là làm thêm đâu.
Dialogue: 0,0:07:24.04,0:07:27.64,ichigo,,0000,0000,0000,,Vậy, hôm nay cô bị gì?
Dialogue: 0,0:07:28.41,0:07:29.90,ichigo,,0000,0000,0000,,Tiếp hả?
Dialogue: 0,0:07:33.95,0:07:34.92,ichigo,,0000,0000,0000,,Tốt.
Dialogue: 0,0:07:35.05,0:07:36.85,ichigo,,0000,0000,0000,,Đừng có trang bị cho họ.
Dialogue: 0,0:07:37.52,0:07:41.36,ichigo,,0000,0000,0000,,Này, này, bệnh nhân không nên cử động mạnh.
Dialogue: 0,0:07:41.63,0:07:43.36,ichigo,,0000,0000,0000,,Tớ là bệnh nhân?
Dialogue: 0,0:07:43.93,0:07:45.45,ichigo,,0000,0000,0000,,Hôm nay cô thấy sao?
Dialogue: 0,0:07:46.83,0:07:48.66,ichigo,,0000,0000,0000,,Ừm, để coi...
Dialogue: 0,0:07:48.74,0:07:53.43,ichigo,,0000,0000,0000,,Nước mũi chảy liên tục và đầu óc choáng váng.
Dialogue: 0,0:07:53.94,0:07:56.34,ichigo,,0000,0000,0000,,Tôi hiểu, chắc khổ sở lắm.
Dialogue: 0,0:08:01.75,0:08:03.08,ichigo,,0000,0000,0000,,Cảm lạnh đấy.
Dialogue: 0,0:08:03.15,0:08:04.88,ichigo,,0000,0000,0000,,Cái khoảng trống vừa rồi là sao?
Dialogue: 0,0:08:04.95,0:08:08.25,ichigo,,0000,0000,0000,,Tớ nghĩ cậu sẽ nói gì đó sau câu "chắc khổ sở lắm" chứ.
Dialogue: 0,0:08:08.69,0:08:10.88,ichigo,,0000,0000,0000,,Dường như cô còn tỉnh táo quá ha.
Dialogue: 0,0:08:11.39,0:08:12.88,ichigo,,0000,0000,0000,,Sakuragi-kun, ghi vào!
Dialogue: 0,0:08:13.79,0:08:16.06,ichigo,,0000,0000,0000,,Matsuri-chan, cậu không cần phải chơi cùng đâu.
Dialogue: 0,0:08:16.33,0:08:18.06,ichigo,,0000,0000,0000,,Mình thì sao?
Dialogue: 0,0:08:18.26,0:08:19.75,ichigo,,0000,0000,0000,,Cậu đến đây.
Dialogue: 0,0:08:22.70,0:08:24.10,ichigo,,0000,0000,0000,,Cậu làm gì vậy?
Dialogue: 0,0:08:24.64,0:08:26.23,ichigo,,0000,0000,0000,,Không có gì, kiểm tra sức khoẻ thôi.
Dialogue: 0,0:08:26.31,0:08:26.86,ichigo,,0000,0000,0000,,Hả?
Dialogue: 0,0:08:26.94,0:08:29.07,ichigo,,0000,0000,0000,,Đừng nói mấy chuyện dễ gây hiểu nhầm.
Dialogue: 0,0:08:29.41,0:08:31.17,ichigo,,0000,0000,0000,,Và chị đang nhìn cái gì thế?
Dialogue: 0,0:08:31.24,0:08:32.51,ichigo,,0000,0000,0000,,À... không có gì.
Dialogue: 0,0:08:32.58,0:08:35.91,ichigo,,0000,0000,0000,,Này, này, bệnh nhân không nên la lối.
Dialogue: 0,0:08:36.82,0:08:38.58,ichigo,,0000,0000,0000,,Cho xem bụng nào.
Dialogue: 0,0:08:39.72,0:08:40.78,ichigo,,0000,0000,0000,,Nào, làm đi.
Dialogue: 0,0:08:43.36,0:08:45.76,ichigo,,0000,0000,0000,,Hmm... tôi hiểu.
Dialogue: 0,0:08:48.80,0:08:50.29,ichigo,,0000,0000,0000,,Cậu bị phình bụng.
Dialogue: 0,0:08:50.36,0:08:51.89,ichigo,,0000,0000,0000,,Muốn ăn đạp hả?
Dialogue: 0,0:08:52.20,0:08:53.76,ichigo,,0000,0000,0000,,Sakuragi-kun, đưa cái biểu đồ.
Dialogue: 0,0:08:54.07,0:08:55.36,ichigo,,0000,0000,0000,,V-Vâng.
Dialogue: 0,0:08:59.41,0:09:01.94,ichigo,,0000,0000,0000,,Tôi nói này, nó thành "ga" hết cả rồi.
Dialogue: 0,0:09:02.44,0:09:03.60,ichigo,,0000,0000,0000,,Cái gì?
Dialogue: 0,0:09:03.81,0:09:06.78,ichigo,,0000,0000,0000,,Không, nó đã trở thành "ga" rất tinh vi...
Dialogue: 0,0:09:07.58,0:09:09.17,ichigo,,0000,0000,0000,,Cái quái gì thế?
Dialogue: 0,0:09:12.12,0:09:13.11,ichigo,,0000,0000,0000,,Là cảm lạnh.
Dialogue: 0,0:09:13.19,0:09:14.92,ichigo,,0000,0000,0000,,Không phải từ đầu cậu đã nói rồi sao!
Dialogue: 0,0:09:14.99,0:09:17.58,ichigo,,0000,0000,0000,,Đừng có trẻ con thế! Chỉ cảm xoàng thôi!
Dialogue: 0,0:09:21.66,0:09:23.13,ichigo,,0000,0000,0000,,A, nó đi rồi.
Dialogue: 0,0:09:23.83,0:09:25.73,ichigo,,0000,0000,0000,,Có chuyện gì à?
Dialogue: 0,0:09:25.80,0:09:26.96,ichigo,,0000,0000,0000,,Em đó.
Dialogue: 0,0:09:27.77,0:09:30.07,ichigo,,0000,0000,0000,,Lần này chị làm bác sĩ đi.
Dialogue: 0,0:09:30.11,0:09:31.03,ichigo,,0000,0000,0000,,Hả?
Dialogue: 0,0:09:31.60,0:09:33.00,ichigo,,0000,0000,0000,,Điên khùng quá đấy.
Dialogue: 0,0:09:33.54,0:09:34.80,ichigo,,0000,0000,0000,,Gọi tên bọn em đi.
Dialogue: 0,0:09:40.15,0:09:42.21,ichigo,,0000,0000,0000,,Matsuoka-san, xin mời vào.
Dialogue: 0,0:09:44.45,0:09:47.28,ichigo,,0000,0000,0000,,Em ổn chứ, vợ yêu?
Dialogue: 0,0:09:44.45,0:09:47.28,nói trùng,,0000,0000,0000,,Ô... ô em cảm thấy nó sắp ra rồi...
Dialogue: 0,0:09:47.35,0:09:48.91,ichigo,,0000,0000,0000,,Chuyện gì thế?
Dialogue: 0,0:09:48.99,0:09:50.46,ichigo,,0000,0000,0000,,Tệ rồi bác sĩ ơi.
Dialogue: 0,0:09:48.99,0:09:50.46,nói trùng,,0000,0000,0000,,Em bé...
Dialogue: 0,0:09:50.52,0:09:51.59,ichigo,,0000,0000,0000,,Dường như cô ấy sắp sinh rồi.
Dialogue: 0,0:09:51.55,0:09:53.19,nói trùng,,0000,0000,0000,,Đây là khoa sản à?
Dialogue: 0,0:09:53.29,0:09:54.59,ichigo,,0000,0000,0000,,Bác sĩ, xin làm ơn!
Dialogue: 0,0:09:54.59,0:09:55.49,ichigo,,0000,0000,0000,,Để đó cho tôi.
Dialogue: 0,0:09:55.56,0:09:56.09,ichigo,,0000,0000,0000,,Cho cô ấy nằm xuống.
Dialogue: 0,0:09:56.09,0:09:57.50,ichigo,,0000,0000,0000,,Vâng.
Dialogue: 0,0:09:59.60,0:10:01.59,ichigo,,0000,0000,0000,,Họ định tiếp tục đến bao lâu nữa đây?
Dialogue: 0,0:10:03.44,0:10:04.60,ichigo,,0000,0000,0000,,Chậc.
Dialogue: 0,0:10:05.00,0:10:08.67,ichigo,,0000,0000,0000,,Do mấy đứa này mà mình vẫn chưa kiếm ra việc.
Dialogue: 0,0:10:11.41,0:10:15.07,ichigo,,0000,0000,0000,,Việc gì cũng được, miễn là trả tiền theo giờ cao một chút.
Dialogue: 0,0:10:27.73,0:10:29.19,ichigo,,0000,0000,0000,,Không hút thuốc.
Dialogue: 0,0:10:40.81,0:10:44.54,ichigo,,0000,0000,0000,,Loại đồng phục đó thật đáng xấu hổ, mình không mặc được.
Dialogue: 0,0:10:45.55,0:10:47.48,ichigo,,0000,0000,0000,,Cho dù tiền công có cao cỡ nào...
Dialogue: 0,0:11:05.57,0:11:07.16,ichigo,,0000,0000,0000,,Hoan nghênh.
Dialogue: 0,0:11:07.53,0:11:09.36,ichigo,,0000,0000,0000,,Cảm ơn rất nhiều ạ.
Dialogue: 0,0:11:10.07,0:11:11.90,ichigo,,0000,0000,0000,,Chúc quý khách vui vẻ ạ.
Dialogue: 0,0:11:17.94,0:11:22.38,ichigo,,0000,0000,0000,,Cấm hút thuốc trong khi làm việc đấy, Nobu-chan.
Dialogue: 0,0:11:22.62,0:11:24.24,ichigo,,0000,0000,0000,,Ế, vậy à?
Dialogue: 0,0:11:26.12,0:11:27.42,ichigo,,0000,0000,0000,,Cẩn thận nhé.
Dialogue: 0,0:11:40.47,0:11:42.06,ichigo,,0000,0000,0000,,Con không cần phải vội.
Dialogue: 0,0:11:49.21,0:11:50.23,ichigo,,0000,0000,0000,,Nè mẹ...
Dialogue: 0,0:11:50.31,0:11:52.64,ichigo,,0000,0000,0000,,Rất tốt cho sức khoẻ đấy, ăn hết đi nào.
Dialogue: 0,0:12:00.02,0:12:01.11,ichigo,,0000,0000,0000,,Xin lỗi!
Dialogue: 0,0:12:01.32,0:12:02.29,ichigo,,0000,0000,0000,,Vâng?
Dialogue: 0,0:12:03.59,0:12:05.96,ichigo,,0000,0000,0000,,Xin lỗi, tôi muốn gọi thêm món này.
Dialogue: 0,0:12:06.03,0:12:06.99,ichigo,,0000,0000,0000,,Vâng.
Dialogue: 0,0:12:09.66,0:12:11.86,ichigo,,0000,0000,0000,,Coi kìa, đó là thói xấu đấy.
Dialogue: 0,0:12:12.17,0:12:14.16,ichigo,,0000,0000,0000,,Chừng nào con chưa ăn xong thì chúng ta sẽ không về đâu!
Dialogue: 0,0:12:28.68,0:12:30.44,ichigo,,0000,0000,0000,,Con ăn rồi, về thôi mẹ ơi.
Dialogue: 0,0:12:32.32,0:12:34.15,ichigo,,0000,0000,0000,,Cảm ơn rất nhiều ạ!
Dialogue: 0,0:12:36.29,0:12:37.85,ichigo,,0000,0000,0000,,Muốn đổi việc sổ sách với tôi không?
Dialogue: 0,0:12:37.92,0:12:39.02,ichigo,,0000,0000,0000,,Không sao.
Dialogue: 0,0:12:39.09,0:12:41.29,ichigo,,0000,0000,0000,,Cô có thấy khách hàng vừa rồi không?
Dialogue: 0,0:12:41.36,0:12:44.33,ichigo,,0000,0000,0000,,Một cô bé ngoại quốc dễ thương cực kì.
Dialogue: 0,0:12:44.63,0:12:47.16,ichigo,,0000,0000,0000,,Ế, tôi chả để ý.
Dialogue: 0,0:12:47.90,0:12:49.06,ichigo,,0000,0000,0000,,Chả sao,
Dialogue: 0,0:12:49.14,0:12:52.00,ichigo,,0000,0000,0000,,có một bé ngoại quốc dễ thương cũng tới nhà tôi hoài.
Dialogue: 0,0:12:52.34,0:12:53.27,ichigo,,0000,0000,0000,,Thuốc lá.
Dialogue: 0,0:12:53.34,0:12:54.31,ichigo,,0000,0000,0000,,Hửm?
Dialogue: 0,0:12:54.57,0:12:55.54,ichigo,,0000,0000,0000,,Ồ.
Dialogue: 0,0:12:58.01,0:12:59.81,ichigo,,0000,0000,0000,,Mình không nhầm vào đâu được.
Dialogue: 0,0:13:00.18,0:13:01.65,ichigo,,0000,0000,0000,,Onee-chan?
Dialogue: 0,0:13:01.71,0:13:03.77,ichigo,,0000,0000,0000,,Đang bận váy ngắn trong một nhà hàng gia đình?
Dialogue: 0,0:13:06.22,0:13:08.92,ichigo,,0000,0000,0000,,Đầu tiên mình cũng chả tin đâu.
Dialogue: 0,0:13:09.12,0:13:12.61,ichigo,,0000,0000,0000,,Ừm... bả có nhắc đến việc bắt đầu làm thêm...
Dialogue: 0,0:13:12.76,0:13:16.39,ichigo,,0000,0000,0000,,À, giờ nghĩ lại thì bả không nói là làm thêm công việc gì.
Dialogue: 0,0:13:16.76,0:13:18.73,ichigo,,0000,0000,0000,,Vậy đó là Onee-chan thật à?
Dialogue: 0,0:13:19.00,0:13:20.36,ichigo,,0000,0000,0000,,Mình đã nói là thật mà.
Dialogue: 0,0:13:22.77,0:13:23.86,ichigo,,0000,0000,0000,,Chúng ta đi xem thử.
Dialogue: 0,0:13:23.97,0:13:28.27,ichigo,,0000,0000,0000,,Nhưng nếu làm thế, Onee-chan chắc chắn sẽ...
Dialogue: 0,0:13:28.44,0:13:30.60,ichigo,,0000,0000,0000,,Ừ, vì thế nên bọn mình sẽ nhìn trộm từ bên ngoài.
Dialogue: 0,0:13:31.04,0:13:33.31,ichigo,,0000,0000,0000,,Dĩ nhiên là sẽ giữ bí mật với Micchan.
Dialogue: 0,0:13:33.75,0:13:35.77,ichigo,,0000,0000,0000,,Nếu nó phát hịên, thì tớ không biết nó sẽ làm cái thứ gì nữa...
Dialogue: 0,0:13:43.56,0:13:45.46,ichigo,,0000,0000,0000,,Chắc tớ tưởng tượng thôi.
Dialogue: 0,0:13:46.76,0:13:47.59,ichigo,,0000,0000,0000,,Gọi tớ hả?
Dialogue: 0,0:13:47.91,0:13:48.95,ichigo,,0000,0000,0000,,Nó xuất hiện rồi!
Dialogue: 0,0:13:49.33,0:13:51.30,ichigo,,0000,0000,0000,,Cậu làm gì ở đây thế?
Dialogue: 0,0:13:51.80,0:13:54.23,ichigo,,0000,0000,0000,,Chơi trốn tìm, nhưng tớ chưa nói với ai hết.
Dialogue: 0,0:13:54.47,0:13:56.09,ichigo,,0000,0000,0000,,Nếu không nói thì đừng có chơi!
Dialogue: 0,0:13:57.54,0:13:59.03,ichigo,,0000,0000,0000,,Tớ chả thèm quan tâm nữa.
Dialogue: 0,0:13:59.44,0:14:01.60,ichigo,,0000,0000,0000,,Vậy, chúng ta đến cái nhà hàng gia đình đó chứ?
Dialogue: 0,0:14:02.34,0:14:03.60,ichigo,,0000,0000,0000,,Hoan nghênh.
Dialogue: 0,0:14:18.46,0:14:19.92,ichigo,,0000,0000,0000,,Xin lỗi Nobu-chan...
Dialogue: 0,0:14:20.13,0:14:22.79,ichigo,,0000,0000,0000,,Cô có thể mang bốn cốc nước đến bàn số 10 không?
Dialogue: 0,0:14:22.96,0:14:24.36,ichigo,,0000,0000,0000,,Ừ, tôi biết rồi.
Dialogue: 0,0:14:30.70,0:14:33.26,ichigo,,0000,0000,0000,,À, nhầm bàn, xin lỗi nhé.
Dialogue: 0,0:14:33.81,0:14:36.07,ichigo,,0000,0000,0000,,Chờ đã, chị đi đâu thế, Nobue-neechan?
Dialogue: 0,0:14:36.14,0:14:38.11,ichigo,,0000,0000,0000,,Quý khách nhầm tôi với ai ạ?
Dialogue: 0,0:14:38.31,0:14:41.44,ichigo,,0000,0000,0000,,Onee-chan mặc loại đồng phục này,
Dialogue: 0,0:14:41.71,0:14:43.15,ichigo,,0000,0000,0000,,đúng lần đầu tiên thấy.
Dialogue: 0,0:14:43.22,0:14:44.11,ichigo,,0000,0000,0000,,Ừm...
Dialogue: 0,0:14:44.18,0:14:45.38,ichigo,,0000,0000,0000,,Em chụp hình được không?
Dialogue: 0,0:14:49.16,0:14:51.06,ichigo,,0000,0000,0000,,Liệu hồn đấy.
Dialogue: 0,0:14:51.16,0:14:52.62,ichigo,,0000,0000,0000,,Ai đã nói với em?
Dialogue: 0,0:14:52.96,0:14:54.39,ichigo,,0000,0000,0000,,E-Em xin lỗi.
Dialogue: 0,0:14:55.36,0:14:56.29,ichigo,,0000,0000,0000,,Là em ạ.
Dialogue: 0,0:14:56.40,0:14:58.52,ichigo,,0000,0000,0000,,Không, đâu thể khác được.
Dialogue: 0,0:14:58.83,0:15:01.06,ichigo,,0000,0000,0000,,Không phải lỗi của cậu đâu Ana-chan.
Dialogue: 0,0:15:01.47,0:15:03.63,ichigo,,0000,0000,0000,,Bởi vì Onee-chan mặc bộ đồng phục này...
Dialogue: 0,0:15:05.07,0:15:06.66,ichigo,,0000,0000,0000,,Lạnh quá! Lạnh quá!
Dialogue: 0,0:15:06.74,0:15:08.15,ichigo,,0000,0000,0000,,Ngoài chuyện quần áo ra,
Dialogue: 0,0:15:08.15,0:15:10.70,ichigo,,0000,0000,0000,,rất khó để tìm một công việc nào lương cao thế này.
Dialogue: 0,0:15:10.84,0:15:12.31,ichigo,,0000,0000,0000,,Em biết, em biết.
Dialogue: 0,0:15:15.85,0:15:17.08,ichigo,,0000,0000,0000,,Nghe đây, Miu.
Dialogue: 0,0:15:18.25,0:15:19.81,ichigo,,0000,0000,0000,,Không được làm điều gì quái đản!
Dialogue: 0,0:15:20.15,0:15:22.31,ichigo,,0000,0000,0000,,Hiểu rồi, Onee-chan.
Dialogue: 0,0:15:22.52,0:15:24.82,ichigo,,0000,0000,0000,,Và đừng có gọi "Onee-chan" nữa!
Dialogue: 0,0:15:25.26,0:15:27.78,ichigo,,0000,0000,0000,,Giờ chị và mấy đứa là người lạ.
Dialogue: 0,0:15:27.86,0:15:28.38,ichigo,,0000,0000,0000,,Hiểu chưa?
Dialogue: 0,0:15:28.38,0:15:29.33,ichigo,,0000,0000,0000,,Dạ, dạ.
Dialogue: 0,0:15:29.53,0:15:31.00,ichigo,,0000,0000,0000,,Tốt.
Dialogue: 0,0:15:32.93,0:15:35.66,ichigo,,0000,0000,0000,,Gọi tôi khi quý khách đã sẵn sàng gọi món.
Dialogue: 0,0:15:41.07,0:15:43.51,ichigo,,0000,0000,0000,,Bả không để ý vì váy quá ngắn.
Dialogue: 0,0:15:43.81,0:15:45.90,ichigo,,0000,0000,0000,,Nè, Micchan, cậu sẽ bị giết đấy.
Dialogue: 0,0:15:46.91,0:15:48.81,ichigo,,0000,0000,0000,,Cô bé đó là em của cô hả?
Dialogue: 0,0:15:48.88,0:15:50.35,ichigo,,0000,0000,0000,,Ờ, có thể nói là vậy.
Dialogue: 0,0:15:50.65,0:15:52.14,ichigo,,0000,0000,0000,,Đừng để ý đến chúng.
Dialogue: 0,0:15:54.49,0:15:56.46,ichigo,,0000,0000,0000,,À, là bàn số 10.
Dialogue: 0,0:15:56.72,0:15:57.85,ichigo,,0000,0000,0000,,Cô đi đi.
Dialogue: 0,0:15:57.92,0:16:00.82,ichigo,,0000,0000,0000,,Ế? Chúng đến đây để gặp cô mà, đi đi.
Dialogue: 0,0:16:02.83,0:16:03.95,ichigo,,0000,0000,0000,,Trời ạ.
Dialogue: 0,0:16:04.70,0:16:06.69,ichigo,,0000,0000,0000,,Quý khách đã sẵn sàng gọi món?
Dialogue: 0,0:16:06.90,0:16:08.70,ichigo,,0000,0000,0000,,Một bữa sáng đầy đủ.
Dialogue: 0,0:16:09.04,0:16:11.03,ichigo,,0000,0000,0000,,Giờ là buổi tối nên chúng tôi không thể.
Dialogue: 0,0:16:11.17,0:16:12.26,ichigo,,0000,0000,0000,,Vậy à...
Dialogue: 0,0:16:12.81,0:16:15.97,ichigo,,0000,0000,0000,,Nè, chị nhân viên, giá ở đây quá đắt phải không?
Dialogue: 0,0:16:22.11,0:16:23.95,ichigo,,0000,0000,0000,,Chị đem trẻ con ra làm trò đùa à?
Dialogue: 0,0:16:24.08,0:16:25.38,ichigo,,0000,0000,0000,,Em muốn bị đập không hả?!
Dialogue: 0,0:16:28.72,0:16:30.71,ichigo,,0000,0000,0000,,Hết cách rồi, chúng ta hãy thương lượng chuyện này.
Dialogue: 0,0:16:39.17,0:16:42.16,ichigo,,0000,0000,0000,,Nè, nè, tớ đãi đấy, đừng khách sáo nhé!
Dialogue: 0,0:16:42.77,0:16:44.24,ichigo,,0000,0000,0000,,Có ổn không vậy?
Dialogue: 0,0:16:44.47,0:16:46.90,ichigo,,0000,0000,0000,,Cậu dám không ăn ly kem sôcôla tớ đãi...
Dialogue: 0,0:16:46.97,0:16:47.70,ichigo,,0000,0000,0000,,Ế?
Dialogue: 0,0:16:47.77,0:16:49.97,ichigo,,0000,0000,0000,,Có phải là cậu đãi đâu.
Dialogue: 0,0:16:50.34,0:16:54.07,ichigo,,0000,0000,0000,,Ừ, thế này thật đáng thương cho Onee-sama.
Dialogue: 0,0:16:54.41,0:16:57.08,ichigo,,0000,0000,0000,,Vậy cậu không ăn hả?
Dialogue: 0,0:16:57.32,0:16:58.28,ichigo,,0000,0000,0000,,Ế?
Dialogue: 0,0:16:58.42,0:17:01.48,ichigo,,0000,0000,0000,,Ờ... thì...
Dialogue: 0,0:17:01.85,0:17:04.52,ichigo,,0000,0000,0000,,Onee-sama đã ưu đãi như vậy thì...
Dialogue: 0,0:17:05.32,0:17:06.88,ichigo,,0000,0000,0000,,Đúng rồi.
Dialogue: 0,0:17:07.06,0:17:09.93,ichigo,,0000,0000,0000,,Nếu không ăn hết bọn mình thật khiếm nhã với Nobue-neechan.
Dialogue: 0,0:17:10.06,0:17:11.32,ichigo,,0000,0000,0000,,Phải không, Matsuri-chan?
Dialogue: 0,0:17:11.40,0:17:14.46,ichigo,,0000,0000,0000,,Ừ, chắc là vậy.
Dialogue: 0,0:17:14.77,0:17:17.00,ichigo,,0000,0000,0000,,Đúng rồi, còn cách nào khác đâu.
Dialogue: 0,0:17:27.05,0:17:28.01,ichigo,,0000,0000,0000,,Nobu-chan!
Dialogue: 0,0:17:29.05,0:17:30.91,ichigo,,0000,0000,0000,,Người quản lý... ở sau kìa.
Dialogue: 0,0:17:32.45,0:17:33.58,ichigo,,0000,0000,0000,,Thuốc lá.
Dialogue: 0,0:17:35.62,0:17:37.61,ichigo,,0000,0000,0000,,Hoan nghênh!
Dialogue: 0,0:17:40.66,0:17:43.79,ichigo,,0000,0000,0000,,Tại sao... mấy đứa hôm nay lại đến?
Dialogue: 0,0:17:44.33,0:17:46.23,ichigo,,0000,0000,0000,,Onee-chan, đồng phục của chị!
Dialogue: 0,0:17:46.30,0:17:47.79,ichigo,,0000,0000,0000,,Hôm qua em đã nói thế rồi.
Dialogue: 0,0:17:48.13,0:17:50.53,ichigo,,0000,0000,0000,,Onee-sama, em đã nói là không nên đến.
Dialogue: 0,0:17:50.64,0:17:51.90,ichigo,,0000,0000,0000,,Em cũng vậy.
Dialogue: 0,0:17:51.97,0:17:53.10,ichigo,,0000,0000,0000,,Chị biết.
Dialogue: 0,0:17:53.54,0:17:54.51,ichigo,,0000,0000,0000,,Này.
Dialogue: 0,0:17:57.28,0:17:59.30,ichigo,,0000,0000,0000,,Không có ai ngoài em đâu, Miu.
Dialogue: 0,0:18:01.15,0:18:03.14,ichigo,,0000,0000,0000,,My name is Katherine.
Dialogue: 0,0:18:03.35,0:18:04.37,ichigo,,0000,0000,0000,,Sao em...
Dialogue: 0,0:18:05.67,0:18:07.81,ichigo,,0000,0000,0000,,Xin hãy nướng cái này với muối.
Dialogue: 0,0:18:08.35,0:18:11.65,ichigo,,0000,0000,0000,,Nhà hàng chúng tôi không đáp ứng loại dịch vụ đó.
Dialogue: 0,0:18:11.92,0:18:13.32,ichigo,,0000,0000,0000,,Vậy thì nướng với tương.
Dialogue: 0,0:18:15.23,0:18:15.92,ichigo,,0000,0000,0000,,Nướng với bột đậu nành đi.
Dialogue: 0,0:18:15.92,0:18:17.89,ichigo,,0000,0000,0000,,Làm thế sao được hả?
Dialogue: 0,0:18:21.27,0:18:22.70,ichigo,,0000,0000,0000,,Itadakimasu!
Dialogue: 0,0:18:22.77,0:18:24.00,ichigo,,0000,0000,0000,,Ổn không vậy?
Dialogue: 0,0:18:24.07,0:18:27.33,ichigo,,0000,0000,0000,,Nhưng bỏ lại thì lãng phí quá...
Dialogue: 0,0:18:27.57,0:18:29.34,ichigo,,0000,0000,0000,,C-Cậu nói đúng đấy.
Dialogue: 0,0:18:35.15,0:18:36.11,ichigo,,0000,0000,0000,,Nobu-chan.
Dialogue: 0,0:18:37.12,0:18:37.91,ichigo,,0000,0000,0000,,Ờ, ờ.
Dialogue: 0,0:18:42.35,0:18:44.72,ichigo,,0000,0000,0000,,Em đã nói là không nên đến mà.
Dialogue: 0,0:18:44.99,0:18:47.02,ichigo,,0000,0000,0000,,Em xin lỗi, Nobue-neechan.
Dialogue: 0,0:18:47.33,0:18:50.39,ichigo,,0000,0000,0000,,Hôm nay sẽ là ngày cuối cùng em đến đây.
Dialogue: 0,0:18:52.20,0:18:54.10,ichigo,,0000,0000,0000,,Cho một phần ăn lớn... với nhiều nước thịt.
Dialogue: 0,0:18:55.73,0:18:57.70,ichigo,,0000,0000,0000,,Hôm nay là lần cuối! Lần cuối!
Dialogue: 0,0:18:58.04,0:18:59.27,ichigo,,0000,0000,0000,,Nhớ là thật đấy nhé.
Dialogue: 0,0:18:59.94,0:19:01.03,ichigo,,0000,0000,0000,,Itadakimasu!
Dialogue: 0,0:19:01.27,0:19:02.83,ichigo,,0000,0000,0000,,Itadakimasu!
Dialogue: 0,0:19:10.25,0:19:11.22,ichigo,,0000,0000,0000,,Nè...
Dialogue: 0,0:19:11.85,0:19:13.94,ichigo,,0000,0000,0000,,Cô có thấy lọ tương ớt ở đâu không?
Dialogue: 0,0:19:14.79,0:19:16.28,ichigo,,0000,0000,0000,,Không.
Dialogue: 0,0:19:28.40,0:19:30.56,ichigo,,0000,0000,0000,,Em không gọi thức uống mà, phải không?
Dialogue: 0,0:19:30.90,0:19:31.50,ichigo,,0000,0000,0000,,Ghi vào hóa đơn đi.
Dialogue: 0,0:19:31.50,0:19:32.56,ichigo,,0000,0000,0000,,Ai thanh toán?
Dialogue: 0,0:19:37.54,0:19:39.01,ichigo,,0000,0000,0000,,Quý khách ổn chứ?
Dialogue: 0,0:19:39.08,0:19:40.91,ichigo,,0000,0000,0000,,Xin hãy cẩn thận nhé.
Dialogue: 0,0:19:41.01,0:19:42.67,ichigo,,0000,0000,0000,,Ờ-Ờ, cảm ơn.
Dialogue: 0,0:19:47.92,0:19:50.91,ichigo,,0000,0000,0000,,Chị nhân viên, không sao chứ? Cẩn thận nhé.
Dialogue: 0,0:19:51.09,0:19:52.15,ichigo,,0000,0000,0000,,À, ừ.
Dialogue: 0,0:20:19.12,0:20:21.71,ichigo,,0000,0000,0000,,Ừm, Nobu-chan?
Dialogue: 0,0:20:42.81,0:20:44.37,ichigo,,0000,0000,0000,,Ôi trời...
Dialogue: 0,0:20:46.41,0:20:49.44,ichigo,,0000,0000,0000,,Onee-sama sẽ không sao chứ?
Dialogue: 0,0:20:55.19,0:20:58.45,ichigo,,0000,0000,0000,,Nobu-chan, quản lý muốn nói chuyện với cô.
Dialogue: 0,0:20:58.52,0:21:00.39,ichigo,,0000,0000,0000,,Ừm, được.
Dialogue: 0,0:21:04.30,0:21:06.26,ichigo,,0000,0000,0000,,Nào, nào, hôm nay lại đi nào!
Dialogue: 0,0:21:06.43,0:21:08.56,ichigo,,0000,0000,0000,,Ế, tớ không muốn đi đâu.
Dialogue: 0,0:21:09.00,0:21:10.37,ichigo,,0000,0000,0000,,Đây cũng vậy.
Dialogue: 0,0:21:10.44,0:21:12.96,ichigo,,0000,0000,0000,,Nếu chúng ta không thôi, Onee-chan sẽ...
Dialogue: 0,0:21:13.04,0:21:16.34,ichigo,,0000,0000,0000,,Kem Kashiwamochi trông ngon lắm đấy.
Dialogue: 0,0:21:17.51,0:21:19.37,ichigo,,0000,0000,0000,,Đi nào!
Dialogue: 0,0:21:21.38,0:21:22.97,ichigo,,0000,0000,0000,,Onee-chan, chị không đi làm à?
Dialogue: 0,0:21:25.18,0:21:25.95,ichigo,,0000,0000,0000,,Chị bỏ rồi.
Dialogue: 0,0:21:30.59,0:21:32.35,ichigo,,0000,0000,0000,,O-Onee-chan, em phản đối bạo lực.
Dialogue: 0,0:21:39.73,0:21:42.26,ichigo,,0000,0000,0000,,Nghe vui đấy, thật không, thật không vậy?
Dialogue: 0,0:21:52.38,0:21:53.35,ichigo,,0000,0000,0000,,Ờ...
Dialogue: 0,0:21:53.51,0:21:55.14,ichigo,,0000,0000,0000,,Giờ thì chẳng làm được gì nữa rồi.
Dialogue: 0,0:22:02.76,0:22:04.15,ichigo,,0000,0000,0000,,Vẫn không sạch.
Dialogue: 0,0:23:41.12,0:23:46.32,ichigo,,0000,0000,0000,,Nè, Nobue-oneechan. Giữa em, Miu-chan, Coppola-chan, và những người khác, chị thích ai?
Dialogue: 0,0:23:46.43,0:23:48.83,ichigo,,0000,0000,0000,,Em sẽ không buông tha chừng nào chị làm rõ.
Dialogue: 0,0:23:48.89,0:23:50.89,ichigo,,0000,0000,0000,,Sao chị gắp thức ăn ở nồi mà không chú ý gì cả vậy?
Dialogue: 0,0:23:51.00,0:23:53.33,ichigo,,0000,0000,0000,,Gắp thịt cho em nữa, thịt.
Dialogue: 0,0:23:54.07,0:23:58.33,ichigo,,0000,0000,0000,,Nói chung, Nobue-oneechan, chị hơi chậm chạp và lẩn thẩn đấy, phải không.
Dialogue: 0,0:23:58.60,0:24:00.90,ichigo,,0000,0000,0000,,Nhìn đây nè, Nobue-nee!
Dialogue: 0,0:24:02.17,0:24:04.84,ichigo,,0000,0000,0000,,Không biết có ai đang bàn tán về mình không nữa.
Dialogue: 0,0:24:05.68,0:24:08.34,ichigo,,0000,0000,0000,,Đón xem tập kế tiếp của "Mashimaro"!
Dialogue: 0,0:24:08.34,0:24:10.34,Default,,0000,0000,0000,,

Dialogue: 0,0:00:02.16,0:00:07.96,Opviet,,0000,0000,0000,,Sự dễ thương không thể diễn đạt được bằng lời
Dialogue: 0,0:00:20.43,0:00:26.23,Opviet,,0000,0000,0000,,cuối cùng thì "những quả dâu mềm" đã tới
Dialogue: 0,0:00:26.23,0:00:31.28,Opviet,,0000,0000,0000,,những âm thanh phiền phức cũng đến theo
Dialogue: 0,0:00:32.44,0:00:38.19,Opviet,,0000,0000,0000,,cuối cùng thì "những quả dâu mềm" đã tới
Dialogue: 0,0:00:38.24,0:00:43.12,Opviet,,0000,0000,0000,,chúng ngập tràn hương vị ngọt ngào
Dialogue: 0,0:00:43.60,0:00:49.59,Opviet,,0000,0000,0000,,màu sắc không hợp cũng không vấn đề gì,
Dialogue: 0,0:00:49.61,0:00:53.01,Opviet,,0000,0000,0000,,mắc phải lỗi lầm là điều tự nhiên thôi,
Dialogue: 0,0:00:53.12,0:00:57.68,Opviet,,0000,0000,0000,,vậy nên hãy tha thứ cho bọn em
Dialogue: 0,0:00:59.01,0:01:02.18,Opviet,,0000,0000,0000,,đừng nổi giận, đừng nổi giận,
Dialogue: 0,0:01:02.31,0:01:04.94,Opviet,,0000,0000,0000,,hãy để bọn em làm theo ý thích!
Dialogue: 0,0:01:04.98,0:01:07.60,Opviet,,0000,0000,0000,,người lớn sẽ không hiểu được
Dialogue: 0,0:01:07.69,0:01:10.62,Opviet,,0000,0000,0000,,sự tuyệt vời, kỳ diệu, và hoàn hảo
Dialogue: 0,0:01:10.98,0:01:16.74,Opviet,,0000,0000,0000,,sự dễ thương không thể diễn đạt được bằng lời
Dialogue: 0,0:01:16.75,0:01:20.24,Opviet,,0000,0000,0000,,không có lý do gì ẩn chứa đằng sau những nụ cười
Dialogue: 0,0:01:20.28,0:01:24.45,Opviet,,0000,0000,0000,,những cô bé giống như dâu tây
Dialogue: 0,0:22:15.68,0:22:26.19,EDviệt,,0000,0000,0000,,Nếu tôi hồi tưởng lại giấc mơ đã quên lãng,
Dialogue: 0,0:22:27.17,0:22:37.10,EDviệt,,0000,0000,0000,,nụ cười của bạn sẽ vô tình băng qua tâm trí tôi
Dialogue: 0,0:22:37.85,0:22:43.91,EDviệt,,0000,0000,0000,,tôi không biết ngày mai sẽ ra sao
Dialogue: 0,0:22:43.91,0:22:49.88,EDviệt,,0000,0000,0000,,nè, hãy ghi nhớ những ngày chúng ta vui đùa bên nhau
Dialogue: 0,0:22:49.98,0:23:03.87,EDviệt,,0000,0000,0000,,con đường là dốc đứng, nhưng chúng ta vẫn chạy như không.
Dialogue: 0,0:23:03.87,0:23:07.07,EDviệt,,0000,0000,0000,,chạy vội vã, vội vã
Dialogue: 0,0:23:07.12,0:23:14.97,EDviệt,,0000,0000,0000,,lời thì thầm của bạn vào tai tôi, rủ tôi cùng nghịch bóng.
Dialogue: 0,0:23:15.24,0:23:23.54,EDviệt,,0000,0000,0000,,váy chúng tôi lắc lư theo hơi lạnh lướt qua đầu gối
Dialogue: 0,0:23:23.92,0:23:33.89,EDviệt,,0000,0000,0000,,Nè, giờ bạn đang ở đâu rồi!
Dialogue: 0,0:01:31.52,0:01:33.03,ichigo,Comment,0000,0000,0000,,Đ-Đồ ngốc!
Dialogue: 0,0:01:33.44,0:01:34.78,ichigo,Comment,0000,0000,0000,,Đầu hói!
Dialogue: 0,0:01:34.99,0:01:36.15,ichigo,Comment,0000,0000,0000,,Thịt lợn chua ngọt!
Dialogue: 0,0:01:36.15,0:01:38.16,ichigo,Comment,0000,0000,0000,,Món hầm hổ lốn!
Dialogue: 0,0:01:38.16,0:01:39.16,ichigo,Comment,0000,0000,0000,,C-Con gấu!
Dialogue: 0,0:01:39.37,0:01:41.53,ichigo,Comment,0000,0000,0000,,R-Rốn lồi!
Dialogue: 0,0:01:41.95,0:01:42.91,ichigo,Comment,0000,0000,0000,,Đầu hói!
Dialogue: 0,0:01:44.24,0:01:45.62,ichigo,Comment,0000,0000,0000,,Cái đó tớ nói rồi!
Dialogue: 0,0:01:46.71,0:01:49.29,ichigo,Comment,0000,0000,0000,,Yay! Đồ thộn! Ukkari Hachibe...!
Dialogue: 0,0:01:49.29,0:01:50.25,ichigo,Comment,0000,0000,0000,,Uge!
Dialogue: 0,0:01:50.67,0:01:51.79,ichigo,Comment,0000,0000,0000,,Uge?
Dialogue: 0,0:01:51.79,0:01:53.55,ichigo,Comment,0000,0000,0000,,"Uge" là cái gì?
Dialogue: 0,0:01:51.79,0:01:52.96,nói trùng,Comment,0000,0000,0000,,Xấu xí!
Dialogue: 0,0:01:52.96,0:01:53.75,nói trùng,Comment,0000,0000,0000,,Gấu chó!
Dialogue: 0,0:01:53.55,0:01:56.26,ichigo,Comment,0000,0000,0000,,"Quyến rũ bạn trai với một chiếc váy ngắn."
Dialogue: 0,0:01:53.75,0:01:55.38,nói trùng,Comment,0000,0000,0000,,Đồ óc chim!
Dialogue: 0,0:01:55.38,0:01:56.59,nói trùng,Comment,0000,0000,0000,,Củ cải!
Dialogue: 0,0:01:56.59,0:01:57.42,ichigo,Comment,0000,0000,0000,,Bạch tuộc!
Dialogue: 0,0:01:57.42,0:01:58.18,ichigo,Comment,0000,0000,0000,,Mực ống!
Dialogue: 0,0:01:58.18,0:02:00.68,ichigo,Comment,0000,0000,0000,,Chị, "uge" là gì?
Dialogue: 0,0:01:58.18,0:01:59.26,nói trùng,Comment,0000,0000,0000,,Hyottoko!
Dialogue: 0,0:01:59.26,0:02:00.68,nói trùng,Comment,0000,0000,0000,,Mặt hề!
Dialogue: 0,0:02:00.68,0:02:01.76,ichigo,Comment,0000,0000,0000,,Bí ngô!
Dialogue: 0,0:02:01.76,0:02:02.89,ichigo,Comment,0000,0000,0000,,Đồ ngu!
Dialogue: 0,0:02:03.18,0:02:06.68,ichigo,Comment,0000,0000,0000,,Trò gọi tên của con nít này mà chơi cái gì vậy?
Dialogue: 0,0:02:03.18,0:02:04.26,nói trùng,Comment,0000,0000,0000,,Mặt trơ trẽn!
Dialogue: 0,0:02:04.26,0:02:05.60,nói trùng,Comment,0000,0000,0000,,Mặt lộn ngược!
Dialogue: 0,0:02:05.60,0:02:06.85,nói trùng,Comment,0000,0000,0000,,Đãng trí!
Dialogue: 0,0:02:06.85,0:02:08.10,ichigo,Comment,0000,0000,0000,,Băng rốn!
Dialogue: 0,0:02:08.48,0:02:09.60,ichigo,Comment,0000,0000,0000,,Chảy nước mũi!
Dialogue: 0,0:02:09.60,0:02:11.06,ichigo,Comment,0000,0000,0000,,Đồ lùn!
Dialogue: 0,0:02:12.69,0:02:15.11,ichigo,Comment,0000,0000,0000,,Tuy là người Anh nhưng cậu quá nhỏ con.
Dialogue: 0,0:02:15.11,0:02:16.53,ichigo,Comment,0000,0000,0000,,Tớ không nhỏ!
Dialogue: 0,0:02:27.79,0:02:29.71,ichigo,Comment,0000,0000,0000,,Hai đứa nó đang làm gì vậy?
Dialogue: 0,0:02:34.13,0:02:35.71,ichigo,Comment,0000,0000,0000,,Đây là...
Dialogue: 0,0:02:35.71,0:02:37.01,ichigo,Comment,0000,0000,0000,,Là gì vậy?
Dialogue: 0,0:02:49.56,0:02:52.65,ichigo,Comment,0000,0000,0000,,Quan hệ của hai đứa tệ đến vậy à?
Dialogue: 0,0:02:52.65,0:02:54.44,ichigo,Comment,0000,0000,0000,,À... Thật ra...
Dialogue: 0,0:02:54.82,0:02:56.69,ichigo,Comment,0000,0000,0000,,Emergency.
Dialogue: 0,0:02:57.07,0:02:59.49,ichigo,Comment,0000,0000,0000,,Matsuri-chan đang làm gì vậy?
Dialogue: 0,0:03:00.32,0:03:01.61,ichigo,Comment,0000,0000,0000,,Tiếng Anh...
Dialogue: 0,0:03:04.03,0:03:07.04,ichigo,Comment,0000,0000,0000,,Không biết giờ thể dục hôm nay là làm gì nhỉ?
Dialogue: 0,0:03:07.58,0:03:08.95,ichigo,Comment,0000,0000,0000,,Nhảy ngựa chăng?
Dialogue: 0,0:03:08.95,0:03:10.96,ichigo,Comment,0000,0000,0000,,Hở, Tớ không nghĩ vậy.
Dialogue: 0,0:03:11.62,0:03:12.81,ichigo,Comment,0000,0000,0000,,Hay là môn xà ngang?
Dialogue: 0,0:03:12.81,0:03:14.67,ichigo,Comment,0000,0000,0000,,Hy vọng là không, tớ ghét môn đó lắm.
Dialogue: 0,0:03:15.09,0:03:17.05,ichigo,Comment,0000,0000,0000,,Tớ cũng vậy.
Dialogue: 0,0:03:20.55,0:03:21.63,ichigo,Comment,0000,0000,0000,,Sao vậy, Cop...?
Dialogue: 0,0:03:22.59,0:03:27.64,ichigo,Comment,0000,0000,0000,,Chuyện là, Ana-chan lo lắng vì không được nói tiếng Nhật.
Dialogue: 0,0:03:27.64,0:03:29.35,ichigo,Comment,0000,0000,0000,,Thế thì cứ nói là được mà?
Dialogue: 0,0:03:29.35,0:03:31.81,ichigo,Comment,0000,0000,0000,,Nếu nói được thì tớ đã chẳng lo.
Dialogue: 0,0:03:31.81,0:03:35.86,ichigo,Comment,0000,0000,0000,,Ở trường, chuyện Ana-chan nói được tiếng Nhật là bí mật.
Dialogue: 0,0:03:35.86,0:03:37.15,ichigo,Comment,0000,0000,0000,,Ra thế.
Dialogue: 0,0:03:37.15,0:03:40.15,ichigo,Comment,0000,0000,0000,,Vậy duy nhất có chúng ta biết cậu ấy không nói được tiếng Anh à?
Dialogue: 0,0:03:41.74,0:03:46.58,ichigo,Comment,0000,0000,0000,,Ana-chan vẫn chưa nói tiếng Anh được, nên cậu ấy thích im lặng hơn.
Dialogue: 0,0:03:46.58,0:03:48.45,ichigo,Comment,0000,0000,0000,,Cậu ấy không nói được nhỉ?
Dialogue: 0,0:03:51.04,0:03:55.75,ichigo,Comment,0000,0000,0000,,Có cần phải lặp đi lặp lại câu "không nói được" mãi không hả?!
Dialogue: 0,0:03:56.54,0:03:57.80,ichigo,Comment,0000,0000,0000,,Tớ xin lỗi.
Dialogue: 0,0:03:57.80,0:04:01.05,ichigo,Comment,0000,0000,0000,,Nhưng tớ nghĩ sẽ tốt hơn nếu giải thích với mọi người.
Dialogue: 0,0:04:01.67,0:04:03.34,ichigo,Comment,0000,0000,0000,,Vẫn chưa ai biết họ của cậu phải không?
Dialogue: 0,0:04:03.88,0:04:06.80,ichigo,Comment,0000,0000,0000,,Ừ, mọi người mà biết thì thật kinh khủng!
Dialogue: 0,0:04:07.26,0:04:09.01,ichigo,Comment,0000,0000,0000,,Thế à! Cop...
Dialogue: 0,0:04:11.81,0:04:13.85,ichigo,Comment,0000,0000,0000,,Tớ thắc mắc mãi...
Dialogue: 0,0:04:13.85,0:04:15.02,ichigo,Comment,0000,0000,0000,,Ế?
Dialogue: 0,0:04:15.02,0:04:18.48,ichigo,Comment,0000,0000,0000,,Cậu với Ana-chan thân nhau ghê ha.
Dialogue: 0,0:04:18.48,0:04:21.32,ichigo,Comment,0000,0000,0000,,Hai cậu luôn đến trường cùng nhau.
Dialogue: 0,0:04:21.32,0:04:23.49,ichigo,Comment,0000,0000,0000,,Thế các cậu nói chuyện với nhau thế nào?
Dialogue: 0,0:04:23.49,0:04:24.41,ichigo,Comment,0000,0000,0000,,Hê?
Dialogue: 0,0:04:24.41,0:04:25.70,ichigo,Comment,0000,0000,0000,,Vì cậu ấy nói tiếng Anh, đúng không?
Dialogue: 0,0:04:25.70,0:04:29.03,ichigo,Comment,0000,0000,0000,,Ana-chan nói rất chậm cho tớ nghe...
Dialogue: 0,0:04:29.66,0:04:33.21,ichigo,Comment,0000,0000,0000,,...nên đại khái tớ cũng hiểu được phần nào, và...
Dialogue: 0,0:04:33.21,0:04:35.12,ichigo,Comment,0000,0000,0000,,Nếu nhìn vào mắt cậu ấy...
Dialogue: 0,0:04:35.54,0:04:36.83,ichigo,Comment,0000,0000,0000,,...Tớ cũng có thể đoán được cậu ấy đang nghĩ gì.
Dialogue: 0,0:04:39.17,0:04:41.96,ichigo,Comment,0000,0000,0000,,Ghê nha! Cậu đã nhận được vài lá rồi đấy.
Dialogue: 0,0:04:41.96,0:04:44.09,ichigo,Comment,0000,0000,0000,,Mặc dù cậu chỉ mới chuyển tới đây.
Dialogue: 0,0:04:44.09,0:04:46.03,ichigo,Comment,0000,0000,0000,,Lá thứ ba rồi! Thứ ba rồi!
Dialogue: 0,0:04:46.03,0:04:47.26,ichigo,Comment,0000,0000,0000,,Không thể nào!
Dialogue: 0,0:04:47.55,0:04:50.22,ichigo,Comment,0000,0000,0000,,Nè, nè, không biết ở bên trong viết gì nhỉ.
Dialogue: 0,0:04:50.56,0:04:52.47,ichigo,Comment,0000,0000,0000,,Đừng nôn nóng chứ!
Dialogue: 0,0:04:52.47,0:04:54.23,ichigo,Comment,0000,0000,0000,,Để tớ đọc trước!
Dialogue: 0,0:04:57.73,0:04:59.23,ichigo,Comment,0000,0000,0000,,Tớ không đọc được...
Dialogue: 0,0:05:01.57,0:05:02.86,ichigo,Comment,0000,0000,0000,,Nhìn xem, nó viết...
Dialogue: 0,0:05:02.86,0:05:04.32,ichigo,Comment,0000,0000,0000,,..."love" ở đây nè.
Dialogue: 0,0:05:04.32,0:05:06.41,ichigo,Comment,0000,0000,0000,,Đây nữa. Đây nữa. Đúng không?
Dialogue: 0,0:05:10.45,0:05:14.25,ichigo,Comment,0000,0000,0000,,Vừa nãy có người hỏi làm sao tớ nói chuyện với cậu được.
Dialogue: 0,0:05:15.46,0:05:17.71,ichigo,Comment,0000,0000,0000,,Tớ biết. Tớ có nghe mà.
Dialogue: 0,0:05:17.96,0:05:20.42,ichigo,Comment,0000,0000,0000,,Tớ nói tớ hiểu được bằng cách nhìn vào mắt cậu.
Dialogue: 0,0:05:22.80,0:05:25.17,ichigo,Comment,0000,0000,0000,,Cậu biết tớ đang nghĩ gì không?
Dialogue: 0,0:05:28.34,0:05:30.18,ichigo,Comment,0000,0000,0000,,Cậu muốn nói được tiếng Anh phải không?
Dialogue: 0,0:05:32.26,0:05:33.77,ichigo,Comment,0000,0000,0000,,Tớ đúng chứ?
Dialogue: 0,0:05:37.23,0:05:42.19,ichigo,Comment,0000,0000,0000,,"Để cho nó giữ được lâu hơn, tôi tết rơm cẩn thận."
Dialogue: 0,0:05:42.19,0:05:43.98,ichigo,Comment,0000,0000,0000,,Rất tốt, Sakuragi.
Dialogue: 0,0:05:43.98,0:05:45.32,ichigo,Comment,0000,0000,0000,,Kế tiếp là Ana.
Dialogue: 0,0:05:45.32,0:05:46.70,ichigo,Comment,0000,0000,0000,,Let's read.
Dialogue: 0,0:05:46.70,0:05:48.78,ichigo,Comment,0000,0000,0000,,Chỉ cần đọc đoạn nào mà em hiểu.
Dialogue: 0,0:05:49.74,0:05:51.62,ichigo,Comment,0000,0000,0000,,"Với tôi làm những thứ như vậy..."
Dialogue: 0,0:05:52.66,0:05:54.33,ichigo,Comment,0000,0000,0000,,Được. Tiếp theo nào.
Dialogue: 0,0:05:54.33,0:05:55.62,ichigo,Comment,0000,0000,0000,,Sasatsuka.
Dialogue: 0,0:05:55.62,0:05:58.29,ichigo,Comment,0000,0000,0000,,Bỏ qua đoạn Ana vừa đọc và đọc tiếp.
Dialogue: 0,0:05:58.29,0:06:01.25,ichigo,Comment,0000,0000,0000,,Không, thưa Sensei, em có thể đọc bình thường mà...
Dialogue: 0,0:06:01.25,0:06:02.59,ichigo,Comment,0000,0000,0000,,Ra hành lang đứng đi.
Dialogue: 0,0:06:03.13,0:06:06.67,ichigo,Comment,0000,0000,0000,,Một ông khổng lồ đang tới.
Dialogue: 0,0:06:06.67,0:06:10.05,ichigo,Comment,0000,0000,0000,,Băng qua những cánh đồng, những ngọn núi và biển cả.
Dialogue: 0,0:06:10.43,0:06:13.89,ichigo,Comment,0000,0000,0000,,Một bà khổng lồ cũng đang tới.
Dialogue: 0,0:06:13.89,0:06:17.60,ichigo,Comment,0000,0000,0000,,Băng qua những tòa nhà, những cây cầu. Qua cả núi Amagi.
Dialogue: 0,0:06:17.60,0:06:19.06,ichigo,Comment,0000,0000,0000,,Hay lắm.
Dialogue: 0,0:06:19.06,0:06:21.15,ichigo,Comment,0000,0000,0000,,Em hát rất hay, Sakuragi.
Dialogue: 0,0:06:22.23,0:06:23.82,ichigo,Comment,0000,0000,0000,,Tiếp theo, Ana.
Dialogue: 0,0:06:23.82,0:06:24.61,ichigo,Comment,0000,0000,0000,,Let's sing.
Dialogue: 0,0:06:24.61,0:06:26.53,ichigo,Comment,0000,0000,0000,,Cứ hát phần nào em biết.
Dialogue: 0,0:06:26.53,0:06:27.82,ichigo,Comment,0000,0000,0000,,Khổng lồ...
Dialogue: 0,0:06:27.82,0:06:28.95,ichigo,Comment,0000,0000,0000,,Được rồi.
Dialogue: 0,0:06:29.82,0:06:31.28,ichigo,Comment,0000,0000,0000,,Ra hành lang đứng, Sasatsuka.
Dialogue: 0,0:06:34.74,0:06:37.33,ichigo,Comment,0000,0000,0000,,Đúng rồi. Giỏi lắm.
Dialogue: 0,0:06:38.16,0:06:41.42,ichigo,Comment,0000,0000,0000,,Thật là tốt khi không phải nói trong giờ Toán.
Dialogue: 0,0:06:42.17,0:06:44.67,ichigo,Comment,0000,0000,0000,,Rồi, Sasatsuka, ra hành lang đứng nào.
Dialogue: 0,0:06:50.59,0:06:54.64,ichigo,Comment,0000,0000,0000,,Quyển sách đó có vẻ thú vị ha. Bữa sau cho tớ mượn nha?
Dialogue: 0,0:06:54.64,0:06:57.64,ichigo,Comment,0000,0000,0000,,Ừm. Mai tớ mang cho.
Dialogue: 0,0:06:57.64,0:06:59.60,ichigo,Comment,0000,0000,0000,,Hay quá! Tớ đợi đó nha!
Dialogue: 0,0:07:02.10,0:07:03.73,ichigo,Comment,0000,0000,0000,,Trở lại chuyện khi nãy...
Dialogue: 0,0:07:03.73,0:07:06.69,ichigo,Comment,0000,0000,0000,,...nhưng anh trai tớ thích cosplay và...
Dialogue: 0,0:07:07.15,0:07:08.40,ichigo,Comment,0000,0000,0000,,Cosplay?
Dialogue: 0,0:07:08.40,0:07:09.36,ichigo,Comment,0000,0000,0000,,Ừ.
Dialogue: 0,0:07:13.07,0:07:15.03,ichigo,Comment,0000,0000,0000,,"Cosplay" là gì nhỉ?
Dialogue: 0,0:07:24.63,0:07:26.50,ichigo,Comment,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:07:27.17,0:07:29.34,ichigo,Comment,0000,0000,0000,,Là thứ làm tóc cậu mượt ấy.
Dialogue: 0,0:07:31.72,0:07:38.02,ichigo,Comment,0000,0000,0000,,Vậy nên, tớ sẽ thì thầm vào tai cậu và cậu sẽ nói cho mọi người.
Dialogue: 0,0:07:38.02,0:07:40.89,ichigo,Comment,0000,0000,0000,,Như thế thì tớ có thể tham gia trò chuyện rồi!
Dialogue: 0,0:07:41.48,0:07:43.85,ichigo,Comment,0000,0000,0000,,Sao giờ tớ mới nghĩ ra nhỉ?
Dialogue: 0,0:07:44.19,0:07:49.28,ichigo,Comment,0000,0000,0000,,Nhưng tớ phải giỏi tiếng Anh lắm mới làm vậy được...
Dialogue: 0,0:07:49.28,0:07:51.74,ichigo,Comment,0000,0000,0000,,Vì thế cậu nên học tiếng Anh đi.
Dialogue: 0,0:07:52.15,0:07:52.82,ichigo,Comment,0000,0000,0000,,Ế?
Dialogue: 0,0:07:55.03,0:08:00.20,ichigo,Comment,0000,0000,0000,,A. B. C. D. E. F. G.
Dialogue: 0,0:08:00.20,0:08:02.87,ichigo,Comment,0000,0000,0000,,Micchan, cậu đang che tầm nhìn của tớ.
Dialogue: 0,0:08:02.87,0:08:04.12,ichigo,Comment,0000,0000,0000,,Hả?
Dialogue: 0,0:08:04.88,0:08:06.21,ichigo,Comment,0000,0000,0000,,Đến đây nào!
Dialogue: 0,0:08:06.21,0:08:07.38,ichigo,Comment,0000,0000,0000,,Strike!
Dialogue: 0,0:08:07.38,0:08:09.59,ichigo,Comment,0000,0000,0000,,Cậu lại đang âm mưu gì đấy phải không?
Dialogue: 0,0:08:10.05,0:08:10.92,ichigo,Comment,0000,0000,0000,,Gì là gì?
Dialogue: 0,0:08:12.55,0:08:14.97,ichigo,Comment,0000,0000,0000,,H. I. J. K.
Dialogue: 0,0:08:14.97,0:08:17.18,ichigo,Comment,0000,0000,0000,,L. M. N.
Dialogue: 0,0:08:17.18,0:08:19.01,ichigo,Comment,0000,0000,0000,,Bóng mềm.
Dialogue: 0,0:08:19.01,0:08:20.43,ichigo,Comment,0000,0000,0000,,Nhanh ném tới gôn đầu tiên đi!
Dialogue: 0,0:08:20.93,0:08:23.56,ichigo,Comment,0000,0000,0000,,Tụi nó chưa ghi được điểm nào đến khi chạy qua bục nhà cơ mà.
Dialogue: 0,0:08:23.56,0:08:25.10,ichigo,Comment,0000,0000,0000,,Lo làm chi.
Dialogue: 0,0:08:25.65,0:08:28.98,ichigo,Comment,0000,0000,0000,,Vả lại, đáng lẽ cậu ở gôn ba mà. Cậu làm gì ở đây?
Dialogue: 0,0:08:54.93,0:08:57.22,ichigo,Comment,0000,0000,0000,,Tay đập dự bị, Matsuoka!
Dialogue: 0,0:08:58.35,0:09:00.06,ichigo,Comment,0000,0000,0000,,Come on!
Dialogue: 0,0:09:00.06,0:09:01.43,ichigo,Comment,0000,0000,0000,,Micchan!
Dialogue: 0,0:09:01.85,0:09:03.27,ichigo,Comment,0000,0000,0000,,Ôi trời...
Dialogue: 0,0:09:04.06,0:09:05.02,ichigo,Comment,0000,0000,0000,,Play!
Dialogue: 0,0:09:06.81,0:09:07.86,ichigo,Comment,0000,0000,0000,,Xem tớ tỏa sáng đây.
Dialogue: 0,0:09:17.57,0:09:19.20,ichigo,Comment,0000,0000,0000,,Oh, mistake!
Dialogue: 0,0:09:19.20,0:09:20.45,ichigo,Comment,0000,0000,0000,,Tiếng Nhật quả là khó quá!
Dialogue: 0,0:09:20.45,0:09:21.83,ichigo,Comment,0000,0000,0000,,Bass!
Dialogue: 0,0:09:32.46,0:09:33.76,ichigo,Comment,0000,0000,0000,,Nguy hiểm quá!
Dialogue: 0,0:09:33.76,0:09:36.84,ichigo,Comment,0000,0000,0000,,Sasatsuka, ra hành lang đứng!
Dialogue: 0,0:09:34.26,0:09:36.84,nói trùng,Comment,0000,0000,0000,,Người nào mà mạnh thế.
Dialogue: 0,0:09:41.47,0:09:44.52,ichigo,Comment,0000,0000,0000,,Em xin lỗi, nhưng trái banh nó bay vào đây.
Dialogue: 0,0:09:45.02,0:09:46.14,ichigo,Comment,0000,0000,0000,,A, đây rồi.
Dialogue: 0,0:09:48.35,0:09:49.06,ichigo,Comment,0000,0000,0000,,Ủa?
Dialogue: 0,0:09:49.52,0:09:52.57,ichigo,Comment,0000,0000,0000,,A, Coppola-chan! Yahoo!
Dialogue: 0,0:09:52.57,0:09:55.11,ichigo,Comment,0000,0000,0000,,Ra đây là khối 5, lớp 2.
Dialogue: 0,0:09:57.03,0:09:58.28,ichigo,Comment,0000,0000,0000,,{\fad(500,500)}Ouch!
Dialogue: 0,0:09:58.28,0:10:00.87,ichigo,Comment,0000,0000,0000,,Coppola? Coppola? Coppola?
Dialogue: 0,0:10:00.87,0:10:03.29,ichigo,Comment,0000,0000,0000,,A, không, không phải Coppola. Ý tớ là Ana-chan.
Dialogue: 0,0:10:03.29,0:10:04.62,ichigo,Comment,0000,0000,0000,,Là Ana-chan đó!
Dialogue: 0,0:10:04.87,0:10:06.62,ichigo,Comment,0000,0000,0000,,Nhỉ, Coppola? Ai da...
Dialogue: 0,0:10:09.96,0:10:10.75,ichigo,Comment,0000,0000,0000,,Micchan!
Dialogue: 0,0:10:12.00,0:10:13.00,ichigo,Comment,0000,0000,0000,,Biệt danh ấy mà!
Dialogue: 0,0:10:13.00,0:10:15.76,ichigo,Comment,0000,0000,0000,,Là biệt danh của cậu ấy, coppola...
Dialogue: 0,0:10:15.76,0:10:17.55,ichigo,Comment,0000,0000,0000,,Nói là biệt danh cũng không ổn...
Dialogue: 0,0:10:21.47,0:10:22.72,ichigo,Comment,0000,0000,0000,,Ra là thế.
Dialogue: 0,0:10:23.18,0:10:26.06,ichigo,Comment,0000,0000,0000,,Đúng như chị nghĩ, con bé này là ngọn nguồn rắc rối mà.
Dialogue: 0,0:10:27.48,0:10:30.69,ichigo,Comment,0000,0000,0000,,Nhưng mà Ana-chan, em đặt cái đó xuống đi.
Dialogue: 0,0:10:33.02,0:10:37.44,ichigo,Comment,0000,0000,0000,,Theo lời em kể thì, chưa chắc là Miu cố ý làm thế.
Dialogue: 0,0:10:37.44,0:10:39.36,ichigo,Comment,0000,0000,0000,,Nhưng...
Dialogue: 0,0:10:42.03,0:10:46.00,ichigo,Comment,0000,0000,0000,,Ý chị là, rõ ràng là lỗi của nó, nhưng ngay cả Miu cũng...
Dialogue: 0,0:10:46.00,0:10:50.08,ichigo,Comment,0000,0000,0000,,Ngay cả em cũng biết cái nên làm và cái không nên làm khác nhau như thế nào mà.
Dialogue: 0,0:10:50.08,0:10:51.25,ichigo,Comment,0000,0000,0000,,Đúng là thế ha.
Dialogue: 0,0:10:53.29,0:10:54.42,ichigo,Comment,0000,0000,0000,,Gray.
Dialogue: 0,0:11:01.39,0:11:02.59,ichigo,Comment,0000,0000,0000,,Nói thật đi.
Dialogue: 0,0:11:03.35,0:11:04.22,ichigo,Comment,0000,0000,0000,,Là cố ý.
Dialogue: 0,0:11:10.69,0:11:11.56,ichigo,Comment,0000,0000,0000,,Hammer.
Dialogue: 0,0:11:26.91,0:11:30.12,ichigo,Comment,0000,0000,0000,,Được, ai là người dễ thương nhất trong tụi em?
Dialogue: 0,0:11:38.30,0:11:40.30,ichigo,Comment,0000,0000,0000,,Cả hai đều dễ thương.
Dialogue: 0,0:11:42.97,0:11:44.80,ichigo,Comment,0000,0000,0000,,Thế, thế, nếu vậy...
Dialogue: 0,0:11:45.55,0:11:48.64,ichigo,Comment,0000,0000,0000,,"Ai là người bạn muốn hẹn hò nhất" à?
Dialogue: 0,0:11:49.31,0:11:50.98,ichigo,Comment,0000,0000,0000,,Xem nào.
Dialogue: 0,0:11:53.52,0:11:55.61,ichigo,Comment,0000,0000,0000,,Vậy thì, vậy thì...
Dialogue: 0,0:11:55.98,0:11:58.86,ichigo,Comment,0000,0000,0000,,"Nếu bạn chỉ được dắt theo một người tới một hòn đảo hoang..."
Dialogue: 0,0:12:06.33,0:12:08.08,ichigo,Comment,0000,0000,0000,,Xem nào.
Dialogue: 0,0:12:08.08,0:12:09.87,ichigo,Comment,0000,0000,0000,,Cần mua tỏi tây, cải bắp Trung Hoa...
Dialogue: 0,0:12:09.87,0:12:12.71,ichigo,Comment,0000,0000,0000,,Đậu phụ, hành lá, nấm shiitake...
Dialogue: 0,0:12:12.71,0:12:13.92,ichigo,Comment,0000,0000,0000,,Thịt heo.
Dialogue: 0,0:12:14.63,0:12:16.59,ichigo,Comment,0000,0000,0000,,Mua hết rồi phải không ta?
Dialogue: 0,0:12:16.59,0:12:17.46,ichigo,Comment,0000,0000,0000,,A?
Dialogue: 0,0:12:20.96,0:12:22.05,ichigo,Comment,0000,0000,0000,,Onee-chan!
Dialogue: 0,0:12:22.84,0:12:24.18,ichigo,Comment,0000,0000,0000,,Kẹo!
Dialogue: 0,0:12:24.18,0:12:25.30,ichigo,Comment,0000,0000,0000,,Ư-Ừ...
Dialogue: 0,0:12:25.76,0:12:28.51,ichigo,Comment,0000,0000,0000,,Ờ, bữa nay chị được ba mẹ cho tiền rồi.
Dialogue: 0,0:12:28.97,0:12:30.98,ichigo,Comment,0000,0000,0000,,Mấy đứa cứ mua cái gì tùy thích.
Dialogue: 0,0:12:30.98,0:12:31.93,ichigo,Comment,0000,0000,0000,,Thật chứ ạ?
Dialogue: 0,0:12:32.94,0:12:33.94,ichigo,Comment,0000,0000,0000,,Thế thì...!
Dialogue: 0,0:12:33.94,0:12:35.35,ichigo,Comment,0000,0000,0000,,Ana-chan! Matsuri-chan!
Dialogue: 0,0:12:35.35,0:12:36.31,ichigo,Comment,0000,0000,0000,,Ừ!
Dialogue: 0,0:12:37.86,0:12:41.74,ichigo,Comment,0000,0000,0000,,Hôm nay ba mẹ đều đi lễ truy điệu một người bà con.
Dialogue: 0,0:12:41.74,0:12:44.20,ichigo,Comment,0000,0000,0000,,Nên chị gọi mấy đứa đến ngủ cùng.
Dialogue: 0,0:12:46.99,0:12:48.49,ichigo,Comment,0000,0000,0000,,Onee-chan! Onee-chan!
Dialogue: 0,0:12:48.49,0:12:50.70,ichigo,Comment,0000,0000,0000,,Em muốn mua cái này.
Dialogue: 0,0:12:52.41,0:12:54.50,ichigo,Comment,0000,0000,0000,,Em mua cái này được không ạ?
Dialogue: 0,0:12:54.50,0:12:56.71,ichigo,Comment,0000,0000,0000,,Ừ, em muốn gì cũng được.
Dialogue: 0,0:12:56.71,0:12:58.21,ichigo,Comment,0000,0000,0000,,Này, tại sao thế?!
Dialogue: 0,0:12:58.71,0:13:00.21,ichigo,Comment,0000,0000,0000,,Bởi vì.
Dialogue: 0,0:13:06.93,0:13:09.01,ichigo,Comment,0000,0000,0000,,Có vấn đề gì ạ?
Dialogue: 0,0:13:09.01,0:13:11.22,ichigo,Comment,0000,0000,0000,,Chị nói cái gì tụi em thích cũng được mà.
Dialogue: 0,0:13:11.22,0:13:13.02,ichigo,Comment,0000,0000,0000,,Bánh Phô Mai Loại Nhỏ...
Dialogue: 0,0:13:14.31,0:13:15.73,ichigo,Comment,0000,0000,0000,,...không phải là khó đọc lắm sao?
Dialogue: 0,0:13:15.73,0:13:16.60,ichigo,Comment,0000,0000,0000,,Hả?
Dialogue: 0,0:13:16.60,0:13:19.61,ichigo,Comment,0000,0000,0000,,Có hai chữ "chi" liên tiếp, nên đọc khó lắm.
Dialogue: 0,0:13:19.61,0:13:20.98,ichigo,Comment,0000,0000,0000,,Puchi Chiizu...
Dialogue: 0,0:13:21.61,0:13:23.49,ichigo,Comment,0000,0000,0000,,Hình như có nhiều âm "ii" quá.
Dialogue: 0,0:13:23.49,0:13:25.03,ichigo,Comment,0000,0000,0000,,Vì thế mà em không được mua?
Dialogue: 0,0:13:25.03,0:13:26.57,ichigo,Comment,0000,0000,0000,,Đúng rồi đấy, ngực lép!
Dialogue: 0,0:13:27.28,0:13:28.78,ichigo,Comment,0000,0000,0000,,Chị nói ai ngực lép?!
Dialogue: 0,0:13:28.78,0:13:29.91,ichigo,Comment,0000,0000,0000,,Đồ bủn xỉn!
Dialogue: 0,0:13:35.25,0:13:37.25,ichigo,Comment,0000,0000,0000,,Puchi Chiizu.
Dialogue: 0,0:13:37.25,0:13:38.50,ichigo,Comment,0000,0000,0000,,Đọc dễ mà.
Dialogue: 0,0:13:41.63,0:13:42.80,ichigo,Comment,0000,0000,0000,,Được thôi.
Dialogue: 0,0:13:45.05,0:13:47.26,ichigo,Comment,0000,0000,0000,,Đừng có lúc nào cũng bám chặt Onee-chan.
Dialogue: 0,0:13:47.26,0:13:48.76,ichigo,Comment,0000,0000,0000,,Hai người hẹn hò hay sao hả, đồ ngực...!
Dialogue: 0,0:13:54.14,0:13:57.73,ichigo,Comment,0000,0000,0000,,Tụi chị không hẹn hò, nhưng đây là lần đầu tiên Ana-chan tới cửa hàng này.
Dialogue: 0,0:13:57.73,0:14:00.06,ichigo,Comment,0000,0000,0000,,Nếu em ấy bị lạc thì tệ lắm, biết chưa?
Dialogue: 0,0:14:00.06,0:14:01.02,ichigo,Comment,0000,0000,0000,,Nhỉ?
Dialogue: 0,0:14:06.20,0:14:07.49,ichigo,Comment,0000,0000,0000,,Mình đi chứ, Ana-chan?
Dialogue: 0,0:14:08.82,0:14:11.16,ichigo,Comment,0000,0000,0000,,Để dành tiền mà mua, ngực lép!
Dialogue: 0,0:14:11.53,0:14:14.79,ichigo,Comment,0000,0000,0000,,Cái này khác mà! Là Bánh Phô Mai Loại Nhỏ Hiếm mà!
Dialogue: 0,0:14:18.12,0:14:19.17,ichigo,Comment,0000,0000,0000,,Hơ?
Dialogue: 0,0:14:19.71,0:14:21.25,ichigo,Comment,0000,0000,0000,,Hai cậu cũng ngực lép luôn!
Dialogue: 0,0:14:28.09,0:14:29.30,ichigo,Comment,0000,0000,0000,,Hử?
Dialogue: 0,0:14:29.30,0:14:32.35,ichigo,Comment,0000,0000,0000,,Chị đã nói "không" rồi mà!
Dialogue: 0,0:14:32.35,0:14:35.31,ichigo,Comment,0000,0000,0000,,Ồ. Cái này thì được.
Dialogue: 0,0:14:39.40,0:14:41.73,ichigo,Comment,0000,0000,0000,,Dạ. Dạ.
Dialogue: 0,0:14:42.06,0:14:43.65,ichigo,Comment,0000,0000,0000,,Tụi con sắp ăn rồi.
Dialogue: 0,0:14:44.27,0:14:47.70,ichigo,Comment,0000,0000,0000,,Onee-chan chẳng chịu giúp gì cả. Toàn là con làm...
Dialogue: 0,0:14:50.11,0:14:51.95,ichigo,Comment,0000,0000,0000,,Đau quá chừng à!
Dialogue: 0,0:14:51.95,0:14:53.45,ichigo,Comment,0000,0000,0000,,Trông ngon quá!
Dialogue: 0,0:14:54.95,0:14:57.79,ichigo,Comment,0000,0000,0000,,Bữa tối chỉ toàn trẻ con thế này thấy hay hay nhỉ.
Dialogue: 0,0:14:57.79,0:15:00.54,ichigo,Comment,0000,0000,0000,,Mẹ nói bố nhậu say nên đi ngủ rồi.
Dialogue: 0,0:15:01.79,0:15:03.13,ichigo,Comment,0000,0000,0000,,Chúng ta bắt đầu nhé?
Dialogue: 0,0:15:03.13,0:15:05.17,ichigo,Comment,0000,0000,0000,,Itadakimasu!
Dialogue: 0,0:15:12.93,0:15:15.85,ichigo,Comment,0000,0000,0000,,Itadakimasu! Chà! Trông cực kỳ ngon!
Dialogue: 0,0:15:16.22,0:15:17.10,ichigo,Comment,0000,0000,0000,,Này.
Dialogue: 0,0:15:17.10,0:15:19.56,ichigo,Comment,0000,0000,0000,,Đây. Đũa của cậu này, Ana-chan.
Dialogue: 0,0:15:19.56,0:15:21.06,ichigo,Comment,0000,0000,0000,,A, cám ơn cậu.
Dialogue: 0,0:15:21.35,0:15:23.36,ichigo,Comment,0000,0000,0000,,Chị muốn ăn cái gì, hở Nobue-oneechan?
Dialogue: 0,0:15:23.98,0:15:26.28,ichigo,Comment,0000,0000,0000,,Được rồi. Chị tự lấy được.
Dialogue: 0,0:15:26.28,0:15:27.74,ichigo,Comment,0000,0000,0000,,Không cần phải ngại đâu ạ!
Dialogue: 0,0:15:27.74,0:15:30.82,ichigo,Comment,0000,0000,0000,,Có thật là em thích chị đến vậy không?
Dialogue: 0,0:15:30.82,0:15:32.03,ichigo,Comment,0000,0000,0000,,Em thích thật mà.
Dialogue: 0,0:15:32.03,0:15:32.99,ichigo,Comment,0000,0000,0000,,Em yêu chị à?
Dialogue: 0,0:15:33.32,0:15:34.70,ichigo,Comment,0000,0000,0000,,Em yêu chị.
Dialogue: 0,0:15:37.29,0:15:38.66,ichigo,Comment,0000,0000,0000,,Ái!
Dialogue: 0,0:15:38.66,0:15:40.04,ichigo,Comment,0000,0000,0000,,Có sao không, Matsuri-chan?
Dialogue: 0,0:15:40.91,0:15:42.88,ichigo,Comment,0000,0000,0000,,Phải thổi thật mạnh đấy nhé.
Dialogue: 0,0:15:43.42,0:15:44.38,ichigo,Comment,0000,0000,0000,,Đây.
Dialogue: 0,0:15:45.09,0:15:47.63,ichigo,Comment,0000,0000,0000,,Không biết có mặn lắm không.
Dialogue: 0,0:15:47.63,0:15:49.30,ichigo,Comment,0000,0000,0000,,Không sao chứ, Matsuri-chan?
Dialogue: 0,0:15:49.30,0:15:51.51,ichigo,Comment,0000,0000,0000,,Gia vị thế này là ổn mà.
Dialogue: 0,0:15:51.51,0:15:52.59,ichigo,Comment,0000,0000,0000,,Ừ.
Dialogue: 0,0:15:57.18,0:15:58.77,ichigo,Comment,0000,0000,0000,,Matsuri-chan! Matsuri-chan!
Dialogue: 0,0:15:59.77,0:16:01.98,ichigo,Comment,0000,0000,0000,,Đây là thân con lợn.
Dialogue: 0,0:16:01.98,0:16:03.85,ichigo,Comment,0000,0000,0000,,Đừng có nói ba cái chuyện kinh tởm.
Dialogue: 0,0:16:03.85,0:16:04.81,ichigo,Comment,0000,0000,0000,,Matsuri-chan?
Dialogue: 0,0:16:06.07,0:16:08.03,ichigo,Comment,0000,0000,0000,,Em thích ăn cái gì?
Dialogue: 0,0:16:08.03,0:16:12.70,ichigo,Comment,0000,0000,0000,,Um... Em nghĩ là nấm shiitake.
Dialogue: 0,0:16:13.41,0:16:14.91,ichigo,Comment,0000,0000,0000,,Matsuri-chan! Matsuri-chan!
Dialogue: 0,0:16:17.83,0:16:19.29,ichigo,Comment,0000,0000,0000,,Bắp cải héo.
Dialogue: 0,0:16:20.12,0:16:22.04,ichigo,Comment,0000,0000,0000,,Bình thường thôi mà, Matsuri-chan.
Dialogue: 0,0:16:25.79,0:16:27.59,ichigo,Comment,0000,0000,0000,,Nhai bắp cải.
Dialogue: 0,0:16:28.13,0:16:30.09,ichigo,Comment,0000,0000,0000,,Cảm ơn vì bữa ăn!
Dialogue: 0,0:16:46.36,0:16:47.56,ichigo,Comment,0000,0000,0000,,Được.
Dialogue: 0,0:16:48.11,0:16:49.78,ichigo,Comment,0000,0000,0000,,Có ai muốn tắm không?
Dialogue: 0,0:16:51.15,0:16:53.03,ichigo,Comment,0000,0000,0000,,Em sẽ tắm sau.
Dialogue: 0,0:16:53.03,0:16:54.03,ichigo,Comment,0000,0000,0000,,Ana-chan?
Dialogue: 0,0:16:54.78,0:16:56.95,ichigo,Comment,0000,0000,0000,,Lần đầu tiên em ở lại đây, chúng ta tắm chung đi.
Dialogue: 0,0:16:56.95,0:16:58.28,ichigo,Comment,0000,0000,0000,,À, vâng.
Dialogue: 0,0:16:58.28,0:16:59.95,ichigo,Comment,0000,0000,0000,,Ê cậu kia!
Dialogue: 0,0:17:00.41,0:17:02.79,ichigo,Comment,0000,0000,0000,,Em hét âm r để làm gì vậy?
Dialogue: 0,0:17:02.79,0:17:03.96,ichigo,Comment,0000,0000,0000,,Em cũng tắm!
Dialogue: 0,0:17:23.60,0:17:27.15,ichigo,Comment,0000,0000,0000,,N-Nè hai người, đừng nhìn chằm chằm như vậy!
Dialogue: 0,0:17:27.60,0:17:28.65,ichigo,Comment,0000,0000,0000,,À, xin lỗi.
Dialogue: 0,0:17:29.19,0:17:32.28,ichigo,Comment,0000,0000,0000,,Cậu đang quấn khăn tắm, có sao đâu mà?
Dialogue: 0,0:17:32.28,0:17:37.41,ichigo,Comment,0000,0000,0000,,Ư-Ừm, giờ em sẽ kì cọ thân thể nên làm ơn quay đầu sang hướng khác.
Dialogue: 0,0:17:37.41,0:17:39.99,ichigo,Comment,0000,0000,0000,,Đ-Được. Miu, quay đầu sang đây.
Dialogue: 0,0:17:41.41,0:17:44.83,ichigo,Comment,0000,0000,0000,,Ưm, chị nhìn được không?
Dialogue: 0,0:17:44.83,0:17:46.17,ichigo,Comment,0000,0000,0000,,K-Không được.
Dialogue: 0,0:17:46.71,0:17:48.67,ichigo,Comment,0000,0000,0000,,Tớ chỉ nhìn ngực cậu thôi mà.
Dialogue: 0,0:17:48.67,0:17:49.96,ichigo,Comment,0000,0000,0000,,Không!
Dialogue: 0,0:17:49.96,0:17:51.67,ichigo,Comment,0000,0000,0000,,Nè, bồn tắm sẵn sàng rồi này!
Dialogue: 0,0:17:52.92,0:17:53.92,ichigo,Comment,0000,0000,0000,,Ồ!
Dialogue: 0,0:17:55.13,0:17:57.51,ichigo,Comment,0000,0000,0000,,Ana-chan, em để tóc suôn cũng dễ thương lắm.
Dialogue: 0,0:17:57.51,0:17:59.05,ichigo,Comment,0000,0000,0000,,T-Thật ạ?
Dialogue: 0,0:17:59.05,0:18:00.01,ichigo,Comment,0000,0000,0000,,Ừ.
Dialogue: 0,0:18:01.14,0:18:03.27,ichigo,Comment,0000,0000,0000,,Nè, nè, tóc em giờ cũng suôn nè...
Dialogue: 0,0:18:10.61,0:18:13.90,ichigo,Comment,0000,0000,0000,,Sau khi tắm em luôn làm thế à?
Dialogue: 0,0:18:13.90,0:18:15.15,ichigo,Comment,0000,0000,0000,,À, vâng.
Dialogue: 0,0:18:16.45,0:18:18.41,ichigo,Comment,0000,0000,0000,,Ưm, vậy cũng dễ thương đó.
Dialogue: 0,0:18:21.78,0:18:23.95,ichigo,Comment,0000,0000,0000,,Không phải vẫn như thường lệ sao?
Dialogue: 0,0:18:29.00,0:18:30.29,ichigo,Comment,0000,0000,0000,,Tại sao vậy?
Dialogue: 0,0:18:30.29,0:18:32.84,ichigo,Comment,0000,0000,0000,,Ai quan trọng với chị hơn, em hay Ana-chan?
Dialogue: 0,0:18:33.92,0:18:37.34,ichigo,Comment,0000,0000,0000,,Lẽ nào vì thế mà trước giờ em cạnh tranh với Ana-chan?
Dialogue: 0,0:18:37.76,0:18:39.76,ichigo,Comment,0000,0000,0000,,Quên chuyện đó đi! Làm rõ xem nào!
Dialogue: 0,0:18:39.76,0:18:41.80,ichigo,Comment,0000,0000,0000,,Em không để chị về chừng nào chị chưa nói.
Dialogue: 0,0:18:41.80,0:18:43.85,ichigo,Comment,0000,0000,0000,,Ờ... Đây là nhà chị mà.
Dialogue: 0,0:18:45.27,0:18:46.56,ichigo,Comment,0000,0000,0000,,Để xem.
Dialogue: 0,0:18:47.14,0:18:48.31,ichigo,Comment,0000,0000,0000,,Chị nghĩ là Ana-chan.
Dialogue: 0,0:18:48.81,0:18:50.10,ichigo,Comment,0000,0000,0000,,Tỉ số là 8-2
Dialogue: 0,0:18:58.28,0:19:01.28,ichigo,Comment,0000,0000,0000,,Tại sao? Tại sao vậy?
Dialogue: 0,0:19:01.28,0:19:03.58,ichigo,Comment,0000,0000,0000,,Ế, em tưởng thật à?
Dialogue: 0,0:19:05.87,0:19:07.50,ichigo,Comment,0000,0000,0000,,Onee-chan là đồ ngốc.
Dialogue: 0,0:19:07.50,0:19:09.16,ichigo,Comment,0000,0000,0000,,A, xin lỗi, xin lỗi, em mới là số một.
Dialogue: 0,0:19:10.29,0:19:11.25,ichigo,Comment,0000,0000,0000,,Im đi!
Dialogue: 0,0:19:15.46,0:19:16.42,ichigo,Comment,0000,0000,0000,,Um...
Dialogue: 0,0:19:17.34,0:19:18.61,ichigo,Comment,0000,0000,0000,,Xin lỗi, Ana-chan.
Dialogue: 0,0:19:18.61,0:19:21.97,ichigo,Comment,0000,0000,0000,,Em nói với Chi là Miu ngủ với chị tối nay được không?
Dialogue: 0,0:19:21.97,0:19:22.93,ichigo,Comment,0000,0000,0000,,Dạ.
Dialogue: 0,0:19:33.65,0:19:37.82,ichigo,Comment,0000,0000,0000,,Onee-chan chỉ thích mỗi Ana-chan.
Dialogue: 0,0:19:39.07,0:19:40.95,ichigo,Comment,0000,0000,0000,,Không phải đâu.
Dialogue: 0,0:19:41.61,0:19:44.49,ichigo,Comment,0000,0000,0000,,Petit... Petit-chi...
Dialogue: 0,0:19:44.49,0:19:46.18,ichigo,Comment,0000,0000,0000,,Petit-Cheese Pie, hả?
Dialogue: 0,0:19:46.18,0:19:49.37,ichigo,Comment,0000,0000,0000,,Được rồi. Mai chúng ta sẽ đi mua.
Dialogue: 0,0:19:49.37,0:19:50.62,ichigo,Comment,0000,0000,0000,,Chị sẽ mua cho em hai cái.
Dialogue: 0,0:19:51.21,0:19:52.46,ichigo,Comment,0000,0000,0000,,Em không muốn.
Dialogue: 0,0:19:52.92,0:19:54.46,ichigo,Comment,0000,0000,0000,,Em không muốn?
Dialogue: 0,0:19:54.96,0:19:58.30,ichigo,Comment,0000,0000,0000,,Em luôn là người cuối cùng.
Dialogue: 0,0:19:59.05,0:20:01.59,ichigo,Comment,0000,0000,0000,,Cuối cùng? Gì vậy?
Dialogue: 0,0:20:02.09,0:20:04.93,ichigo,Comment,0000,0000,0000,,Ana-chan luôn được ưu tiên.
Dialogue: 0,0:20:05.55,0:20:08.72,ichigo,Comment,0000,0000,0000,,Ana-chan luôn...
Dialogue: 0,0:20:10.56,0:20:12.31,ichigo,Comment,0000,0000,0000,,Chị nói rồi...
Dialogue: 0,0:20:13.06,0:20:15.44,ichigo,Comment,0000,0000,0000,,Đối xử với Matsuri-chan như...
Dialogue: 0,0:20:33.21,0:20:36.42,ichigo,Comment,0000,0000,0000,,Vậy ra nó cũng rất cô đơn.
Dialogue: 0,0:20:37.80,0:20:39.59,ichigo,Comment,0000,0000,0000,,Mình phải đoán trước mới phải.
Dialogue: 0,0:20:52.14,0:20:53.35,ichigo,Comment,0000,0000,0000,,Trễ vậy rồi sao?
Dialogue: 0,0:20:57.98,0:20:59.53,ichigo,Comment,0000,0000,0000,,Gối đâu rồi?
Dialogue: 0,0:20:59.53,0:21:01.07,ichigo,Comment,0000,0000,0000,,Chào buổi sáng, Onee-chan!
Dialogue: 0,0:21:07.37,0:21:08.45,ichigo,Comment,0000,0000,0000,,Nhanh quá đấy!
Dialogue: 0,0:21:09.28,0:21:11.54,ichigo,Comment,0000,0000,0000,,Quả là có chuyện gì không đúng ở đây.
Dialogue: 0,0:21:14.96,0:21:16.75,ichigo,Comment,0000,0000,0000,,Vui thật đấy.
Dialogue: 0,0:21:16.75,0:21:19.96,ichigo,Comment,0000,0000,0000,,Ừ, bắp ngô trong bữa sáng ngon ghê.
Dialogue: 0,0:21:19.96,0:21:22.38,ichigo,Comment,0000,0000,0000,,Chúng kẹt ở kẽ răng tớ này!
Dialogue: 0,0:21:22.38,0:21:23.34,ichigo,Comment,0000,0000,0000,,Tớ cũng vậy!
Dialogue: 0,0:21:25.01,0:21:29.51,ichigo,Comment,0000,0000,0000,,Và cam cũng ngon nữa, và... ừm...
Dialogue: 0,0:21:29.51,0:21:30.81,ichigo,Comment,0000,0000,0000,,Quả sơn trà.
Dialogue: 0,0:21:30.81,0:21:32.47,ichigo,Comment,0000,0000,0000,,Đã là mùa hè rồi mà?
Dialogue: 0,0:21:33.68,0:21:36.06,ichigo,Comment,0000,0000,0000,,Onee-chan, nếu không nhanh lên, chị sẽ bị trễ đấy!
Dialogue: 0,0:21:36.65,0:21:38.02,ichigo,Comment,0000,0000,0000,,Xin lỗi đã làm phiền chị.
Dialogue: 0,0:21:38.61,0:21:39.69,ichigo,Comment,0000,0000,0000,,Tạm biệt!
Dialogue: 0,0:21:39.69,0:21:40.86,ichigo,Comment,0000,0000,0000,,See you!
Dialogue: 0,0:21:41.94,0:21:44.86,ichigo,Comment,0000,0000,0000,,Nè, giày em đâu rồi?
Dialogue: 0,0:21:44.86,0:21:46.74,ichigo,Comment,0000,0000,0000,,Đằng kia, cạnh chỗ để dù á.
Dialogue: 0,0:21:46.74,0:21:47.70,ichigo,Comment,0000,0000,0000,,Ồ.
Dialogue: 0,0:21:53.16,0:21:54.87,ichigo,Comment,0000,0000,0000,,Vậy bọn em đi đây!
Dialogue: 0,0:21:54.87,0:21:56.33,ichigo,Comment,0000,0000,0000,,Ờ, nhớ cẩn thận.
Dialogue: 0,0:21:59.34,0:22:00.92,ichigo,Comment,0000,0000,0000,,Micchan, cẩn thận!
Dialogue: 0,0:22:04.01,0:22:05.26,ichigo,Comment,0000,0000,0000,,Buồn ngủ quá.
Dialogue: 0,0:22:06.05,0:22:08.01,ichigo,Comment,0000,0000,0000,,Mình sẽ ngủ thêm chút nữa.
Dialogue: 0,0:23:40.81,0:23:42.19,ichigo,Comment,0000,0000,0000,,Hè rồi!
Dialogue: 0,0:23:42.19,0:23:45.98,ichigo,Comment,0000,0000,0000,,Mùa hè là...  mùa nóng đến nỗi bạn không muốn làm gì.
Dialogue: 0,0:23:45.98,0:23:47.99,ichigo,Comment,0000,0000,0000,,Thảm hại quá đấy, Micchan!
Dialogue: 0,0:23:47.99,0:23:49.82,ichigo,Comment,0000,0000,0000,,Đỡ này!
Dialogue: 0,0:23:49.82,0:23:51.20,ichigo,Comment,0000,0000,0000,,Mở cửa sổ!
Dialogue: 0,0:23:51.20,0:23:54.28,ichigo,Comment,0000,0000,0000,,Dừng lại! Khí mát từ máy điều hoà bay đi hết rồi!
Dialogue: 0,0:23:54.28,0:23:58.00,ichigo,Comment,0000,0000,0000,,Thấy chưa, cậu cũng là trẻ con thời hiện đại yếu đuối như tớ thôi.
Dialogue: 0,0:23:58.00,0:24:00.66,ichigo,Comment,0000,0000,0000,,Cậu càng ồn ào thì càng thấy nóng đó.
Dialogue: 0,0:24:00.66,0:24:02.50,ichigo,Comment,0000,0000,0000,,Chúng ta cứ ở yên trong phòng đi.
Dialogue: 0,0:24:02.50,0:24:04.59,ichigo,Comment,0000,0000,0000,,Ờ cậu nói đúng, mùa hè mà.
Dialogue: 0,0:24:05.92,0:24:08.88,ichigo,Comment,0000,0000,0000,,Đón xem tập kế tiếp của "Mashimaro"!
Dialogue: 0,0:24:08.88,0:24:10.88,ichigo,,0000,0000,0000,,

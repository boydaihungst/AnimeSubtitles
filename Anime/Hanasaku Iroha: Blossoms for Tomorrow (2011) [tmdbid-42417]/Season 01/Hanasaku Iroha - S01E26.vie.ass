Dialogue: 0,0:23:27.88,0:23:31.38,Signs,,0,0,0,,{\an5\c&H646464&\fnLNTH-Semi-Casual\fs120\b0\blur0.7\\fscx90\2c&H666666&}Hết
Dialogue: 0,0:06:58.99,0:07:03.37,Dialogue,Bean,0,0,0,,{}Tôi định sẽ về sống cùng với con trai của mình.
Dialogue: 0,0:07:03.62,0:07:08.58,Dialogue,Bean,0,0,0,,{}Thời gian trôi qua thật nhanh, tôi cứ ngỡ ngày mình \Nbắt đầu công việc bên lò nung chỉ mới hôm qua thôi.
Dialogue: 0,0:07:08.58,0:07:12.50,Dialogue,Bean,0,0,0,,{}Có lẽ tôi cũng nên tính đến \Nchuyện hậu sự của mình.
Dialogue: 0,0:07:13.09,0:07:19.39,Dialogue,Bean,0,0,0,,{}Và tôi sẽ không bao giờ quên những kỷ niệm\Nbuồn vui đã cùng mọi người trải qua ở nơi này.
Dialogue: 0,0:07:20.39,0:07:23.31,Dialogue,Bean,0,0,0,,{}Từ tận đáy lòng, tôi cám ơn mọi người.
Dialogue: 0,0:07:24.31,0:07:27.77,Dialogue,Bean,0,0,0,,{}Và bà chủ, cho phép tôi được cám ơn bà.
Dialogue: 0,0:14:37.82,0:14:40.08,Dialogue,Bean,0,0,0,,{}Chào nhé, Sui-chan.
Dialogue: 0,0:01:24.65,0:01:26.15,Dialogue,Boy,0,0,0,,{}"Con muốn thành tàu điện cao tốc!"
Dialogue: 0,0:01:23.15,0:01:24.65,Dialogue,Dad,0,0,0,,{}Con viết gì lên đó thế?
Dialogue: 0,0:01:51.84,0:01:54.76,Dialogue,Enishi,0,0,0,,{}Nè em... thế này có ổn không?
Dialogue: 0,0:07:35.94,0:07:38.74,Dialogue,Enishi,0,0,0,,{}Mẹ, con xin phép.
Dialogue: 0,0:07:39.82,0:07:41.62,Dialogue,Enishi,0,0,0,,{}Con muốn mọi người nghe điều này.
Dialogue: 0,0:07:42.32,0:07:46.79,Dialogue,Enishi,0,0,0,,{}Con nhận ra mình còn nhiều điều phải học hỏi.
Dialogue: 0,0:07:47.29,0:07:50.50,Dialogue,Enishi,0,0,0,,{}Con không thể bắt kịp mẹ hay chị hai.
Dialogue: 0,0:07:57.09,0:08:01.84,Dialogue,Enishi,0,0,0,,{}Tôi đã kêu gọi mọi người theo mình,\N nhưng tôi vẫn chưa đủ khả năng điều hành mọi thứ.
Dialogue: 0,0:08:02.76,0:08:07.27,Dialogue,Enishi,0,0,0,,{}Cho nên con đồng ý với mẹ \Nvề việc đóng cửa Kissuisou.
Dialogue: 0,0:08:10.02,0:08:11.81,Dialogue,Enishi,0,0,0,,{}Nhưng chỉ là tạm thời thôi.
Dialogue: 0,0:08:12.40,0:08:14.77,Dialogue,Enishi,0,0,0,,{}Con không muốn mọi chuyện chấm dứt mãi mãi.
Dialogue: 0,0:08:14.77,0:08:20.70,Dialogue,Enishi,0,0,0,,{}Con sẽ đi học hỏi và rèn luyện\N để một ngày nào đó mở cửa lại nó.
Dialogue: 0,0:08:21.16,0:08:22.53,Dialogue,Enishi,0,0,0,,{}Và khi thời điểm đến...
Dialogue: 0,0:08:23.03,0:08:27.29,Dialogue,Enishi,0,0,0,,{}Con có thể dùng lại cái tên "Kissuisou", \Nđược không ạ?
Dialogue: 0,0:08:27.79,0:08:34.42,Dialogue,Enishi,0,0,0,,{}Con muốn gầy dựng nên một nhà nghỉ \Ncó thể làm cho mẹ cảm thấy hạnh phúc.
Dialogue: 0,0:08:50.64,0:08:52.77,Dialogue,Enishi,0,0,0,,{}Nào mọi người, không say không ngủ!
Dialogue: 0,0:14:28.19,0:14:29.77,Dialogue,Girl,0,0,0,,{}Matsumae-san!
Dialogue: 0,0:14:30.52,0:14:32.48,Dialogue,Girl,0,0,0,,{}Mình sẽ nhắn tin cho cậu!
Dialogue: 0,0:14:32.48,0:14:34.44,Dialogue,Girl,0,0,0,,{}Chắc chắn mình sẽ đến thăm!
Dialogue: 0,0:04:59.03,0:04:59.82,Dialogue,Guy,0,0,0,,{}Có ngay!
Dialogue: 0,0:05:31.39,0:05:33.56,Dialogue,Guy,0,0,0,,{}Đây! Suất đặc biệt của hai đứa!
Dialogue: 0,0:14:02.62,0:14:04.12,Dialogue,Guy,0,0,0,,{}Được rồi, từ từ thôi.
Dialogue: 0,0:00:30.09,0:00:33.10,Dialogue,Jiro,0,0,0,,{}Từ bỏ như thế này có sớm quá không?
Dialogue: 0,0:08:52.77,0:08:53.69,Dialogue,Jiro,0,0,0,,{}Tới bến luôn!
Dialogue: 0,0:11:37.81,0:11:40.56,Dialogue,Jiro,0,0,0,,{}Thì tại vì... đây là phòng anh...
Dialogue: 0,0:04:31.46,0:04:33.04,Dialogue,Ko,0,0,0,,{}Dĩ nhiên rồi.
Dialogue: 0,0:04:36.51,0:04:39.88,Dialogue,Ko,0,0,0,,{}Bộ đồ đó... hợp với bà lắm đấy...
Dialogue: 0,0:04:54.44,0:04:55.82,Dialogue,Ko,0,0,0,,{}Vậy sao.
Dialogue: 0,0:04:56.48,0:04:59.03,Dialogue,Ko,0,0,0,,{}Oji-san, cho một dĩa yakisoba ạ!
Dialogue: 0,0:05:06.54,0:05:10.25,Dialogue,Ko,0,0,0,,{}Đứng trước gian hàng yakisoba vào buổi tối,\N cảm giác đặc biệt hẳn ra nhỉ?
Dialogue: 0,0:05:14.46,0:05:18.34,Dialogue,Ko,0,0,0,,{}Mấy thứ mua ở tiệm hay tự làm ở nhà\N chẳng thể bằng được.
Dialogue: 0,0:05:19.17,0:05:21.84,Dialogue,Ko,0,0,0,,{}Ừ, tui cũng thích ăn yakisoba\N ở mấy sạp bán khuya hơn.
Dialogue: 0,0:05:36.15,0:05:38.03,Dialogue,Ko,0,0,0,,{} Cám ơn ạ!\N Cám ơn ạ!
Dialogue: 0,0:08:57.44,0:08:59.57,Dialogue,Ko,0,0,0,,{}Dạ thôi... Em chưa đủ tuổi ạ...
Dialogue: 0,0:10:15.06,0:10:16.94,Dialogue,Ko,0,0,0,,{}Etou...
Dialogue: 0,0:10:20.90,0:10:23.07,Dialogue,Ko,0,0,0,,{}Ồ... Vậy sao.
Dialogue: 0,0:10:29.03,0:10:30.62,Dialogue,Ko,0,0,0,,{}Không sao đâu.
Dialogue: 0,0:10:30.62,0:10:33.33,Dialogue,Ko,0,0,0,,{}Thật tình thì tui thấy hơi ngại vì được ở miễn phí.
Dialogue: 0,0:10:40.67,0:10:43.30,Dialogue,Ko,0,0,0,,{}Nhưng bà nói đúng, \Nnhà nghỉ này thật tuyệt vời.
Dialogue: 0,0:10:44.17,0:10:47.18,Dialogue,Ko,0,0,0,,{}Nhà nghỉ này \Nvà những người làm việc ở đây...
Dialogue: 0,0:10:47.72,0:10:49.22,Dialogue,Ko,0,0,0,,{}Bà tìm thấy tất cả phải không?
Dialogue: 0,0:10:52.93,0:10:57.31,Dialogue,Ko,0,0,0,,{}Nhìn bà, tui nhận ra rằng không có nơi nào\N là bắt đầu cho chúng ta cả...
Dialogue: 0,0:10:58.15,0:11:04.40,Dialogue,Ko,0,0,0,,{}Chúng ta phải tìm kiếm nơi đó \Nvà gầy dựng nó bằng chính đôi tay mình.
Dialogue: 0,0:11:06.74,0:11:10.95,Dialogue,Ko,0,0,0,,{}Tui cũng sẽ tìm thấy nơi mình thuộc về.
Dialogue: 0,0:11:12.03,0:11:13.83,Dialogue,Ko,0,0,0,,{}Nhỡ như..
Dialogue: 0,0:11:14.24,0:11:21.71,Dialogue,Ko,0,0,0,,{}Nơi mà chúng ta thuộc về...\N lại tình cờ giống nhau... thì...
Dialogue: 0,0:11:22.67,0:11:23.88,Dialogue,Ko,0,0,0,,{}Ohana...
Dialogue: 0,0:11:25.71,0:11:26.97,Dialogue,Ko,0,0,0,,{}O...
Dialogue: 0,0:11:27.80,0:11:28.59,Dialogue,Ko,0,0,0,,{}O...
Dialogue: 0,0:00:50.41,0:00:52.53,Dialogue,Minko,0,0,0,,{}Để nhỏ đi thế có sao không?
Dialogue: 0,0:03:49.17,0:03:50.08,Dialogue,Minko,0,0,0,,{}Đừng nhìn!
Dialogue: 0,0:03:56.72,0:03:57.55,Dialogue,Minko,0,0,0,,{}Nako!
Dialogue: 0,0:03:57.55,0:03:58.22,Dialogue,Minko,0,0,0,,{}Chờ đã{\fscx200}-
Dialogue: 0,0:04:02.64,0:04:03.81,Dialogue,Minko,0,0,0,,{}Đừng cười nữa!
Dialogue: 0,0:09:01.53,0:09:02.78,Dialogue,Minko,0,0,0,,{}Tooru-san!
Dialogue: 0,0:11:47.78,0:11:48.78,Dialogue,Minko,0,0,0,,{}Vậy sao?
Dialogue: 0,0:11:49.28,0:11:51.11,Dialogue,Minko,0,0,0,,{}Cậu ta chỉ gầy và cao thôi.
Dialogue: 0,0:11:51.45,0:11:55.24,Dialogue,Minko,0,0,0,,{}Tiếc cho Tooru-san lại thua người như vậy.
Dialogue: 0,0:11:58.21,0:12:01.46,Dialogue,Minko,0,0,0,,{}V... vậy sao...
Dialogue: 0,0:12:04.00,0:12:05.17,Dialogue,Minko,0,0,0,,{}Ủa?
Dialogue: 0,0:12:27.86,0:12:29.28,Dialogue,Minko,0,0,0,,{}Lạnh quá.
Dialogue: 0,0:12:29.28,0:12:30.78,Dialogue,Minko,0,0,0,,{}Cửa hàng tạp hóa lại xa nữa.
Dialogue: 0,0:12:48.46,0:12:49.63,Dialogue,Minko,0,0,0,,{}Này các cậu...
Dialogue: 0,0:12:50.76,0:12:54.89,Dialogue,Minko,0,0,0,,{}Mình đang tự hỏi tại sao\N mình chỉ muốn làm việc ở Kissuisou.
Dialogue: 0,0:12:58.52,0:12:59.89,Dialogue,Minko,0,0,0,,{}Không phải!
Dialogue: 0,0:12:59.89,0:13:02.14,Dialogue,Minko,0,0,0,,{}T-Thì là vậy, nhưng không phải vậy!
Dialogue: 0,0:13:03.19,0:13:05.77,Dialogue,Minko,0,0,0,,{}Ước mơ... Ước mơ của mình.
Dialogue: 0,0:13:06.15,0:13:08.48,Dialogue,Minko,0,0,0,,{}Nó đã thay đổi.
Dialogue: 0,0:13:10.36,0:13:13.45,Dialogue,Minko,0,0,0,,{}Mình chỉ muốn trở thành đầu bếp.
Dialogue: 0,0:13:14.28,0:13:15.57,Dialogue,Minko,0,0,0,,{}Nhưng giờ...
Dialogue: 0,0:13:16.28,0:13:18.83,Dialogue,Minko,0,0,0,,{}Mình muốn là đầu bếp của Kissuisou.
Dialogue: 0,0:13:45.94,0:13:48.32,Dialogue,Minko,0,0,0,,{}Đừng dùng thì quá khứ chứ.
Dialogue: 0,0:13:52.90,0:13:53.82,Dialogue,Minko,0,0,0,,{}Vẫn ở đó.
Dialogue: 0,0:04:17.36,0:04:18.53,Dialogue,Minko,0,0,40,,{}{\a6}Hột vịt lộn!
Dialogue: 0,0:02:05.10,0:02:08.36,Dialogue,Mom,0,0,0,,{}Mẹ à, mua cho con cây kẹo táo đi...
Dialogue: 0,0:02:11.94,0:02:13.99,Dialogue,Mom,0,0,0,,{}Keo quá đi, cứ y như Ohana vậy.
Dialogue: 0,0:02:17.66,0:02:19.45,Dialogue,Mom,0,0,0,,{}"Tôi muốn có 5,000,000 yên."
Dialogue: 0,0:02:19.45,0:02:21.83,Dialogue,Mom,0,0,0,,{}"Ước gì cổ chân tôi mềm mại hơn."
Dialogue: 0,0:02:22.25,0:02:24.92,Dialogue,Mom,0,0,0,,{}Mọi người ước đơn giản thật nhỉ?
Dialogue: 0,0:02:24.92,0:02:27.00,Dialogue,Mom,0,0,0,,{}Ai cũng có ước mơ của riêng mình,
Dialogue: 0,0:02:28.92,0:02:30.88,Dialogue,Mom,0,0,0,,{}Nhưng xem trộm cũng vui lắm.
Dialogue: 0,0:02:32.80,0:02:34.72,Dialogue,Mom,0,0,0,,{}Mẹ.
Dialogue: 0,0:02:35.09,0:02:36.30,Dialogue,Mom,0,0,0,,{}Xem này.
Dialogue: 0,0:03:27.40,0:03:29.81,Dialogue,Mom,0,0,0,,{}Vậy là... con thua rồi.
Dialogue: 0,0:03:30.19,0:03:35.11,Dialogue,Mom,0,0,0,,{}Nhưng như vậy nghĩa là \Ncon phải "bonboru" nhiều hơn.
Dialogue: 0,0:03:37.36,0:03:39.24,Dialogue,Mom,0,0,0,,{}Không phải Ohana dùng suốt đó sao?
Dialogue: 0,0:07:50.50,0:07:53.63,Dialogue,Mom,0,0,0,,{}Chỉ vì tài năng của chị\N hơn em quá nhiều thôi mà.
Dialogue: 0,0:07:55.46,0:07:56.59,Dialogue,Mom,0,0,0,,{}Xin lỗi.
Dialogue: 0,0:08:08.48,0:08:09.64,Dialogue,Mom,0,0,0,,{}Enishi...
Dialogue: 0,0:08:45.51,0:08:47.68,Dialogue,Mom,0,0,0,,{}Con cái càng ngốc càng đáng yêu nhỉ?
Dialogue: 0,0:09:03.91,0:09:05.78,Dialogue,Mom,0,0,0,,{}Đừng ở lại lâu quá nhé!
Dialogue: 0,0:09:05.78,0:09:07.45,Dialogue,Mom,0,0,0,,{}Nếu có "lỡ làm gì",
Dialogue: 0,0:09:11.58,0:09:13.46,Dialogue,Mom,0,0,0,,{}Có lẽ mình nên qua đó "thám thính" một tý.
Dialogue: 0,0:09:16.00,0:09:17.17,Dialogue,Mom,0,0,0,,{}Chậc...
Dialogue: 0,0:09:23.22,0:09:24.59,Dialogue,Mom,0,0,0,,{}Mẹ này?
Dialogue: 0,0:09:24.97,0:09:26.72,Dialogue,Mom,0,0,0,,{}Tiếp theo mẹ định làm gì?
Dialogue: 0,0:09:32.73,0:09:35.73,Dialogue,Mom,0,0,0,,{}Sao mẹ không đến Tokyo với Ohana?
Dialogue: 0,0:09:44.28,0:09:45.57,Dialogue,Mom,0,0,0,,{}Làm việc ạ?
Dialogue: 0,0:09:53.91,0:09:55.21,Dialogue,Mom,0,0,0,,{}Mẹ...
Dialogue: 0,0:10:09.97,0:10:12.56,Dialogue,Mom,0,0,0,,{}Cám ơn mẹ.
Dialogue: 0,0:21:14.68,0:21:16.30,Dialogue,Mom,0,0,0,,{}Đi học vui vẻ nha!
Dialogue: 0,0:00:10.74,0:00:11.78,Dialogue,Nako,0,0,0,,{}Đã quá!
Dialogue: 0,0:00:18.41,0:00:20.92,Dialogue,Nako,0,0,0,,{}Bởi vì khách từ khắp nơi \Nđổ dồn về đây mà.
Dialogue: 0,0:03:54.71,0:03:56.72,Dialogue,Nako,0,0,0,,{}Tuyệt quá, Min-chi!
Dialogue: 0,0:11:43.52,0:11:47.78,Dialogue,Nako,0,0,0,,{}Bạn trai Ohana-chan dễ mến quá ha?
Dialogue: 0,0:11:55.54,0:11:58.21,Dialogue,Nako,0,0,0,,{}Không phải là chuyện thắng thua!
Dialogue: 0,0:12:05.17,0:12:08.17,Dialogue,Nako,0,0,0,,{}Không phải cậu ngủ lại phòng của bà chủ sao?
Dialogue: 0,0:12:25.40,0:12:27.86,Dialogue,Nako,0,0,0,,{}Mình nghe nói mùa này \Nchúng hay xuất hiện lắm.
Dialogue: 0,0:12:35.03,0:12:37.54,Dialogue,Nako,0,0,0,,{}Ố! Chúng ta quên mua bánh bao rồi...
Dialogue: 0,0:12:46.25,0:12:47.67,Dialogue,Nako,0,0,0,,{}Đẹp quá...
Dialogue: 0,0:13:21.29,0:13:23.50,Dialogue,Nako,0,0,0,,{}Cậu tuyệt thật đấy, Min-chi.
Dialogue: 0,0:13:23.50,0:13:27.67,Dialogue,Nako,0,0,0,,{}Mình vẫn chưa biết ước mơ của mình là gì,
Dialogue: 0,0:13:28.30,0:13:32.30,Dialogue,Nako,0,0,0,,{}...nhưng mình muốn một ngày nào đó\N có thể quay lại Kissuisou.
Dialogue: 0,0:13:32.97,0:13:35.55,Dialogue,Nako,0,0,0,,{}Nơi mà những giấc mơ được sinh ra.
Dialogue: 0,0:13:42.81,0:13:45.94,Dialogue,Nako,0,0,0,,{}Chúng mình đã ở đó nhỉ.
Dialogue: 0,0:13:50.86,0:13:52.90,Dialogue,Nako,0,0,0,,{}Ừm, đang ở đó.
Dialogue: 0,0:08:44.22,0:08:45.51,Dialogue,Nako,0,0,40,,{\an8}Em nữa!
Dialogue: 0,0:00:12.70,0:00:14.29,Dialogue,Ohana,0,0,0,,{}Tuyệt quá!
Dialogue: 0,0:00:14.29,0:00:16.66,Dialogue,Ohana,0,0,0,,{}Không ngờ là có nhiều người\N tham quan Yunosagi đến vậy!
Dialogue: 0,0:00:22.09,0:00:23.92,Dialogue,Ohana,0,0,0,,{}Phải rồi, mấy tấm thẻ ước!
Dialogue: 0,0:00:40.10,0:00:43.40,Dialogue,Ohana,0,0,0,,{}Xin lỗi mọi người, \Nem phải đi đón Kou-chan đây!
Dialogue: 0,0:00:44.82,0:00:47.53,Dialogue,Ohana,0,0,0,,{}Không sao đâu ạ! \NMọi người cứ tự nhiên "bonboru" đi!
Dialogue: 0,0:01:27.48,0:01:30.07,Dialogue,Ohana,0,0,0,,{}{\i1}Mình muốn trở thành gì nhỉ?
Dialogue: 0,0:01:38.04,0:01:39.41,Dialogue,Ohana,0,0,0,,{}{\i1}Không...{\i0}
Dialogue: 0,0:01:39.95,0:01:41.46,Dialogue,Ohana,0,0,0,,{}{\i1}Không phải vậy!{\i0}
Dialogue: 0,0:02:44.23,0:02:47.11,Dialogue,Ohana,0,0,0,,{}{\i1}Mình đã tìm được một mục tiêu.
Dialogue: 0,0:02:47.86,0:02:50.86,Dialogue,Ohana,0,0,0,,{}{\i1}Mình muốn giống như bà chủ,
Dialogue: 0,0:02:51.28,0:02:55.11,Dialogue,Ohana,0,0,0,,{}{\i1}luôn tự hào về công việc của mình \Nvà cố gắng hết sức vì nó,
Dialogue: 0,0:02:55.61,0:03:00.74,Dialogue,Ohana,0,0,0,,{}{\i1}nhưng đôi khi lại hơi trẻ con \Nvà không bao giờ quên điều ước đầu tiên{\fscx200}-
Dialogue: 0,0:03:03.33,0:03:07.54,Dialogue,Ohana,0,0,0,,{}{\i1}Nhưng mình sẽ không thể \Ntìm được mục tiêu đó...
Dialogue: 0,0:03:07.75,0:03:11.88,Dialogue,Ohana,0,0,0,,{}{\i1}...nếu không có lồng đèn \Ncủa mọi người dẫn lối.
Dialogue: 0,0:04:27.16,0:04:29.92,Dialogue,Ohana,0,0,0,,{}Kou-chan, cuối cùng ông cũng đã đến...
Dialogue: 0,0:04:33.04,0:04:34.84,Dialogue,Ohana,0,0,0,,{}Đến rồi...
Dialogue: 0,0:04:41.51,0:04:42.60,Dialogue,Ohana,0,0,0,,{}Kou-chan...
Dialogue: 0,0:04:45.64,0:04:47.43,Dialogue,Ohana,0,0,0,,{}Etou, Anou...
Dialogue: 0,0:04:47.77,0:04:48.60,Dialogue,Ohana,0,0,0,,{}Tui...
Dialogue: 0,0:04:50.52,0:04:52.36,Dialogue,Ohana,0,0,0,,{}Tui muốn ăn yakisoba!
Dialogue: 0,0:05:01.70,0:05:02.91,Dialogue,Ohana,0,0,0,,{}{\i1}Không...{\i0}
Dialogue: 0,0:05:02.91,0:05:05.33,Dialogue,Ohana,0,0,0,,{}{\i1}Mình... mình phải nói với cậu ấy!{\i0}
Dialogue: 0,0:05:18.34,0:05:19.17,Dialogue,Ohana,0,0,0,,{}Tui thích...
Dialogue: 0,0:05:21.84,0:05:22.97,Dialogue,Ohana,0,0,0,,{}...Kou-chan đó!
Dialogue: 0,0:05:25.01,0:05:27.22,Dialogue,Ohana,0,0,0,,{}Tui thích Kou-chan lắm!
Dialogue: 0,0:05:27.68,0:05:28.89,Dialogue,Ohana,0,0,0,,{}Yêu lắm!
Dialogue: 0,0:05:43.74,0:05:49.16,Dialogue,Ohana,0,0,0,,{}{\i1}Lễ hội này lập ra nhằm gửi ước muốn \Ncủa mọi người lên trời cao.
Dialogue: 0,0:07:53.63,0:07:54.59,Dialogue,Ohana,0,0,0,,{}Mẹ à!
Dialogue: 0,0:09:09.83,0:09:11.12,Dialogue,Ohana,0,0,0,,{}Trời ạ!
Dialogue: 0,0:10:16.94,0:10:18.44,Dialogue,Ohana,0,0,0,,{}A! Không phải, không phải!
Dialogue: 0,0:10:18.44,0:10:20.90,Dialogue,Ohana,0,0,0,,{}Gấp ngược lại chỗ này.
Dialogue: 0,0:10:23.57,0:10:26.07,Dialogue,Ohana,0,0,0,,{}Xin lỗi vì ông phải ngủ ở đây,
Dialogue: 0,0:10:33.70,0:10:35.12,Dialogue,Ohana,0,0,0,,{}Không sao đâu!
Dialogue: 0,0:10:35.12,0:10:39.33,Dialogue,Ohana,0,0,0,,{}Thật ra bà cũng cảm thấy vui \Nvì không ngờ rằng tui lại mời bạn đến đây.
Dialogue: 0,0:10:49.80,0:10:51.68,Dialogue,Ohana,0,0,0,,{}Tìm thấy?
Dialogue: 0,0:11:05.49,0:11:06.74,Dialogue,Ohana,0,0,0,,{}Kou-chan...
Dialogue: 0,0:11:29.09,0:11:29.97,Dialogue,Ohana,0,0,0,,{}O...
Dialogue: 0,0:11:29.97,0:11:30.80,Dialogue,Ohana,0,0,0,,{}O{\fscx200}-
Dialogue: 0,0:11:34.31,0:11:35.93,Dialogue,Ohana,0,0,0,,{}Ông làm cái gì vậy hả?!
Dialogue: 0,0:12:02.83,0:12:04.00,Dialogue,Ohana,0,0,0,,{}Xin lỗi, mình đi hơi lâu!
Dialogue: 0,0:12:08.17,0:12:09.42,Dialogue,Ohana,0,0,0,,{}Không.
Dialogue: 0,0:12:09.72,0:12:13.05,Dialogue,Ohana,0,0,0,,{}Ở Kissuisou, đây là nơi mình thuộc về.
Dialogue: 0,0:12:22.77,0:12:25.40,Dialogue,Ohana,0,0,0,,{}Đây là lần đầu mình rung chuông đuổi gấu đấy!
Dialogue: 0,0:12:31.03,0:12:35.03,Dialogue,Ohana,0,0,0,,{}"Tiệc ngủ" lại tiếp tục kéo dài đến ngày tiếp theo!
Dialogue: 0,0:12:54.89,0:12:58.52,Dialogue,Ohana,0,0,0,,{}Có lẽ vì cậu có thể ở gần Tooru-san.
Dialogue: 0,0:13:19.62,0:13:20.87,Dialogue,Ohana,0,0,0,,{}Min-chi...
Dialogue: 0,0:13:36.05,0:13:39.14,Dialogue,Ohana,0,0,0,,{}Nơi mà giấc mơ được sinh ra...
Dialogue: 0,0:13:40.93,0:13:42.31,Dialogue,Ohana,0,0,0,,{}Đẹp tuyệt.
Dialogue: 0,0:13:48.73,0:13:50.86,Dialogue,Ohana,0,0,0,,{}Chúng mình vẫn ở đó, \Nchắc chắn là vậy.
Dialogue: 0,0:13:54.15,0:13:58.28,Dialogue,Ohana,0,0,0,,{}Nhất định trong tương lai...\N chúng mình cũng sẽ ở đó.
Dialogue: 0,0:14:12.84,0:14:18.85,Dialogue,Ohana,0,0,0,,{}{\i1}Mọi người đồng ý xem Kissuisou như một công trình \Nkiến trúc lịch sử ở Yunosagi cần phải bảo tồn.{\i0}
Dialogue: 0,0:14:19.30,0:14:22.93,Dialogue,Ohana,0,0,0,,{}{\i1}Giờ là lúc phải tạm biệt khoảng thời gian\N ngắn ngủi đẹp đẽ vừa qua.{\i0}
Dialogue: 0,0:14:22.93,0:14:26.23,Dialogue,Ohana,0,0,0,,{}{\i1}và đồng thời cũng là khoảng thời gian\N dường như dài vô tận.{\i0}
Dialogue: 0,0:14:26.65,0:14:27.85,Dialogue,Ohana,0,0,0,,{}{\i1}Tất cả mọi người...{\i0}
Dialogue: 0,0:16:53.88,0:16:55.21,Dialogue,Ohana,0,0,0,,{}À...
Dialogue: 0,0:16:55.59,0:16:58.34,Dialogue,Ohana,0,0,0,,{}Etou, con lỡ mất chuyến tàu,
Dialogue: 0,0:17:01.05,0:17:09.14,Dialogue,Ohana,0,0,0,,{}Con muốn nói lời cảm tạ,\N và chào tạm biệt Kissuisou.
Dialogue: 0,0:17:09.60,0:17:12.02,Dialogue,Ohana,0,0,0,,{}À, bà chủ có biết không?
Dialogue: 0,0:17:12.02,0:17:16.36,Dialogue,Ohana,0,0,0,,{}Chỗ này trên sàn gỗ nhìn như mặt người ấy!
Dialogue: 0,0:17:16.36,0:17:18.48,Dialogue,Ohana,0,0,0,,{}À, cả chỗ này nữa.
Dialogue: 0,0:17:18.48,0:17:20.40,Dialogue,Ohana,0,0,0,,{}Nhưng chỗ này thì như...
Dialogue: 0,0:17:22.40,0:17:25.20,Dialogue,Ohana,0,0,0,,{}Khi lần đầu đến Kissuisou,
Dialogue: 0,0:17:30.91,0:17:35.71,Dialogue,Ohana,0,0,0,,{}Và đúng vậy, nó như một vở kịch \Nvới đủ thứ chuyện trên trời dưới đất.
Dialogue: 0,0:17:35.71,0:17:38.25,Dialogue,Ohana,0,0,0,,{}Quá nhiều thứ xảy ra chỉ trong một ngày.
Dialogue: 0,0:17:38.96,0:17:43.76,Dialogue,Ohana,0,0,0,,{}Nhưng bởi vì mọi người đều đang "bonboru" hết mình,\N phải không ạ?
Dialogue: 0,0:17:44.72,0:17:48.93,Dialogue,Ohana,0,0,0,,{}Nếu như lúc nào mọi người \Ncũng quây quần bên nhau và "bonboru",
Dialogue: 0,0:17:51.64,0:17:55.56,Dialogue,Ohana,0,0,0,,{}Thứ làm nên vở kịch là cảm xúc của mỗi diễn viên.
Dialogue: 0,0:17:56.06,0:17:57.11,Dialogue,Ohana,0,0,0,,{}Vậy nên...
Dialogue: 0,0:17:57.11,0:17:58.57,Dialogue,Ohana,0,0,0,,{}Không cần phải ở đây.
Dialogue: 0,0:17:59.02,0:18:02.11,Dialogue,Ohana,0,0,0,,{}Vở kịch có thể diễn ra ở bất kỳ nơi đâu.
Dialogue: 0,0:18:04.86,0:18:06.36,Dialogue,Ohana,0,0,0,,{}Dù vậy!
Dialogue: 0,0:18:06.74,0:18:10.20,Dialogue,Ohana,0,0,0,,{}Dù vậy, một ngày nào đó \Ncon sẽ quay về!
Dialogue: 0,0:18:32.52,0:18:34.64,Dialogue,Ohana,0,0,0,,{}C-Con được phép ạ?
Dialogue: 0,0:18:36.60,0:18:38.69,Dialogue,Ohana,0,0,0,,{}C-Con không trả đâu!
Dialogue: 0,0:18:38.69,0:18:39.94,Dialogue,Ohana,0,0,0,,{}Con...
Dialogue: 0,0:18:39.94,0:18:41.98,Dialogue,Ohana,0,0,0,,{}Con sẽ viết phần còn lại thay ông Đậu!
Dialogue: 0,0:18:42.36,0:18:44.11,Dialogue,Ohana,0,0,0,,{}Vì vậy...
Dialogue: 0,0:18:44.11,0:18:46.32,Dialogue,Ohana,0,0,0,,{}Vì vậy con sẽ quay lại...
Dialogue: 0,0:18:54.83,0:18:56.00,Dialogue,Ohana,0,0,0,,{}Vâng ạ!
Dialogue: 0,0:21:01.87,0:21:03.58,Dialogue,Ohana,0,0,0,,{}Mẹ ơi, đến giờ dậy rồi!
Dialogue: 0,0:21:04.50,0:21:06.80,Dialogue,Ohana,0,0,0,,{}Đừng để trễ buổi họp!
Dialogue: 0,0:21:13.05,0:21:14.26,Dialogue,Ohana,0,0,0,,{}Con đi đây!
Dialogue: 0,0:22:58.91,0:23:01.62,Dialogue,Ohana,0,0,0,,{}{\i1}Giờ đây mình vẫn chỉ là một nụ hoa.
Dialogue: 0,0:23:02.87,0:23:08.37,Dialogue,Ohana,0,0,0,,{}{\i1}Và vì thế mình tiếp tục \Nhướng về mặt trời trên cao kia.
Dialogue: 0,0:23:09.42,0:23:11.67,Dialogue,Ohana,0,0,0,,{}{\i1}Để thỏa cơn khát nước.
Dialogue: 0,0:23:16.34,0:23:20.89,Dialogue,Ohana,0,0,0,,{}{\i1}Mình là... một nụ hoa cố gắng nở rộ!
Dialogue: 0,0:01:30.95,0:01:33.95,Dialogue,Ohana,0,0,0,,{}{\i1}Nhìn từ nhiều khía cạnh, \Ntui là con người thực tế.
Dialogue: 0,0:01:34.78,0:01:36.91,Dialogue,Ohana,0,0,0,,{}{\i1}Như "hình mẫu" mà tui đang sống cùng.
Dialogue: 0,0:06:55.43,0:06:58.39,Signs,Ohana,0,0,0,,{\an7\fscx4000\fscy2000\pos(-12,0)\fnFansub Block\c&HFCFCFC&}A
Dialogue: 0,0:06:55.43,0:06:58.39,Signs,Ohana,0,0,0,,{\an7\c&H000000&\fnLNTH-Semi-Casual\fs100\b0\blur0.7\fscx90}Tập cuối
Dialogue: 0,0:06:55.43,0:06:58.39,Signs,Ohana,0,0,0,,{\an5\c&H000000&\fnLNTH-Semi-Casual\fs120\b0\blur0.7\\fscx90}Khi nụ hoa nở rộ
Dialogue: 0,0:07:27.89,0:07:29.35,Dialogue,Renji,0,0,0,,{}Thật vinh hạnh khi được làm việc cùng ông.
Dialogue: 0,0:08:34.42,0:08:37.17,Dialogue,Renji,0,0,0,,{}Cậu chủ, khi thời điểm đó đến,
Dialogue: 0,0:02:08.48,0:02:10.23,Dialogue,Sui,0,0,0,,{}Thôi giỡn đi.
Dialogue: 0,0:02:10.23,0:02:11.94,Dialogue,Sui,0,0,0,,{}Tự mà mua lấy.
Dialogue: 0,0:02:14.78,0:02:17.08,Dialogue,Sui,0,0,0,,{}Vậy thì con phải nói là Ohana giống ta chứ.
Dialogue: 0,0:02:27.00,0:02:28.92,Dialogue,Sui,0,0,0,,{}...và con không nên xem trộm như thế.
Dialogue: 0,0:02:31.63,0:02:32.80,Dialogue,Sui,0,0,0,,{}Con thiệt là...
Dialogue: 0,0:03:35.49,0:03:36.65,Dialogue,Sui,0,0,0,,{}"Bonboru"?
Dialogue: 0,0:03:40.49,0:03:42.54,Dialogue,Sui,0,0,0,,{}"Bonboru" sao?
Dialogue: 0,0:06:27.24,0:06:28.58,Dialogue,Sui,0,0,0,,{}{\i1}Mình ơi...{\i0}
Dialogue: 0,0:06:29.54,0:06:36.50,Dialogue,Sui,0,0,0,,{}{\i1}Kể từ lúc mình qua đời, tôi luôn nghĩ rằng\N tôi sẽ mãi lẻ loi trên cõi đời này...{\i0}
Dialogue: 0,0:06:38.00,0:06:39.80,Dialogue,Sui,0,0,0,,{}{\i1}Nhưng tôi đã lầm.{\i0}
Dialogue: 0,0:06:40.96,0:06:43.80,Dialogue,Sui,0,0,0,,{}{\i1}Tôi có đứa cháu gái\N muốn sống cuộc sống giống tôi.
Dialogue: 0,0:06:45.80,0:06:48.51,Dialogue,Sui,0,0,0,,{}{\i1}Nếu như có thể, xin hãy một lần nữa...{\i0}
Dialogue: 0,0:06:52.35,0:06:54.06,Dialogue,Sui,0,0,0,,{}{\i1}Chỉ một lần nữa thôi...{\i0}
Dialogue: 0,0:07:34.07,0:07:35.44,Dialogue,Sui,0,0,0,,{}Vinh hạnh đó là của tôi.
Dialogue: 0,0:08:43.14,0:08:45.30,Dialogue,Sui,0,0,0,,{}Mấy người đúng là một lũ ngốc...
Dialogue: 0,0:08:48.81,0:08:50.10,Dialogue,Sui,0,0,0,,{}Đúng nhỉ.
Dialogue: 0,0:09:13.46,0:09:15.08,Dialogue,Sui,0,0,0,,{}Con không nên làm thế.
Dialogue: 0,0:09:27.10,0:09:32.10,Dialogue,Sui,0,0,0,,{}Mẹ sẽ đến chỗ một người bạn\N để làm việc trong một khoảng thời gian.
Dialogue: 0,0:09:38.02,0:09:39.61,Dialogue,Sui,0,0,0,,{}Không đi thì hay hơn.
Dialogue: 0,0:09:39.61,0:09:43.95,Dialogue,Sui,0,0,0,,{}Mẹ định sẽ ở lại đây và làm phục vụ.
Dialogue: 0,0:09:45.82,0:09:48.78,Dialogue,Sui,0,0,0,,{}Mẹ đã nhận ra một điều.
Dialogue: 0,0:09:48.78,0:09:50.37,Dialogue,Sui,0,0,0,,{}Mẹ tự mình làm việc thì tốt hơn là...
Dialogue: 0,0:09:55.54,0:09:57.96,Dialogue,Sui,0,0,0,,{}Mẹ cũng sẽ "bonboru" nữa.
Dialogue: 0,0:09:58.75,0:10:03.38,Dialogue,Sui,0,0,0,,{}Vì con, mẹ sẽ tiếp tục là người mẹ\N mà con ghét cay ghét đắng.
Dialogue: 0,0:14:10.96,0:14:12.21,Dialogue,Sui,0,0,0,,{}Cám ơn.
Dialogue: 0,0:16:12.04,0:16:13.59,Dialogue,Sui,0,0,0,,{}Tạm biệt.
Dialogue: 0,0:16:17.59,0:16:18.92,Dialogue,Sui,0,0,0,,{}Mình...
Dialogue: 0,0:16:49.79,0:16:50.87,Dialogue,Sui,0,0,0,,{}Con...
Dialogue: 0,0:16:51.58,0:16:53.38,Dialogue,Sui,0,0,0,,{}Ta tưởng con đi rồi chứ?
Dialogue: 0,0:18:02.94,0:18:04.20,Dialogue,Sui,0,0,0,,{}Ohana...
Dialogue: 0,0:18:21.63,0:18:22.76,Dialogue,Sui,0,0,0,,{}Ohana.
Dialogue: 0,0:18:26.09,0:18:28.72,Dialogue,Sui,0,0,0,,{}Nếu con định sẽ quay lại,
Dialogue: 0,0:18:35.19,0:18:36.60,Dialogue,Sui,0,0,0,,{}Nếu không muốn cũng không sao.
Dialogue: 0,0:18:50.58,0:18:53.12,Dialogue,Sui,0,0,0,,{}À, ta sẽ đợi con.
Dialogue: 0,0:19:06.63,0:19:08.05,Dialogue,Sui,0,0,0,,{}Ta sẽ đợi.
Dialogue: 0,0:01:49.21,0:01:50.30,Dialogue,Takako,0,0,0,,{}Enishingu.
Dialogue: 0,0:01:50.97,0:01:51.84,Dialogue,Takako,0,0,0,,{}Em vừa thấy{\fscx200}-
Dialogue: 0,0:01:58.14,0:02:03.19,Dialogue,Takako,0,0,0,,{}Không phải em đã bảo rằng \Nchúng ta có thể "say anything" với nhau sao?
Dialogue: 0,0:00:24.59,0:00:27.09,Dialogue,Tohru,0,0,0,,{}Đừng lo, mọi người đều cầm theo cả rồi.
Dialogue: 0,0:00:43.40,0:00:44.82,Dialogue,Tohru,0,0,0,,{}Này! Có cần anh chở đi không?
Dialogue: 0,0:00:48.07,0:00:50.41,Dialogue,Tohru,0,0,0,,{}Nhỏ vẫn ồn ào như xưa nhỉ.
Dialogue: 0,0:00:53.07,0:00:54.45,Dialogue,Tohru,0,0,0,,{}Tất nhiên là không.
Dialogue: 0,0:04:14.48,0:04:19.28,Dialogue,Tohru,0,0,0,,{}Em phải nỗ lực hơn nữa,\N nếu muốn đuổi kịp được anh.
Dialogue: 0,0:08:38.34,0:08:39.42,Dialogue,Tohru,0,0,0,,{}Ren-san, cháu cũng vậy!
Dialogue: 0,0:08:53.69,0:08:55.23,Dialogue,Tohru,0,0,0,,{}Nghe vui đấy!
Dialogue: 0,0:08:55.23,0:08:57.11,Dialogue,Tohru,0,0,0,,{}Này, chú mày cũng uống luôn nhé!
Dialogue: 0,0:08:59.57,0:09:01.53,Dialogue,Tohru,0,0,0,,{}Giữ nề nếp làm gì nữa?

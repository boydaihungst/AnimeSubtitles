Dialogue: 0,0:04:48.88,0:04:49.82,[Rito],,0000,0000,0000,,Lép dữ?
Dialogue: 0,0:04:57.00,0:04:59.89,[Harem],,0000,0000,0000,,Ngực lép thì kệ xác người ta~~~!!!
Dialogue: 0,0:04:59.89,0:05:01.37,[Rito],,0000,0000,0000,,Em có phán gì đâu!
Dialogue: 0,0:05:01.60,0:05:02.79,[Sub_Phụ],,0000,0000,0000,,Mọi người à.
Dialogue: 0,0:05:03.00,0:05:05.96,[Sub_Phụ],,0000,0000,0000,,Chúng ta chơi bổ dưa không nào?
Dialogue: 0,0:05:06.19,0:05:06.93,[Harem],,0000,0000,0000,,Ah~!
Dialogue: 0,0:05:06.93,0:05:08.14,[Harem],,0000,0000,0000,,Nghe hay đó!
Dialogue: 0,0:05:12.26,0:05:14.12,[Sub_Phụ][00],,0000,0000,0000,,Ta sẽ cho các người thấy...
Dialogue: 0,0:05:14.12,0:05:18.68,[Sub_Phụ][00],,0000,0000,0000,,Tuyệt kĩ bổ dưa "quỷ khốc thần sầu" ta để dành đến dịp mới phô diễn ra!
Dialogue: 0,0:05:19.53,0:05:21.29,[Sub_Phụ],,0000,0000,0000,,Nữ hoàng dưa sẽ là t–
Dialogue: 0,0:05:21.29,0:05:22.33,[Sub_Phụ],,0000,0000,0000,,Ah~~~!
Dialogue: 0,0:05:21.29,0:05:22.34,[Sub_Phụ],,0000,0000,0000,,Saki-sama!
Dialogue: 0,0:05:30.44,0:05:31.75,[Sub_Phụ],,0000,0000,0000,,Yuuki Rito~!
Dialogue: 0,0:05:32.52,0:05:35.58,[Sub_Phụ],,0000,0000,0000,,Sao ngươi lần nào cũng phá ta là sao...!
Dialogue: 0,0:05:37.26,0:05:39.24,[Harem],,0000,0000,0000,,Anh làm trò gì vậy?
Dialogue: 0,0:05:39.24,0:05:41.24,[Harem],,0000,0000,0000,,Vẫn lộ mặt dâm tặc mọi ngày thôi mà.
Dialogue: 0,0:05:41.51,0:05:42.65,[Sub_Phụ],,0000,0000,0000,,Thật là...
Dialogue: 0,0:05:42.65,0:05:45.85,[Sub_Phụ],,0000,0000,0000,,Làm phí hết dưa hàng hiệu nhà người ta rồi!
Dialogue: 0,0:05:45.93,0:05:47.59,[Harem],,0000,0000,0000,,Đều tại Nana cả đó.
Dialogue: 0,0:05:48.70,0:05:49.97,[Harem],,0000,0000,0000,,A, phải rồi!
Dialogue: 0,0:05:49.97,0:05:51.33,[Harem],,0000,0000,0000,,Momo, em có dưa không?
Dialogue: 0,0:05:52.98,0:05:58.30,[Harem],,0000,0000,0000,,Momo nhà em sưu tầm đủ loại cây trái khắp vũ trụ đó!
Dialogue: 0,0:05:58.30,0:06:01.73,[Harem],,0000,0000,0000,,Ở hành tinh dưa hấu có loại quả ăn được đó, Chị hai!
Dialogue: 0,0:06:13.42,0:06:14.75,[Harem],,0000,0000,0000,,Thế này đã đủ tiêu chuẩn chưa nào?
Dialogue: 0,0:06:21.28,0:06:23.30,[Sub_Phụ],,0000,0000,0000,,Chơi bổ dưa với cái thứ này á?
Dialogue: 0,0:06:23.30,0:06:25.85,[Sub_Phụ],,0000,0000,0000,,Cơ mà, cái này mà cũng là dưa á?
Dialogue: 0,0:06:25.85,0:06:27.35,[Harem],,0000,0000,0000,,Các chị khỏi phải lo!
Dialogue: 0,0:06:27.35,0:06:30.01,[Harem],,0000,0000,0000,,Nó là thức ăn, nên cũng biết điều lắm.
Dialogue: 0,0:06:30.01,0:06:30.94,[Harem],,0000,0000,0000,,Mikan.
Dialogue: 0,0:06:31.58,0:06:33.57,[Harem],,0000,0000,0000,,Bổ dưa là chơi thế nào vậy?
Dialogue: 0,0:06:36.83,0:06:38.64,[Harem],,0000,0000,0000,,Có ổn không vậy?
Dialogue: 0,0:06:38.64,0:06:40.31,[Harem],,0000,0000,0000,,Thấy ghê ghê sao á.
Dialogue: 0,0:06:41.48,0:06:42.99,[Sub_Phụ],,0000,0000,0000,,Trông mà lợm cả giọng.
Dialogue: 0,0:06:42.99,0:06:44.86,[Sub_Phụ],,0000,0000,0000,,Ừa, phải đó.
Dialogue: 0,0:06:46.90,0:06:49.01,[Harem],,0000,0000,0000,,Đừng có nói vậy!
Dialogue: 0,0:06:49.01,0:06:51.89,[Harem],,0000,0000,0000,,Nó tự hào về mùi vị của mình lắm!
Dialogue: 0,0:06:53.48,0:06:55.12,[Sub_Phụ],,0000,0000,0000,,Để đây cho mà hay...
Dialogue: 0,0:06:55.12,0:06:59.41,[Sub_Phụ],,0000,0000,0000,,Thức ăn chỉ cần trông ngon mắt thì có dở mấy vẫn thấy ngon như thường.
Dialogue: 0,0:06:59.41,0:07:00.42,[Sub_Phụ],,0000,0000,0000,,Có điều...
Dialogue: 0,0:07:00.42,0:07:04.17,[Sub_Phụ],,0000,0000,0000,,Dưa hấu cũng bày đặt có lòng tự trọng rõ đúng nực cười.
Dialogue: 0,0:07:07.92,0:07:10.05,[Harem],,0000,0000,0000,,Ngài dưa hấu, hãy bình tĩnh!
Dialogue: 0,0:07:14.03,0:07:15.74,[Sub_Phụ],,0000,0000,0000,,Dính bê bết hết cả rồi...
Dialogue: 0,0:07:15.74,0:07:17.37,[Sub_Phụ],,0000,0000,0000,,Saki-sama, cố lên!
Dialogue: 0,0:07:17.41,0:07:17.93,[Sub_Phụ],,0000,0000,0000,,Khoan...
Dialogue: 0,0:07:17.93,0:07:19.45,[Sub_Phụ],,0000,0000,0000,,Nước dưa hấu hả!?
Dialogue: 0,0:07:24.40,0:07:25.21,[Sub_Phụ],,0000,0000,0000,,Rito~!
Dialogue: 0,0:07:25.40,0:07:27.85,[Rito],,0000,0000,0000,,Saruyama~~~!
Dialogue: 0,0:07:27.85,0:07:28.52,[Harem],,0000,0000,0000,,Mọi người!
Dialogue: 0,0:07:28.52,0:07:30.75,[Harem],,0000,0000,0000,,Ngài dưa hấu, xin hãy bình tĩnh!
Dialogue: 0,0:07:31.31,0:07:32.19,[Sub_Phụ],,0000,0000,0000,,Đợi đã!
Dialogue: 0,0:07:33.39,0:07:35.95,[Sub_Phụ],,0000,0000,0000,,Còn bố láo làm càn, ta xử đừng có trách!
Dialogue: 0,0:07:35.95,0:07:37.18,[Harem],,0000,0000,0000,,Ren-chan!
Dialogue: 0,0:07:39.41,0:07:41.93,[Harem],,0000,0000,0000,,{\an8}Ren-chan! Cậu có lo được không đấy?
Dialogue: 0,0:07:41.93,0:07:43.99,[Harem],,0000,0000,0000,,{\an8}Cẩn thận không là bị xơi đó.
Dialogue: 0,0:07:40.27,0:07:42.77,[Sub_Phụ],,0000,0000,0000,,Cơ hội hiếm có khoe độ manly với Lala-chan.
Dialogue: 0,0:07:42.77,0:07:44.76,[Sub_Phụ],,0000,0000,0000,,Đâu thể nào không chiến được!
Dialogue: 0,0:07:44.76,0:07:49.65,[Sub_Phụ],,0000,0000,0000,,Nhất chiêu! K.O. Cyclone Grena~~~
Dialogue: 0,0:07:49.65,0:07:51.26,[Harem],,0000,0000,0000,,Ren-chan~!
Dialogue: 0,0:07:55.86,0:07:56.99,[Rito],,0000,0000,0000,,Ren, cậu không sao chứ?
Dialogue: 0,0:07:56.99,0:07:58.02,[Harem],,0000,0000,0000,,Ắt xì~!
Dialogue: 0,0:07:59.47,0:08:00.55,[Rito],,0000,0000,0000,,R- Run!
Dialogue: 0,0:08:01.78,0:08:03.71,[Harem],,0000,0000,0000,,Cuối cùng gặp được anh rồi...
Dialogue: 0,0:08:03.71,0:08:05.26,[Harem],,0000,0000,0000,,Rito-kun!
Dialogue: 0,0:08:02.42,0:08:05.26,[Rito],,0000,0000,0000,,{\an8}N-N-N-N-N-Ngực của cậu...!
Dialogue: 0,0:08:05.85,0:08:07.38,[Harem],,0000,0000,0000,,Rito-kun~!
Dialogue: 0,0:08:06.15,0:08:07.90,[Rito],,0000,0000,0000,,{\an8}Khó thở quá đi!
Dialogue: 0,0:08:08.38,0:08:09.05,[Rito],,0000,0000,0000,,Mikan!
Dialogue: 0,0:08:10.31,0:08:12.29,[Harem],,0000,0000,0000,,Thả ta ra!
Dialogue: 0,0:08:14.46,0:08:15.45,[Harem],,0000,0000,0000,,Quả dưa kia!
Dialogue: 0,0:08:15.45,0:08:18.29,[Harem],,0000,0000,0000,,Buông Mikan ra ngay!
Dialogue: 0,0:08:19.92,0:08:21.78,[Harem],,0000,0000,0000,,UUUU-i daaaaa!
Dialogue: 0,0:08:28.66,0:08:29.76,[Harem],,0000,0000,0000,,Lala-chan!
Dialogue: 0,0:08:31.90,0:08:34.00,[Harem],,0000,0000,0000,,Đ-Đừng bóp đuôi mà...
Dialogue: 0,0:08:34.63,0:08:35.74,[Peke],,0000,0000,0000,,Lala-sama!
Dialogue: 0,0:08:36.36,0:08:37.26,[Harem],,0000,0000,0000,,Chị!
Dialogue: 0,0:08:37.26,0:08:38.72,[Harem],,0000,0000,0000,,Ngài dưa hấu!
Dialogue: 0,0:08:38.72,0:08:40.84,[Harem],,0000,0000,0000,,Đủ rồi! Xin dừng lại đi!
Dialogue: 0,0:08:40.84,0:08:42.08,[Harem],,0000,0000,0000,,{\an8}Ngài dưa hấu!
Dialogue: 0,0:08:41.43,0:08:44.50,[Harem],,0000,0000,0000,,A~~~! Đừng mà~~~!
Dialogue: 0,0:08:42.51,0:08:43.68,[Harem],,0000,0000,0000,,{\an8}Ngài dưa hấu...
Dialogue: 0,0:08:44.39,0:08:45.46,[Harem],,0000,0000,0000,,Ngài dưa hấu!
Dialogue: 0,0:08:45.46,0:08:47.26,[Harem],,0000,0000,0000,,Ah~! Chị...
Dialogue: 0,0:08:47.07,0:08:49.46,[Harem],,0000,0000,0000,,{\an8}Dừng lại đi~~~!
Dialogue: 0,0:08:47.96,0:08:48.86,[Harem],,0000,0000,0000,,Momo!?
Dialogue: 0,0:08:48.86,0:08:52.31,[Harem],,0000,0000,0000,,Điệu này chưa thấy quan tài chưa đổ lệ đây mà.
Dialogue: 0,0:08:52.31,0:08:53.86,[Rito],,0000,0000,0000,,Dừng lại~!
Dialogue: 0,0:08:53.86,0:08:56.90,[Rito],,0000,0000,0000,,Buông Lala ra!
Dialogue: 0,0:08:56.90,0:08:58.55,[Rito],,0000,0000,0000,,Lala, em không sao chứ!?
Dialogue: 0,0:08:58.55,0:08:59.55,[Harem],,0000,0000,0000,,Rito!
Dialogue: 0,0:09:00.90,0:09:01.79,[Rito],,0000,0000,0000,,Mày...!
Dialogue: 0,0:09:01.79,0:09:04.45,[Rito],,0000,0000,0000,,Mau buông cô ấy ra ngay!
Dialogue: 0,0:09:05.22,0:09:06.22,[Harem],,0000,0000,0000,,Yuuki-kun!
Dialogue: 0,0:09:06.22,0:09:06.97,[Harem],,0000,0000,0000,,Nguy hiểm lắm!
Dialogue: 0,0:09:06.97,0:09:07.95,[Harem],,0000,0000,0000,,Ra khỏi đó đi!
Dialogue: 0,0:09:22.93,0:09:24.55,[Harem],,0000,0000,0000,,Làm thế này có đúng không?
Dialogue: 0,0:09:24.55,0:09:26.29,[Harem],,0000,0000,0000,,Bổ dưa là phải thế này chứ gì.
Dialogue: 0,0:09:28.42,0:09:31.75,[Harem],,0000,0000,0000,,Coi vậy chứ cậu ấy cũng rành luật chơi đó ha.
Dialogue: 0,0:09:34.27,0:09:37.10,[Harem],,0000,0000,0000,,Trái dưa đúng ngon quá đi!
Dialogue: 0,0:09:37.10,0:09:38.78,[Sub_Phụ],,0000,0000,0000,,Ừm, phải đó.
Dialogue: 0,0:09:38.78,0:09:40.24,[Harem],,0000,0000,0000,,Thật tốt quá.
Dialogue: 0,0:09:40.24,0:09:43.31,[Harem],,0000,0000,0000,,Coi như là quà đền bù của ngài dưa hấu ha!
Dialogue: 0,0:09:43.31,0:09:45.95,[Harem],,0000,0000,0000,,Xin lỗi mọi người vì mọi rắc rối ạ.
Dialogue: 0,0:09:45.95,0:09:47.66,[Harem],,0000,0000,0000,,Mọi người ăn dưa đi ạ.
Dialogue: 0,0:09:48.16,0:09:50.14,[Sub_Phụ],,0000,0000,0000,,Thôi khỏi.
Dialogue: 0,0:09:53.68,0:09:54.27,[Harem],,0000,0000,0000,,Rito...
Dialogue: 0,0:09:56.81,0:09:59.16,[Peke],,0000,0000,0000,,Lala-sama, có chuyện gì à?
Dialogue: 0,0:10:00.94,0:10:02.15,[Harem],,0000,0000,0000,,Không có gì!
Dialogue: 0,0:10:04.41,0:10:06.23,[Rito],,0000,0000,0000,,Ngó qua ngó lại chả thấy Saruyama đâu nhỉ?
Dialogue: 0,0:10:22.64,0:10:23.68,[Sub_Phụ],,0000,0000,0000,,Haruna!
Dialogue: 0,0:10:23.68,0:10:24.85,[Sub_Phụ],,0000,0000,0000,,Bài kiểm tra của cậu ra sao?
Dialogue: 0,0:10:24.99,0:10:25.52,[Sub_Phụ],,0000,0000,0000,,Cho xem nào!
Dialogue: 0,0:10:25.52,0:10:26.19,[Sub_Phụ][01],,0000,0000,0000,,Cho xem nào!
Dialogue: 0,0:10:26.93,0:10:27.72,[Sub_Phụ],,0000,0000,0000,,Wua~!
Dialogue: 0,0:10:27.72,0:10:28.99,[Sub_Phụ],,0000,0000,0000,,98!?
Dialogue: 0,0:10:28.99,0:10:29.94,[Sub_Phụ],,0000,0000,0000,,Thiệt không đó?
Dialogue: 0,0:10:29.94,0:10:32.75,[Harem],,0000,0000,0000,,Này, đừng có la lớn quá!
Dialogue: 0,0:10:32.75,0:10:34.52,[Sub_Phụ],,0000,0000,0000,,Cậu đúng giỏi ghê ha.
Dialogue: 0,0:10:34.52,0:10:36.33,[Harem],,0000,0000,0000,,Thôi, mau trả cho tớ đi!
Dialogue: 0,0:10:36.33,0:10:39.63,[Sub_Phụ],,0000,0000,0000,,Nấn ná coi chút có chết chóc gì đâu.
Dialogue: 0,0:10:39.73,0:10:40.56,[Rito],,0000,0000,0000,,Toi rồi!
Dialogue: 0,0:10:41.12,0:10:42.67,[Rito],,0000,0000,0000,,Điệu này là toi thật rồi!
Dialogue: 0,0:10:43.17,0:10:46.68,[Rito],,0000,0000,0000,,Điểm đợt này sao mà thấp quá vậy trời!
Dialogue: 0,0:10:46.94,0:10:48.08,[Rito],,0000,0000,0000,,Cứ thế này thì...
Dialogue: 0,0:10:50.52,0:10:53.36,[Harem],,0000,0000,0000,,Yuuki-kun, thiệt là cậu ngốc đến thế à?
Dialogue: 0,0:10:54.64,0:10:55.77,[Harem],,0000,0000,0000,,Xin lỗi nhá!
Dialogue: 0,0:10:55.77,0:10:56.88,[Harem],,0000,0000,0000,,Đây là...
Dialogue: 0,0:10:56.88,0:10:58.76,[Harem],,0000,0000,0000,,Ghét mấy đứa ngu ngếch lắm.
Dialogue: 0,0:10:59.58,0:11:00.57,[Rito],,0000,0000,0000,,Đừng mà!
Dialogue: 0,0:11:00.57,0:11:02.25,[Rito],,0000,0000,0000,,Mai là phải kiểm lại rồi!
Dialogue: 0,0:11:02.25,0:11:03.45,[Rito],,0000,0000,0000,,Làm thế nào bây giờ!?
Dialogue: 0,0:11:03.45,0:11:04.60,[Rito],,0000,0000,0000,,Làm sao bây giờ!?
Dialogue: 0,0:11:05.42,0:11:08.22,[Rito],,0000,0000,0000,,Kiểu này phải quyết chí học hành thôi.
Dialogue: 0,0:11:08.22,0:11:10.23,[Rito],,0000,0000,0000,,Phải nhồi càng nhiều càng tốt...
Dialogue: 0,0:11:24.09,0:11:25.95,[Harem],,0000,0000,0000,,Tính nhìn tới chừng nào mới thỏa mãn hả?
Dialogue: 0,0:11:27.03,0:11:28.44,[Rito],,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:11:31.75,0:11:32.68,[Rito],,0000,0000,0000,,A, cái đó...!
Dialogue: 0,0:11:35.44,0:11:36.79,[Harem],,0000,0000,0000,,Trả nè.
Dialogue: 0,0:11:36.79,0:11:38.88,[Rito],,0000,0000,0000,,Em vừa mới cười đúng không hả?
Dialogue: 0,0:11:41.05,0:11:42.22,[Harem],,0000,0000,0000,,Học...
Dialogue: 0,0:11:42.22,0:11:45.59,[Harem],,0000,0000,0000,,Với người khác há chẳng tốt hơn sao?
Dialogue: 0,0:11:46.22,0:11:48.89,[Harem],,0000,0000,0000,,Ở xứ này vẫn gọi nó là "học nhóm" là gì?
Dialogue: 0,0:11:51.05,0:11:54.55,[Harem],,0000,0000,0000,,Có những người hoàn hảo gần ngay trước mắt ngươi đó.
Dialogue: 0,0:12:02.96,0:12:04.31,[Rito],,0000,0000,0000,,Lala làm đấy ư?
Dialogue: 0,0:12:04.56,0:12:05.58,[Rito],,0000,0000,0000,,Cô ấy...
Dialogue: 0,0:12:05.58,0:12:07.61,[Rito],,0000,0000,0000,,Thông minh đến thế sao kìa?
Dialogue: 0,0:12:07.75,0:12:09.36,[Peke],,0000,0000,0000,,Lảm nhảm cái gì vậy?
Dialogue: 0,0:12:09.59,0:12:14.58,[Peke],,0000,0000,0000,,Lala-sama là một trong những thiên tài vĩ đại nhất ngân hà đó.
Dialogue: 0,0:12:14.58,0:12:21.37,[Peke],,0000,0000,0000,,Đến nỗi có nhiều kẻ tính lợi dụng trí thông thông đó để chế tạo vũ khí nữa.
Dialogue: 0,0:12:21.53,0:12:25.55,[Rito],,0000,0000,0000,,Nhưng hồi trước anh thấy điểm thi của Lala...
Dialogue: 0,0:12:25.73,0:12:30.65,[Peke],,0000,0000,0000,,Chẳng qua, là hồi đó Lala-sama chưa biết chữ Trái đất đó thôi.
Dialogue: 0,0:12:31.68,0:12:34.03,[Rito],,0000,0000,0000,,Thì ra thế cô ấy mới không hiểu câu hỏi à...
Dialogue: 0,0:12:34.03,0:12:34.71,[Peke],,0000,0000,0000,,Mà...
Dialogue: 0,0:12:34.71,0:12:41.40,[Peke],,0000,0000,0000,,Xài trí thông minh tuyệt đỉnh đó vào mấy phát minh vớ vẩn đúng là lãng phí quá mà.
Dialogue: 0,0:12:43.96,0:12:45.82,[Harem],,0000,0000,0000,,Thật dễ chịu quá đi, Rito~!
Dialogue: 0,0:12:46.09,0:12:48.23,[Rito],,0000,0000,0000,,Đừng có một mình một khăn đi ra chứ!
Dialogue: 0,0:12:48.23,0:12:50.79,[Harem],,0000,0000,0000,,Kệ đi mà! Đâu phải nhồng nhỗng hết đâu anh.
Dialogue: 0,0:12:51.95,0:12:52.92,[Harem],,0000,0000,0000,,Anh đang làm gì vậy?
Dialogue: 0,0:12:52.92,0:12:54.09,[Harem],,0000,0000,0000,,Học à?
Dialogue: 0,0:12:54.20,0:12:55.51,[Rito],,0000,0000,0000,,Ơ-Ừm...
Dialogue: 0,0:12:55.93,0:12:58.37,[Rito],,0000,0000,0000,,Nhưng anh chả hiểu gì hết trơn.
Dialogue: 0,0:12:58.37,0:12:58.65,[Harem],,0000,0000,0000,,Đâu nào!
Dialogue: 0,0:12:58.65,0:12:59.42,[Harem][01],,0000,0000,0000,,Đâu nào!
Dialogue: 0,0:13:03.24,0:13:04.75,[Harem],,0000,0000,0000,,Chỗ bài này thì...
Dialogue: 0,0:13:06.48,0:13:07.55,[Harem],,0000,0000,0000,,Sao thế anh?
Dialogue: 0,0:13:08.74,0:13:11.67,[Rito],,0000,0000,0000,,Em chỉ bài cho thì không sao...
Dialogue: 0,0:13:11.67,0:13:14.31,[Rito],,0000,0000,0000,,Nhưng làm ơn mặc đồ vô cái đi.
Dialogue: 0,0:13:15.81,0:13:17.37,[Harem],,0000,0000,0000,,Vậy, Peke!
Dialogue: 0,0:13:19.49,0:13:20.56,[Peke],,0000,0000,0000,,Vâng...
Dialogue: 0,0:13:41.47,0:13:43.01,[Harem],,0000,0000,0000,,Bài này thì...
Dialogue: 0,0:13:43.01,0:13:45.84,[Harem],,0000,0000,0000,,Anh dùng công thức này đó.
Dialogue: 0,0:13:45.84,0:13:47.07,[Rito],,0000,0000,0000,,Ra là thế ha!
Dialogue: 0,0:13:47.51,0:13:48.92,[Rito],,0000,0000,0000,,Thế còn bài này?
Dialogue: 0,0:13:48.92,0:13:51.55,[Harem],,0000,0000,0000,,Chỗ đó anh dùng lũy thừa nha...
Dialogue: 0,0:13:54.47,0:13:55.45,[Harem],,0000,0000,0000,,Có thế thôi.
Dialogue: 0,0:13:56.30,0:13:57.77,[Rito],,0000,0000,0000,,Ghê thật!
Dialogue: 0,0:13:57.77,0:13:59.66,[Rito],,0000,0000,0000,,Lala em thông minh ghê!
Dialogue: 0,0:13:59.91,0:14:01.67,[Rito],,0000,0000,0000,,Giỏi thật đó!
Dialogue: 0,0:14:04.28,0:14:05.60,[Harem],,0000,0000,0000,,Hông hiểu sao...
Dialogue: 0,0:14:05.60,0:14:07.82,[Harem],,0000,0000,0000,,Được Rito khen làm em vui lắm.
Dialogue: 0,0:14:13.07,0:14:14.57,[Harem],,0000,0000,0000,,Em nhặt hộ anh nhé?
Dialogue: 0,0:14:15.15,0:14:15.51,[Rito],,0000,0000,0000,,Được rồi.
Dialogue: 0,0:14:15.51,0:14:16.41,[Rito][01],,0000,0000,0000,,Được rồi.
Dialogue: 0,0:14:17.83,0:14:18.32,[Rito][00],,0000,0000,0000,,Chết rồi!
Dialogue: 0,0:14:18.32,0:14:18.94,[Rito][02],,0000,0000,0000,,Chết rồi!
Dialogue: 0,0:14:18.94,0:14:20.75,[Rito][00],,0000,0000,0000,,Rối hết cả lên rồi.
Dialogue: 0,0:14:20.75,0:14:22.98,[Rito][00],,0000,0000,0000,,Phải tập trung học mới được.
Dialogue: 0,0:14:24.29,0:14:26.08,[Rito],,0000,0000,0000,,Lala, đồ em kìa...!
Dialogue: 0,0:14:26.55,0:14:27.63,[Harem],,0000,0000,0000,,Peke!?
Dialogue: 0,0:14:27.63,0:14:31.95,[Peke],,0000,0000,0000,,Xin lỗi, em hết hơi rồi.
Dialogue: 0,0:14:34.40,0:14:37.95,[Rito],,0000,0000,0000,,M-M-M-M-Mau ch...!
Dialogue: 0,0:14:40.97,0:14:42.13,[Harem],,0000,0000,0000,,Rito...
Dialogue: 0,0:14:42.13,0:14:43.55,[Harem],,0000,0000,0000,,Anh vất vả rồi.
Dialogue: 0,0:14:43.55,0:14:45.93,[Harem],,0000,0000,0000,,Em đem c-à phê-ê...
Dialogue: 0,0:14:47.54,0:14:49.96,[Rito],,0000,0000,0000,,H-Hiểu n-nh-ầm...
Dialogue: 0,0:14:49.96,0:14:55.59,[Rito],,0000,0000,0000,,Tụi a-a-anh... đa-ng học... đ-đ-đang... đang... đang h-h-học...
Dialogue: 0,0:14:56.34,0:14:58.71,[Harem],,0000,0000,0000,,Tiếp tục chăm chỉ đi nhá.
Dialogue: 0,0:14:58.71,0:15:00.82,[Harem],,0000,0000,0000,,Em chả biết anh đang học cái gì nữa.
Dialogue: 0,0:15:00.82,0:15:03.56,[Rito],,0000,0000,0000,,HIỂU LẦM MÀ~~~!!!
Dialogue: 0,0:15:09.60,0:15:10.82,[Rito],,0000,0000,0000,,Xong rồi!!!
Dialogue: 0,0:15:10.82,0:15:12.91,[Rito],,0000,0000,0000,,Chữ bay hết còn đâu!
Dialogue: 0,0:15:12.91,0:15:13.84,[Sub_Phụ],,0000,0000,0000,,Câm đi!
Dialogue: 0,0:15:24.49,0:15:25.73,[Harem],,0000,0000,0000,,Rito!
Dialogue: 0,0:15:25.73,0:15:26.79,[Rito],,0000,0000,0000,,Lala!
Dialogue: 0,0:15:28.62,0:15:31.48,[Rito],,0000,0000,0000,,Đã bảo đừng có bạ chút là ôm mà~!
Dialogue: 0,0:15:31.24,0:15:32.57,[Harem],,0000,0000,0000,,Rito~!
Dialogue: 0,0:15:34.08,0:15:37.99,[Sub_Phụ],,0000,0000,0000,,Coi bộ chả tiến triển gì mấy nhỉ, Lala-chi.
Dialogue: 0,0:15:38.93,0:15:44.47,[Sub_Phụ],,0000,0000,0000,,Theo tớ nghĩ, cậu nên học thêm về tình yêu đi.
Dialogue: 0,0:15:44.47,0:15:46.17,[Harem],,0000,0000,0000,,Học yêu ư?
Dialogue: 0,0:15:47.85,0:15:50.06,[Harem],,0000,0000,0000,,Cậu nói phải.
Dialogue: 0,0:15:50.06,0:15:54.29,[Harem],,0000,0000,0000,,Tớ không biết làm sao hấp dẫn ảnh nữa.
Dialogue: 0,0:15:54.70,0:15:58.35,[Sub_Phụ],,0000,0000,0000,,Chính vì thế tớ sẽ cho cậu mượn bảo bối vậy!
Dialogue: 0,0:15:58.61,0:15:59.74,[Sub_Phụ],,0000,0000,0000,,Da~!
Dialogue: 0,0:15:59.74,0:16:01.87,[Sub_Phụ],,0000,0000,0000,,Hàng hot hiện giờ đó, 100 kĩ thuật quyến rũ!
Dialogue: 0,0:16:02.27,0:16:05.14,[Sub_Phụ],,0000,0000,0000,,Cái này mà xài với Yuuki là nhất.
Dialogue: 0,0:16:05.14,0:16:06.59,[Harem],,0000,0000,0000,,Xài cái này...
Dialogue: 0,0:16:07.23,0:16:08.83,[Harem],,0000,0000,0000,,Với Rito?
Dialogue: 0,0:16:13.86,0:16:15.95,[Harem],,0000,0000,0000,,Yêu ư?
Dialogue: 0,0:16:15.95,0:16:19.21,[Harem],,0000,0000,0000,,Có vẻ khó nhưng mình sẽ cố gắng ha!
Dialogue: 0,0:16:20.01,0:16:21.28,[Harem],,0000,0000,0000,,Để coi...
Dialogue: 0,0:16:21.28,0:16:24.30,[Sub_Phụ],,0000,0000,0000,,{\i1}Đầu tiên, hãy hẹn hò với cậu ta.{\i0}
Dialogue: 0,0:16:24.30,0:16:27.51,[Sub_Phụ],,0000,0000,0000,,{\i1}Nhưng đừng có bảo đó là hẹn hò.{\i0}
Dialogue: 0,0:16:27.51,0:16:30.37,[Sub_Phụ],,0000,0000,0000,,{\i1}Cách này là mình chỉ cần cho nó diễn ra một cách tự nhiên.{\i0}
Dialogue: 0,0:16:30.37,0:16:31.99,[Harem],,0000,0000,0000,,Ra là thế!
Dialogue: 0,0:16:32.18,0:16:32.83,[Harem],,0000,0000,0000,,Được!
Dialogue: 0,0:16:35.98,0:16:37.04,[Harem],,0000,0000,0000,,Rito!
Dialogue: 0,0:16:37.97,0:16:39.30,[Rito],,0000,0000,0000,,Sao thế?
Dialogue: 0,0:16:40.04,0:16:41.45,[Harem],,0000,0000,0000,,Umm...
Dialogue: 0,0:16:42.46,0:16:44.00,[Harem],,0000,0000,0000,,Sao chúng mình không...
Dialogue: 0,0:16:45.79,0:16:48.69,[Harem],,0000,0000,0000,,Đi đâu đó...
Dialogue: 0,0:16:48.69,0:16:49.73,[Harem],,0000,0000,0000,,Lúc tan học nhỉ?
Dialogue: 0,0:16:50.15,0:16:51.69,[Rito],,0000,0000,0000,,Ừ, được thôi.
Dialogue: 0,0:16:51.85,0:16:52.79,[Harem],,0000,0000,0000,,Yatta!
Dialogue: 0,0:16:52.79,0:16:54.09,[Harem],,0000,0000,0000,,Mắc câu rồi!
Dialogue: 0,0:16:54.09,0:16:55.97,[Rito],,0000,0000,0000,,Em có chuyện gì đặc biệt phải làm à?
Dialogue: 0,0:16:55.97,0:16:56.57,[Harem],,0000,0000,0000,,Ừm, vài chuyện thôi.
Dialogue: 0,0:16:56.57,0:16:57.40,[Harem],,0000,0000,0000,,Vài chuyện thôi.
Dialogue: 0,0:16:59.21,0:17:00.48,[Harem][00],,0000,0000,0000,,Được lắm~!
Dialogue: 0,0:17:00.48,0:17:02.72,[Harem][00],,0000,0000,0000,,Mình sẽ cố quyến rũ Rito!
Dialogue: 0,0:17:03.04,0:17:05.04,[Peke],,0000,0000,0000,,Chả biết có sao không?
Dialogue: 0,0:17:08.17,0:17:10.01,[Harem],,0000,0000,0000,,Là Magical Kyoko đó!
Dialogue: 0,0:17:10.01,0:17:13.24,[Rito],,0000,0000,0000,,Oy! Em có chuyện phải làm mà, nhanh lên đi.
Dialogue: 0,0:17:13.24,0:17:14.12,[Harem],,0000,0000,0000,,Đợi với!
Dialogue: 0,0:17:15.44,0:17:19.61,[Sub_Phụ],,0000,0000,0000,,{\i1}Xác suất thành công sẽ tăng nếu cậu\N vừa nói vừa hành động xấu hổ với cậu ta!{\i0}
Dialogue: 0,0:17:19.61,0:17:20.80,[Harem],,0000,0000,0000,,Xấu hổ hả?
Dialogue: 0,0:17:21.00,0:17:21.94,[Harem],,0000,0000,0000,,Được!
Dialogue: 0,0:17:22.20,0:17:24.48,[Harem],,0000,0000,0000,,N-Nè, Rito...
Dialogue: 0,0:17:27.60,0:17:31.86,[Harem],,0000,0000,0000,,H-Hôm nay, đi học v-vui ghê ha.
Dialogue: 0,0:17:32.33,0:17:33.94,[Rito],,0000,0000,0000,,Ừ, phải ha.
Dialogue: 0,0:17:36.16,0:17:37.58,[Harem],,0000,0000,0000,,Sao hông hiệu quả gì hết vậy!?
Dialogue: 0,0:17:37.58,0:17:39.95,[Harem],,0000,0000,0000,,Lúc mình nói có vấn đề sao?
Dialogue: 0,0:17:40.43,0:17:43.37,[Sub_Phụ],,0000,0000,0000,,{\i1}Chơi đẩy người cũng cần lắm đó.{\i0}
Dialogue: 0,0:17:43.37,0:17:44.68,[Harem],,0000,0000,0000,,Hiểu rồi!
Dialogue: 0,0:17:44.79,0:17:46.22,[Harem],,0000,0000,0000,,Thì ra là thế.
Dialogue: 0,0:17:46.22,0:17:47.10,[Harem],,0000,0000,0000,,Được!
Dialogue: 0,0:17:48.06,0:17:49.57,[Harem],,0000,0000,0000,,Lên!
Dialogue: 0,0:17:54.78,0:17:57.34,[Harem],,0000,0000,0000,,Có tác dụng gì không vậy trời?
Dialogue: 0,0:17:57.45,0:18:00.36,[Rito],,0000,0000,0000,,{\an8}Lala~~~!!!
Dialogue: 0,0:17:58.74,0:18:01.13,[Harem],,0000,0000,0000,,Chỉ tổ ảnh giận lên thôi!
Dialogue: 0,0:18:01.13,0:18:03.65,[Rito],,0000,0000,0000,,Thật là, đùa nữa thì anh về nhà luôn!
Dialogue: 0,0:18:04.13,0:18:05.93,[Harem],,0000,0000,0000,,Khoan đã, Rito!
Dialogue: 0,0:18:06.20,0:18:07.66,[Harem],,0000,0000,0000,,Không hay rồi.
Dialogue: 0,0:18:07.66,0:18:09.58,[Harem],,0000,0000,0000,,Mình cần làm gì đó.
Dialogue: 0,0:18:09.58,0:18:12.78,[Sub_Phụ],,0000,0000,0000,,{\i1}Khiến con trai quan tâm là điều rất quan trọng.{\i0}
Dialogue: 0,0:18:12.78,0:18:18.96,[Sub_Phụ],,0000,0000,0000,,{\i1}Chẳng hạn nói mấy câu linh tinh như \N" Em là người ngoài hành tinh". Hay cái gì na ná như thế.{\i1}
Dialogue: 0,0:18:18.96,0:18:19.99,[Harem],,0000,0000,0000,,Được!
Dialogue: 0,0:18:20.04,0:18:21.29,[Harem],,0000,0000,0000,,R-Rito!
Dialogue: 0,0:18:21.66,0:18:22.61,[Rito],,0000,0000,0000,,Gì nữa?
Dialogue: 0,0:18:22.80,0:18:27.10,[Harem],,0000,0000,0000,,Thật ra, em tới từ hành tinh Deviluke!
Dialogue: 0,0:18:28.65,0:18:29.54,[Rito],,0000,0000,0000,,Chuyện này ai chẳng biết.
Dialogue: 0,0:18:31.06,0:18:33.98,[Harem],,0000,0000,0000,,Mấy câu linh tinh không hiệu quả với Rito!
Dialogue: 0,0:18:34.97,0:18:36.02,[Harem],,0000,0000,0000,,Còn gì khác không!?
Dialogue: 0,0:18:38.28,0:18:39.26,[Rito],,0000,0000,0000,,Lala...
Dialogue: 0,0:18:39.26,0:18:42.16,[Rito],,0000,0000,0000,,Hôm nay, em hành động lạ quá.
Dialogue: 0,0:18:44.53,0:18:46.30,[Harem],,0000,0000,0000,,Đừng có hiểu nhầm!
Dialogue: 0,0:18:46.30,0:18:49.78,[Harem],,0000,0000,0000,,Làm như tôi thích anh lắm á!
Dialogue: 0,0:18:51.95,0:18:52.79,[Rito],,0000,0000,0000,,Lala?
Dialogue: 0,0:18:52.79,0:18:53.52,[Rito],,0000,0000,0000,,{\i1}*Tsun*{\i0}
Dialogue: 0,0:18:56.63,0:18:58.92,[Rito],,0000,0000,0000,,A~! Hông hiểu gì ráo!
Dialogue: 0,0:18:58.92,0:19:01.05,[Rito],,0000,0000,0000,,Sao cô ấy là người giận chứ!?
Dialogue: 0,0:19:01.05,0:19:02.08,[Rito],,0000,0000,0000,,Đợi đã, Lala!
Dialogue: 0,0:19:02.08,0:19:03.70,[Rito],,0000,0000,0000,,Em bị cái quái gì vậy!?
Dialogue: 0,0:19:10.31,0:19:12.09,[Harem],,0000,0000,0000,,Được! Lần này...!
Dialogue: 0,0:19:17.62,0:19:21.95,[Sub_Phụ],,0000,0000,0000,,{\i1}Hãy dằn nén tình cảm của mình xuống \Nrồi thể hiện qua hành động của cậu...{\i0}
Dialogue: 0,0:19:21.95,0:19:23.60,[Sub_Phụ],,0000,0000,0000,,{\i1}Bằng cách này, cậu sẽ thành công!{\i0}
Dialogue: 0,0:19:23.60,0:19:29.00,[Sub_Phụ],,0000,0000,0000,,{\i1}Ngoài ra, quyến rũ cậu ta bằng \Nmột bộ hở hang thiếu vải cũng không tồi đâu!{\i0}
Dialogue: 0,0:19:29.00,0:19:32.12,[Sub_Phụ],,0000,0000,0000,,{\i1}Hãy dùng tất cả thứ mình có để hấp dẫn cậu ta!{\i0}
Dialogue: 0,0:19:33.28,0:19:34.28,[Harem],,0000,0000,0000,,Vậy nhé, Peke.
Dialogue: 0,0:19:34.45,0:19:37.46,[Peke],,0000,0000,0000,,M-Một bộ thiếu vải hả!?
Dialogue: 0,0:19:37.46,0:19:40.03,[Peke],,0000,0000,0000,,Không hiểu lắm, nhưng...
Dialogue: 0,0:19:40.56,0:19:42.20,[Harem],,0000,0000,0000,,Open change!
Dialogue: 0,0:19:43.17,0:19:44.57,[Rito],,0000,0000,0000,,Ê, Lala.
Dialogue: 0,0:19:44.57,0:19:46.74,[Rito],,0000,0000,0000,,Em đang làm gì ở nơi như thế nà–
Dialogue: 0,0:19:50.93,0:19:52.20,[Harem],,0000,0000,0000,,Ri-to...
Dialogue: 0,0:19:53.39,0:19:55.68,[Rito],,0000,0000,0000,,Em đang mặc cái gì vậy hả!?
Dialogue: 0,0:19:58.47,0:20:00.10,[Harem],,0000,0000,0000,,Em xin lỗi chuyện mới nãy nha.
Dialogue: 0,0:20:00.96,0:20:02.70,[Harem],,0000,0000,0000,,Tuy nói như thế...
Dialogue: 0,0:20:03.49,0:20:04.95,[Harem],,0000,0000,0000,,Nhưng thật ra...
Dialogue: 0,0:20:07.20,0:20:09.57,[Harem],,0000,0000,0000,,Em rất...
Dialogue: 0,0:20:09.57,0:20:10.69,[Harem],,0000,0000,0000,,Rất yêu anh.
Dialogue: 0,0:20:10.91,0:20:13.06,[Rito],,0000,0000,0000,,L-Lala...
Dialogue: 0,0:20:13.53,0:20:14.59,[Harem],,0000,0000,0000,,Chính vì thế...
Dialogue: 0,0:20:24.80,0:20:26.66,[Harem],,0000,0000,0000,,Rồi sau đó làm sao nữa?
Dialogue: 0,0:20:36.79,0:20:40.12,[Rito],,0000,0000,0000,,{\i1}Thật là, hôm nay cô ấy bị sao vậy?{\i0}
Dialogue: 0,0:20:40.12,0:20:42.64,[Rito],,0000,0000,0000,,{\i1}Sau khi bị cô ấy kéo hết chỗ này tới chỗ khác mình kiệt sức luôn.{\i0}
Dialogue: 0,0:20:44.31,0:20:46.33,[Harem],,0000,0000,0000,,{\i1}Mình đã cố lắm rồi...{\i0}
Dialogue: 0,0:20:46.33,0:20:48.58,[Harem],,0000,0000,0000,,{\i1}Nhưng rốt cuộc vẫn thất bại.{\i0}
Dialogue: 0,0:20:48.58,0:20:49.89,[Harem],,0000,0000,0000,,{\i1}Chẳng hiểu tại sao...{\i0}
Dialogue: 0,0:21:04.42,0:21:06.13,[Rito],,0000,0000,0000,,Sao vậy, Lala?
Dialogue: 0,0:21:06.13,0:21:07.76,[Harem],,0000,0000,0000,,K-Không có gì!
Dialogue: 0,0:21:08.41,0:21:10.50,[Harem],,0000,0000,0000,,{\i1}S-Sao mình lại khóc?{\i0}
Dialogue: 0,0:21:24.89,0:21:26.17,[Rito],,0000,0000,0000,,{\i1}Đó là...{\i0}
Dialogue: 0,0:21:26.17,0:21:28.07,[Rito],,0000,0000,0000,,{\i1}Lẽ nào Lala...?{\i0}
Dialogue: 0,0:21:28.07,0:21:29.04,[Rito],,0000,0000,0000,,{\i1}Là thế à...{\i0}
Dialogue: 0,0:21:31.43,0:21:33.06,[Harem],,0000,0000,0000,,Xin lỗi nhé, Rito.
Dialogue: 0,0:21:33.06,0:21:34.22,[Harem],,0000,0000,0000,,Giờ chúng mình về thôi!
Dialogue: 0,0:21:37.28,0:21:39.15,[Rito],,0000,0000,0000,,N-Nhưng, em biết đó...
Dialogue: 0,0:21:41.88,0:21:46.30,[Rito],,0000,0000,0000,,Cùng nhau đi vòng vèo như bữa nay cũng...
Dialogue: 0,0:21:47.30,0:21:48.20,[Rito],,0000,0000,0000,,Cũng...
Dialogue: 0,0:21:49.63,0:21:51.32,[Rito],,0000,0000,0000,,Cũng vui lắm...
Dialogue: 0,0:21:53.80,0:21:55.10,[Harem],,0000,0000,0000,,Rito!
Dialogue: 0,0:21:55.71,0:21:56.23,[Harem],,0000,0000,0000,,Hum!
Dialogue: 0,0:21:56.89,0:21:59.69,[Rito],,0000,0000,0000,,Đã bảo đừng có ôm anh như thế mà!
Dialogue: 0,0:21:58.53,0:21:59.68,[Harem],,0000,0000,0000,,Rito!
Dialogue: 0,0:21:59.68,0:21:59.90,[Harem],,0000,0000,0000,,Rito!
Dialogue: 0,0:21:59.90,0:22:00.25,[Harem][01],,0000,0000,0000,,Rito!
Dialogue: 0,0:22:00.25,0:22:01.10,[Harem],,0000,0000,0000,,Rito!
Dialogue: 0,0:21:59.79,0:22:01.00,[Rito],,0000,0000,0000,,Thật là...
Dialogue: 0,0:22:04.77,0:22:07.47,[Harem],,0000,0000,0000,,Rito, mai tụi mình đi đâu?
Dialogue: 0,0:22:07.47,0:22:08.62,[Rito],,0000,0000,0000,,{\fad(0,700)}Mai đi học.
Dialogue: 0,0:23:40.52,0:23:42.70,[Next_Episode],,0000,0000,0000,,Cuối cùng đã đến lượt của ta!

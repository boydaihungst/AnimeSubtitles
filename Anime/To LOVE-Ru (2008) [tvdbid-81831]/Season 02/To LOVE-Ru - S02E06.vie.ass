Dialogue: 0,0:01:46.76,0:01:47.61,[Harem],,0000,0000,0000,,Woa~!
Dialogue: 0,0:01:47.61,0:01:48.65,[Harem],,0000,0000,0000,,Tuyệt thật!
Dialogue: 0,0:01:48.65,0:01:49.89,[Harem],,0000,0000,0000,,Biển đẹp quá!
Dialogue: 0,0:01:49.89,0:01:51.14,[Sub_Phụ],,0000,0000,0000,,Đúng là vậy!
Dialogue: 0,0:01:54.60,0:01:56.80,[Harem],,0000,0000,0000,,S-Sóng cũng nhiều nhỉ.
Dialogue: 0,0:02:02.37,0:02:03.40,[Sub_Phụ],,0000,0000,0000,,Bầu trời thanh tao!
Dialogue: 0,0:02:03.40,0:02:04.56,[Sub_Phụ],,0000,0000,0000,,Mây trắng trên cao!
Dialogue: 0,0:02:05.02,0:02:08.50,[Sub_Phụ],,0000,0000,0000,,Và ta, Tenjouin Saki!
Dialogue: 0,0:02:08.50,0:02:09.99,[Sub_Phụ],,0000,0000,0000,,Chào mừng tất cả mọi người!
Dialogue: 0,0:02:09.99,0:02:12.36,[Sub_Phụ],,0000,0000,0000,,Đến với bãi biển tư nhân của gia đình Tenjouin!
Dialogue: 0,0:02:14.55,0:02:16.78,[Sub_Phụ],,0000,0000,0000,,Nghe ta nói cái coi mấy người kia!
Dialogue: 0,0:02:16.78,0:02:21.10,[Sub_Phụ],,0000,0000,0000,,Các người nghĩ ai đã mang các người đến chỗ này hả!?
Dialogue: 0,0:02:21.10,0:02:25.01,[Sub_Phụ],,0000,0000,0000,,Biết điều còn không mau đa tạ Saki-sama đi.
Dialogue: 0,0:02:25.01,0:02:27.57,[Sub_Phụ],,0000,0000,0000,,Cám ơn rất nhiều!
Dialogue: 0,0:02:25.01,0:02:27.57,[Harem],,0000,0000,0000,,Cám ơn rất nhiều!
Dialogue: 0,0:02:29.53,0:02:31.33,[Sub_Phụ],,0000,0000,0000,,Không có chi!
Dialogue: 0,0:02:31.33,0:02:33.96,[Sub_Phụ],,0000,0000,0000,,Giờ thì chơi nhiệt tình hết sức mình đi nhé!
Dialogue: 0,0:02:59.03,0:03:00.75,[Rito],,0000,0000,0000,,{\i1}Haruna-chan...{\i0}
Dialogue: 0,0:03:00.95,0:03:02.73,[Sub_Phụ],,0000,0000,0000,,Ê, Rito.
Dialogue: 0,0:03:02.73,0:03:04.42,[Sub_Phụ],,0000,0000,0000,,Chỗ này đích thị là thiên đường rồi.
Dialogue: 0,0:03:04.98,0:03:06.74,[Sub_Phụ],,0000,0000,0000,,Lala-chan có số đo ba vòng thật tuyệt.
Dialogue: 0,0:03:06.74,0:03:10.59,[Sub_Phụ],,0000,0000,0000,,Nhưng Sairenji cũng chẳng kém cạnh.
Dialogue: 0,0:03:10.92,0:03:12.60,[Sub_Phụ],,0000,0000,0000,,Hai tên đực rựa kia...
Dialogue: 0,0:03:12.60,0:03:15.45,[Sub_Phụ],,0000,0000,0000,,Hai đứa bây tới đây mà chỉ nghĩ tới mỗi thứ đó thôi hả?
Dialogue: 0,0:03:17.17,0:03:17.63,[Sub_Phụ],,0000,0000,0000,,Ai đây?
Dialogue: 0,0:03:17.63,0:03:18.50,[Sub_Phụ],,0000,0000,0000,,Ta là Ren! Ren!
Dialogue: 0,0:03:18.77,0:03:19.93,[Harem][00],,0000,0000,0000,,Nè, Ren!
Dialogue: 0,0:03:19.93,0:03:21.97,[Harem][00],,0000,0000,0000,,Anh Rito ở ngay trước mặt đó rồi.
Dialogue: 0,0:03:21.97,0:03:24.72,[Harem][00],,0000,0000,0000,,Mau mau đổi chỗ cho đây cái coi!
Dialogue: 0,0:03:24.92,0:03:25.84,[Sub_Phụ],,0000,0000,0000,,Lắm mồm!
Dialogue: 0,0:03:25.84,0:03:27.34,[Sub_Phụ],,0000,0000,0000,,Giờ là tới lượt của tôi.
Dialogue: 0,0:03:27.34,0:03:28.91,[Sub_Phụ],,0000,0000,0000,,Biết điều thì đi ngủ tiếp đi!
Dialogue: 0,0:03:28.91,0:03:29.80,[Sub_Phụ],,0000,0000,0000,,Cái gì?
Dialogue: 0,0:03:29.80,0:03:31.00,[Sub_Phụ],,0000,0000,0000,,Cô nhờ chút!
Dialogue: 0,0:03:31.00,0:03:32.08,[Sub_Phụ],,0000,0000,0000,,Yuuki-kun!
Dialogue: 0,0:03:34.77,0:03:36.17,[Sub_Phụ],,0000,0000,0000,,Phiền em...
Dialogue: 0,0:03:36.17,0:03:37.65,[Sub_Phụ],,0000,0000,0000,,Thoa dầu hộ cô nha?
Dialogue: 0,0:03:37.93,0:03:39.90,[Rito],,0000,0000,0000,,Cô Mikado!
Dialogue: 0,0:03:37.93,0:03:39.90,[Sub_Phụ],,0000,0000,0000,,Cô Mikado!
Dialogue: 0,0:03:39.90,0:03:43.08,[Sub_Phụ],,0000,0000,0000,,Em thiệt là "chong xáng" quá à.
Dialogue: 0,0:03:43.42,0:03:45.69,[Sub_Phụ],,0000,0000,0000,,Để cô cho em biết mùi đời nhé...
Dialogue: 0,0:03:46.38,0:03:50.16,[Sub_Phụ],,0000,0000,0000,,Khởi đầu bằng màn cọ xát qua da nhé.
Dialogue: 0,0:03:50.95,0:03:51.68,[Sub_Phụ],,0000,0000,0000,,Cọ xát qua da hả?
Dialogue: 0,0:03:53.22,0:03:55.72,[Sub_Phụ],,0000,0000,0000,,Mỗi chừng đó đâu có gì phải sợ.
Dialogue: 0,0:03:55.97,0:03:57.65,[Sub_Phụ],,0000,0000,0000,,Oshizu-chan!
Dialogue: 0,0:03:58.81,0:04:00.19,[Sub_Phụ],,0000,0000,0000,,Vâng!? Thưa cô?
Dialogue: 0,0:04:03.07,0:04:04.42,[Sub_Phụ],,0000,0000,0000,,Hiểu rồi ạ!
Dialogue: 0,0:04:07.43,0:04:08.99,[Rito],,0000,0000,0000,,Mình mất tự chủ rồi!
Dialogue: 0,0:04:09.72,0:04:11.84,[Rito],,0000,0000,0000,,Không, không, tha em, tha em!
Dialogue: 0,0:04:13.03,0:04:14.01,[Sub_Phụ],,0000,0000,0000,,Nào!
Dialogue: 0,0:04:14.01,0:04:15.19,[Sub_Phụ],,0000,0000,0000,,Nhanh lên đi.
Dialogue: 0,0:04:18.56,0:04:19.69,[Sub_Phụ],,0000,0000,0000,,Cường hóa niệm thuật!
Dialogue: 0,0:04:26.87,0:04:29.04,[Sub_Phụ],,0000,0000,0000,,{\an8}Đó! Đó! Đúng rồi đấy!
Dialogue: 0,0:04:26.25,0:04:29.04,[Sub_Phụ],,0000,0000,0000,,Thằng đệt, Rito! Sao đời mày sướng thế!
Dialogue: 0,0:04:29.04,0:04:31.05,[Sub_Phụ],,0000,0000,0000,,Tao xin làm quả tay kia của mày thôi cũng được!
Dialogue: 0,0:04:35.18,0:04:36.67,[Harem],,0000,0000,0000,,Dừng lại, Murasame-san!
Dialogue: 0,0:04:38.32,0:04:39.38,[Harem],,0000,0000,0000,,Thật là...
Dialogue: 0,0:04:39.38,0:04:41.62,[Harem],,0000,0000,0000,,Đâu phải vì cậu là hộ lý cho Mikado-sensei mà...
Dialogue: 0,0:04:42.57,0:04:44.61,[Sub_Phụ],,0000,0000,0000,,Không kiểm soát được niệm lực nữa rồi!
Dialogue: 0,0:04:44.61,0:04:45.01,[Sub_Phụ],,0000,0000,0000,,Ô ra!
Dialogue: 0,0:04:45.26,0:04:46.08,[Rito],,0000,0000,0000,,Tay tôi...!
Dialogue: 0,0:04:46.08,0:04:47.42,[Rito][01],,0000,0000,0000,,Tay tôi...!

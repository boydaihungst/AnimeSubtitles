Dialogue: 0,0:16:04.74,0:16:05.16,[Sub_Phụ],,0000,0000,0000,,Ơ!?
Dialogue: 0,0:16:04.74,0:16:05.16,[Sub_Phụ],,0000,0000,0000,,Eh!?
Dialogue: 0,0:16:05.16,0:16:05.69,[Harem],,0000,0000,0000,,Được!
Dialogue: 0,0:16:05.69,0:16:07.12,[Harem],,0000,0000,0000,,Giờ tới lượt tớ!
Dialogue: 0,0:16:09.86,0:16:11.19,[Sub_Phụ],,0000,0000,0000,,S-Sao vậy?
Dialogue: 0,0:16:11.19,0:16:13.01,[Sub_Phụ],,0000,0000,0000,,Không giống cậu chút nào, Haruna...
Dialogue: 0,0:16:13.21,0:16:14.72,[Harem],,0000,0000,0000,,Có vẻ vui ta!
Dialogue: 0,0:16:14.72,0:16:16.61,[Harem],,0000,0000,0000,,Tớ cũng tham gia luôn!
Dialogue: 0,0:16:19.96,0:16:21.16,[Harem],,0000,0000,0000,,Mấy người kia!
Dialogue: 0,0:16:21.61,0:16:23.91,[Harem],,0000,0000,0000,,Mới sáng mắt ra đừng có làm mấy trò trơ trẽn đó.
Dialogue: 0,0:16:24.31,0:16:26.49,[Harem],,0000,0000,0000,,Các người còn lôi cả lớp trưởng, Sairenji, vào nữa.
Dialogue: 0,0:16:26.49,0:16:27.85,[Harem],,0000,0000,0000,,Mấy người nghĩ mình đang làm gì thế hả?
Dialogue: 0,0:16:28.23,0:16:30.09,[Harem],,0000,0000,0000,,Kotegawa...
Dialogue: 0,0:16:30.09,0:16:31.37,[Harem],,0000,0000,0000,,Vậy, Yui-san.
Dialogue: 0,0:16:31.37,0:16:32.26,[Harem],,0000,0000,0000,,Cũng vào chơi luôn đi?
Dialogue: 0,0:16:32.26,0:16:32.73,[Harem],,0000,0000,0000,,Heh!?
Dialogue: 0,0:16:34.63,0:16:36.15,[Harem],,0000,0000,0000,,Làm cái gì thế hả!?
Dialogue: 0,0:16:36.15,0:16:38.18,[Harem],,0000,0000,0000,,Dừng lại đi mà!
Dialogue: 0,0:16:40.42,0:16:41.94,[Harem],,0000,0000,0000,,Đáng yêu ghê!
Dialogue: 0,0:16:41.94,0:16:43.75,[Harem],,0000,0000,0000,,Đây là gì vậy!?
Dialogue: 0,0:16:45.69,0:16:47.69,[Harem],,0000,0000,0000,,Đáng yêu quá đi à!
Dialogue: 0,0:16:49.28,0:16:52.09,[Harem],,0000,0000,0000,,Không lẽ đó là áo ngực...
Dialogue: 0,0:16:52.09,0:16:53.23,[Harem],,0000,0000,0000,,Của mình...!?
Dialogue: 0,0:16:54.66,0:16:56.19,[Harem],,0000,0000,0000,,Trả đây!
Dialogue: 0,0:16:56.19,0:16:59.12,[Harem],,0000,0000,0000,,Cái đó gọi là gì vậy?
Dialogue: 0,0:16:59.12,0:17:00.87,[Harem],,0000,0000,0000,,Cậu nói áo ngực ấy hả?
Dialogue: 0,0:17:00.87,0:17:03.19,[Harem],,0000,0000,0000,,Cậu cũng dang mặc đó thôi!
Dialogue: 0,0:17:08.73,0:17:09.85,[Harem],,0000,0000,0000,,Đúng thật ha!
Dialogue: 0,0:17:10.21,0:17:11.50,[Sub_Phụ],,0000,0000,0000,,Khoan, Haruna!
Dialogue: 0,0:17:11.50,0:17:13.90,[Sub_Phụ],,0000,0000,0000,,Vậy là hơi quá đà rồi đó.
Dialogue: 0,0:17:13.90,0:17:15.03,[Harem],,0000,0000,0000,,Vậy hả?
Dialogue: 0,0:17:15.03,0:17:17.06,[Sub_Phụ],,0000,0000,0000,,Hôm nay, cậu cư xử lạ quá nha!
Dialogue: 0,0:17:17.44,0:17:19.71,[Peke],,0000,0000,0000,,Bộ Haruna-dono...
Dialogue: 0,0:17:19.71,0:17:21.44,[Peke],,0000,0000,0000,,Lúc nào cũng vậy ạ?
Dialogue: 0,0:17:21.44,0:17:23.24,[Harem],,0000,0000,0000,,Không...
Dialogue: 0,0:17:23.24,0:17:25.06,[Harem],,0000,0000,0000,,Có gì đó là lạ.
Dialogue: 0,0:17:32.07,0:17:35.40,[Rito],,0000,0000,0000,,{\i1}Hôm nay, Haruna-chan quái đản thế nhỉ.{\i0}
Dialogue: 0,0:17:35.40,0:17:37.73,[Rito],,0000,0000,0000,,{\i1}Chả giống Haruna-chan mọi khi tí nào.{\i0}
Dialogue: 0,0:17:37.73,0:17:39.49,[Rito],,0000,0000,0000,,{\i1}Chuyện gì vậy ta?{\i0}
Dialogue: 0,0:17:56.12,0:17:57.36,[Harem],,0000,0000,0000,,Rito-san.
Dialogue: 0,0:18:01.48,0:18:03.94,[Rito],,0000,0000,0000,,S-Sairenji...
Dialogue: 0,0:18:06.74,0:18:09.10,[Rito],,0000,0000,0000,,C-Chuyện gì thế?
Dialogue: 0,0:18:09.10,0:18:10.56,[Rito],,0000,0000,0000,,Cậu cần gì à?
Dialogue: 0,0:18:11.74,0:18:13.43,[Harem],,0000,0000,0000,,Ừ...
Dialogue: 0,0:18:13.43,0:18:15.22,[Harem],,0000,0000,0000,,Ưhm...
Dialogue: 0,0:18:15.22,0:18:17.57,[Harem],,0000,0000,0000,,Tớ muốn nói với cậu vài chuyện.
Dialogue: 0,0:18:18.33,0:18:20.16,[Rito],,0000,0000,0000,,Nói gì cơ?
Dialogue: 0,0:18:20.16,0:18:20.82,[Harem],,0000,0000,0000,,Ừm.
Dialogue: 0,0:18:25.28,0:18:28.00,[Harem],,0000,0000,0000,,Cậu... thấy đấy...
Dialogue: 0,0:18:39.91,0:18:41.88,[Harem],,0000,0000,0000,,Sự thật là...
Dialogue: 0,0:18:41.88,0:18:43.13,[Harem],,0000,0000,0000,,Thật ra...
Dialogue: 0,0:18:46.36,0:18:48.09,[Harem],,0000,0000,0000,,Là mình...
Dialogue: 0,0:18:48.09,0:18:49.01,[Harem],,0000,0000,0000,,Thích cậu.
Dialogue: 0,0:18:50.70,0:18:52.27,[Rito],,0000,0000,0000,,K-Khoan đã nào!
Dialogue: 0,0:18:52.33,0:18:54.01,[Rito],,0000,0000,0000,,Vừa rồi là sao?!
Dialogue: 0,0:18:54.01,0:18:55.86,[Harem],,0000,0000,0000,,Mình nói mình là thích cậu!
Dialogue: 0,0:18:59.07,0:19:00.11,[Rito],,0000,0000,0000,,{\i1}Thích?{\i0}
Dialogue: 0,0:19:00.55,0:19:02.63,[Rito],,0000,0000,0000,,{\i1}Haruna thích mình?{\i0}
Dialogue: 0,0:19:04.16,0:19:05.85,[Rito][00],,0000,0000,0000,,Đến rồi!
Dialogue: 0,0:19:05.85,0:19:10.11,[Rito][00],,0000,0000,0000,,Giấc mơ của ta đã thành hiện thực!
Dialogue: 0,0:19:12.21,0:19:14.09,[Rito],,0000,0000,0000,,{\i1}Đ-Được!{\i0}
Dialogue: 0,0:19:14.09,0:19:15.36,[Rito],,0000,0000,0000,,{\i1}Tới lượt mình...{\i0}
Dialogue: 0,0:19:16.82,0:19:18.21,[Rito],,0000,0000,0000,,S-Sự thật là...
Dialogue: 0,0:19:18.21,0:19:19.89,[Rito],,0000,0000,0000,,Tớ cũng–
Dialogue: 0,0:19:29.30,0:19:33.15,[Sub_Phụ],,0000,0000,0000,,Thôi nào, Josephine!
Dialogue: 0,0:19:38.40,0:19:39.87,[Sub_Phụ],,0000,0000,0000,,Ah~~~!
Dialogue: 0,0:19:39.87,0:19:41.99,[Sub_Phụ],,0000,0000,0000,,C-Chó!
Dialogue: 0,0:19:42.75,0:19:44.91,[Rito],,0000,0000,0000,,Oshizu-chan!?
Dialogue: 0,0:19:45.12,0:19:47.97,[Sub_Phụ],,0000,0000,0000,,Đừng có tới đây!
Dialogue: 0,0:19:49.13,0:19:51.02,[Harem],,0000,0000,0000,,X-Xin lỗi!
Dialogue: 0,0:19:52.07,0:19:54.38,[Sub_Phụ],,0000,0000,0000,,Thôi nào, về thôi!
Dialogue: 0,0:19:54.38,0:19:56.17,[Sub_Phụ],,0000,0000,0000,,Josephine!
Dialogue: 0,0:19:58.14,0:20:01.01,[Sub_Phụ],,0000,0000,0000,,Làm gì với mấy con cẩu tạp chủng đó đi!
Dialogue: 0,0:20:01.01,0:20:05.83,[Sub_Phụ],,0000,0000,0000,,Tụi nó mà đến gần là tớ nhũn cả ra~!
Dialogue: 0,0:20:05.83,0:20:08.10,[Rito],,0000,0000,0000,,Chuyện này là sao?
Dialogue: 0,0:20:08.17,0:20:10.65,[Harem],,0000,0000,0000,,Oshizu-chan nhập vô cơ thể Haruna!?
Dialogue: 0,0:20:12.16,0:20:13.59,[Sub_Phụ],,0000,0000,0000,,Xin lỗi nhé.
Dialogue: 0,0:20:13.81,0:20:16.57,[Sub_Phụ],,0000,0000,0000,,Sao cậu lại biến thành dạng ma rồi?
Dialogue: 0,0:20:16.72,0:20:21.25,[Sub_Phụ],,0000,0000,0000,,Cơ thể vật lý của tớ bảo trì chỗ cô Mikado rồi.
Dialogue: 0,0:20:21.76,0:20:24.22,[Sub_Phụ],,0000,0000,0000,,Vì thế, trong lúc đang nhong nhong đi chơi ở ngoài...
Dialogue: 0,0:20:24.22,0:20:26.25,[Sub_Phụ],,0000,0000,0000,,Tớ bị chó rượt.
Dialogue: 0,0:20:28.22,0:20:30.11,[Sub_Phụ],,0000,0000,0000,,Ai đó kíu với~!
Dialogue: 0,0:20:30.11,0:20:33.27,[Sub_Phụ],,0000,0000,0000,,Chó!
Dialogue: 0,0:20:36.65,0:20:38.95,[Sub_Phụ],,0000,0000,0000,,Trong lúc chạy con chó...
Dialogue: 0,0:20:38.95,0:20:42.87,[Sub_Phụ],,0000,0000,0000,,Tớ đã lỡ đụng vô Haruna-chan.
Dialogue: 0,0:20:44.21,0:20:45.91,[Sub_Phụ],,0000,0000,0000,,Chó!
Dialogue: 0,0:20:48.02,0:20:51.37,[Sub_Phụ],,0000,0000,0000,,Sau đó cậu ở trong cơ thể cậu ấy luôn à?
Dialogue: 0,0:20:51.51,0:20:53.84,[Sub_Phụ],,0000,0000,0000,,Mới đầu cũng chẳng dễ chịu gì...
Dialogue: 0,0:20:53.84,0:20:56.83,[Sub_Phụ],,0000,0000,0000,,Nhưng sau thấy cũng vui vui.
Dialogue: 0,0:20:56.83,0:20:57.87,[Sub_Phụ],,0000,0000,0000,,Vì thế, tớ chỉ...
Dialogue: 0,0:20:57.87,0:21:00.11,[Sub_Phụ],,0000,0000,0000,,Thật là, cậu làm loạn cả lên hết.
Dialogue: 0,0:21:00.45,0:21:02.10,[Harem],,0000,0000,0000,,Có đúng là...
Dialogue: 0,0:21:02.10,0:21:05.37,[Harem],,0000,0000,0000,,Lúc trong cơ thể tớ, cậu đã tỏ tình với Yuuki-kun...
Dialogue: 0,0:21:05.37,0:21:06.44,[Harem],,0000,0000,0000,,Hả!?
Dialogue: 0,0:21:09.08,0:21:11.20,[Sub_Phụ],,0000,0000,0000,,À, tớ cũng không biết tại sao nữa.
Dialogue: 0,0:21:11.79,0:21:16.14,[Sub_Phụ],,0000,0000,0000,,Sao tớ lại cảm thấy vậy nhỉ?
Dialogue: 0,0:21:16.14,0:21:17.05,[Harem],,0000,0000,0000,,Eh!?
Dialogue: 0,0:21:17.29,0:21:20.32,[Sub_Phụ],,0000,0000,0000,,Tớ đâu có định làm thế đâu.
Dialogue: 0,0:21:20.32,0:21:22.34,[Sub_Phụ],,0000,0000,0000,,Xin lỗi nhé, Haruna-san.
Dialogue: 0,0:21:33.36,0:21:35.18,[Sub_Phụ],,0000,0000,0000,,Mặt cậu đỏ kìa...
Dialogue: 0,0:21:35.18,0:21:36.70,[Sub_Phụ],,0000,0000,0000,,Không sao chứ?
Dialogue: 0,0:21:36.87,0:21:38.48,[Rito],,0000,0000,0000,,{\i1}Haruna-chan...
Dialogue: 0,0:21:38.48,0:21:40.77,[Rito],,0000,0000,0000,,{\i1}Trở lại bình thường rồi nhỉ?{\i0}
Dialogue: 0,0:21:41.02,0:21:44.30,[Rito],,0000,0000,0000,,{\i1}Cũng tốt nhưng thấy buồn nao nao...{\i0}
Dialogue: 0,0:21:45.38,0:21:47.13,[Sub_Phụ],,0000,0000,0000,,Để xem...
Dialogue: 0,0:21:47.13,0:21:49.33,[Harem],,0000,0000,0000,,Dừng lại, Risa!
Dialogue: 0,0:21:49.94,0:21:52.47,[Rito],,0000,0000,0000,,C-C-Cậu làm cái gì vậy!?
Dialogue: 0,0:21:52.82,0:21:53.58,[Sub_Phụ],,0000,0000,0000,,Ưm...
Dialogue: 0,0:21:53.58,0:21:54.93,[Sub_Phụ],,0000,0000,0000,,Đúng là Haruna của mềnh rồi.
Dialogue: 0,0:21:54.93,0:21:55.46,[Sub_Phụ],,0000,0000,0000,,Tốt quá rồi!
Dialogue: 0,0:21:55.46,0:21:56.23,[Sub_Phụ][01],,0000,0000,0000,,Tốt quá rồi!
Dialogue: 0,0:21:56.23,0:21:58.41,[Harem],,0000,0000,0000,,Không, dừng lại đi!
Dialogue: 0,0:21:58.41,0:21:59.56,[Harem],,0000,0000,0000,,Có vẻ vui ha!
Dialogue: 0,0:21:59.56,0:22:01.01,[Harem],,0000,0000,0000,,Tớ chơi với!
Dialogue: 0,0:22:01.01,0:22:03.24,[Sub_Phụ],,0000,0000,0000,,Cả tớ cũng muốn chơi nữa!
Dialogue: 0,0:23:40.05,0:23:42.07,[Next_Episode],,0000,0000,0000,,Tớ cũng muốn yêu thử xem sao!
Dialogue: 0,0:23:42.07,0:23:43.99,[Next_Episode],,0000,0000,0000,,Tớ muốn được một chàng lịch lãm tỏ tình
Dialogue: 0,0:23:43.99,0:23:46.60,[Next_Episode],,0000,0000,0000,,Và có mối tình say đắm!
Dialogue: 0,0:23:47.32,0:23:49.81,[Next_Episode],,0000,0000,0000,,Rito! Kì quá đi!
Dialogue: 0,0:23:49.81,0:23:51.81,Default,,0000,0000,0000,,

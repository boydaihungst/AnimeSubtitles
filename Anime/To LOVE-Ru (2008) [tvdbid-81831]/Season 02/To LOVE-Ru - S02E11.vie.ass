Dialogue: 0,0:00:10.40,0:00:16.30,Default,,0000,0000,0000,,{~Opening~}
Dialogue: 0,0:01:41.80,0:01:42.34,[Sub_Phụ],,0000,0000,0000,,Mi-{\alpha&HFF&}Mikan!
Dialogue: 0,0:01:42.34,0:01:43.38,[Sub_Phụ],,0000,0000,0000,,Mi-Mikan!
Dialogue: 0,0:01:43.81,0:01:46.66,[Sub_Phụ],,0000,0000,0000,,H-Hãy hẹn hò với tớ nhé~!?
Dialogue: 0,0:01:51.06,0:01:52.32,[Harem],,0000,0000,0000,,Tớ xin lỗi...
Dialogue: 0,0:01:53.57,0:01:55.71,[Sub_Phụ],,0000,0000,0000,,Không thể nào~!
Dialogue: 0,0:01:54.38,0:01:55.71,[Sub_Phụ],,0000,0000,0000,,{\an8}Ghê chưa~!
Dialogue: 0,0:01:55.71,0:01:58.02,[Sub_Phụ],,0000,0000,0000,,Cả Ooyoshi của lớp C cũng ra rìa luôn!
Dialogue: 0,0:01:58.08,0:02:00.42,[Sub_Phụ],,0000,0000,0000,,T-Tại sao~~~!?
Dialogue: 0,0:02:01.45,0:02:01.72,[Sub_Phụ],,0000,0000,0000,,Nè!
Dialogue: 0,0:02:01.72,0:02:02.09,[Sub_Phụ][01],,0000,0000,0000,,Nè!
Dialogue: 0,0:02:02.42,0:02:06.78,[Sub_Phụ],,0000,0000,0000,,Mikan-chan được hâm mộ thế \Nsao cậu không bốc đại một chàng đi?
Dialogue: 0,0:02:07.93,0:02:09.38,[Harem],,0000,0000,0000,,Thì, cũng đâu sao...
Dialogue: 0,0:02:09.38,0:02:11.81,[Harem],,0000,0000,0000,,Tớ cũng chả để ý lắm...
Dialogue: 0,0:02:11.81,0:02:14.92,[Sub_Phụ],,0000,0000,0000,,Mikan nhà ta yêu anh trai quá mần chi.
Dialogue: 0,0:02:14.92,0:02:15.47,[Harem],,0000,0000,0000,,Khoa—
Dialogue: 0,0:02:15.47,0:02:17.23,[Harem],,0000,0000,0000,,Tự nhiên lại đá sang chuyện đó chứ!?
Dialogue: 0,0:02:17.23,0:02:21.10,[Sub_Phụ],,0000,0000,0000,,Đúng là có tật giật mình, nghi lắm nha~
Dialogue: 0,0:02:21.45,0:02:23.79,[Sub_Phụ],,0000,0000,0000,,Thế anh nhà cậu tuyệt lắm hả?
Dialogue: 0,0:02:23.79,0:02:25.29,[Sub_Phụ],,0000,0000,0000,,Bữa nào giới thiệu cho bọn tớ nhé!
Dialogue: 0,0:02:25.29,0:02:28.47,[Harem],,0000,0000,0000,,Mấy cậu làm gì mà cứ lôi ổng ra nói hoài vậy!
Dialogue: 0,0:02:41.04,0:02:43.14,[Harem],,0000,0000,0000,,Một người anh tuyệt vời...
Dialogue: 0,0:02:43.14,0:02:43.88,[Harem],,0000,0000,0000,,Ư!?
Dialogue: 0,0:02:49.64,0:02:50.27,[Rito],,0000,0000,0000,,Đấy!
Dialogue: 0,0:02:52.58,0:02:54.70,[Harem],,0000,0000,0000,,Mình lại thua mất rồi!
Dialogue: 0,0:02:54.70,0:02:55.75,[Harem],,0000,0000,0000,,Chị, chị...
Dialogue: 0,0:02:55.75,0:02:57.30,[Harem],,0000,0000,0000,,Để em thử phát nào!
Dialogue: 0,0:02:58.20,0:02:59.82,[Harem],,0000,0000,0000,,Em đã chơi trò này rồi à, Nana?
Dialogue: 0,0:02:59.99,0:03:01.27,[Harem],,0000,0000,0000,,Chưa, nhưng mà không sao đâu!
Dialogue: 0,0:03:01.69,0:03:03.46,[Harem],,0000,0000,0000,,Mấy chuyện này dễ ợt thôi mà!
Dialogue: 0,0:03:03.46,0:03:06.72,[Harem],,0000,0000,0000,,Rito-san, anh giỏi mấy cái này nhỉ.
Dialogue: 0,0:03:07.50,0:03:09.41,[Rito],,0000,0000,0000,,Nói tới game thì anh là trùm luôn đó.
Dialogue: 0,0:03:10.98,0:03:12.26,[Harem][00],,0000,0000,0000,,Momo-san...
Dialogue: 0,0:03:14.45,0:03:18.36,[Harem][00],,0000,0000,0000,,Dạo gần đây cậu là cứ sáp gần anh Rito đó nha?
Dialogue: 0,0:03:19.07,0:03:22.48,[Harem][00],,0000,0000,0000,,Rito cũng chẳng thèm để ý là sao.
Dialogue: 0,0:03:32.28,0:03:34.57,[Harem],,0000,0000,0000,,Cậu muốn tớ giữ khoảng cách với anh Rito hả?
Dialogue: 0,0:03:34.89,0:03:35.55,[Harem],,0000,0000,0000,,Ừ.
Dialogue: 0,0:03:36.01,0:03:38.02,[Harem],,0000,0000,0000,,Tớ muốn cậu cư xử đứng đắn một chút.
Dialogue: 0,0:03:39.69,0:03:40.53,[Harem],,0000,0000,0000,,Thì...
Dialogue: 0,0:03:40.53,0:03:42.54,[Harem],,0000,0000,0000,,Lỡ chị Lala nghĩ gì thì sao...
Dialogue: 0,0:03:45.30,0:03:46.78,[Harem],,0000,0000,0000,,Đã vậy...
Dialogue: 0,0:03:46.78,0:03:51.23,[Harem],,0000,0000,0000,,Lâu lâu cậu lại chui vào giường\N của anh Rito lúc đêm khuya nữa chứ.
Dialogue: 0,0:03:51.99,0:03:54.47,[Harem],,0000,0000,0000,,Tớ không biết cậu tính làm gì, nhưng mà...
Dialogue: 0,0:03:54.47,0:03:56.07,[Harem],,0000,0000,0000,,Cậu đã biết thế rồi.
Dialogue: 0,0:03:57.14,0:04:00.13,[Harem],,0000,0000,0000,,Vậy, sao Mikan-san không gia nhập với mình cho vui nè?
Dialogue: 0,0:04:02.68,0:04:04.65,[Harem],,0000,0000,0000,,Đùa tí thôi ấy mà~
Dialogue: 0,0:04:04.65,0:04:06.01,[Harem],,0000,0000,0000,,Xin lỗi nha.
Dialogue: 0,0:04:06.01,0:04:10.80,[Harem],,0000,0000,0000,,Tại lơ tơ mơ nên lỡ chui vô \Ngiường anh Rito hồi nào chả biết luôn.
Dialogue: 0,0:04:10.80,0:04:12.71,[Harem],,0000,0000,0000,,Từ giờ tớ sẽ cẩn thận!
Dialogue: 0,0:04:14.54,0:04:15.67,[Harem],,0000,0000,0000,,N-{\alpha&HFF&}Nói dối trắng trợn luôn...
Dialogue: 0,0:04:15.67,0:04:17.64,[Harem],,0000,0000,0000,,N-Nói dối trắng trợn luôn...
Dialogue: 0,0:04:34.46,0:04:35.61,[Harem],,0000,0000,0000,,Momo-san...
Dialogue: 0,0:04:36.30,0:04:38.13,[Harem],,0000,0000,0000,,Liếc sơ cũng đủ biết cậu ta chịu dừng chết liền.
Dialogue: 0,0:04:45.14,0:04:48.21,[Harem],,0000,0000,0000,,Momo-san mà mò vào, là mình quạt cho biết tay thì thôi.
Dialogue: 0,0:04:59.63,0:05:00.99,[Harem],,0000,0000,0000,,Không đến à...
Dialogue: 0,0:05:05.31,0:05:07.51,[Harem],,0000,0000,0000,,Người đâu ngủ mà vô tư dữ...
Dialogue: 0,0:05:08.62,0:05:13.04,[Harem],,0000,0000,0000,,Tại anh dễ dãi quá nên thân làm em \Nmới phải nhọc thân cực người thế này đó...
Dialogue: 0,0:05:15.15,0:05:16.59,[Harem],,0000,0000,0000,,Anh biết không hả?
Dialogue: 0,0:05:16.59,0:05:17.00,[Harem],,0000,0000,0000,,Nè!
Dialogue: 0,0:05:23.15,0:05:26.19,[Harem],,0000,0000,0000,,A-Anh nghĩ tay anh mò vô đâu vậy hả?
Dialogue: 0,0:05:26.86,0:05:29.72,[Rito],,0000,0000,0000,,Ngon quá đi~ kẹo dẻo~
Dialogue: 0,0:05:32.34,0:05:34.28,[Harem],,0000,0000,0000,,Nó không phải dẹo dẻo đâu nhớ!
Dialogue: 0,0:05:43.39,0:05:44.96,[Harem],,0000,0000,0000,,Ri-{\alpha&HFF&}Rito...
Dialogue: 0,0:05:44.96,0:05:46.34,[Harem],,0000,0000,0000,,Ri-Rito...
Dialogue: 0,0:05:48.67,0:05:50.88,[Harem],,0000,0000,0000,,Đủ rồi nha~!
Dialogue: 0,0:05:50.88,0:05:53.44,[Rito],,0000,0000,0000,,Kẹo dẻo cắn mình...
Dialogue: 0,0:05:54.17,0:05:55.33,[Harem],,0000,0000,0000,,Nguy hiểm, nguy hiểm...
Dialogue: 0,0:06:03.12,0:06:05.31,[Harem],,0000,0000,0000,,Mình đang làm cái khỉ gì vậy nè?
Dialogue: 0,0:06:10.15,0:06:12.40,[Harem],,0000,0000,0000,,Momo-san coi bộ không đến rồi...
Dialogue: 0,0:06:14.11,0:06:15.28,[Harem],,0000,0000,0000,,Mình cũng về phòng thôi.
Dialogue: 0,0:06:28.21,0:06:32.13,[Harem],,0000,0000,0000,,Sấm chớp nửa đêm thế này là mình ghét nhất à...
Dialogue: 0,0:06:35.22,0:06:37.05,[Rito],,0000,0000,0000,,Em sợ à?
Dialogue: 0,0:06:37.05,0:06:38.20,[Rito],,0000,0000,0000,,Sấm chớp ấy...
Dialogue: 0,0:06:38.67,0:06:40.74,[Rito],,0000,0000,0000,,Không sao đâu...
Dialogue: 0,0:06:41.05,0:06:43.01,[Rito],,0000,0000,0000,,Có anh hai...
Dialogue: 0,0:06:43.01,0:06:44.00,[Rito],,0000,0000,0000,,Ở đây với em rồi...
Dialogue: 0,0:06:45.44,0:06:47.02,[Harem][00],,0000,0000,0000,,Phải rồi...
Dialogue: 0,0:06:47.02,0:06:49.25,[Harem][00],,0000,0000,0000,,Hồi lâu cũng có chuyện này rồi.
Dialogue: 0,0:07:01.06,0:07:03.05,[Harem][00],,0000,0000,0000,,Tự nhiên thấy ấm lòng ghê...
Dialogue: 0,0:07:04.33,0:07:05.93,[Harem][00],,0000,0000,0000,,Vẫn như hồi xưa...
Dialogue: 0,0:07:05.93,0:07:07.36,[Harem][00],,0000,0000,0000,,Mùi anh Rito chẳng thay đổi gì cả.
Dialogue: 0,0:07:17.50,0:07:17.80,[Rito],,0000,0000,0000,,S-{\alpha&HFF&}S-S-S-S-S-S-S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:17.80,0:07:18.02,[Rito],,0000,0000,0000,,S-S{\alpha&HFF&}-S-S-S-S-S-S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:18.02,0:07:18.25,[Rito],,0000,0000,0000,,S-S-S-{\alpha&HFF&}S-S-S-S-S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:18.25,0:07:18.57,[Rito],,0000,0000,0000,,S-S-S-S{\alpha&HFF&}-S-S-S-S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:18.57,0:07:18.97,[Rito],,0000,0000,0000,,S-S-S-S-S{\alpha&HFF&}-S-S-S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:18.97,0:07:19.41,[Rito],,0000,0000,0000,,S-S-S-S-S-S{\alpha&HFF&}-S-S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:19.41,0:07:19.86,[Rito],,0000,0000,0000,,S-S-S-S-S-S-S-{\alpha&HFF&}S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:19.86,0:07:20.28,[Rito],,0000,0000,0000,,S-S-S-S-S-S-S-S{\alpha&HFF&}-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:20.28,0:07:22.03,[Rito],,0000,0000,0000,,S-S-S-S-S-S-S-S-Sao Mikan lại ở trên giường mình thế này!?
Dialogue: 0,0:07:28.11,0:07:30.06,[Harem],,0000,0000,0000,,K-Không phải như anh nghĩ đâu, Rito!
Dialogue: 0,0:07:30.06,0:07:30.94,[Harem],,0000,0000,0000,,Em chỉ...!
Dialogue: 0,0:07:30.94,0:07:32.19,[Harem],,0000,0000,0000,,Á~ Ah~
Dialogue: 0,0:07:32.19,0:07:35.38,[Harem],,0000,0000,0000,,Ra là như thế à, Mikan-san.
Dialogue: 0,0:07:35.60,0:07:36.48,[Rito],,0000,0000,0000,,Momo!
Dialogue: 0,0:07:36.48,0:07:37.79,[Harem],,0000,0000,0000,,Cậu ở đây bao lâu rồi!?
Dialogue: 0,0:07:38.85,0:07:41.95,[Harem],,0000,0000,0000,,Do cậu công lên việc xuống quá nên không để ý đó mà, Xin lỗi nha.
Dialogue: 0,0:07:43.58,0:07:46.59,[Harem],,0000,0000,0000,,Đ-Đừng có lái mọi chuyện thành quái đản đi!
Dialogue: 0,0:07:46.59,0:07:48.52,[Rito],,0000,0000,0000,,Hai đứa em bị sao vậy hả!?
Dialogue: 0,0:07:55.01,0:07:55.69,[Rito],,0000,0000,0000,,Đứng lại!
Dialogue: 0,0:07:55.87,0:07:57.19,[Rito],,0000,0000,0000,,Đã bảo đứng lại mà!
Dialogue: 0,0:07:57.92,0:08:01.95,[Rito],,0000,0000,0000,,Bất cẩn một tí là nó lôi cola trong tủ lạnh ra nốc à!
Dialogue: 0,0:08:02.40,0:08:05.68,[Harem],,0000,0000,0000,,Celine-chan không ngờ lại xỉn vì coke nhỉ.
Dialogue: 0,0:08:05.68,0:08:07.22,[Rito],,0000,0000,0000,,Không chỉ thế đâu!
Dialogue: 0,0:08:07.45,0:08:09.30,[Rito],,0000,0000,0000,,Chúng ta phải mau bắt nó lại!
Dialogue: 0,0:08:14.90,0:08:16.42,[Harem],,0000,0000,0000,,Nếu nhớ không lầm thì ngươi là...
Dialogue: 0,0:08:16.42,0:08:17.30,[Harem],,0000,0000,0000,,Cây hoa của Yuuki Rito...
Dialogue: 0,0:08:23.71,0:08:25.13,[Harem],,0000,0000,0000,,Bắt được rồi nhé~
Dialogue: 0,0:08:25.84,0:08:29.90,[Harem],,0000,0000,0000,,Thế, em lại muốn uống thêm coke hả?
Dialogue: 0,0:08:29.90,0:08:32.37,[Rito],,0000,0000,0000,,Được! Vậy, chúng ta về thôi.
Dialogue: 0,0:08:32.37,0:08:32.90,[Harem],,0000,0000,0000,,Rồi!
Dialogue: 0,0:08:32.90,0:08:33.49,[Harem][01],,0000,0000,0000,,Rồi!
Dialogue: 0,0:08:33.78,0:08:34.88,[Harem],,0000,0000,0000,,Công chúa...
Dialogue: 0,0:08:36.46,0:08:37.78,[Harem],,0000,0000,0000,,Yuuki Rito...
Dialogue: 0,0:08:38.17,0:08:39.56,[Harem],,0000,0000,0000,,Tôi có thể mượn hắn không?
Dialogue: 0,0:08:41.05,0:08:42.32,[Harem],,0000,0000,0000,,Uhm! Chắc rồi!
Dialogue: 0,0:08:42.65,0:08:45.18,[Harem],,0000,0000,0000,,Vậy, tụi em về trước nha~!
Dialogue: 0,0:08:47.72,0:08:50.41,[Harem],,0000,0000,0000,,Có một cái sạp Taiyaki ta muốn đến.
Dialogue: 0,0:08:51.10,0:08:52.42,[Harem],,0000,0000,0000,,Ta sẽ khao.
Dialogue: 0,0:08:55.07,0:08:56.76,[Harem],,0000,0000,0000,,Cho cháu 20 cái như thường đi ạ.
Dialogue: 0,0:08:57.79,0:09:01.04,[Rito][00],,0000,0000,0000,,Ai ngờ Yami lại bị phấn hoa tác động cơ chứ.
Dialogue: 0,0:09:01.71,0:09:02.40,[Sub_Phụ],,0000,0000,0000,,Đây!
Dialogue: 0,0:09:02.86,0:09:05.27,[Sub_Phụ],,0000,0000,0000,,Chú cho cháu thêm vài cái đó, Yami-chan!
Dialogue: 0,0:09:05.86,0:09:07.65,[Sub_Phụ],,0000,0000,0000,,Ăn vui vẻ với bạn trai nhé cháu?
Dialogue: 0,0:09:08.63,0:09:09.37,[Harem],,0000,0000,0000,,Bạn trai...
Dialogue: 0,0:09:16.03,0:09:18.05,[Harem],,0000,0000,0000,,Nào, ăn đi.
Dialogue: 0,0:09:18.91,0:09:19.80,[Rito],,0000,0000,0000,,Cám ơn.
Dialogue: 0,0:09:19.80,0:09:21.16,[Rito],,0000,0000,0000,,Itadakimatsu.
Dialogue: 0,0:09:23.77,0:09:24.27,[Sub_Phụ],,0000,0000,0000,,{\an8}Đây!
Dialogue: 0,0:09:24.27,0:09:26.05,[Sub_Phụ],,0000,0000,0000,,{\an8}Takkun, A~.
Dialogue: 0,0:09:29.14,0:09:30.18,[Sub_Phụ],,0000,0000,0000,,Ngon không?
Dialogue: 0,0:09:30.18,0:09:30.76,[Sub_Phụ],,0000,0000,0000,,{\an8}Um, ngon lắm!
Dialogue: 0,0:09:30.57,0:09:31.62,[Sub_Phụ],,0000,0000,0000,,{\an8}Ăn thêm đi!
Dialogue: 0,0:09:31.62,0:09:32.16,[Sub_Phụ],,0000,0000,0000,,{\an8}Ừm~!
Dialogue: 0,0:09:32.16,0:09:34.93,[Sub_Phụ],,0000,0000,0000,,{\an8}Em thích mấy người ăn nhiều ghê!
Dialogue: 0,0:09:33.46,0:09:34.57,[Harem],,0000,0000,0000,,Yuuki Rito.
Dialogue: 0,0:09:35.85,0:09:38.21,[Harem],,0000,0000,0000,,Ta sẽ đút cho ngươi!
Dialogue: 0,0:09:45.10,0:09:46.28,[Harem],,0000,0000,0000,,Ngon không?
Dialogue: 0,0:09:52.58,0:09:54.60,[Rito],,0000,0000,0000,,Tưởng đâu đi gặp ông bà rồi chứ.
Dialogue: 0,0:09:56.41,0:09:57.71,[Rito],,0000,0000,0000,,Xin lỗi để em đợi.
Dialogue: 0,0:09:58.99,0:10:01.52,[Rito],,0000,0000,0000,,Yami, bộ đồ đó...
Dialogue: 0,0:10:01.52,0:10:05.91,[Harem],,0000,0000,0000,,Nhờ khả năng chuyển đổi biến đồ mình cho hợp mọi người ấy mà.
Dialogue: 0,0:10:06.26,0:10:09.14,[Rito],,0000,0000,0000,,Không ngờ, cả em với Peke đều làm được cơ à!
Dialogue: 0,0:10:09.64,0:10:13.77,[Harem],,0000,0000,0000,,Là hàng chuyển đổi nên nó không trụ lâu được đâu.
Dialogue: 0,0:10:15.24,0:10:16.35,[Harem],,0000,0000,0000,,T-{\alpha&HFF&}Thế nào ạ?
Dialogue: 0,0:10:16.35,0:10:17.34,[Harem],,0000,0000,0000,,T-Thế nào ạ?
Dialogue: 0,0:10:18.89,0:10:20.33,[Rito],,0000,0000,0000,,Hợp với em lắm!
Dialogue: 0,0:10:20.52,0:10:22.24,[Rito],,0000,0000,0000,,Dễ thương quá trời luôn!
Dialogue: 0,0:10:22.99,0:10:26.24,[Harem],,0000,0000,0000,,T-Thật vậy ư!?
Dialogue: 0,0:10:31.66,0:10:34.69,[Rito][00],,0000,0000,0000,,Yami mà lại để ý tới ăn mặc...
Dialogue: 0,0:10:35.57,0:10:38.14,[Rito][00],,0000,0000,0000,,Không lẽ do tác động của phấn hoa?
Dialogue: 0,0:10:40.02,0:10:42.81,[Harem],,0000,0000,0000,,Đừng nhìn ta như thế mà...
Dialogue: 0,0:10:44.17,0:10:45.07,[Rito],,0000,0000,0000,,Xin lỗi!
Dialogue: 0,0:10:48.15,0:10:50.06,[Sub_Phụ],,0000,0000,0000,,Đi đứng thì phải coi chừng chứ!
Dialogue: 0,0:10:58.73,0:10:59.60,[Harem],,0000,0000,0000,,Những tên dâm tặc...
Dialogue: 0,0:11:00.73,0:11:02.21,[Rito][00],,0000,0000,0000,,Bị giết mất thôi!
Dialogue: 0,0:11:05.65,0:11:07.78,[Harem],,0000,0000,0000,,Là ta ghét lắm đấy.
Dialogue: 0,0:11:09.39,0:11:11.90,[Harem],,0000,0000,0000,,Mà ta thấy nếu...
Dialogue: 0,0:11:11.91,0:11:13.50,[Harem],,0000,0000,0000,,Bỏ qua một lần chắc cũng không sao nhỉ.
Dialogue: 0,0:11:15.73,0:11:17.59,[Harem],,0000,0000,0000,,Ta biết...
Dialogue: 0,0:11:17.59,0:11:20.04,[Harem],,0000,0000,0000,,Mình khác với mình thường ngày lắm.
Dialogue: 0,0:11:21.34,0:11:22.07,[Harem],,0000,0000,0000,,Nhưng...
Dialogue: 0,0:11:23.28,0:11:26.08,[Harem],,0000,0000,0000,,Nếu bản thân muốn hiểu hết...
Dialogue: 0,0:11:26.08,0:11:27.98,[Harem],,0000,0000,0000,,Cái cảm giác khác lạ này...
Dialogue: 0,0:11:28.73,0:11:30.58,[Harem],,0000,0000,0000,,Tam trạng này...
Dialogue: 0,0:11:30.58,0:11:32.26,[Harem],,0000,0000,0000,,Sao lại an tâm quá vậy...
Dialogue: 0,0:11:32.26,0:11:33.18,[Harem],,0000,0000,0000,,Ta muốn...
Dialogue: 0,0:11:33.18,0:11:34.66,[Harem],,0000,0000,0000,,Muốn cảm nhận nó nhiều nữa.
Dialogue: 0,0:11:37.31,0:11:38.35,[Harem],,0000,0000,0000,,Thế cho nên...
Dialogue: 0,0:11:39.81,0:11:42.23,[Harem],,0000,0000,0000,,Bọn mình... hôn nhau...
Dialogue: 0,0:11:42.23,0:11:43.83,[Harem],,0000,0000,0000,,Có ổn không anh?
Dialogue: 0,0:11:45.99,0:11:47.99,[Harem],,0000,0000,0000,,Em muốn thử xem...
Dialogue: 0,0:11:47.99,0:11:50.00,[Harem],,0000,0000,0000,,Cảm giác này sẽ đưa em tới đâu.
Dialogue: 0,0:11:50.74,0:11:53.12,[Harem],,0000,0000,0000,,Liệu rằng nụ hôn ấy...
Dialogue: 0,0:11:53.12,0:11:54.80,[Harem],,0000,0000,0000,,Có thể bí ẩn đến cỡ nào...
Dialogue: 0,0:11:58.83,0:12:01.36,[Rito],,0000,0000,0000,,Không, không thể!
Dialogue: 0,0:12:01.36,0:12:03.62,[Rito],,0000,0000,0000,,Thế này chả giống em tí nào!
Dialogue: 0,0:12:03.62,0:12:06.04,[Rito],,0000,0000,0000,,Mau trở lại đi, Yami!
Dialogue: 0,0:12:06.95,0:12:07.94,[Rito],,0000,0000,0000,,Rụng rồi!
Dialogue: 0,0:12:07.94,0:12:09.18,[Rito],,0000,0000,0000,,Vậy là em đã trở lại bình-
Dialogue: 0,0:12:11.42,0:12:13.35,[Harem],,0000,0000,0000,,Đừng có vô duyên vô cớ chạm vào ta.
Dialogue: 0,0:12:14.18,0:12:16.39,[Rito],,0000,0000,0000,,Bình thường đừng hỏi luôn.
Dialogue: 0,0:12:21.42,0:12:23.58,[Harem],,0000,0000,0000,,Sao ngươi lại bảo chúng ta không thể...?
Dialogue: 0,0:12:25.14,0:12:26.10,[Harem],,0000,0000,0000,,Thì là...
Dialogue: 0,0:12:27.16,0:12:28.82,[Harem],,0000,0000,0000,,Nụ hôn vừa nãy ấy.
Dialogue: 0,0:12:30.35,0:12:31.99,[Rito],,0000,0000,0000,,Tại sao à?
Dialogue: 0,0:12:31.99,0:12:34.10,[Rito],,0000,0000,0000,,Ờ thì, chắc tại em tò mò tí thôi.
Dialogue: 0,0:12:34.10,0:12:38.45,[Rito],,0000,0000,0000,,Với lại, em bất thường như vậy\N anh đâu dám tự tiện thế chứ.
Dialogue: 0,0:12:38.82,0:12:42.40,[Rito],,0000,0000,0000,,Kiểu gì sau này Yami cũng sẽ hối tiếc cho mà xem.
Dialogue: 0,0:12:44.04,0:12:45.25,[Harem],,0000,0000,0000,,Ngươi đúng là ngốc quá chừng?
Dialogue: 0,0:12:46.38,0:12:49.65,[Harem],,0000,0000,0000,,Lo lắng cho kẻ luôn muốn lấy mạng ngươi...
Dialogue: 0,0:12:50.43,0:12:53.35,[Harem],,0000,0000,0000,,Nhưng...
Dialogue: 0,0:12:53.35,0:12:56.08,[Harem],,0000,0000,0000,,Mấy tên ngốc thì ta không ghét đâu.
Dialogue: 0,0:12:57.40,0:12:58.75,[Rito][00],,0000,0000,0000,,Con bé cười?
Dialogue: 0,0:12:59.48,0:13:02.84,[Harem],,0000,0000,0000,,Cho nên ta sẽ ban cho ngươi được chết trong yên bình.
Dialogue: 0,0:13:26.12,0:13:27.36,[Harem],,0000,0000,0000,,Đ... {\alpha&HFF&}Đừng ~ mà.
Dialogue: 0,0:13:27.36,0:13:28.91,[Harem],,0000,0000,0000,,Đ... Đừng ~ mà.
Dialogue: 0,0:13:29.54,0:13:30.50,[Rito],,0000,0000,0000,,Momo!
Dialogue: 0,0:13:30.50,0:13:32.27,[Rito],,0000,0000,0000,,Em lại thế nữa rồi!
Dialogue: 0,0:13:32.77,0:13:33.86,[Harem],,0000,0000,0000,,Rito...
Dialogue: 0,0:13:33.86,0:13:35.36,[Harem],,0000,0000,0000,,Buổi sáng chuẩn bị rồi đó.
Dialogue: 0,0:13:35.36,0:13:36.21,[Harem],,0000,0000,0000,,Mikan bảo em...
Dialogue: 0,0:13:36.43,0:13:38.09,[Harem],,0000,0000,0000,,Nhà ngươi đang làm gì Momo thế hả!?
Dialogue: 0,0:13:37.18,0:13:39.21,[Rito],,0000,0000,0000,,{\an8}Không, không, không phải!
Dialogue: 0,0:13:38.40,0:13:40.05,[Harem],,0000,0000,0000,,Tên súc sinh kia!
Dialogue: 0,0:13:39.21,0:13:40.85,[Rito],,0000,0000,0000,,{\an8}Lúc anh thức dậy thì Momo đã....!
Dialogue: 0,0:13:44.11,0:13:47.58,[Harem],,0000,0000,0000,,Em là người đã chui vào giường của Rito à?
Dialogue: 0,0:13:47.87,0:13:49.01,[Harem],,0000,0000,0000,,Phải đấy.
Dialogue: 0,0:13:49.52,0:13:50.52,[Harem],,0000,0000,0000,,Thật là...
Dialogue: 0,0:13:50.52,0:13:52.29,[Harem],,0000,0000,0000,,Tại sao lại là tên súc sinh đó chứ?
Dialogue: 0,0:13:52.90,0:13:56.55,[Harem],,0000,0000,0000,,Hơn nữa, tên đó lại là hôn phu của chị nữa chứ.
Dialogue: 0,0:13:56.95,0:14:00.99,[Harem],,0000,0000,0000,,Thì giờ em là tình nhân của anh Rito cũng đâu sao đâu mà.
Dialogue: 0,0:14:03.82,0:14:05.74,[Harem],,0000,0000,0000,,Khó hiểu quá đi.
Dialogue: 0,0:14:05.83,0:14:08.99,[Harem],,0000,0000,0000,,Hắn có cái gì hay ho chứ, cái tên Yuuki Rito đó?
Dialogue: 0,0:14:09.66,0:14:13.61,[Harem],,0000,0000,0000,,Đàn ông con trai mà yếu nhớt thấy gớm.
Dialogue: 0,0:14:16.97,0:14:18.43,[Harem],,0000,0000,0000,,Cái bản mặt ngộ ghê.
Dialogue: 0,0:14:18.66,0:14:19.87,[Harem],,0000,0000,0000,,Bé nào đây ta?
Dialogue: 0,0:14:33.29,0:14:35.25,[Sub_Phụ],,0000,0000,0000,,Xin lỗi nha, cô bé.
Dialogue: 0,0:14:35.97,0:14:37.47,[Sub_Phụ],,0000,0000,0000,,Nhờ cô mà tôi thoát chết đó.
Dialogue: 0,0:14:37.64,0:14:38.94,[Harem],,0000,0000,0000,,Không có gì, không có gì.
Dialogue: 0,0:14:39.68,0:14:43.23,[Sub_Phụ],,0000,0000,0000,,Nhưng cô là con người mà lại hiểu tôi nói gì cơ đấy...
Dialogue: 0,0:14:43.72,0:14:45.72,[Harem],,0000,0000,0000,,Từ lúc sinh ra tôi đã có khả năng đó rồi.
Dialogue: 0,0:14:46.67,0:14:47.53,[Harem],,0000,0000,0000,,Tôi là Nana.
Dialogue: 0,0:14:48.03,0:14:49.92,[Sub_Phụ],,0000,0000,0000,,Tôi là Sairenji Marron...
Dialogue: 0,0:14:49.92,0:14:52.04,[Sub_Phụ],,0000,0000,0000,,Dòng chó sục Boston đắt tiền đấy.
Dialogue: 0,0:14:52.67,0:14:54.83,[Harem],,0000,0000,0000,,Vừa rồi sao cậu lại đi một mình?
Dialogue: 0,0:14:54.83,0:14:55.76,[Harem],,0000,0000,0000,,Chủ của cậu đâu?
Dialogue: 0,0:14:56.14,0:14:58.28,[Sub_Phụ],,0000,0000,0000,,Đang đi dạo thì bọn tôi tách ra.
Dialogue: 0,0:14:59.05,0:15:04.14,[Sub_Phụ],,0000,0000,0000,,Một vật thể bay bay xuất hiện trước mắt làm tôi chú ý.
Dialogue: 0,0:15:04.49,0:15:06.04,[Sub_Phụ],,0000,0000,0000,,Thật đáng hổ thẹn mà...
Dialogue: 0,0:15:06.75,0:15:10.78,[Sub_Phụ],,0000,0000,0000,,Nhưng tôi không thể kiềm chế bản tính hoang dã của mình.
Dialogue: 0,0:15:10.78,0:15:12.53,[Harem],,0000,0000,0000,,Nói trắng ra là lạc chứ gì.
Dialogue: 0,0:15:12.86,0:15:14.39,[Harem],,0000,0000,0000,,Coi bộ hết cách nhỉ...
Dialogue: 0,0:15:14.39,0:15:16.82,[Harem],,0000,0000,0000,,Thôi để tôi giúp cậu tìm chủ cho.
Dialogue: 0,0:15:16.82,0:15:17.70,[Sub_Phụ],,0000,0000,0000,,Thật ư?
Dialogue: 0,0:15:18.16,0:15:20.16,[Sub_Phụ],,0000,0000,0000,,Cô mà làm vợ là nhất đó, cô bé.
Dialogue: 0,0:15:21.12,0:15:22.99,[Harem],,0000,0000,0000,,Mô tả chủ nhân của cậu đi.
Dialogue: 0,0:15:23.91,0:15:25.78,[Sub_Phụ],,0000,0000,0000,,Cô chủ của tôi...
Dialogue: 0,0:15:27.74,0:15:30.11,[Sub_Phụ],,0000,0000,0000,,Có một mùi hương vừa mềm vừa êm dịu.
Dialogue: 0,0:15:30.47,0:15:31.75,[Harem],,0000,0000,0000,,Nói mùi làm gì!?
Dialogue: 0,0:15:31.75,0:15:33.67,[Harem],,0000,0000,0000,,Nói cái tính cách để xác định con người ấy!
Dialogue: 0,0:15:33.91,0:15:35.60,[Sub_Phụ],,0000,0000,0000,,Nói dễ hơn làm à...
Dialogue: 0,0:15:36.60,0:15:37.12,[Sub_Phụ],,0000,0000,0000,,A, phải rồi!
Dialogue: 0,0:15:37.89,0:15:40.28,[Sub_Phụ],,0000,0000,0000,,Giờ chủ của tôi đang gặp vấn đề tình yêu!
Dialogue: 0,0:15:40.50,0:15:41.30,[Harem],,0000,0000,0000,,Làm như ta quan tâm á!
Dialogue: 0,0:15:41.65,0:15:43.98,[Sub_Phụ],,0000,0000,0000,,Cô ấy có một người con trai trong mộng...
Dialogue: 0,0:15:43.98,0:15:45.91,[Sub_Phụ],,0000,0000,0000,,Tên Yuuki Rito thì phải.
Dialogue: 0,0:15:46.14,0:15:48.38,[Harem],,0000,0000,0000,,Mớm cái tin gì chả có ích gì h–
Dialogue: 0,0:15:48.38,0:15:48.72,[Harem],,0000,0000,0000,,Eh!?
Dialogue: 0,0:15:50.21,0:15:51.04,[Harem],,0000,0000,0000,,Yuuki Rito?
Dialogue: 0,0:15:51.04,0:15:52.01,[Harem],,0000,0000,0000,,Marron!
Dialogue: 0,0:15:52.20,0:15:54.07,[Harem],,0000,0000,0000,,Marron, em không sao chứ hả!
Dialogue: 0,0:15:54.45,0:15:56.98,[Harem],,0000,0000,0000,,Thật là, đừng có tự dưng lại biến mất chứ.
Dialogue: 0,0:15:58.89,0:15:59.79,[Harem],,0000,0000,0000,,Nana-chan!?
Dialogue: 0,0:15:59.79,0:16:00.66,[Harem],,0000,0000,0000,,Haruna.
Dialogue: 0,0:16:07.17,0:16:08.53,[Harem],,0000,0000,0000,,Thật bất ngờ à.
Dialogue: 0,0:16:09.16,0:16:11.34,[Harem],,0000,0000,0000,,Chị Haruna lại có cảm tình với Rito...
Dialogue: 0,0:16:12.33,0:16:13.43,[Harem],,0000,0000,0000,,Thật may quá!
Dialogue: 0,0:16:13.86,0:16:16.24,[Harem],,0000,0000,0000,,Bộ đồ cũ của chị lại vừa với em.
Dialogue: 0,0:16:17.17,0:16:18.74,[Harem],,0000,0000,0000,,Tự nhiên như ở nhà nhé em.
Dialogue: 0,0:16:19.10,0:16:21.01,[Harem],,0000,0000,0000,,Để chị đi làm ít thức uống.
Dialogue: 0,0:16:22.35,0:16:23.83,[Harem],,0000,0000,0000,,Xin lỗi làm phiền nha.
Dialogue: 0,0:16:23.83,0:16:25.38,[Harem],,0000,0000,0000,,Em nói gì vậy?
Dialogue: 0,0:16:25.60,0:16:27.66,[Harem],,0000,0000,0000,,Marron là nhờ công em cứu cả mà.
Dialogue: 0,0:16:27.66,0:16:28.68,[Harem],,0000,0000,0000,,Lẽ thường thôi.
Dialogue: 0,0:16:28.68,0:16:29.76,[Harem],,0000,0000,0000,,Chào.
Dialogue: 0,0:16:30.64,0:16:31.32,[Harem],,0000,0000,0000,,Ai vậy?
Dialogue: 0,0:16:31.94,0:16:33.41,[Harem],,0000,0000,0000,,Chị gái của chị đó.
Dialogue: 0,0:16:33.41,0:16:34.42,[Sub_Phụ],,0000,0000,0000,,Chị là Akiho.
Dialogue: 0,0:16:34.42,0:16:35.96,[Sub_Phụ],,0000,0000,0000,,Rất vui được gặp em, Nana-chan.
Dialogue: 0,0:16:35.96,0:16:36.99,[Harem],,0000,0000,0000,,Ư-Ừm.
Dialogue: 0,0:16:36.99,0:16:37.93,[Harem],,0000,0000,0000,,Chào chị ạ.
Dialogue: 0,0:16:39.54,0:16:42.03,[Sub_Phụ],,0000,0000,0000,,Em gái của Lala-chan có khác nhỉ!
Dialogue: 0,0:16:42.45,0:16:44.31,[Sub_Phụ],,0000,0000,0000,,Đáng yêu quá~!
Dialogue: 0,0:16:44.92,0:16:45.88,[Harem],,0000,0000,0000,,Đ-Đáng yêu!?
Dialogue: 0,0:16:45.88,0:16:46.95,[Harem],,0000,0000,0000,,Em á!?
Dialogue: 0,0:16:46.95,0:16:49.12,[Sub_Phụ],,0000,0000,0000,,Ừ! Em không biết hả?
Dialogue: 0,0:16:49.12,0:16:50.57,[Harem],,0000,0000,0000,,Nè, chị!
Dialogue: 0,0:16:50.57,0:16:52.23,[Harem],,0000,0000,0000,,Nana đang khó chịu đó thấy không?
Dialogue: 0,0:16:52.36,0:16:55.53,[Sub_Phụ],,0000,0000,0000,,Ô ya~! Em ghen hả, Haruna-kun?
Dialogue: 0,0:16:55.53,0:16:59.99,[Sub_Phụ],,0000,0000,0000,,Hồi nhỏ em cứ bám lấy chị và kêu "Chị ơi, chị ơi" cơ đấy!
Dialogue: 0,0:16:59.99,0:17:01.75,[Harem],,0000,0000,0000,,Làm, làm gì có chuyện đó chứ!
Dialogue: 0,0:17:01.97,0:17:03.77,[Sub_Phụ],,0000,0000,0000,,{\an8}Có đấy!
Dialogue: 0,0:17:03.77,0:17:04.85,[Harem],,0000,0000,0000,,{\an8}Không có!
Dialogue: 0,0:17:02.95,0:17:04.30,[Harem][00],,0000,0000,0000,,He~
Dialogue: 0,0:17:04.30,0:17:06.89,[Harem][00],,0000,0000,0000,,Chị Haruna yêu chị gái của mình ghê cơ.
Dialogue: 0,0:17:04.85,0:17:06.27,[Sub_Phụ],,0000,0000,0000,,{\an8}Aww, em đỏ mặt kìa!
Dialogue: 0,0:17:06.27,0:17:07.28,[Harem],,0000,0000,0000,,{\an8}Không có nha!
Dialogue: 0,0:17:07.52,0:17:11.52,[Sub_Phụ],,0000,0000,0000,,{\an8}Haruna lúc bé đáng yêu ghê!
Dialogue: 0,0:17:11.52,0:17:13.24,[Sub_Phụ],,0000,0000,0000,,{\an8}Chị yêu em lắm đó!
Dialogue: 0,0:17:13.24,0:17:16.61,[Harem],,0000,0000,0000,,C-Cái thứ ngoe nguẩy này kích thích ta quá đi...!
Dialogue: 0,0:17:20.17,0:17:21.34,[Harem],,0000,0000,0000,,Dừn–
Dialogue: 0,0:17:21.34,0:17:22.26,[Harem],,0000,0000,0000,,Đồ n–
Dialogue: 0,0:17:22.95,0:17:23.84,[Harem],,0000,0000,0000,,C-Coi kìa!
Dialogue: 0,0:17:23.84,0:17:24.74,[Harem],,0000,0000,0000,,Marron!
Dialogue: 0,0:17:26.27,0:17:27.35,[Sub_Phụ],,0000,0000,0000,,Sao thế?
Dialogue: 0,0:17:27.36,0:17:29.99,[Harem],,0000,0000,0000,,Nana-chan bị chạm vào đuôi là yếu hả em.
Dialogue: 0,0:17:31.28,0:17:34.07,[Sub_Phụ],,0000,0000,0000,,Ai ở Deviluke cũng vậy à?
Dialogue: 0,0:17:34.62,0:17:37.30,[Harem],,0000,0000,0000,,Mỗi người mỗi khác ạ...
Dialogue: 0,0:17:37.30,0:17:41.12,[Harem],,0000,0000,0000,,Nhưng có vẻ chị em em đều quá nhạy cảm với nó.
Dialogue: 0,0:17:41.48,0:17:42.99,[Sub_Phụ],,0000,0000,0000,,Ra là vậy.
Dialogue: 0,0:17:43.44,0:17:45.95,[Sub_Phụ],,0000,0000,0000,,Ô, trễ mất, đến giờ rồi.
Dialogue: 0,0:17:46.21,0:17:47.96,[Harem],,0000,0000,0000,,Chị định đi đâu vậy, chị hai?
Dialogue: 0,0:17:48.17,0:17:50.11,[Sub_Phụ],,0000,0000,0000,,Chị có hứa sẽ gặp một người.
Dialogue: 0,0:17:50.42,0:17:51.25,[Harem],,0000,0000,0000,,Hẹn hò à?
Dialogue: 0,0:17:51.60,0:17:54.58,[Sub_Phụ],,0000,0000,0000,,Tụi chị cũng lâu rồi không gặp.
Dialogue: 0,0:17:54.58,0:17:56.48,[Sub_Phụ],,0000,0000,0000,,Với lại anh ta cũng không tệ.
Dialogue: 0,0:17:56.48,0:17:58.86,[Sub_Phụ],,0000,0000,0000,,Chị nghĩ có lẽ nên chơi đùa với anh ta một chút.
Dialogue: 0,0:17:58.86,0:18:00.17,[Sub_Phụ],,0000,0000,0000,,Vậy cứ tự nhiên đi nhé, Nana-chan.
Dialogue: 0,0:18:00.17,0:18:01.48,[Sub_Phụ],,0000,0000,0000,,Chị đi đây~
Dialogue: 0,0:18:02.61,0:18:03.95,[Harem],,0000,0000,0000,,Chơi đùa tí á?
Dialogue: 0,0:18:04.38,0:18:07.61,[Harem],,0000,0000,0000,,Chị của chị chín chắn ghê ha.
Dialogue: 0,0:18:07.61,0:18:10.89,[Harem],,0000,0000,0000,,Thiệt, biết tỏng chị ấy có tính yêu ai đây chứ.
Dialogue: 0,0:18:12.86,0:18:14.54,[Harem],,0000,0000,0000,,Chị thì sao, Haruna?
Dialogue: 0,0:18:17.42,0:18:19.74,[Harem],,0000,0000,0000,,Y-Yuuki Rito...
Dialogue: 0,0:18:21.21,0:18:23.49,[Harem],,0000,0000,0000,,Chị có muốn hẹn hò với hắn không?
Dialogue: 0,0:18:28.01,0:18:30.02,[Harem],,0000,0000,0000,,Mây đen đầy trời luôn ta.
Dialogue: 0,0:18:30.62,0:18:32.40,[Harem],,0000,0000,0000,,Em nghĩ trời sẽ mưa đó.
Dialogue: 0,0:18:32.93,0:18:34.64,[Harem],,0000,0000,0000,,Nana sẽ ổn chứ?
Dialogue: 0,0:18:35.18,0:18:37.01,[Harem],,0000,0000,0000,,Con bé đi đâu rồi?
Dialogue: 0,0:18:38.50,0:18:41.01,[Harem],,0000,0000,0000,,Chị thích hắn đúng không?
Dialogue: 0,0:18:41.45,0:18:44.53,[Harem],,0000,0000,0000,,N-Nana-chan sao em lại nói chuyện đó?
Dialogue: 0,0:18:44.53,0:18:48.03,[Harem],,0000,0000,0000,,Chị biết là em có thể nói chuyện với động vật mà?
Dialogue: 0,0:18:48.78,0:18:50.44,[Harem],,0000,0000,0000,,Marron nói em nghe đó.
Dialogue: 0,0:18:50.67,0:18:52.09,[Harem],,0000,0000,0000,,Marron ư!?
Dialogue: 0,0:18:52.49,0:18:53.67,[Harem],,0000,0000,0000,,Không được thế chứ!
Dialogue: 0,0:18:53.67,0:18:54.93,[Harem],,0000,0000,0000,,Đồ ngốc, ngốc, ngốc, ngốc...!
Dialogue: 0,0:18:54.93,0:18:56.06,[Harem],,0000,0000,0000,,Đừng lo.
Dialogue: 0,0:18:56.32,0:18:58.14,[Harem],,0000,0000,0000,,Em không nói với ai đâu.
Dialogue: 0,0:18:59.40,0:19:00.43,[Harem],,0000,0000,0000,,Nhưng...
Dialogue: 0,0:19:00.43,0:19:01.96,[Harem],,0000,0000,0000,,Em chỉ muốn biết.
Dialogue: 0,0:19:04.18,0:19:05.42,[Harem],,0000,0000,0000,,Momo, chị em...
Dialogue: 0,0:19:06.01,0:19:08.06,[Harem],,0000,0000,0000,,Và chị Haruna nữa...
Dialogue: 0,0:19:08.06,0:19:10.29,[Harem],,0000,0000,0000,,Sao ai cũng bị Yuuki Rito cuốn hút vậy...
Dialogue: 0,0:19:11.39,0:19:14.91,[Harem],,0000,0000,0000,,Em không hiểu tại sao mọi người ai cũng yêu hắn hết...
Dialogue: 0,0:19:15.52,0:19:17.89,[Harem],,0000,0000,0000,,Cái thứ gì đâu bông lông dữ không biết.
Dialogue: 0,0:19:17.89,0:19:19.07,[Harem],,0000,0000,0000,,Lại còn yếu xìu nữa...
Dialogue: 0,0:19:19.07,0:19:21.13,[Harem],,0000,0000,0000,,Tính tình thì do dự...
Dialogue: 0,0:19:21.13,0:19:22.90,[Harem],,0000,0000,0000,,Nana-chan...
Dialogue: 0,0:19:22.90,0:19:25.13,[Harem],,0000,0000,0000,,Điều khiến Yuuki-kun tuyệt vời là...
Dialogue: 0,0:19:26.72,0:19:31.37,[Harem],,0000,0000,0000,,Khi ở cạnh cậu ấy em cảm thấy ấm lòng hơn.
Dialogue: 0,0:19:32.35,0:19:34.64,[Harem],,0000,0000,0000,,Cảm thấy ấm áp à?
Dialogue: 0,0:19:35.13,0:19:35.85,[Harem],,0000,0000,0000,,Phải.
Dialogue: 0,0:19:36.33,0:19:37.96,[Harem],,0000,0000,0000,,Khó có lắm.
Dialogue: 0,0:19:39.38,0:19:42.44,[Harem],,0000,0000,0000,,Chị chắc Lala-san cũng thấy thế.
Dialogue: 0,0:19:43.71,0:19:46.03,[Harem],,0000,0000,0000,,Chẳng hiểu gì sấc.
Dialogue: 0,0:19:46.99,0:19:49.40,[Harem],,0000,0000,0000,,Chừng nào ở cùng cậu ta em sẽ hiểu.
Dialogue: 0,0:19:49.40,0:19:50.07,[Harem],,0000,0000,0000,,Chắc chắn.
Dialogue: 0,0:19:52.23,0:19:53.04,[Harem],,0000,0000,0000,,Haruna.
Dialogue: 0,0:19:53.04,0:19:53.70,[Harem],,0000,0000,0000,,Gì?
Dialogue: 0,0:19:55.31,0:19:58.52,[Harem],,0000,0000,0000,,Chị chưa nói với Rito tình cảm của mình phải không?
Dialogue: 0,0:20:00.61,0:20:03.63,[Harem],,0000,0000,0000,,Lẽ nào là vì chị hai của em...
Dialogue: 0,0:20:04.23,0:20:07.11,[Harem],,0000,0000,0000,,Mà chị giấu đi tình cảm của mình phải không?
Dialogue: 0,0:20:07.11,0:20:08.39,[Harem],,0000,0000,0000,,Chuyện đó...
Dialogue: 0,0:20:12.37,0:20:13.65,[Harem],,0000,0000,0000,,Ừ.
Dialogue: 0,0:20:13.99,0:20:15.32,[Harem],,0000,0000,0000,,Cũng gần như thế.
Dialogue: 0,0:20:16.53,0:20:17.96,[Harem],,0000,0000,0000,,Vì...
Dialogue: 0,0:20:17.96,0:20:20.62,[Harem],,0000,0000,0000,,Lala-san là người bạn quan trọng nhất của chị.
Dialogue: 0,0:20:21.40,0:20:22.75,[Harem],,0000,0000,0000,,Nếu như...
Dialogue: 0,0:20:22.75,0:20:25.72,[Harem],,0000,0000,0000,,Lala-san phát hiện ra tình cảm của chị...
Dialogue: 0,0:20:26.67,0:20:28.31,[Harem],,0000,0000,0000,,Có lẽ bọn chị sẽ...
Dialogue: 0,0:20:28.31,0:20:31.07,[Harem],,0000,0000,0000,,Không thân nhau như trước nữa.
Dialogue: 0,0:20:36.23,0:20:39.02,[Harem],,0000,0000,0000,,Em ở lâu thêm chút nữa có sao đâu.
Dialogue: 0,0:20:39.02,0:20:41.24,[Harem],,0000,0000,0000,,Chị hai và Momo chắc đang lo cho em lắm.
Dialogue: 0,0:20:41.71,0:20:44.03,[Harem],,0000,0000,0000,,Cám ơn đã giặt đồ cho em.
Dialogue: 0,0:20:47.06,0:20:48.77,[Harem],,0000,0000,0000,,Thật là khổ sở...
Dialogue: 0,0:20:48.77,0:20:49.92,[Harem],,0000,0000,0000,,Khi yêu nhỉ.
Dialogue: 0,0:20:56.51,0:20:58.47,[Harem],,0000,0000,0000,,Đúng là số con rệp.
Dialogue: 0,0:20:58.47,0:21:00.97,[Harem],,0000,0000,0000,,Vừa mới giặt xong, vậy mà...
Dialogue: 0,0:21:01.67,0:21:03.04,[Rito],,0000,0000,0000,,Này~!
Dialogue: 0,0:21:03.55,0:21:04.41,[Harem],,0000,0000,0000,,Rito!?
Dialogue: 0,0:21:04.63,0:21:05.64,[Rito],,0000,0000,0000,,May quá...
Dialogue: 0,0:21:05.64,0:21:07.67,[Rito],,0000,0000,0000,,Tìm được em trước lúc mưa to rồi.
Dialogue: 0,0:21:07.67,0:21:08.70,[Rito],,0000,0000,0000,,Đây, dù.
Dialogue: 0,0:21:12.92,0:21:15.42,[Harem],,0000,0000,0000,,Đừng nói nãy giờ... anh tìm tôi nha?
Dialogue: 0,0:21:15.96,0:21:17.26,[Rito],,0000,0000,0000,,Ừ, phải.
Dialogue: 0,0:21:17.26,0:21:18.84,[Rito],,0000,0000,0000,,Lala và mọi người lo cho em lắm đó.
Dialogue: 0,0:21:20.79,0:21:25.49,[Harem],,0000,0000,0000,,{\i1}Khi ở cạnh cậu ấy em cảm thấy ấm lòng hơn.
Dialogue: 0,0:21:27.99,0:21:29.00,[Rito],,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:21:29.00,0:21:30.05,[Harem],,0000,0000,0000,,Không có gì!
Dialogue: 0,0:21:31.11,0:21:32.79,[Harem],,0000,0000,0000,,Anh xài cái này thế nào hả!?
Dialogue: 0,0:21:32.79,0:21:34.11,[Harem],,0000,0000,0000,,Đồ ăn chơi!
Dialogue: 0,0:21:34.37,0:21:37.84,[Rito],,0000,0000,0000,,Đừng có đặt cho anh cái tên tệ vậy chứ!
Dialogue: 0,0:21:37.84,0:21:39.31,[Rito],,0000,0000,0000,,Ơ kìa, đợi anh với!
Dialogue: 0,0:21:43.50,0:21:45.76,[Harem],,0000,0000,0000,,{\i1}Lẽ nào là vì chị hai của em...
Dialogue: 0,0:21:45.78,0:21:48.43,[Harem],,0000,0000,0000,,{\i1}Mà chị giấu đi tình cảm của mình phải không?
Dialogue: 0,0:21:49.18,0:21:50.23,[Harem],,0000,0000,0000,,{\i1}Vì...
Dialogue: 0,0:21:50.23,0:21:52.65,[Harem],,0000,0000,0000,,{\i1}Lala-san là người bạn quan trọng nhất của chị.
Dialogue: 0,0:21:55.76,0:21:57.31,[Harem],,0000,0000,0000,,{\i1}Nhưng...
Dialogue: 0,0:21:57.31,0:22:00.02,[Harem],,0000,0000,0000,,{\i1}Có phải giấu đi tình cảm thực của mình...
Dialogue: 0,0:22:01.64,0:22:05.43,[Harem],,0000,0000,0000,,{\i1}Sẽ giúp mình có người bạn thực sự?
Dialogue: 0,0:22:18.32,0:22:19.79,[Harem],,0000,0000,0000,,Haruna!
Dialogue: 0,0:22:24.59,0:22:25.84,[Harem],,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:22:25.84,0:22:27.37,[Harem],,0000,0000,0000,,Trông cậu không khỏe.
Dialogue: 0,0:22:28.28,0:22:30.03,[Harem],,0000,0000,0000,,Không có gì đâu.
Dialogue: 0,0:22:34.85,0:22:36.29,[Harem],,0000,0000,0000,,Có vẻ cậu không bị sốt nhỉ.
Dialogue: 0,0:22:36.91,0:22:38.17,[Harem],,0000,0000,0000,,Thật tốt quá!
Dialogue: 0,0:22:38.17,0:22:40.88,[Harem],,0000,0000,0000,,Cậu mà ốm ra đó là tớ chả biết sao đâu à!
Dialogue: 0,0:22:41.80,0:22:42.97,[Harem],,0000,0000,0000,,Lo ư...
Dialogue: 0,0:22:43.19,0:22:43.84,[Harem],,0000,0000,0000,,Ừ!
Dialogue: 0,0:22:44.26,0:22:48.23,[Harem],,0000,0000,0000,,Vì Haruna là người bạn quan trọng nhất của tớ mà!
Dialogue: 0,0:22:53.13,0:22:54.09,[Harem],,0000,0000,0000,,Sao thế?
Dialogue: 0,0:23:00.75,0:23:01.79,[Harem],,0000,0000,0000,,Lala-san...
Dialogue: 0,0:23:05.02,0:23:06.63,[Harem][00],,0000,0000,0000,,Như mình nghĩ...
Dialogue: 0,0:23:06.63,0:23:09.20,[Harem][00],,0000,0000,0000,,Che giấu tình cảm...
Dialogue: 0,0:23:09.20,0:23:11.71,[Harem][00],,0000,0000,0000,,Sẽ không giúp mình có người bạn thực sự.
Dialogue: 0,0:23:11.92,0:23:12.87,[Harem],,0000,0000,0000,,Haruna!?
Dialogue: 0,0:23:14.13,0:23:16.41,[Harem],,0000,0000,0000,,Lala-san...
Dialogue: 0,0:23:16.41,0:23:17.43,[Harem],,0000,0000,0000,,Tớ...
Dialogue: 0,0:23:25.39,0:23:27.03,[Harem],,0000,0000,0000,,Tớ đối với Yuki-kun...
Dialogue: 0,0:23:28.98,0:23:29.98,[Harem],,0000,0000,0000,,Tớ thích cậu ấy.
Dialogue: 0,0:23:40.53,0:23:42.77,[Next_Episode],,0000,0000,0000,,Tớ yêu Yuuki!
Dialogue: 0,0:23:42.77,0:23:44.61,[Next_Episode],,0000,0000,0000,,Một lời công bố thật táo bạo, Haruna!
Dialogue: 0,0:23:44.61,0:23:46.10,[Next_Episode],,0000,0000,0000,,Có vẻ tập sau là tập cuối rồi.
Dialogue: 0,0:23:45.83,0:23:48.16,[Next_Episode],,0000,0000,0000,,Lala-san, thì đúng tập sau là tập cuối đó...
Dialogue: 0,0:23:48.16,0:23:49.59,[Next_Episode],,0000,0000,0000,,Không thể nào~!
Dialogue: 0,0:23:50.13,0:23:52.13,Default,,0000,0000,0000,,

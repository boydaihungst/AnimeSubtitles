Dialogue: 0,0:15:24.33,0:15:27.48,Default,,0000,0000,0000,,Làm sao mà tui bị hại bởi yêu cầu của mình được!
Dialogue: 0,0:15:28.19,0:15:30.64,Default,,0000,0000,0000,,Vậy số 4 là ai nào?
Dialogue: 0,0:15:30.64,0:15:34.12,Default,,0000,0000,0000,,Kẻ đại ngốc nào sẽ phải chịu sự xấu hổ cho hết trò chơi
Dialogue: 0,0:15:34.12,0:15:38.60,Default,,0000,0000,0000,,trong khi những người khác mặc đồ bình thường nè?
Dialogue: 0,0:15:39.29,0:15:40.87,Default,,0000,0000,0000,,An-chan...
Dialogue: 0,0:15:40.87,0:15:43.12,Default,,0000,0000,0000,,Kobato-chan?!
Dialogue: 0,0:15:43.61,0:15:46.20,Default,,0000,0000,0000,,Mệnh lệnh này không hợp lệ! Vô giá trị!
Dialogue: 0,0:15:46.20,0:15:50.50,Default,,0000,0000,0000,,Chị xin lỗi bé nhưng nhà vua đã ra lệnh thì như đinh đóng cột.
Dialogue: 0,0:15:50.50,0:15:55.06,Default,,0000,0000,0000,,Em gái, em có tức thì cứ xả hết vô mặt tên vua này này.
Dialogue: 0,0:15:55.06,0:15:56.30,Default,,0000,0000,0000,,Vậy là tại tui hả?!
Dialogue: 0,0:15:56.69,0:15:59.42,Default,,0000,0000,0000,,Nhưng mình làm gì có đồ bơi cho bé ha...
Dialogue: 0,0:15:59.97,0:16:02.19,Default,,0000,0000,0000,,Em không có đồ bơi nhưng mà...
Dialogue: 0,0:16:07.23,0:16:09.86,Default,,0000,0000,0000,,Ừm... Kobato-chan...
Dialogue: 0,0:16:10.19,0:16:11.78,Default,,0000,0000,0000,,Ta sẽ không bao giờ tha thứ cho ngươi...
Dialogue: 0,0:16:11.78,0:16:14.35,Default,,0000,0000,0000,,Ta sẽ không bao giờ tha thứ cho ngươi!
Dialogue: 0,0:16:14.35,0:16:16.24,Default,,0000,0000,0000,,Chị xin lỗi mà!
Dialogue: 0,0:16:16.59,0:16:18.84,Default,,0000,0000,0000,,Ai là nhà vua?!
Dialogue: 0,0:16:19.49,0:16:21.50,Default,,0000,0000,0000,,Ồ, là mình.
Dialogue: 0,0:16:21.50,0:16:23.54,TakaHand,,0000,0000,0000,,{\frx20\blur1\fnUvn Van\fs50\c&H272B3A&\pos(633.6,452)}Lệnh
Dialogue: 0,0:16:26.79,0:16:29.67,Default,,0000,0000,0000,,Đứa ngốc nào viết cái này đây?!
Dialogue: 0,0:16:29.67,0:16:31.20,Default,,0000,0000,0000,,Ai số 5?
Dialogue: 0,0:16:31.20,0:16:33.44,Default,,0000,0000,0000,,Đọc to mấy câu trong tờ giấy lên!
Dialogue: 0,0:16:33.44,0:16:35.86,Default,,0000,0000,0000,,A, là em nè!
Dialogue: 0,0:16:36.84,0:16:38.54,Default,,0000,0000,0000,,Để coi nào...
Dialogue: 0,0:16:38.54,0:16:41.60,Default,,0000,0000,0000,,D-Dừng lại! Sắp ra rồi!
Dialogue: 0,0:16:41.93,0:16:43.64,Default,,0000,0000,0000,,S-Sắp r-ra rồi!
Dialogue: 0,0:16:44.07,0:16:45.18,Default,,0000,0000,0000,,Phê quá đi à!
Dialogue: 0,0:16:45.18,0:16:47.15,Default,,0000,0000,0000,,Mình thấy nó sắp phọt ra rồi!
Dialogue: 0,0:16:48.45,0:16:50.73,Default,,0000,0000,0000,,Em bắt mọi người đọc cái khỉ gì vậy hả?!
Dialogue: 0,0:16:50.73,0:16:53.16,Default,,0000,0000,0000,,Ồ, biết luôn à?
Dialogue: 0,0:16:53.16,0:16:59.88,Default,,0000,0000,0000,,Em nghĩ sẽ rất vui nếu thấy người khác xấu hổ đọc ra mấy câu đó!
Dialogue: 0,0:16:59.88,0:17:01.16,Default,,0000,0000,0000,,Tiếp thôi.
Dialogue: 0,0:17:01.16,0:17:03.08,Default,,0000,0000,0000,,Mình lại làm vua rồi!
Dialogue: 0,0:17:01.16,0:17:03.08,TakaHand,,0000,0000,0000,,{\frx20\blur1\fnUvn Van\fs70\c&H272B3A&\pos(633.6,452)}Lệnh
Dialogue: 0,0:17:03.08,0:17:05.10,Default,,0000,0000,0000,,Mệnh lệnh là...
Dialogue: 0,0:17:13.17,0:17:14.78,Default,,0000,0000,0000,,Chào quí khách!
Dialogue: 0,0:17:16.13,0:17:18.95,Default,,0000,0000,0000,,Có khách hàng nào vác bộ mặt thế này đến đây chưa?
Dialogue: 0,0:17:18.95,0:17:19.82,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:17:19.82,0:17:22.76,Default,,0000,0000,0000,,Đồ ngu! Siêu trộm Lupin chứ ai!
Dialogue: 0,0:17:24.83,0:17:26.74,Default,,0000,0000,0000,,Cảm ơn quí khách!
Dialogue: 0,0:17:28.73,0:17:31.82,Default,,0000,0000,0000,,Ai viết cái yêu cầu quái quỉ này hả?!
Dialogue: 0,0:17:31.82,0:17:35.10,Default,,0000,0000,0000,,Và tại sao nhà vua lại phải làm theo yêu cầu bằng mọi giá chứ?!
Dialogue: 0,0:17:35.10,0:17:36.82,Default,,0000,0000,0000,,Lệnh của vua như đinh đóng cột.
Dialogue: 0,0:17:37.23,0:17:40.24,Default,,0000,0000,0000,,Cho dù là tự ra lệnh cho mình đi nữa.
Dialogue: 0,0:17:41.01,0:17:43.26,Default,,0000,0000,0000,,Vậy là bà viết chứ ai.
Dialogue: 0,0:17:44.25,0:17:47.96,Default,,0000,0000,0000,,Bả còn đánh dấu ở góc nên không sợ tự mình bốc trúng...
Dialogue: 0,0:17:47.96,0:17:50.75,Default,,0000,0000,0000,,Đúng là một con nhỏ ghê gớm...
Dialogue: 0,0:17:50.75,0:17:52.84,TakaHand,,0000,0000,0000,,{\frx20\blur1\fnUvn Van\fs70\c&H272B3A&\pos(633.6,452)}Lệnh
Dialogue: 0,0:17:52.84,0:17:56.93,Default,,0000,0000,0000,,Số 1 phải khoe ngực cho mọi người coi.
Dialogue: 0,0:17:56.93,0:17:57.84,Default,,0000,0000,0000,,Xấu ghê nha.
Dialogue: 0,0:17:57.84,0:18:00.80,Default,,0000,0000,0000,,Tuyệt vời, Aniki! Anh đúng là đàn ông trong số những tên đàn bà!
Dialogue: 0,0:18:00.80,0:18:03.12,Default,,0000,0000,0000,,Ê, anh viết nó hồi nào!
Dialogue: 0,0:18:03.12,0:18:05.02,Default,,0000,0000,0000,,À, tui viết đó.
Dialogue: 0,0:18:05.02,0:18:09.32,Default,,0000,0000,0000,,Vậy bà tính sao nếu yêu cầu đó trúng vào bà?
Dialogue: 0,0:18:09.93,0:18:11.86,Default,,0000,0000,0000,,Cái đó chả bao giờ xảy ra đâu!
Dialogue: 0,0:18:11.86,0:18:14.92,Default,,0000,0000,0000,,Sao bà chắc dữ vậy?
Dialogue: 0,0:18:14.92,0:18:18.58,Default,,0000,0000,0000,,Vậy ai phải khoe zú nào?
Dialogue: 0,0:18:23.28,0:18:25.29,Default,,0000,0000,0000,,Của em to hơn luôn! {... wasn't there a pool ep last season...?}

Dialogue: 0,0:06:24.79,0:06:27.81,Default,Kyousuke,0000,0000,0000,,Là cái em mua ở Akiba đây mà
Dialogue: 0,0:06:27.81,0:06:28.89,Default,Kirino,0000,0000,0000,,Phải!
Dialogue: 0,0:06:32.99,0:06:34.78,Default,Kirino,0000,0000,0000,,Đây, nhìn này!
Dialogue: 0,0:06:35.47,0:06:38.27,Default,Ayaka,0000,0000,0000,,Cuối cùng cậu cũng tới rồi.
Dialogue: 0,0:06:38.27,0:06:40.23,Default,Ayaka,0000,0000,0000,,Tớ đang đợi cậu đấy.
Dialogue: 0,0:06:40.23,0:06:41.78,Default,Ayaka,0000,0000,0000,,Công việc xong rồi à?
Dialogue: 0,0:06:41.78,0:06:43.69,Default,Kirino,0000,0000,0000,,Ừm, xong rồi.
Dialogue: 0,0:06:43.69,0:06:46.61,Default,,0000,0000,0000,,Xin lỗi để cậu đợi nhé Ayaka.
Dialogue: 0,0:06:46.61,0:06:48.12,Default,,0000,0000,0000,,Mình yêu cậu lắm đó!
Dialogue: 0,0:06:49.05,0:06:50.75,Default,Ayaka,0000,0000,0000,,Ừm, mình vui lắm.
Dialogue: 0,0:06:51.59,0:06:54.19,Default,,0000,0000,0000,,{\an8}Mình cũng yêu cậu lắm.
Dialogue: 0,0:06:51.93,0:06:55.26,Default,Kyousuke,0000,0000,0000,,{\i1}Con bé thật sự đang nói chuyện với cái game sao?
Dialogue: 0,0:06:55.93,0:06:58.23,Default,Kirino,0000,0000,0000,,Cái tuyệt vời của game này đấy.
Dialogue: 0,0:06:58.23,0:07:03.91,Default,,0000,0000,0000,,Nó cho mình cảm giác như hẹn hò thật sự với các cô gái trong game vậy.
Dialogue: 0,0:07:03.91,0:07:07.59,Default,Kirino,0000,0000,0000,,Nó thu lại giọng nói của ta nên có thể nói chuyện được luôn.
Dialogue: 0,0:07:07.59,0:07:10.25,Default,,0000,0000,0000,,Được đồng bộ thời gian ở trong game với thế giới thật nữa
Dialogue: 0,0:07:10.25,0:07:12.91,Default,,0000,0000,0000,,và cũng có thể hẹn ngày hẹn hò với nhân vật nữa.
Dialogue: 0,0:07:12.91,0:07:14.41,Default,Kirino,0000,0000,0000,,Nếu tới ngày hẹn
Dialogue: 0,0:07:14.41,0:07:19.03,Default,,0000,0000,0000,,Không bật game lên canh chừng event là bị người ta giận đó.
Dialogue: 0,0:07:19.53,0:07:22.17,Default,Kyousuke,0000,0000,0000,,Game rắc rối nhỉ.

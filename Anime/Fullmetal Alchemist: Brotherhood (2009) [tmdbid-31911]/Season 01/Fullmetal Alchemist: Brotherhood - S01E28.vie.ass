Dialogue: 0,0:00:16.53,0:00:19.08,Default,Al,0000,0000,0000,,Sao mình cứ cảm giác chỗ này rợn rợn.
Dialogue: 0,0:00:19.16,0:00:23.83,Default,Glut,0000,0000,0000,,Phụ thân ơi! Con đưa Nhân Trụ tới đây!
Dialogue: 0,0:00:23.83,0:00:26.42,Default,Al,0000,0000,0000,,Phụ thân ư? Tôi vẫn chưa sẵn sàng...
Dialogue: 0,0:00:30.96,0:00:31.92,Default,Father,0000,0000,0000,,Ai đấy?
Dialogue: 0,0:00:37.64,0:00:38.93,Default,Al,0000,0000,0000,,Bố?
Dialogue: 0,0:00:39.43,0:00:42.43,Title - TS,Nar,0000,0000,0000,,{\pos(332,168)}Tập 28
Dialogue: 0,0:00:39.43,0:00:42.43,Title - TS,Nar,0000,0000,0000,,{\fs80\pos(640,552)}Phụ Thân
Dialogue: 0,0:00:39.43,0:00:42.43,Credit,Nar,0000,0000,0000,,{}Subbed by BLACK - ATeam
Dialogue: 0,0:01:14.67,0:01:15.84,Default,Father,0000,0000,0000,,Envy.
Dialogue: 0,0:01:16.55,0:01:17.30,Default,Al,0000,0000,0000,,Envy!?
Dialogue: 0,0:01:17.93,0:01:19.01,Default,Al,0000,0000,0000,,Đây là hắn sao!?
Dialogue: 0,0:01:21.93,0:01:22.77,Default,Al,0000,0000,0000,,Anh hai!
Dialogue: 0,0:01:26.14,0:01:27.31,Default,Al,0000,0000,0000,,Anh ơi!
Dialogue: 0,0:01:29.52,0:01:30.40,Default,Ed,0000,0000,0000,,Al?
Dialogue: 0,0:01:31.15,0:01:34.07,Default,Ed,0000,0000,0000,,Al ở trong bộ giáp... vậy tức là...
Dialogue: 0,0:01:37.15,0:01:39.07,Default,Ling,0000,0000,0000,,Trở lại thế giới cũ rồi.
Dialogue: 0,0:01:39.49,0:01:40.41,Default,Ed,0000,0000,0000,,Ừ.
Dialogue: 0,0:01:40.41,0:01:41.49,Default,Al,0000,0000,0000,,Anh hai!
Dialogue: 0,0:01:41.49,0:01:42.66,Default,Ed,0000,0000,0000,,Au au au au!
Dialogue: 0,0:01:42.66,0:01:43.33,Default,Ed,0000,0000,0000,,Ối ối ối ối
Dialogue: 0,0:01:43.33,0:01:46.29,Default,Al,0000,0000,0000,,May quá, anh vẫn không sao cả!
Dialogue: 0,0:01:43.49,0:01:44.41,Default-alt,Ed,0000,0000,0000,,Đau quá Al ơi!
Dialogue: 0,0:01:44.41,0:01:46.79,Default-alt,Ed,0000,0000,0000,,Gai trên áo giáp đâm anh đau quá!
Dialogue: 0,0:01:49.46,0:01:54.09,Default,Al,0000,0000,0000,,Em... thấy anh còn sống... nên mừng quá...
Dialogue: 0,0:01:56.76,0:01:58.63,Default,Ed,0000,0000,0000,,Thế à.
Dialogue: 0,0:01:58.63,0:01:59.84,Default,Ed,0000,0000,0000,,Xin lỗi em.
Dialogue: 0,0:02:02.01,0:02:03.35,Default,Ed,0000,0000,0000,,Để em phải lo rồi.
Dialogue: 0,0:02:08.35,0:02:11.11,Default,Ed,0000,0000,0000,,Nhưng đây là chỗ nào vậy?
Dialogue: 0,0:02:11.11,0:02:13.11,Default,Father,0000,0000,0000,,Ngạc nhiên thật.
Dialogue: 0,0:02:13.11,0:02:15.44,Default,Father,0000,0000,0000,,Con người có thể chui ra từ bụng nó ư?
Dialogue: 0,0:02:17.99,0:02:20.03,Default,Ed,0000,0000,0000,,Hohen... heim!?
Dialogue: 0,0:02:20.03,0:02:23.62,Default,Father,0000,0000,0000,,Tay chân làm bằng thép, và một bộ giáp.
Dialogue: 0,0:02:27.91,0:02:29.67,Default,Father,0000,0000,0000,,Các ngươi là Anh Em Elric hả?
Dialogue: 0,0:02:30.67,0:02:32.75,Default,Ed,0000,0000,0000,,Không phải lão ta à?
Dialogue: 0,0:02:32.75,0:02:35.34,Default,Father,0000,0000,0000,,Các ngươi nhầm ta với ai hả?
Dialogue: 0,0:02:35.34,0:02:37.05,Default,Father,0000,0000,0000,,Khoan đã...
Dialogue: 0,0:02:37.05,0:02:39.05,Default,Father,0000,0000,0000,,Hohen...
Dialogue: 0,0:02:39.51,0:02:43.55,Default,Father,0000,0000,0000,,Không lẽ các ngươi đang nói tới Van Hohenheim?
Dialogue: 0,0:02:43.55,0:02:45.81,Default,Father,0000,0000,0000,,Các ngươi có quan hệ gì với hắn?
Dialogue: 0,0:02:46.89,0:02:49.06,Default,Al,0000,0000,0000,,Ông ấy... là bố tụi tôi.
Dialogue: 0,0:02:51.85,0:02:53.31,Default,Father,0000,0000,0000,,Bố ư!?
Dialogue: 0,0:02:53.31,0:02:57.78,Default,Father,0000,0000,0000,,Ngạc nhiên quá đi! Hắn ta cũng có con cơ à!?
Dialogue: 0,0:03:01.61,0:03:06.62,Default,Father,0000,0000,0000,,Nhưng không phải họ các ngươi là Elric sao?
Dialogue: 0,0:03:06.62,0:03:08.62,Default,Ed,0000,0000,0000,,Elric là họ của mẹ tôi!
Dialogue: 0,0:03:09.33,0:03:10.75,Default,Father,0000,0000,0000,,Ra là vậy.
Dialogue: 0,0:03:11.17,0:03:13.33,Default,Father,0000,0000,0000,,Thế giờ hắn đâu rồi?
Dialogue: 0,0:03:13.33,0:03:14.75,Default,Ed,0000,0000,0000,,Ai mà biết!
Dialogue: 0,0:03:14.75,0:03:17.13,Default,Ed,0000,0000,0000,,Còn lão là ai!?
Dialogue: 0,0:03:17.13,0:03:19.05,Default,Ed,0000,0000,0000,,Tại sao lão lại giống y hệt Hohenheim vậy!?
Dialogue: 0,0:03:19.05,0:03:21.26,Default,Father,0000,0000,0000,,Đúng là hắn vẫn chưa chết thật.
Dialogue: 0,0:03:20.34,0:03:21.68,Default-alt,Ed,0000,0000,0000,,Đừng có bơ tôi!
Dialogue: 0,0:03:21.26,0:03:23.72,Default,Father,0000,0000,0000,,Đã thế lại còn có con nữa...
Dialogue: 0,0:03:21.68,0:03:23.26,Default-alt,Al,0000,0000,0000,,Anh hai, người này là...
Dialogue: 0,0:03:24.30,0:03:27.47,Default,Father,0000,0000,0000,,Ồ? Ngươi bị thương hả?
Dialogue: 0,0:03:27.47,0:03:29.64,Default,Father,0000,0000,0000,,Còn ngươi bị mất bàn tay trái.
Dialogue: 0,0:03:34.56,0:03:36.40,Default,Father,0000,0000,0000,,Thế đã được chưa?
Dialogue: 0,0:03:37.32,0:03:37.48,Default,Ed,0000,0000,0000,,Ui!
Dialogue: 0,0:03:37.48,0:03:39.32,Default,Father,0000,0000,0000,,Bị gãy xương à?
Dialogue: 0,0:03:47.08,0:03:49.20,Default,Ed,0000,0000,0000,,Lành... lại rồi.
Dialogue: 0,0:03:51.50,0:03:52.46,Default,Ed,0000,0000,0000,,Này!
Dialogue: 0,0:03:54.04,0:03:54.79,Default,Ed,0000,0000,0000,,Úi da!
Dialogue: 0,0:03:54.79,0:03:56.67,Default,Father,0000,0000,0000,,Giờ là xương sườn.
Dialogue: 0,0:04:01.51,0:04:05.09,Default,Father,0000,0000,0000,,Hai ngươi là nhân tài rất quan trọng với chúng ta.
Dialogue: 0,0:04:05.09,0:04:07.14,Default,Father,0000,0000,0000,,Nhớ phải giữ gìn cơ thể cho cẩn thận đấy.
Dialogue: 0,0:04:07.89,0:04:11.85,Think,Ed,0000,0000,0000,,Lão này... sử dụng giả kim thuật mà không có tư thế gì đặc biệt.
Dialogue: 0,0:04:11.85,0:04:14.10,Think,Al,0000,0000,0000,,Độ dày của áo giáp cũng không bị thay đổi...
Dialogue: 0,0:04:14.56,0:04:16.98,Default,Both,0000,0000,0000,,Vậy có nghĩa... đó không phải là "trao đổi đồng giá".
Dialogue: 0,0:04:16.98,0:04:18.98,Default,Ling,0000,0000,0000,,Ngươi là cái quái gì thế?
Dialogue: 0,0:04:18.98,0:04:19.90,Default,Ling,0000,0000,0000,,Không thể nào!
Dialogue: 0,0:04:19.90,0:04:21.99,Default,Ling,0000,0000,0000,,Tại sao bên trong cơ thể ngươi lại có thể như thế!?
Dialogue: 0,0:04:23.74,0:04:25.61,Default,Father,0000,0000,0000,,Ta cũng đang muốn hỏi ngươi.
Dialogue: 0,0:04:25.61,0:04:26.99,Default,Father,0000,0000,0000,,Ngươi là cái thứ gì thế?
Dialogue: 0,0:04:29.99,0:04:31.70,Default,Father,0000,0000,0000,,Ăn hắn đi cũng được.
Dialogue: 0,0:04:31.70,0:04:32.79,Default,Glut,0000,0000,0000,,Vâng!
Dialogue: 0,0:04:32.79,0:04:33.91,Default,Ed,0000,0000,0000,,Khoan đã!
Dialogue: 0,0:04:33.91,0:04:36.08,Default,Ed,0000,0000,0000,,Cậu ta là bạn của tôi mà!
Dialogue: 0,0:04:36.08,0:04:39.38,Default,Ed,0000,0000,0000,,Tôi là Nhân Trụ, vậy hãy nể tôi mà tha cho cậu ấy, nhé?
Dialogue: 0,0:04:39.38,0:04:41.46,Default,Father,0000,0000,0000,,Ta không cần biết.
Dialogue: 0,0:04:41.46,0:04:44.59,Default,Father,0000,0000,0000,,Với ta, hắn hoàn toàn không cần thiết.
Dialogue: 0,0:04:44.59,0:04:46.01,Default,Ed,0000,0000,0000,,Lão nói gì!?
Dialogue: 0,0:04:46.01,0:04:50.35,Default,Al,0000,0000,0000,,Anh hai, các Homunculus gọi lão ta là "phụ thân" đấy.
Dialogue: 0,0:04:50.35,0:04:52.60,Default,Al,0000,0000,0000,,Chính lão ấy đã tạo ra chúng.
Dialogue: 0,0:04:52.60,0:04:53.56,Default,Ed,0000,0000,0000,,Cái gì!?
Dialogue: 0,0:04:54.10,0:04:56.02,Default,Al,0000,0000,0000,,Dù lão ta đã chữa trị cho mình...
Dialogue: 0,0:04:56.02,0:04:58.11,Default,Ling,0000,0000,0000,,Không thể ưa nổi.
Dialogue: 0,0:04:59.06,0:05:00.69,Default,Ling,0000,0000,0000,,Cái thái độ của ngươi...
Dialogue: 0,0:05:01.32,0:05:04.24,Default,Ling,0000,0000,0000,,là của những kẻ luôn cười nhạo loài người ngu ngốc.
Dialogue: 0,0:05:05.28,0:05:09.28,Default,Father,0000,0000,0000,,Ngươi có bao giờ nghĩ rằng sâu bọ dưới đất là đồ ngu ngốc không?
Dialogue: 0,0:05:10.78,0:05:12.07,Default,Father,0000,0000,0000,,Dù chúng có phản kháng,
Dialogue: 0,0:05:12.07,0:05:14.72,Default,Father,0000,0000,0000,,chênh lệch sức mạnh giữa ngươi và chúng vẫn quá lớn,
Dialogue: 0,0:05:14.72,0:05:16.46,Default,Father,0000,0000,0000,,nên ngươi cũng chẳng thèm bận tâm.
Dialogue: 0,0:05:16.92,0:05:21.55,Default,Father,0000,0000,0000,,Với ta, loài người cũng như vậy đấy.
Dialogue: 0,0:05:25.34,0:05:27.54,Default,Ed,0000,0000,0000,,Tuy được lão chữa lành vết thương,
Dialogue: 0,0:05:27.54,0:05:29.43,Default,Ed,0000,0000,0000,,bọn ta cũng không ưa được lão!
Dialogue: 0,0:05:29.43,0:05:31.97,Default,Ed,0000,0000,0000,,Xem ra chính lão là nguồn cơn của tất cả mọi chuyện!
Dialogue: 0,0:05:31.97,0:05:34.22,Default,Ed,0000,0000,0000,,Ta sẽ hạ lão nhanh thôi.
Dialogue: 0,0:05:34.22,0:05:36.35,Default,Ed,0000,0000,0000,,Ta không có thì giờ chơi với lũ ruồi muỗi!
Dialogue: 0,0:05:36.35,0:05:39.19,Default,Envy,0000,0000,0000,,Mày vừa gọi tao là ruồi muỗi đấy hả, thằng lùn?
Dialogue: 0,0:05:39.94,0:05:41.77,Default,Ed,0000,0000,0000,,"Thằng lùn"!?
Dialogue: 0,0:05:47.49,0:05:49.03,Default,Ed,0000,0000,0000,,Đánh rắn phải đánh dập đầu!
Dialogue: 0,0:06:01.79,0:06:03.00,Default,Al,0000,0000,0000,,Ling!
Dialogue: 0,0:06:04.63,0:06:05.80,Default,Al,0000,0000,0000,,Cái gì thế này!?
Dialogue: 0,0:06:07.59,0:06:11.76,Default,Ed,0000,0000,0000,,Tại sao lão ta đứng yên mà vẫn phát động được giả kim thuật!?
Dialogue: 0,0:06:14.56,0:06:16.35,Default,Father,0000,0000,0000,,Lãng phí thời giờ quá.
Dialogue: 0,0:06:48.72,0:06:52.01,Think,Both,0000,0000,0000,,Giả kim thuật... bị vô hiệu hóa!?
Dialogue: 0,0:06:52.59,0:06:54.47,Default,Ling,0000,0000,0000,,Ed! Al!
Dialogue: 0,0:06:59.56,0:07:01.52,Default,Ed,0000,0000,0000,,Lão già râu ấy vừa làm gì vậy!?
Dialogue: 0,0:07:01.52,0:07:02.40,Default,Al,0000,0000,0000,,Tại sao...
Dialogue: 0,0:07:02.40,0:07:04.94,Default,Al,0000,0000,0000,,Tại sao mình không thể dùng giả kim thuật!?
Dialogue: 0,0:07:07.11,0:07:10.45,Default,Envy,0000,0000,0000,,Bọn này đúng là buồn cười thật.
Dialogue: 0,0:07:10.45,0:07:13.99,Default,Envy,0000,0000,0000,,Vừa có được chút sức mạnh đã tưởng mình là nhất rồi.
Dialogue: 0,0:07:13.99,0:07:16.20,Default,Envy,0000,0000,0000,,Chúng nó còn chẳng biết mình đang sử dụng cái gì nữa.
Dialogue: 0,0:07:17.49,0:07:22.79,Default,Envy,0000,0000,0000,,Các ngươi tưởng sự phồn vinh hiện tại là do các ngươi tự tay tạo ra sao?
Dialogue: 0,0:07:22.79,0:07:26.00,Default,Envy,0000,0000,0000,,Ta thấy buồn cười quá, bọn đần ạ.
Dialogue: 0,0:07:27.92,0:07:29.63,Default,Ed,0000,0000,0000,,Ngươi đang nói cái gì thế!?
Dialogue: 0,0:07:29.63,0:07:32.59,Default,Ed,0000,0000,0000,,Ngươi đã hứa sẽ nói cho ta biết khi thoát ra cơ mà!
Dialogue: 0,0:07:32.59,0:07:33.76,Default,Envy,0000,0000,0000,,Hả?
Dialogue: 0,0:07:33.76,0:07:37.93,Default,Envy,0000,0000,0000,,Ta không nhớ là có hứa gì với đồ sâu bọ hết.
Dialogue: 0,0:07:37.93,0:07:39.06,Default,Ed,0000,0000,0000,,Tên khốn...
Dialogue: 0,0:07:39.06,0:07:41.60,Default,Father,0000,0000,0000,,Con nói hơi nhiều rồi đấy, Envy.
Dialogue: 0,0:07:41.60,0:07:42.77,Default,Envy,0000,0000,0000,,Dạ dạ...
Dialogue: 0,0:07:44.81,0:07:47.69,Default,Father,0000,0000,0000,,Có khi tên này cũng được việc đấy.
Dialogue: 0,0:07:49.44,0:07:52.61,Default,Father,0000,0000,0000,,Biết đâu ta sẽ lại tạo ra được một thủ hạ đắc lực.
Dialogue: 0,0:07:56.83,0:07:59.08,Default,Al,0000,0000,0000,,Đó là... Hòn Đá Triết Gia!
Dialogue: 0,0:07:59.08,0:08:00.20,Default,Ling,0000,0000,0000,,Sao cơ!?
Dialogue: 0,0:08:00.62,0:08:02.46,Default,Envy,0000,0000,0000,,Vậy là phụ thân lại làm "chuyện đó" ạ?
Dialogue: 0,0:08:02.46,0:08:03.46,Default,Ed,0000,0000,0000,,Chuyện gì?
Dialogue: 0,0:08:03.46,0:08:05.83,Default,Envy,0000,0000,0000,,Tạo ra Homunculus.
Dialogue: 0,0:08:07.21,0:08:10.17,Default,Envy,0000,0000,0000,,Phụ thân sẽ đưa Hòn Đá Triết Gia vào người hắn.
Dialogue: 0,0:08:10.17,0:08:11.42,Default,Envy,0000,0000,0000,,Nếu thuận lợi,
Dialogue: 0,0:08:11.42,0:08:14.01,Default,Envy,0000,0000,0000,,một Homunculus có nền tảng là con người sẽ được sinh ra.
Dialogue: 0,0:08:18.14,0:08:21.26,Default,Envy,0000,0000,0000,,Nhưng bình thường, đa phần con người đều bỏ mạng
Dialogue: 0,0:08:21.26,0:08:22.64,Default,Envy,0000,0000,0000,,vì không chịu nổi sức mạnh của Hòn Đá.
Dialogue: 0,0:08:22.64,0:08:24.56,Default,Ed,0000,0000,0000,,Ta không để ngươi làm thế đâu!
Dialogue: 0,0:08:26.31,0:08:28.15,Default,Al,0000,0000,0000,,Thả ta ra, Envy!
Dialogue: 0,0:08:28.15,0:08:30.57,Default,Ed,0000,0000,0000,,Khốn kiếp, chuyện quái gì thế này!?
Dialogue: 0,0:08:30.57,0:08:32.53,Default,Ed,0000,0000,0000,,Tại sao ta không thể dùng giả kim thuật!?
Dialogue: 0,0:08:34.15,0:08:35.41,Default,Ed,0000,0000,0000,,Này, lão kia!
Dialogue: 0,0:08:35.41,0:08:36.78,Default,Ed,0000,0000,0000,,Lão già râu, dừng tay!
Dialogue: 0,0:08:36.82,0:08:39.62,Default,Ed,0000,0000,0000,,Vẫn có người đang đợi cậu ta quay về đấy!
Dialogue: 0,0:08:39.62,0:08:41.24,Default,Ed,0000,0000,0000,,Thả ta ra!!!
Dialogue: 0,0:08:46.50,0:08:48.79,Default,Envy,0000,0000,0000,,Chỉ cần làm thế này là ngươi hết dám bắn...
Dialogue: 0,0:08:48.79,0:08:50.63,Default,Envy,0000,0000,0000,,Nhỉ?
Dialogue: 0,0:08:51.21,0:08:53.09,Default,Ed,0000,0000,0000,,Tên khốn...
Dialogue: 0,0:08:53.09,0:08:54.51,Default,Ling,0000,0000,0000,,Không sao, đừng có bắn!
Dialogue: 0,0:08:55.09,0:08:57.47,Default,Ling,0000,0000,0000,,Đừng có xía vào chuyện của tôi.
Dialogue: 0,0:08:57.47,0:08:58.64,Default,Ling,0000,0000,0000,,Tôi chấp nhận chuyện này!
Dialogue: 0,0:08:58.64,0:08:59.89,Default,Ed,0000,0000,0000,,Cái gì?
Dialogue: 0,0:08:59.89,0:09:02.89,Default,Ling,0000,0000,0000,,Tôi tới nước này là để tìm kiếm Hòn Đá Triết Gia.
Dialogue: 0,0:09:02.89,0:09:05.35,Default,Ling,0000,0000,0000,,Thế mà có người sắp biếu không tôi một viên như thế.
Dialogue: 0,0:09:05.35,0:09:06.94,Default,Ling,0000,0000,0000,,Đúng là cầu được ước thấy còn gì!
Dialogue: 0,0:09:06.94,0:09:08.27,Default,Ed,0000,0000,0000,,Cậu nói cái quái gì thế!?
Dialogue: 0,0:09:08.27,0:09:10.77,Default,Father,0000,0000,0000,,Ồ, thú vị đây.
Dialogue: 0,0:09:10.77,0:09:13.53,Default,Father,0000,0000,0000,,Vậy thì nó là của ngươi đấy.
Dialogue: 0,0:09:15.45,0:09:16.32,Default,Ed,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:09:25.58,0:09:26.37,Default,Ed,0000,0000,0000,,Ling!
Dialogue: 0,0:09:27.00,0:09:30.17,Default,Ling,0000,0000,0000,,Tôi... đã bảo là...
Dialogue: 0,0:09:30.17,0:09:32.00,Default,Ling,0000,0000,0000,,đừng có xía vào mà!
Dialogue: 0,0:09:35.97,0:09:40.68,Default,Ling,0000,0000,0000,,Không sao... cứ chờ đấy...
Dialogue: 0,0:09:42.51,0:09:45.27,Default,Ling,0000,0000,0000,,Tôi là kẻ sẽ trở thành hoàng đế nước Xing...
Dialogue: 0,0:09:45.27,0:09:46.48,Default,Ling,0000,0000,0000,,Ling Yao!
Dialogue: 0,0:10:06.29,0:10:09.37,Default,Greed,0000,0000,0000,,Tại sao lại có thằng nhóc ở đây thế này!?
Dialogue: 0,0:10:10.25,0:10:11.33,Default,Greed,0000,0000,0000,,Thôi kệ.
Dialogue: 0,0:10:11.33,0:10:13.34,Default,Greed,0000,0000,0000,,Cho ta cơ thể của ngươi đi.
Dialogue: 0,0:10:13.34,0:10:15.05,Default,Greed,0000,0000,0000,,Ta sẽ sử dụng nó thật tốt.
Dialogue: 0,0:10:15.71,0:10:17.34,Default,Ling,0000,0000,0000,,Được thôi...
Dialogue: 0,0:10:17.34,0:10:19.55,Default,Ling,0000,0000,0000,,Cứ chiếm lấy cơ thể này đi!
Dialogue: 0,0:10:19.55,0:10:20.59,Default,Greed,0000,0000,0000,,Hở?
Dialogue: 0,0:10:21.01,0:10:23.18,Default,Ling,0000,0000,0000,,Ta bảo là ta chấp nhận ngươi!
Dialogue: 0,0:10:23.18,0:10:25.18,Default,Greed,0000,0000,0000,,Bình thường các ngươi phải phản đối chứ nhỉ?
Dialogue: 0,0:10:25.18,0:10:27.73,Default,Ling,0000,0000,0000,,Ta sẽ trở thành hoàng đế của nước Xing.
Dialogue: 0,0:10:27.73,0:10:31.80,Default,Ling,0000,0000,0000,,Nếu không dám chứa chấp hai ba chục kẻ khác trong người mình
Dialogue: 0,0:10:31.80,0:10:32.77,Default,Ling,0000,0000,0000,,thì làm vua thế nào được!?
Dialogue: 0,0:10:34.94,0:10:37.44,Default,Greed,0000,0000,0000,,Ta khoái mấy tên cứng cỏi như ngươi đấy.
Dialogue: 0,0:10:37.44,0:10:39.45,Default,Greed,0000,0000,0000,,Nhưng mà đừng có hối hận đó.
Dialogue: 0,0:10:39.45,0:10:41.16,Default,Ling,0000,0000,0000,,Hối hận ấy à?
Dialogue: 0,0:10:41.16,0:10:42.62,Default,Ling,0000,0000,0000,,Nếu tay không trở về,
Dialogue: 0,0:10:42.62,0:10:47.16,Default,Ling,0000,0000,0000,,ta sao dám nhìn mặt người thuộc hạ đã tự chặt tay mình để bảo vệ ta chứ?
Dialogue: 0,0:10:49.54,0:10:51.08,Default,Ling,0000,0000,0000,,Ta muốn...
Dialogue: 0,0:10:51.08,0:10:56.21,Default,Ling,0000,0000,0000,,Ta muốn có sức mạnh tuyệt đối để bảo vệ tất cả và giành lấy tất cả!
Dialogue: 0,0:10:56.21,0:10:58.05,Default,Ling,0000,0000,0000,,Từ đầu ta đã biết thế này là liều lĩnh rồi!
Dialogue: 0,0:10:59.72,0:11:02.09,Default,Greed,0000,0000,0000,,Ta khoái cái sự "tham lam" của ngươi rồi đó.
Dialogue: 0,0:11:02.09,0:11:06.85,Default,Greed,0000,0000,0000,,Vậy thì hãy cho ta xem ngươi đã chuẩn bị sẵn sàng đến đâu!
Dialogue: 0,0:11:17.28,0:11:18.19,Default,Ed,0000,0000,0000,,Ling?
Dialogue: 0,0:11:18.94,0:11:23.37,Default,Greed,0000,0000,0000,,Hả? À, ngươi đang nói chủ cũ của cơ thể này hả?
Dialogue: 0,0:11:23.37,0:11:27.95,Default,Greed,0000,0000,0000,,Xin lỗi, nhưng giờ nó thuộc về Greed đại nhân ta đây.
Dialogue: 0,0:11:29.04,0:11:31.08,Default,Al,0000,0000,0000,,Không phải Ling, mà là...
Dialogue: 0,0:11:31.08,0:11:32.96,Default,Ed,0000,0000,0000,,Greed!?
Dialogue: 0,0:11:40.72,0:11:44.47,Default,Ed,0000,0000,0000,,Ngươi là tên Greed đó ư?
Dialogue: 0,0:11:45.68,0:11:48.10,Default,Greed,0000,0000,0000,,"Greed đó" là Greed nào?
Dialogue: 0,0:11:48.10,0:11:49.64,Default,Ed,0000,0000,0000,,Ngươi không nhớ à!?
Dialogue: 0,0:11:49.64,0:11:51.14,Default,Al,0000,0000,0000,,Hồi ở Dublith...
Dialogue: 0,0:11:51.14,0:11:53.35,Default,Greed,0000,0000,0000,,Ta hỏi là các ngươi đang nói tới Greed nào?
Dialogue: 0,0:11:53.35,0:11:56.11,Default,Father,0000,0000,0000,,Là Greed sinh ra trước con.
Dialogue: 0,0:11:56.11,0:11:58.48,Default,Greed,0000,0000,0000,,À, ra là thế.
Dialogue: 0,0:11:58.48,0:12:00.03,Default,Greed,0000,0000,0000,,Tình hình là,
Dialogue: 0,0:12:00.03,0:12:03.41,Default,Greed,0000,0000,0000,,ta hoàn toàn khác với tên Greed mà các ngươi biết.
Dialogue: 0,0:12:03.86,0:12:05.12,Default,Ed,0000,0000,0000,,Ling sao rồi?
Dialogue: 0,0:12:06.32,0:12:08.54,Default,Greed,0000,0000,0000,,Thằng nhóc đó thú vị lắm.
Dialogue: 0,0:12:08.54,0:12:11.16,Default,Greed,0000,0000,0000,,Vừa gặp nhau cái là hắn chấp nhận ta luôn.
Dialogue: 0,0:12:12.46,0:12:13.61,Default,Ed,0000,0000,0000,,Không thể nào!
Dialogue: 0,0:12:13.61,0:12:16.17,Default,Ed,0000,0000,0000,,Cậu ấy không thể dễ bị kiểm soát như thế!
Dialogue: 0,0:12:16.17,0:12:18.34,Default,Ed,0000,0000,0000,,Trả lời tôi đi! Ling!
Dialogue: 0,0:12:18.34,0:12:19.75,Default,Ed,0000,0000,0000,,Ling!!!
Dialogue: 0,0:12:31.93,0:12:33.23,Default,Scar,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:12:33.23,0:12:34.39,Default,May,0000,0000,0000,,Không...
Dialogue: 0,0:12:34.94,0:12:36.65,Default,May,0000,0000,0000,,Người đó... sợ quá...
Dialogue: 0,0:12:36.65,0:12:39.77,Default,May,0000,0000,0000,,Ông ta là người, nhưng cũng không phải là người!
Dialogue: 0,0:12:41.23,0:12:45.24,Default,Scar,0000,0000,0000,,Nhóc nói đúng, ở đây toàn lũ không phải là người thôi.
Dialogue: 0,0:12:47.57,0:12:48.78,Default,May,0000,0000,0000,,Shao May!
Dialogue: 0,0:12:48.78,0:12:50.79,Default,May,0000,0000,0000,,May mà em không sao cả!
Dialogue: 0,0:12:50.79,0:12:52.66,Default,May,0000,0000,0000,,Em có biết chị lo như thế nào không?
Dialogue: 0,0:12:53.29,0:12:56.79,Default,Greed,0000,0000,0000,,Ồ, cái này người ta gọi là "cuộc hội ngộ vô cùng xúc động" nhỉ?
Dialogue: 0,0:13:01.25,0:13:04.13,Think,Scar,0000,0000,0000,,Xem ra chúng không cùng phe với bọn Homunculus.
Dialogue: 0,0:13:04.67,0:13:06.72,Default,Scar,0000,0000,0000,,Cang Giả Kim Thuật Sư.
Dialogue: 0,0:13:06.72,0:13:08.09,Default,May,0000,0000,0000,,Hả? Đâu đâu?
Dialogue: 0,0:13:08.09,0:13:10.35,Default,May,0000,0000,0000,,Công tử Edward đâu ạ?
Dialogue: 0,0:13:10.35,0:13:11.31,Default,Scar,0000,0000,0000,,Kia kìa.
Dialogue: 0,0:13:13.02,0:13:15.23,Default,May,0000,0000,0000,,Cháu có thấy gì đâu!
Dialogue: 0,0:13:15.23,0:13:16.85,Default,Scar,0000,0000,0000,,Ngay hướng tay ta chỉ đấy.
Dialogue: 0,0:13:17.35,0:13:20.36,Default,Scar,0000,0000,0000,,Tên lùn đó là Cang Giả Kim Thuật Sư.
Dialogue: 0,0:13:38.79,0:13:42.34,Default,May,0000,0000,0000,,Ngươi dám đùa giỡn trái tim thiếu nữ trong trắng của ta, đồ lùn!
Dialogue: 0,0:13:42.34,0:13:44.17,Default,Ed,0000,0000,0000,,Ai đùa với mày hả con nhỏ lùn!
Dialogue: 0,0:13:44.17,0:13:47.34,Default-alt,Al,0000,0000,0000,,Anh hai, anh làm gì cô bé đó thế hả!?
Dialogue: 0,0:13:44.59,0:13:47.93,Default,Glut,0000,0000,0000,,Hắn là tên Ishval mà con không ăn được đấy.
Dialogue: 0,0:13:47.34,0:13:48.97,Default-alt,Ed,0000,0000,0000,,Anh có làm gì đâu!
Dialogue: 0,0:13:48.18,0:13:50.80,Default,Father,0000,0000,0000,,Lũ người vô giáo dục...
Dialogue: 0,0:13:50.80,0:13:53.36,Default,Father,0000,0000,0000,,Hắn chính là kẻ sử dụng giả kim thuật phá hoại
Dialogue: 0,0:13:53.36,0:13:56.39,Default,Father,0000,0000,0000,,và cản trở kế hoạch của ta đúng không?
Dialogue: 0,0:13:56.39,0:13:58.90,Default,Envy,0000,0000,0000,,Xử lý chúng nó đi, Gluttony.
Dialogue: 0,0:13:58.90,0:14:01.61,Default,Envy,0000,0000,0000,,Lúc này hắn không thể dùng giả kim thuật đâu.
Dialogue: 0,0:14:01.61,0:14:03.61,Default,Glut,0000,0000,0000,,Vâng!
Dialogue: 0,0:14:08.86,0:14:09.61,Default,Envy,0000,0000,0000,,Cái gì!?
Dialogue: 0,0:14:09.61,0:14:11.20,Default,May,0000,0000,0000,,Không thể tha thứ được!
Dialogue: 0,0:14:11.20,0:14:15.41,Default,May,0000,0000,0000,,Đã chà đạp lên trái tim thiếu nữ, lại còn bắt cóc cả Shao May!
Dialogue: 0,0:14:16.83,0:14:18.83,Default,May,0000,0000,0000,,Tất cả các ngươi sẽ bị...
Dialogue: 0,0:14:18.83,0:14:20.33,Default,May,0000,0000,0000,,trời phạt!!!
Dialogue: 0,0:14:24.21,0:14:25.71,Default,Ed,0000,0000,0000,,Hên quá!
Dialogue: 0,0:14:29.59,0:14:31.76,Default,Al,0000,0000,0000,,Vẫn không thể phát động giả kim thuật!
Dialogue: 0,0:14:31.76,0:14:33.10,Default,Ed,0000,0000,0000,,Tại sao lại thế!?
Dialogue: 0,0:14:33.93,0:14:36.10,Default,Envy,0000,0000,0000,,Tại sao các ngươi có thể dùng giả kim thuật?
Dialogue: 0,0:14:37.14,0:14:38.60,Default,Scar,0000,0000,0000,,Ý ngươi là sao?
Dialogue: 0,0:14:39.94,0:14:41.98,Default,Al,0000,0000,0000,,Thế này là thế nào?
Dialogue: 0,0:14:41.98,0:14:43.69,Default,Al,0000,0000,0000,,Tại sao Scar và cô bé đó...
Dialogue: 0,0:14:43.69,0:14:44.73,Default,Ed,0000,0000,0000,,Anh cũng chịu.
Dialogue: 0,0:14:44.73,0:14:46.36,Default,Ed,0000,0000,0000,,Không thể hiểu được, nhưng mà...
Dialogue: 0,0:14:46.36,0:14:49.36,Default,Ed,0000,0000,0000,,Đây là lúc lợi dụng bọn họ.
Dialogue: 0,0:14:49.36,0:14:50.28,Default,Ed,0000,0000,0000,,Scar!
Dialogue: 0,0:14:52.78,0:14:55.79,Default,Ed,0000,0000,0000,,Ta sẽ nói cho ngươi biết sự thật đằng sau cuộc nội chiến Ishval!
Dialogue: 0,0:14:56.37,0:14:57.83,Default,Scar,0000,0000,0000,,Sự thật?
Dialogue: 0,0:14:57.83,0:15:00.79,Default,Ed,0000,0000,0000,,Vụ án sát hại đứa bé và châm ngòi cuộc chiến ấy...
Dialogue: 0,0:15:00.79,0:15:05.50,Default,Ed,0000,0000,0000,,là do tên Envy kia giả trang thành quân nhân gây ra!
Dialogue: 0,0:15:07.17,0:15:10.43,Default,Ed,0000,0000,0000,,Chúng ở phía sau giật dây tất cả!
Dialogue: 0,0:15:19.06,0:15:24.15,Default,Scar,0000,0000,0000,,Xem ra ta phải bắt các ngươi giải thích cho rõ mới được.
Dialogue: 0,0:15:31.95,0:15:33.53,Default,Scar,0000,0000,0000,,Trả lời ta.
Dialogue: 0,0:15:33.53,0:15:36.83,Default,Scar,0000,0000,0000,,Tại sao dân tộc của ta phải bị tuyệt diệt?
Dialogue: 0,0:15:38.33,0:15:41.83,Default,Scar,0000,0000,0000,,Tùy theo câu trả lời của các ngươi, ta sẽ...
Dialogue: 0,0:15:41.83,0:15:42.87,Default,Scar,0000,0000,0000,,Không.
Dialogue: 0,0:15:42.87,0:15:47.05,Default,Scar,0000,0000,0000,,Ta sẽ không cho các ngươi tới miền cực lạc nơi đồng bào ta an nghỉ!
Dialogue: 0,0:15:47.05,0:15:50.42,Default,Scar,0000,0000,0000,,Các ngươi không đáng được các bậc thần linh cứu rỗi!!!
Dialogue: 0,0:15:57.22,0:15:59.47,Default,Greed,0000,0000,0000,,Ối, ghê quá!
Dialogue: 0,0:15:59.47,0:16:01.60,Default,Greed,0000,0000,0000,,Hắn là con người mà cũng khá ghê.
Dialogue: 0,0:16:01.60,0:16:04.69,Default,Father,0000,0000,0000,,Greed, xử lý hết lũ chúng nó đi.
Dialogue: 0,0:16:04.69,0:16:05.94,Default,Greed,0000,0000,0000,,Vâng vâng...
Dialogue: 0,0:16:11.69,0:16:14.70,Default,Ed,0000,0000,0000,,Tôi không tin đâu, Ling!
Dialogue: 0,0:16:16.99,0:16:18.41,Default,Greed,0000,0000,0000,,Ta là Greed.
Dialogue: 0,0:16:24.58,0:16:25.63,Default,May,0000,0000,0000,,Người đó...
Dialogue: 0,0:16:25.63,0:16:29.00,Default,May,0000,0000,0000,,Hình như là... Ling của tộc Yao?
Dialogue: 0,0:16:29.96,0:16:32.01,Default,Glut,0000,0000,0000,,Bé gái!
Dialogue: 0,0:16:33.17,0:16:35.01,Default,Glut,0000,0000,0000,,Thịt rất mềm!
Dialogue: 0,0:16:40.47,0:16:44.14,Default,Father,0000,0000,0000,,Này, tại sao ngươi có thể dùng được giả kim thuật?
Dialogue: 0,0:16:47.52,0:16:50.86,Default,Father,0000,0000,0000,,Hừm, đúng là đang phát động thuật.
Dialogue: 0,0:16:52.53,0:16:54.28,Default,Father,0000,0000,0000,,Phá hoại cơ thể...
Dialogue: 0,0:16:54.28,0:16:56.45,Default,Father,0000,0000,0000,,Không, là phân giải.
Dialogue: 0,0:17:02.12,0:17:06.04,Think,Scar,0000,0000,0000,,Chỉ cần chậm một khắc thôi là ta đã...
Dialogue: 0,0:17:06.92,0:17:08.33,Default,May,0000,0000,0000,,Chú Scar!
Dialogue: 0,0:17:13.55,0:17:16.55,Default,May,0000,0000,0000,,Shao May... chạy đi...
Dialogue: 0,0:17:17.22,0:17:19.18,Default,Glut,0000,0000,0000,,Xơi thôi nào!
Dialogue: 0,0:17:20.89,0:17:21.89,Default,Al,0000,0000,0000,,Em có sao không!?
Dialogue: 0,0:17:27.35,0:17:29.65,Default,May,0000,0000,0000,,Ai khiến anh cứu tôi...
Dialogue: 0,0:17:29.65,0:17:31.69,Default,Al,0000,0000,0000,,Nói vớ vẩn cái gì thế!?
Dialogue: 0,0:17:31.69,0:17:34.65,Default,Al,0000,0000,0000,,Bọn chúng sẵn sàng thủ tiêu kẻ nào không có giá trị lợi dụng.
Dialogue: 0,0:17:34.65,0:17:36.28,Default,Al,0000,0000,0000,,Mau thoát khỏi đây đi!
Dialogue: 0,0:17:39.99,0:17:41.58,Default,Al,0000,0000,0000,,Chúng đông quá!
Dialogue: 0,0:17:45.91,0:17:49.00,Default,Scar,0000,0000,0000,,Hai đứa này... vẫn chưa chạy trốn à!?
Dialogue: 0,0:17:56.17,0:17:59.43,Default,Al,0000,0000,0000,,Scar, ông có thể đưa cô bé này ra khỏi đây không?
Dialogue: 0,0:18:00.05,0:18:02.26,Default,Scar,0000,0000,0000,,Ngươi định để ta chạy thoát đấy à?
Dialogue: 0,0:18:02.26,0:18:05.52,Default,Scar,0000,0000,0000,,Dù chính ta đã giết vợ chồng bác sĩ đó...
Dialogue: 0,0:18:05.52,0:18:08.77,Default,Al,0000,0000,0000,,Ờ, tôi vẫn đang muốn đập cho ông tan xương nát thịt đấy.
Dialogue: 0,0:18:08.77,0:18:11.77,Default,Al,0000,0000,0000,,Nói thật, tôi không muốn phải chịu ơn ông chút nào.
Dialogue: 0,0:18:11.77,0:18:12.40,Default,Al,0000,0000,0000,,Nhưng bây giờ,
Dialogue: 0,0:18:12.40,0:18:14.53,Default,Al,0000,0000,0000,,quan trọng nhất là phải giúp cô bé này chạy thoát.
Dialogue: 0,0:18:15.15,0:18:16.74,Default,Scar,0000,0000,0000,,Còn các ngươi thì sao?
Dialogue: 0,0:18:19.66,0:18:23.62,Default,Al,0000,0000,0000,,Hình như đối với chúng, anh em tôi vẫn rất quan trọng.
Dialogue: 0,0:18:23.62,0:18:27.12,Default,Al,0000,0000,0000,,Dù có ở lại đây chúng tôi cũng không bị giết đâu.
Dialogue: 0,0:18:27.91,0:18:32.71,Default,Scar,0000,0000,0000,,Bây giờ ta cũng không thể vừa bảo vệ con bé vừa chạy thoát được.
Dialogue: 0,0:18:33.13,0:18:36.21,Default,Scar,0000,0000,0000,,Mà ta cũng không có ý định chạy trốn khỏi đây!
Dialogue: 0,0:18:38.42,0:18:40.18,Default,Al,0000,0000,0000,,Phân giải nước à?
Dialogue: 0,0:18:50.85,0:18:53.73,Default,Al,0000,0000,0000,,Cái lão này, làm gì thì phải báo trước chứ!
Dialogue: 0,0:18:53.73,0:18:55.36,Default,Al,0000,0000,0000,,Thô lỗ quá đi mất!
Dialogue: 0,0:18:57.53,0:19:01.20,Default,Envy,0000,0000,0000,,Nổ khí hydro à? Ngươi cũng khá lắm.
Dialogue: 0,0:19:03.53,0:19:04.78,Default,Envy,0000,0000,0000,,Hắn đâu rồi?
Dialogue: 0,0:19:04.78,0:19:06.83,Default,Envy,0000,0000,0000,,Hắn đi đâu rồi?
Dialogue: 0,0:19:06.83,0:19:07.91,Default,Al,0000,0000,0000,,Ta không biết!
Dialogue: 0,0:19:07.91,0:19:10.37,Default,Envy,0000,0000,0000,,Gluttony, đánh hơi hắn đi!
Dialogue: 0,0:19:13.58,0:19:16.21,Default,Envy,0000,0000,0000,,Đã hết khả năng tái sinh rồi cơ à!?
Dialogue: 0,0:19:16.21,0:19:17.71,Default,Envy,0000,0000,0000,,Chết lắm quá đấy!
Dialogue: 0,0:19:20.30,0:19:22.22,Default,Greed,0000,0000,0000,,Đá hay lắm.
Dialogue: 0,0:19:22.22,0:19:24.80,Default,Greed,0000,0000,0000,,Có khi vụ này vui hơn ta tưởng đây.
Dialogue: 0,0:19:26.64,0:19:28.56,Default,Ed,0000,0000,0000,,Hoàng tử ngốc!
Dialogue: 0,0:19:29.56,0:19:30.98,Default,Ed,0000,0000,0000,,Mau tỉnh lại đi!
Dialogue: 0,0:19:30.98,0:19:31.85,Default,Greed,0000,0000,0000,,Ta là Greed.
Dialogue: 0,0:19:31.85,0:19:32.77,Default,Ed,0000,0000,0000,,Không cần biết!
Dialogue: 0,0:19:32.77,0:19:34.27,Default,Ed,0000,0000,0000,,Mau trả lại cơ thể cho Ling đi.
Dialogue: 0,0:19:34.27,0:19:36.61,Default,Greed,0000,0000,0000,,Không có chuyện đó đâu!
Dialogue: 0,0:19:41.32,0:19:44.16,Default,Ed,0000,0000,0000,,Cậu bị đần thật đấy hả!?
Dialogue: 0,0:19:44.16,0:19:45.91,Default,Ed,0000,0000,0000,,Đất nước của cậu...
Dialogue: 0,0:19:45.91,0:19:47.79,Default,Ed,0000,0000,0000,,Cả Lan Fan nữa, cậu tính sao đây!?
Dialogue: 0,0:19:57.42,0:19:59.76,Default,Greed,0000,0000,0000,,Phụ thân! Con tóm được hắn rồi.
Dialogue: 0,0:20:00.21,0:20:03.43,Default,Father,0000,0000,0000,,Đưa hắn lên chỗ của Wrath đi.
Dialogue: 0,0:20:05.30,0:20:08.81,Default,Envy,0000,0000,0000,,Các ngươi toàn làm chuyện thừa thãi...
Dialogue: 0,0:20:08.81,0:20:13.64,Default,Envy,0000,0000,0000,,Cứ ngoan ngoãn đi thì ai thèm động vào làm gì.
Dialogue: 0,0:20:14.98,0:20:15.94,Default,Al,0000,0000,0000,,Anh hai.
Dialogue: 0,0:20:17.77,0:20:18.90,Default,Ed,0000,0000,0000,,Cậu ấy...
Dialogue: 0,0:20:19.61,0:20:20.69,Default,Ed,0000,0000,0000,,Ling...
Dialogue: 0,0:20:21.28,0:20:22.45,Default,Ed,0000,0000,0000,,vẫn còn ở trong đó.
Dialogue: 0,0:20:29.74,0:20:33.71,Default,Father,0000,0000,0000,,Con dùng hết năng lực tái sinh rồi à?
Dialogue: 0,0:20:34.17,0:20:37.08,Default,Father,0000,0000,0000,,Đừng lo, con trai của ta.
Dialogue: 0,0:20:50.22,0:20:52.85,Default,Father,0000,0000,0000,,Ta sẽ tái tạo con.
Dialogue: 0,0:20:52.85,0:20:54.98,Default,Father,0000,0000,0000,,Cả ký ức của con cũng sẽ được giữ lại.
Dialogue: 0,0:20:59.15,0:21:01.33,Default,Nar,0000,0000,0000,,Con người dù lạc lối trong hỗn độn
Dialogue: 0,0:21:01.33,0:21:04.03,Default,Nar,0000,0000,0000,,vẫn gắng sức đấu tranh và khóc than trong đau đớn.
Dialogue: 0,0:21:04.03,0:21:06.91,Default,Nar,0000,0000,0000,,Bởi vì đó là bằng chứng cho thấy chúng vẫn đang sống trên đời.
Dialogue: 0,0:21:07.53,0:21:11.79,Default,Nar,0000,0000,0000,,Và rồi, chúng tiếp tục tranh đấu và quỳ dưới chân ta.
Dialogue: 0,0:21:12.41,0:21:16.96,Default,Nar,0000,0000,0000,,Lần tới, Cang Giả Kim Thuật Sư - Fullmetal Alchemist:
Dialogue: 0,0:21:16.96,0:21:20.96,Next,Nar,0000,0000,0000,,{\pos(346,162)}Tập 29
Dialogue: 0,0:21:16.96,0:21:20.96,Next,Nar,0000,0000,0000,,{\pos(638,467)\fn UVN Bui Doi}Cuộc Đấu Tranh Của Kẻ Ngốc
Dialogue: 0,0:21:16.96,0:21:20.96,Default,Nar,0000,0000,0000,,Tập 29, Cuộc Đấu Tranh Của Kẻ Ngốc.
Dialogue: 0,0:21:21.50,0:21:23.43,Default,Nar,0000,0000,0000,,Liệu con người chân tay không lành lặn ấy
Dialogue: 0,0:21:23.43,0:21:26.22,Default,Nar,0000,0000,0000,,có thể tiếp tục đứng dậy hay không?
Dialogue: 0,0:21:26.22,0:21:28.22,Default,,0000,0000,0000,,

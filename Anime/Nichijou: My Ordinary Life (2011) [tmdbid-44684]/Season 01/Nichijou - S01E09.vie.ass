Dialogue: 0,0:04:43.16,0:04:47.34,Default,Mio,0,0,0,,Cái miếng dâu đâu rồi?
Dialogue: 0,0:04:54.44,0:04:55.85,Default,Mio,0,0,0,,Bụng!
Dialogue: 0,0:04:59.16,0:05:01.52,Default,Mio,0,0,0,,Đồ chị ngốc!
Dialogue: 0,0:05:01.52,0:05:06.24,Default,,0,0,0,,Bánh không có dâu khác gì bánh mì!
Dialogue: 0,0:05:07.58,0:05:11.94,Default,Mio,0,0,0,,Chả bằng bánh ngọt nữa!
Dialogue: 0,0:05:22.55,0:05:26.55,Default,Yoshino,0,0,0,,Happy birthday to me.
Dialogue: 0,0:05:27.02,0:05:31.58,Default,,0,0,0,,Happy birthday to me.
Dialogue: 0,0:05:31.58,0:05:36.10,Default,Yoshino,0,0,0,,Happy birthday dear Watashi{redundant but should add a TL Note: Watashi means "I" in japanese.}{kb_z}
Dialogue: 0,0:05:37.13,0:05:42.65,Default,Yoshino,0,0,0,,Happy birthday to me.
Dialogue: 0,0:05:42.98,0:05:44.22,Default,Mio,0,0,0,,Chị ơi...
Dialogue: 0,0:05:47.09,0:05:48.66,Default,Yoshino,0,0,0,,Mio
Dialogue: 0,0:05:50.61,0:05:53.08,Default,Mio,0,0,0,,Sinh nhật chị con khỉ!
Dialogue: 0,0:06:23.55,0:06:26.24,Default,Yukko,0,0,0,,Muốn đến lâu rồi
Dialogue: 0,0:06:26.62,0:06:28.20,Default,Mio,0,0,0,,Báo đăng nhỉ?
Dialogue: 0,0:06:28.20,0:06:29.29,Default,Yukko,0,0,0,,Đúng rồi!
Dialogue: 0,0:06:29.29,0:06:31.55,Default,,0,0,0,,Có cả giảm giá này!
Dialogue: 0,0:06:31.55,0:06:33.06,Default,Mio,0,0,0,,Đâu cần lôi ra
Dialogue: 0,0:06:33.06,0:06:34.52,Default,Mai,0,0,0,,Mình ăn mì hầm
Dialogue: 0,0:06:34.52,0:06:36.97,Default,Yukko,0,0,0,,Nóng thế cũng ăn
Dialogue: 0,0:06:36.97,0:06:38.28,Default,Mai,0,0,0,,Không cá
Dialogue: 0,0:06:39.36,0:06:41.80,Default,Yukko,0,0,0,,Mì thường mất rồi!
Dialogue: 0,0:06:44.94,0:06:46.90,Default,Yukko,0,0,0,,Mio ăn gì nào?
Dialogue: 0,0:06:46.90,0:06:48.73,Default,Mio,0,0,0,,Mình ăn mì lạnh
Dialogue: 0,0:06:48.73,0:06:50.40,Default,Yukko,0,0,0,,Chán thế
Dialogue: 0,0:06:50.40,0:06:52.68,Default,Yukko,0,0,0,,Đúng là nghiệp dư
Dialogue: 0,0:06:52.68,0:06:53.89,Default,Mio,0,0,0,,Có sao đâu
Dialogue: 0,0:06:53.89,0:06:54.78,Default,,0,0,0,,Yukko ăn gì?
Dialogue: 0,0:06:54.78,0:06:58.48,Default,Yukko,0,0,0,,Mình ăn cơm trộn rau
Dialogue: 0,0:06:58.48,0:07:01.60,Default,Yukko,0,0,0,,Vào tiệm mì gọi cơm mới hay
Dialogue: 0,0:07:02.35,0:07:03.34,Default,Mio,0,0,0,,Ngầu quá!
Dialogue: 0,0:07:03.34,0:07:04.70,Default,Yukko,0,0,0,,Thấy chưa
Dialogue: 0,0:07:04.70,0:07:07.19,Default,Yukko,0,0,0,,Dịp đặc biệt nữa
Dialogue: 0,0:07:07.19,0:07:10.24,Default,Yukko,0,0,0,,Ai biết khi nào mới được ăn
Dialogue: 0,0:07:10.61,0:07:11.70,Default,Mio,0,0,0,,Năm sau
Dialogue: 0,0:07:13.84,0:07:16.75,Default,Yukko,0,0,0,,Mio thực tế quá
Dialogue: 0,0:07:16.75,0:07:18.70,Default,Yukko,0,0,0,,Hiện tại mới là nhất
Dialogue: 0,0:07:19.02,0:07:20.89,Default,Mio,0,0,0,,Rồi, hiểu
Dialogue: 0,0:07:20.89,0:07:23.37,Default,Yukko,0,0,0,,Mio chưa đến tuổi đâu
Dialogue: 0,0:07:23.37,0:07:24.08,Default,Mio,0,0,0,,Rồi, rồi
Dialogue: 0,0:07:30.19,0:07:31.84,Default,Yukko,0,0,0,,Chị ơi
Dialogue: 0,0:07:32.85,0:07:35.37,Default,Waitress,0,0,0,,Gọi món gì ạ?
Dialogue: 0,0:07:35.37,0:07:37.35,Default,Yukko,0,0,0,,Mì hầm, mì lạnh
Dialogue: 0,0:07:38.02,0:07:42.19,Default,Yukko,0,0,0,,với cơm trộn rau ạ
Dialogue: 0,0:07:42.67,0:07:46.69,Default,Waitress,0,0,0,,Mì hầm, mì lạnh, cơm trộn rau
Dialogue: 0,0:07:46.69,0:07:48.74,Default,Waitress,0,0,0,,Xin chờ chút
Dialogue: 0,0:07:53.03,0:07:57.07,Default,Mio,0,0,0,,Có ghi là mì tự làm này
Dialogue: 0,0:07:57.07,0:07:59.88,Default,Yukko,0,0,0,,Nghiêm túc ghê ha
Dialogue: 0,0:07:59.88,0:08:02.33,Default,Yukko,0,0,0,,Mì mạch là gì nhỉ?
Dialogue: 0,0:08:02.33,0:08:05.37,Default,Mio,0,0,0,,Dùng lúa mạch làm sợi
Dialogue: 0,0:08:05.37,0:08:07.31,Default,Yukko,0,0,0,,Thế à?
Dialogue: 0,0:08:07.31,0:08:08.88,Default,Waitress,0,0,0,,Thưa quý khách
Dialogue: 0,0:08:09.88,0:08:15.05,Default,Waitress,0,0,0,,Một mì hầm, một mì lạnh
Dialogue: 0,0:08:17.41,0:08:20.07,Default,Yukko,0,0,0,,Cứ ăn trước đi
Dialogue: 0,0:08:20.07,0:08:21.03,Default,Mio,0,0,0,,Ngại quá
Dialogue: 0,0:08:21.03,0:08:22.28,Default,Mai,0,0,0,,Ăn kẻo nguội
Dialogue: 0,0:08:22.28,0:08:23.81,Default,Mio,0,0,0,,Mình ăn lạnh mà
Dialogue: 0,0:08:23.81,0:08:27.06,Default,Yukko,0,0,0,,Món mình là đặc biệt rồi
Dialogue: 0,0:08:27.06,0:08:30.90,Default,Yukko,0,0,0,,nên phải làm cho chu đáo
Dialogue: 0,0:08:49.35,0:08:50.96,Default,Mio,0,0,0,,Yukko đợi lâu thế 
Dialogue: 0,0:08:51.49,0:08:54.55,Default,Mio,0,0,0,,chiên thịt chắc hơi lâu
Dialogue: 0,0:08:54.55,0:08:58.51,Default,Yukko,0,0,0,,Chắc đang đi mua rau
Dialogue: 0,0:08:58.81,0:09:01.85,Default,Mio,0,0,0,,Hèn gì mà chậm thế
Dialogue: 0,0:09:15.42,0:09:18.53,Default,Yukko,0,0,0,,Ở đây có nhiều tráng miệng nhỉ
Dialogue: 0,0:09:18.53,0:09:20.10,Default,Mio,0,0,0,,Thế à?
Dialogue: 0,0:09:25.58,0:09:26.04,Default,Yukko,0,0,0,,Đây!
Dialogue: 0,0:09:29.02,0:09:30.30,Default,Mio,0,0,0,,Nhầm rồi
Dialogue: 0,0:09:30.30,0:09:31.39,Default,Mai,0,0,0,,Ngại quá
Dialogue: 0,0:09:36.29,0:09:37.72,Default,Yukko,0,0,0,,Khốn nạn!
Dialogue: 0,0:09:37.72,0:09:39.29,Default,Mio,0,0,0,,Yukko bình tĩnh!
Dialogue: 0,0:09:39.29,0:09:41.15,Default,Yukko,0,0,0,,Sao được! Chậm quá
Dialogue: 0,0:09:41.15,0:09:43.42,Default,Yukko,0,0,0,,Chưa nói đến vị
Dialogue: 0,0:09:43.42,0:09:44.77,Default,,0,0,0,,lâu quá rồi
Dialogue: 0,0:09:44.77,0:09:46.17,Default,Mio,0,0,0,,Có sao đâu!
Dialogue: 0,0:09:46.17,0:09:47.56,Default,Yukko,0,0,0,,Phải la mới được!
Dialogue: 0,0:09:47.91,0:09:49.40,Default,Waitress,0,0,0,,Đây thưa quý khách
Dialogue: 0,0:09:49.71,0:09:51.15,Default,Yukko,0,0,0,,Chậm quá-
Dialogue: 0,0:09:56.52,0:09:57.86,Default,Yukko,0,0,0,,Cảm ơn
Dialogue: 0,0:10:08.60,0:10:10.44,Default,Yukko,0,0,0,,Đọc gì đi
Dialogue: 0,0:10:10.44,0:10:12.16,Default,,0,0,0,,Xong ngay giờ
Dialogue: 0,0:10:18.43,0:10:20.22,Default,Yukko and Mio,0,0,0,,Thứ siêu ngầu nè!
Dialogue: 0,0:10:29.93,0:10:31.40,Default,Yukko and Mio,0,0,0,,Thần đồng cờ tướng!
Dialogue: 0,0:10:47.86,0:10:49.92,Default,Yukko,0,0,0,,Nhang muỗi đốt rồi
Dialogue: 0,0:10:49.92,0:10:52.77,Default,,0,0,0,,Chờ chút cho thấm
Dialogue: 0,0:10:53.35,0:10:56.92,Default,Yukko,0,0,0,,Nhưng không thấm như sông Mogami
Dialogue: 0,0:11:20.16,0:11:23.37,Default,Yukko,0,0,0,,Đến muỗi cũng có linh hồn
Dialogue: 0,0:11:23.37,0:11:25.70,Default,Yukko,0,0,0,,Ngủ đi vậy
Dialogue: 0,0:11:26.12,0:11:26.87,Default,Yukko,0,0,0,,Chết này!
Dialogue: 0,0:11:36.05,0:11:37.30,Default,Yukko,0,0,0,,Hay!
Dialogue: 0,0:11:38.13,0:11:38.92,Default,Yukko,0,0,0,,Đỡ này!
Dialogue: 0,0:11:49.13,0:11:51.66,Default,Yukko,0,0,0,,Phí một kiếp
Dialogue: 0,0:11:53.23,0:11:54.06,Default,Yukko,0,0,0,,Chết này!
Dialogue: 0,0:12:01.32,0:12:02.11,Default,Yukko,0,0,0,,Đánh 
Dialogue: 0,0:12:02.11,0:12:02.99,Default,Yukko,0,0,0,,Đánh
Dialogue: 0,0:12:03.45,0:12:05.49,Default,Yukko,0,0,0,,Không hiệu quả!
Dialogue: 0,0:12:05.49,0:12:09.70,Default,Yukko,0,0,0,,Biết làm sao đây!
Dialogue: 0,0:12:10.73,0:12:11.77,Default,Yukko,0,0,0,,Mẹ ơi!
Dialogue: 0,0:12:11.77,0:12:13.79,Default,Yukko,0,0,0,,Nhà này hỏng rồi!
Dialogue: 0,0:12:14.39,0:12:15.92,Default,Yukko,0,0,0,,Chuyển đi thôi!
Dialogue: 0,0:12:26.60,0:12:28.56,Default,Mom,0,0,0,,Biết mấy giờ không?
Dialogue: 0,0:12:29.14,0:12:30.93,Default,Mom,0,0,0,,Ngủ ngay!
Dialogue: 0,0:13:07.94,0:13:09.56,Default,Angel,0,0,0,,Chirashizushi
Dialogue: 0,0:13:09.56,0:13:10.22,Default,Angel,0,0,0,,Mời
Dialogue: 0,0:13:12.55,0:13:17.40,Default,Angel,0,0,0,,Không, Chi Ra Shi Zu Shi
Dialogue: 0,0:13:17.40,0:13:17.94,Default,Angel,0,0,0,,Mời
Dialogue: 0,0:13:21.88,0:13:22.57,Default,Angel,0,0,0,,Non, non.
Dialogue: 0,0:13:22.90,0:13:23.57,Default,Angel,0,0,0,,Chi
Dialogue: 0,0:13:23.57,0:13:24.61,Default,Demon,0,0,0,,Chi
Dialogue: 0,0:13:24.94,0:13:25.70,Default,Angel,0,0,0,,Ra.
Dialogue: 0,0:13:25.70,0:13:26.78,Default,Demon,0,0,0,,Ra.
Dialogue: 0,0:13:26.78,0:13:27.99,Default,Angel,0,0,0,,Shi.
Dialogue: 0,0:13:27.99,0:13:29.37,Default,Demon,0,0,0,,Shi.
Dialogue: 0,0:13:29.82,0:13:30.62,Default,Angel,0,0,0,,Zu.
Dialogue: 0,0:13:30.62,0:13:31.83,Default,Demon,0,0,0,,Zu.
Dialogue: 0,0:13:31.83,0:13:32.87,Default,Angel,0,0,0,,Shi.
Dialogue: 0,0:13:32.87,0:13:33.83,Default,Demon,0,0,0,,Shi.
Dialogue: 0,0:13:34.25,0:13:36.01,Default,Angel,0,0,0,,Đúng
Dialogue: 0,0:13:36.01,0:13:37.59,Default,Angel,0,0,0,,Chirashizushi
Dialogue: 0,0:13:37.59,0:13:38.00,Default,Angel,0,0,0,,Mời
Dialogue: 0,0:14:01.39,0:14:02.39,Default,Professor,0,0,0,,Sakamoto!
Dialogue: 0,0:14:02.39,0:14:03.57,Default,,0,0,0,,Nè, nè
Dialogue: 0,0:14:03.96,0:14:06.11,Default,Professor,0,0,0,,Dấu thắng bên dưới!
Dialogue: 0,0:14:07.24,0:14:08.78,Default,Professor,0,0,0,,Không vui hả?
Dialogue: 0,0:14:08.78,0:14:11.00,Default,Nano,0,0,0,,Không được ăn đâu
Dialogue: 0,0:14:11.00,0:14:12.72,Default,Professor,0,0,0,,Không được là sao?
Dialogue: 0,0:14:12.72,0:14:15.80,Default,Nano,0,0,0,,Sao mà ăn cơm đây
Dialogue: 0,0:14:16.45,0:14:18.04,Default,Professor,0,0,0,,Cơm trứng
Dialogue: 0,0:14:18.64,0:14:23.98,Default,Sakamoto,0,0,0,,Thắng mỗi que kem mà cũng nhặng lên
Dialogue: 0,0:14:23.98,0:14:25.70,Default,Professor,0,0,0,,Đâu chỉ thế!
Dialogue: 0,0:14:25.70,0:14:27.77,Default,,0,0,0,,In bên dưới đó!
Dialogue: 0,0:14:27.77,0:14:30.99,Default,Sakamoto,0,0,0,,Trên hay dưới như nhau
Dialogue: 0,0:14:30.99,0:14:34.22,Default,Sakamoto,0,0,0,,Có lúc này lúc kia
Dialogue: 0,0:14:34.98,0:14:37.48,Default,Sakamoto,0,0,0,,Đúng là con nít
Dialogue: 0,0:14:37.48,0:14:40.65,Default,Sakamoto,0,0,0,,Có chút cũng vui
Dialogue: 0,0:14:40.65,0:14:41.77,Default,Sakamoto,0,0,0,,Cua à!
Dialogue: 0,0:14:42.31,0:14:45.51,Default,Sakamoto,0,0,0,,Cái này hiếm lắm!
Dialogue: 0,0:14:45.51,0:14:48.69,Default,Sakamoto,0,0,0,,Để ăn cuối luôn
Dialogue: 0,0:14:50.88,0:14:53.33,Default,Nano,0,0,0,,Ăn no rồi
Dialogue: 0,0:14:53.33,0:14:56.75,Default,Nano,0,0,0,,Giáo sư chưa ăn gì à?
Dialogue: 0,0:14:57.24,0:15:00.97,Default,Nano,0,0,0,,Do ăn vặt chiều đúng không?
Dialogue: 0,0:15:00.97,0:15:02.66,Default,Professor,0,0,0,,Em xin lỗi
Dialogue: 0,0:15:02.66,0:15:06.04,Default,Nano,0,0,0,,Nè, ăn bao nhiêu rồi hả?
Dialogue: 0,0:15:10.52,0:15:11.22,Default,Professor,0,0,0,,Hết sạch
Dialogue: 0,0:15:11.22,0:15:12.36,Default,Nano,0,0,0,,Hết sạch?
Dialogue: 0,0:15:12.36,0:15:14.92,Default,,0,0,0,,Ăn hết đồ cả tháng?
Dialogue: 0,0:15:16.26,0:15:17.52,Default,Nano,0,0,0,,Ai khen đâu
Dialogue: 0,0:15:17.87,0:15:19.03,Default,Nano,0,0,0,,Đúng là
Dialogue: 0,0:15:19.03,0:15:21.77,Default,Professor,0,0,0,,Sakamoto bảo em ăn
Dialogue: 0,0:15:21.77,0:15:22.90,Default,Nano,0,0,0,,Chú Sakamoto?!
Dialogue: 0,0:15:34.91,0:15:38.38,Default,Professor,0,0,0,,Sakamoto bảo đồ ăn hè nhanh hỏng
Dialogue: 0,0:15:38.38,0:15:39.84,Default,Sakamoto,0,0,0,,Không có nhé!
Dialogue: 0,0:15:39.84,0:15:41.95,Default,Professor,0,0,0,,Ăn vặt có dạ dày riêng
Dialogue: 0,0:15:41.95,0:15:43.63,Default,Sakamoto,0,0,0,,Bộ bò chắc
Dialogue: 0,0:15:43.63,0:15:44.46,Default,Nano,0,0,0,,Chú Sakamoto
Dialogue: 0,0:15:44.75,0:15:46.77,Default,Sakamoto,0,0,0,,Mắc gì làm thế?
Dialogue: 0,0:15:46.77,0:15:48.28,Default,Sakamoto,0,0,0,,Chú vô tội mà
Dialogue: 0,0:15:48.28,0:15:50.70,Default,Sakamoto,0,0,0,,Chú trắng rõ ràng
Dialogue: 0,0:15:50.70,0:15:52.08,Default,Professor,0,0,0,,Đen mà Sakamoto
Dialogue: 0,0:15:52.08,0:15:52.74,Default,Nano,0,0,0,,Đúng đó
Dialogue: 0,0:15:52.74,0:15:54.90,Default,Sakamoto,0,0,0,,Sinh ra thế rồi
Dialogue: 0,0:15:54.90,0:15:55.60,Default,Nano,0,0,0,,Đủ rồi
Dialogue: 0,0:15:56.19,0:15:58.57,Default,,0,0,0,,Em không nghe lời
Dialogue: 0,0:15:59.23,0:16:02.64,Default,Nano,0,0,0,,khỏi uống nước dâu
Dialogue: 0,0:16:03.95,0:16:05.94,Default,Professor,0,0,0,,Không ăn vặt đâu!
Dialogue: 0,0:16:05.94,0:16:06.82,Default,Nano,0,0,0,,Mặc kệ
Dialogue: 0,0:16:06.82,0:16:09.19,Default,Professor,0,0,0,,Cho que kem trúng thưởng!
Dialogue: 0,0:16:09.19,0:16:10.07,Default,Nano,0,0,0,,Khỏi cần!
Dialogue: 0,0:16:10.07,0:16:12.53,Default,Professor,0,0,0,,Em ăn rau mà!
Dialogue: 0,0:16:12.53,0:16:14.57,Default,Nano,0,0,0,,Mặc kệ chứ
Dialogue: 0,0:16:19.56,0:16:23.29,Default,Professor,0,0,0,,Em rút chìa khóa!
Dialogue: 0,0:16:27.98,0:16:30.13,Default,Nano,0,0,0,,Tha cho em đó
Dialogue: 0,0:16:30.13,0:16:32.84,Default,Nano,0,0,0,,Tái phạm là cấm ăn vặt cả tháng
Dialogue: 0,0:16:32.84,0:16:33.68,Default,Professor,0,0,0,,Dạ!
Dialogue: 0,0:16:33.68,0:16:35.43,Default,Nano,0,0,0,,Giờ
Dialogue: 0,0:16:36.47,0:16:40.93,Default,Nano,0,0,0,,Mình đưa thì thể nào cũng quỵt
Dialogue: 0,0:16:41.68,0:16:45.76,Default,Nano,0,0,0,,Rút chìa khóa ra mói được
Dialogue: 0,0:16:47.64,0:16:49.85,Default,Professor,0,0,0,,Quay đầu lại
Dialogue: 0,0:16:49.85,0:16:50.69,Default,Nano,0,0,0,,Rồi!
Dialogue: 0,0:17:07.75,0:17:09.21,Default,Professor,0,0,0,,Tay trái chị thua
Dialogue: 0,0:17:09.21,0:17:11.42,Default,Nano,0,0,0,,Lắp cái này chi?
Dialogue: 0,0:17:11.42,0:17:12.97,Default,Nano,0,0,0,,Ai khen đâu
Dialogue: 0,0:17:12.97,0:17:15.13,Default,Nano,0,0,0,,Giáo sư hư quá
Dialogue: 0,0:17:15.45,0:17:18.60,Default,Nano,0,0,0,,Phải ngoan hơn nghe chưa
Dialogue: 0,0:17:18.60,0:17:20.60,Default,Nano,0,0,0,,Hư quá!
Dialogue: 0,0:17:35.93,0:17:36.66,Default,Class,0,0,0,,Búa!
Dialogue: 0,0:17:36.93,0:17:37.66,Default,Class,0,0,0,,Bao!
Dialogue: 0,0:17:38.09,0:17:39.35,Default,Class,0,0,0,,Kéo!
Dialogue: 0,0:18:00.05,0:18:03.75,Default,Narrator,0,0,0,,Cứ tìm rồi sẽ thấy thôi
Dialogue: 0,0:18:10.15,0:18:11.44,Default,Yoshino,0,0,0,,A, Mio

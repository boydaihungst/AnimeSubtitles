Dialogue: 0,0:22:04.71,0:22:07.97,ED English,,0,0,0,,Thức dậy mỗi sáng, đánh răng rửa mặt
Dialogue: 0,0:22:07.97,0:22:11.10,ED English,,0,0,0,,Mới ngoảnh lại đã 10 giờ đêm
Dialogue: 0,0:22:11.10,0:22:14.18,ED English,,0,0,0,,Hôm nay cười hoài à
Dialogue: 0,0:22:14.18,0:22:17.19,ED English,,0,0,0,,Chơi vui lắm cơ
Dialogue: 0,0:22:17.19,0:22:23.57,ED English,,0,0,0,,Ngốc nghếch vui vẻ bên bạn bè cả ngày nè
Dialogue: 0,0:22:23.57,0:22:26.65,ED English,,0,0,0,,Bỗng hai ta nhìn nhau
Dialogue: 0,0:22:26.65,0:22:29.32,ED English,,0,0,0,,tình cờ thôi nhỉ
Dialogue: 0,0:22:29.32,0:22:32.83,ED English,,0,0,0,,Không phải là tình đầu đâu nha
Dialogue: 0,0:22:32.83,0:22:36.00,ED English,,0,0,0,,Em không phải là thế này
Dialogue: 0,0:22:36.00,0:22:42.04,ED English,,0,0,0,,Nè, mai ta lại gặp nhau nữa nha
Dialogue: 0,0:22:42.04,0:22:45.13,ED English,,0,0,0,,Mắt nhắm chặt lại
Dialogue: 0,0:22:45.13,0:22:48.26,ED English,,0,0,0,,anh cứ lấp la lấp lánh trong em
Dialogue: 0,0:22:48.26,0:22:51.59,ED English,,0,0,0,,Hôm nay vui ghê ha
Dialogue: 0,0:22:51.59,0:22:55.52,ED English,,0,0,0,,A, em yêu mất rồi
Dialogue: 0,0:23:00.94,0:23:04.07,ED English,,0,0,0,,Vui quá, vui ghê
Dialogue: 0,0:23:04.07,0:23:07.15,ED English,,0,0,0,,Gặp mối tình này em vui lắm
Dialogue: 0,0:23:07.15,0:23:10.32,ED English,,0,0,0,,Mai ta lại gặp nhau nhé
Dialogue: 0,0:23:10.32,0:23:12.49,ED English,,0,0,0,,Chúc anh ngủ ngon
Dialogue: 0,0:23:13.41,0:23:17.54,ED English,,0,0,0,,Giờ ta gặp trong giấc mơ
Dialogue: 0,0:23:18.04,0:23:23.63,ED English,,0,0,0,,Chúc anh ngủ ngon ha
Dialogue: 0,0:00:15.35,0:00:17.52,Default,Tanaka,0,0,0,,Nóng quá đi
Dialogue: 0,0:00:17.52,0:00:19.04,Default,Tanaka,0,0,0,,Nóng quá
Dialogue: 0,0:00:20.32,0:00:24.65,Default,Mohawk,0,0,0,,Tanaka biết là đang nóng rồi
Dialogue: 0,0:00:24.65,0:00:28.92,Default,Mohawk,0,0,0,,mà cứ nói nóng thì càng thấy nóng hơn
Dialogue: 0,0:00:28.92,0:00:30.71,Default,Mohawk,0,0,0,,Đừng nói nóng nữa
Dialogue: 0,0:00:31.10,0:00:32.17,Default,Tanaka,0,0,0,,Rồi, rồi
Dialogue: 0,0:00:33.78,0:00:35.18,Default,Tanaka,0,0,0,,À...
Dialogue: 0,0:00:35.18,0:00:36.43,Default,Tanaka,0,0,0,,Hè quá!...
Dialogue: 0,0:00:36.87,0:00:38.80,Default,Tanaka,0,0,0,,Hè ghê, hè ghê
Dialogue: 0,0:00:39.27,0:00:40.26,Default,Tanaka,0,0,0,,Đùa đó
Dialogue: 0,0:00:40.26,0:00:42.27,Default,Tanaka,0,0,0,,Đúng là hè ghê
Dialogue: 0,0:00:42.27,0:00:43.77,Default,Tanaka,0,0,0,,Hè thật ta
Dialogue: 0,0:00:43.77,0:00:46.21,Default,Tanaka,0,0,0,,Hè quá!
Dialogue: 0,0:00:46.21,0:00:48.89,Default,Mohawk,0,0,0,,Thằng này...đánh cho quá
Dialogue: 0,0:00:48.89,0:00:51.28,Default,Tanaka,0,0,0,,Hè khiếp!
Dialogue: 0,0:00:51.64,0:00:54.03,Default,Tanaka,0,0,0,,Hèn quá đi!
Dialogue: 0,0:02:26.33,0:02:28.12,Default,Yukko,0,0,0,,Tỏa sáng!
Dialogue: 0,0:02:28.12,0:02:31.13,Default,Yukko,0,0,0,,Hôm nay trong xanh thật
Dialogue: 0,0:02:32.95,0:02:37.97,Default,Yukko,0,0,0,,Thời tiết, con người, cả trong lớp
Dialogue: 0,0:02:38.60,0:02:39.88,Default,Yukko,0,0,0,,Lí do là
Dialogue: 0,0:02:41.29,0:02:44.48,Default,Yukko,0,0,0,,mình làm bài rồi!
Dialogue: 0,0:02:46.21,0:02:49.29,Default,Yukko,0,0,0,,Khoe Mio chơi
Dialogue: 0,0:02:54.00,0:02:54.61,Default,Yukko,0,0,0,,Hả...
Dialogue: 0,0:02:54.94,0:02:56.56,Default,Yukko,0,0,0,,Hả?
Dialogue: 0,0:02:57.19,0:03:00.95,Default,Yukko,0,0,0,,H-hả?
Dialogue: 0,0:03:01.57,0:03:05.70,Default,Yukko,0,0,0,,Không có!
Dialogue: 0,0:03:06.35,0:03:06.95,Default,Yukko,0,0,0,,Đùa à!
Dialogue: 0,0:03:07.28,0:03:08.20,Default,,0,0,0,,Sao được!
Dialogue: 0,0:03:08.61,0:03:11.27,Default,Yukko,0,0,0,,Mình làm bài rồi!
Dialogue: 0,0:03:11.27,0:03:12.71,Default,Yukko,0,0,0,,Sao? Sao? Sao? Sao?
Dialogue: 0,0:03:12.71,0:03:14.84,Default,Yukko,0,0,0,,Sao không có?
Dialogue: 0,0:03:16.26,0:03:18.24,Default,Yukko,0,0,0,,Thôi chép vậy
Dialogue: 0,0:03:18.24,0:03:19.92,Default,Yukko,0,0,0,,Mio ơi...
Dialogue: 0,0:03:21.40,0:03:22.76,Default,Yukko,0,0,0,,Khoan...
Dialogue: 0,0:03:23.15,0:03:25.57,Default,Yukko,0,0,0,,Khoan nào...
Dialogue: 0,0:03:25.57,0:03:29.43,Default,Yukko,0,0,0,,Bài tập làm trong phiếu mà
Dialogue: 0,0:03:29.43,0:03:32.95,Default,Yukko,0,0,0,,Chết rồi
Dialogue: 0,0:03:34.95,0:03:36.61,Default,Mio,0,0,0,,Sao thế Yukko?
Dialogue: 0,0:03:36.61,0:03:37.53,Default,Mio,0,0,0,,Bức xúc thế
Dialogue: 0,0:03:37.88,0:03:40.74,Default,Yukko,0,0,0,,Quên bài tập rồi
Dialogue: 0,0:03:40.74,0:03:44.16,Default,Yukko,0,0,0,,Người ngoài hành tinh xâm lược cái
Dialogue: 0,0:03:44.63,0:03:46.24,Default,Mio,0,0,0,,Lại quên à?
Dialogue: 0,0:03:46.24,0:03:47.83,Default,Yukko,0,0,0,,Đừng khinh nhau!
Dialogue: 0,0:03:47.83,0:03:49.79,Default,Yukko,0,0,0,,Làm rồi mà mất!
Dialogue: 0,0:03:49.79,0:03:51.41,Default,Mio,0,0,0,,Ai khinh đâu...
Dialogue: 0,0:03:51.80,0:03:54.18,Default,Mio,0,0,0,,Phiếu bài tập đúng không?
Dialogue: 0,0:03:54.18,0:03:56.77,Default,Mio,0,0,0,,Hay kẹt đâu rồi
Dialogue: 0,0:03:56.77,0:03:58.46,Default,Mio,0,0,0,,Tìm kĩ lại đi
Dialogue: 0,0:03:58.46,0:04:01.58,Default,Yukko,0,0,0,,Tìm hết rồi mà
Dialogue: 0,0:04:01.58,0:04:03.39,Default,Yukko,0,0,0,,Không thấy gì
Dialogue: 0,0:04:03.39,0:04:03.97,Default,Yukko,0,0,0,,Hả?
Dialogue: 0,0:04:09.85,0:04:11.85,Default,Yukko,0,0,0,,Mẹ ơi!
Dialogue: 0,0:04:11.85,0:04:14.60,Default,Yukko,0,0,0,,Hội bánh dày là sao?
Dialogue: 0,0:04:15.41,0:04:17.80,Default,Yukko,0,0,0,,Hội là sao...
Dialogue: 0,0:04:17.80,0:04:19.22,Default,Mio,0,0,0,,Thôi nào
Dialogue: 0,0:04:19.22,0:04:22.70,Default,Mio,0,0,0,,Có khi bàn giáo viên còn dư
Dialogue: 0,0:04:25.11,0:04:26.12,Default,Mio,0,0,0,,A!
Dialogue: 0,0:04:26.12,0:04:26.96,Default,Yukko,0,0,0,,Thật không!
Dialogue: 0,0:04:26.96,0:04:28.12,Default,,0,0,0,,Có à?
Dialogue: 0,0:04:30.12,0:04:32.79,Default,Yukko,0,0,0,,Khỉ gió!
Dialogue: 0,0:04:32.79,0:04:36.13,Default,,0,0,0,,Khỉ gió!
Dialogue: 0,0:04:36.87,0:04:38.03,Default,Mio,0,0,0,,Về lấy à?
Dialogue: 0,0:04:38.03,0:04:38.84,Default,Mio,0,0,0,,Sao mà kịp...
Dialogue: 0,0:04:44.01,0:04:45.34,Default,Yukko,0,0,0,,Dù có thế...
Dialogue: 0,0:04:45.34,0:04:46.22,Default,Yukko,0,0,0,,vẫn đi!
Dialogue: 0,0:04:54.10,0:04:56.02,Default,Manabu,0,0,0,,Sắp vào tiết!
Dialogue: 0,0:04:56.02,0:04:56.90,Default,Manabu,0,0,0,,Về chỗ
Dialogue: 0,0:05:41.03,0:05:44.28,Default,Professor,0,0,0,,Quay, quay, quay
Dialogue: 0,0:05:44.28,0:05:46.47,Default,Professor,0,0,0,,Lắc, lắc, lắc
Dialogue: 0,0:05:46.47,0:05:48.91,Default,Narrator,0,0,0,,Nhẹ nhàng sẽ quay
Dialogue: 0,0:05:51.91,0:05:53.91,Default,Yukko,0,0,0,,Lại bị bắt đứng
Dialogue: 0,0:05:54.34,0:05:57.79,Default,Yukko,0,0,0,,Sao mình quên hoài thế này
Dialogue: 0,0:05:58.65,0:06:01.74,Default,Yukko,0,0,0,,Có làm rồi cũng quên

Dialogue: 0,0:00:05.81,0:00:06.81,Default,Yukko,0,0,0,,Đến đầu!
Dialogue: 0,0:00:10.34,0:00:12.61,Default,Yukko,0,0,0,,Bọ sừng!
Dialogue: 0,0:00:12.61,0:00:14.74,Default,Takasaki,0,0,0,,Làm gì đấy Aioi?
Dialogue: 0,0:00:15.14,0:00:17.62,Default,Yukko,0,0,0,,Thầy đến gì sớm thế?
Dialogue: 0,0:00:17.62,0:00:19.24,Default,Takasaki,0,0,0,,Thầy trực mà
Dialogue: 0,0:00:19.66,0:00:22.12,Default,Yukko,0,0,0,,Đúng rồi, đổ rác hộ thầy
Dialogue: 0,0:00:22.12,0:00:25.36,Default,Yukko,0,0,0,,Thầy đừng kể ai nhé
Dialogue: 0,0:00:25.36,0:00:26.29,Default,Takasaki,0,0,0,,Cái đó để xem
Dialogue: 0,0:00:26.29,0:00:28.04,Default,Yukko,0,0,0,,Quá đáng!
Dialogue: 0,0:00:32.47,0:00:34.01,Default,Nano,0,0,0,,Mất hồn!
Dialogue: 0,0:02:05.85,0:02:06.98,Default,Naganohara,0,0,0,,Mio
Dialogue: 0,0:02:07.31,0:02:07.98,Default,Mio,0,0,0,,Hả?
Dialogue: 0,0:02:08.52,0:02:10.98,Default,Naganohara,0,0,0,,Chị làm mứt nè
Dialogue: 0,0:02:10.98,0:02:12.19,Default,Naganohara,0,0,0,,Ăn thử không?
Dialogue: 0,0:02:12.19,0:02:14.61,Default,Mio,0,0,0,,Tự dưng tốt thế
Dialogue: 0,0:02:15.32,0:02:19.07,Default,Naganohara,0,0,0,,Chị ăn hết mứt của em mà
Dialogue: 0,0:02:19.07,0:02:21.03,Default,Mio,0,0,0,,Thế à
Dialogue: 0,0:02:21.57,0:02:23.74,Default,Mio,0,0,0,,Nhưng chị làm mứt
Dialogue: 0,0:02:23.74,0:02:26.25,Default,Mio,0,0,0,,là nữ tính rồi
Dialogue: 0,0:02:26.55,0:02:28.75,Default,Naganohara,0,0,0,,Đừng có trêu chị!
Dialogue: 0,0:02:28.75,0:02:30.88,Default,Naganohara,0,0,0,,Đoán thử là mứt gì đi
Dialogue: 0,0:02:30.88,0:02:31.42,Default,Mio,0,0,0,,Rồi, rồi
Dialogue: 0,0:02:31.42,0:02:33.34,Default,Mio,0,0,0,,Để xem
Dialogue: 0,0:02:35.88,0:02:37.42,Default,Mio,0,0,0,,Dở quá!
Dialogue: 0,0:02:37.42,0:02:39.18,Default,Mio,0,0,0,,Dở, dở, dở, dở, dở ,dở!
Dialogue: 0,0:02:39.18,0:02:41.01,Default,Mio,0,0,0,,Dở đến chả biết gì
Dialogue: 0,0:02:41.34,0:02:42.26,Default,Naganohara,0,0,0,,Nè Mio
Dialogue: 0,0:02:42.26,0:02:43.97,Default,Naganohara,0,0,0,,Uống trà Nga nè!
Dialogue: 0,0:02:46.22,0:02:47.43,Default,Mio,0,0,0,,Nóng quá!
Dialogue: 0,0:02:47.79,0:02:48.60,Default,Mio,0,0,0,,Nóng! Nóng!
Dialogue: 0,0:02:48.60,0:02:50.94,Default,Mio,0,0,0,,Vừa nóng lại vừa dở!
Dialogue: 0,0:02:51.30,0:02:55.36,Default,Mio,0,0,0,,Dở, dở, dở, dở!
Dialogue: 0,0:02:52.98,0:02:53.86,Default-Alt,Naganohara,0,0,0,,Nè Mio
Dialogue: 0,0:02:53.86,0:02:55.36,Default-Alt,Naganohara,0,0,0,,Ăn chút mứt!
Dialogue: 0,0:02:55.36,0:02:56.90,Default,Mio,0,0,0,,Dở!
Dialogue: 0,0:02:56.90,0:02:58.86,Default,Mio,0,0,0,,Dở, dở, dở, dở, dở!
Dialogue: 0,0:02:58.86,0:03:00.95,Default,Mio,0,0,0,,Dở, dở, dở, dở, dở!
Dialogue: 0,0:03:01.37,0:03:02.07,Default,Mio,0,0,0,,Nước!
Dialogue: 0,0:03:02.66,0:03:03.49,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:03.49,0:03:04.41,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:04.41,0:03:05.37,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:05.37,0:03:06.29,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:06.29,0:03:07.29,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:07.29,0:03:08.16,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:08.16,0:03:09.29,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:09.29,0:03:11.71,Default,Mio,0,0,0,,Nước, nước, nước!
Dialogue: 0,0:03:11.71,0:03:13.79,Default,Mio,0,0,0,,Nước!
Dialogue: 0,0:03:19.30,0:03:20.17,Default,Mio,0,0,0,,Sữa!
Dialogue: 0,0:03:22.55,0:03:23.39,Default,Mio,0,0,0,,Sữa!
Dialogue: 0,0:03:23.39,0:03:24.22,Default,Mio,0,0,0,,Sữa!
Dialogue: 0,0:03:24.22,0:03:25.81,Default,Mio,0,0,0,,Sữa! Sữa! Sữa!
Dialogue: 0,0:03:27.97,0:03:31.94,Default,Naganohara,0,0,0,,Ủa Mio, mứt trong đó đấy
Dialogue: 0,0:03:33.65,0:03:34.77,Default,Mio,0,0,0,,Gì thế?
Dialogue: 0,0:03:34.77,0:03:36.93,Default,Mio,0,0,0,,Vị gì dở thế này!
Dialogue: 0,0:03:37.57,0:03:42.18,Default,Mio,0,0,0,,Dở, dở, dở, dở, dở, dở, dở, dở!
Dialogue: 0,0:03:42.18,0:03:44.53,Default,Mio,0,0,0,,Dở, dở, dở, dở, dở, dở, dở, dở!
Dialogue: 0,0:04:21.05,0:04:23.20,Default,Naganohara,0,0,0,,Nè Mio sao không?
Dialogue: 0,0:04:23.20,0:04:24.45,Default,Mio,0,0,0,,Sao không à?
Dialogue: 0,0:04:24.45,0:04:26.12,Default,Mio,0,0,0,,Sao không à?
Dialogue: 0,0:04:26.12,0:04:27.70,Default,Mio,0,0,0,,Có sao đó
Dialogue: 0,0:04:27.70,0:04:28.86,Default,Mio,0,0,0,,Dở tệ
Dialogue: 0,0:04:28.86,0:04:30.75,Default,,0,0,0,,Làm hồi tưởng lúc bé!
Dialogue: 0,0:04:31.12,0:04:32.87,Default,Naganohara,0,0,0,,Biết không ngon
Dialogue: 0,0:04:32.87,0:04:35.12,Default,Naganohara,0,0,0,,ai ngờ lại đến thế
Dialogue: 0,0:04:35.12,0:04:37.67,Default,Naganohara,0,0,0,,Sao làm thứ dở thế?
Dialogue: 0,0:04:38.07,0:04:40.59,Default,Naganohara,0,0,0,,Chị nghĩ là ngon mà
Dialogue: 0,0:04:40.59,0:04:42.05,Default,Mio,0,0,0,,Bà chị ngốc
Dialogue: 0,0:04:42.47,0:04:43.51,Default,Naganohara,0,0,0,,Thôi kệ
Dialogue: 0,0:04:43.51,0:04:47.22,Default,Naganohara,0,0,0,,Em nghĩ mứt gì nào?
Dialogue: 0,0:04:47.22,0:04:47.85,Default,Mio,0,0,0,,Ai biết!
Dialogue: 0,0:04:48.22,0:04:50.47,Default,Mio,0,0,0,,Chắc bỏ cá chết!
Dialogue: 0,0:04:52.09,0:04:52.93,Default,Naganohara,0,0,0,,Gần đúng
Dialogue: 0,0:04:58.73,0:05:00.11,Default,Naganohara,0,0,0,,Đáp án là
Dialogue: 0,0:05:00.11,0:05:01.78,Default,Naganohara,0,0,0,,mứt cá khô!
Dialogue: 0,0:05:01.78,0:05:03.53,Default,Mio,0,0,0,,Hôi quá!
Dialogue: 0,0:05:04.49,0:05:09.25,Default,Mio,0,0,0,,Lần đầu và cuối ăn cá khô muối!
Dialogue: 0,0:05:50.45,0:05:51.41,Default,Sakurai,0,0,0,,Mako ơi!
Dialogue: 0,0:05:52.49,0:05:53.91,Default,Sakurai,0,0,0,,Bố đi làm muộn
Dialogue: 0,0:05:53.91,0:05:55.20,Default,Sakurai,0,0,0,,chị đặt đồ ăn
Dialogue: 0,0:05:55.20,0:05:57.58,Default,Sakurai,0,0,0,,Em ăn gì nào?
Dialogue: 0,0:05:58.62,0:06:01.28,Default,Sakurai,0,0,0,,Bừa bộn quá...
Dialogue: 0,0:06:02.50,0:06:05.55,Default,Sakurai,0,0,0,,Bảo dọn mà vẫn bừa
Dialogue: 0,0:06:05.55,0:06:09.22,Default,Sakurai,0,0,0,,Tí phải la cho mới được
Dialogue: 0,0:06:27.99,0:06:31.53,Default,Sakurai,0,0,0,,Mình nghĩ gì thế?
Dialogue: 0,0:06:36.54,0:06:38.37,Default,Mako,0,0,0,,Chị làm gì đấy?
Dialogue: 0,0:06:39.46,0:06:41.13,Default,Sakurai,0,0,0,,Mất hồn!
Dialogue: 0,0:06:41.50,0:06:44.00,Default,Mako,0,0,0,,Đừng có vào phòng em!
Dialogue: 0,0:06:44.38,0:06:46.34,Default,Mako,0,0,0,,Lần tới vào là liệu
Dialogue: 0,0:06:46.34,0:06:48.30,Default,Mako,0,0,0,,Liệu đấy nhé!
Dialogue: 0,0:06:48.30,0:06:49.76,Default,Sakurai,0,0,0,,Chị xin lỗi
Dialogue: 0,0:07:05.11,0:07:06.41,Default,Sakurai,0,0,0,,Đúng rồi
Dialogue: 0,0:07:06.41,0:07:07.69,Default,,0,0,0,,Phải mắng nó!
Dialogue: 0,0:07:08.23,0:07:08.90,Default,Sakurai,0,0,0,,Nè!
Dialogue: 0,0:07:08.90,0:07:12.07,Default,Mako,0,0,0,,Bảo đừng có tự vào mà!
Dialogue: 0,0:07:12.07,0:07:13.20,Default,Sakurai,0,0,0,,À, xin lỗi!
Dialogue: 0,0:07:13.57,0:07:16.33,Default,Mako,0,0,0,,Mắc gì chị lại muốn dọn
Dialogue: 0,0:07:16.33,0:07:19.50,Default,Sakurai,0,0,0,,Ai bảo Mako toàn để bừa!
Dialogue: 0,0:07:19.50,0:07:21.58,Default,Mako,0,0,0,,Thế là đẹp rồi!
Dialogue: 0,0:07:21.88,0:07:23.84,Default,Mako,0,0,0,,Đừng có vào phòng!
Dialogue: 0,0:07:28.63,0:07:30.17,Default,Sakurai,0,0,0,,Mako ơi!
Dialogue: 0,0:07:30.17,0:07:32.89,Default,Mako,0,0,0,,Nói đừng có vào mà!
Dialogue: 0,0:07:34.60,0:07:38.64,Default,Sakurai,0,0,0,,Em chưa đến tuổi coi cái này!
Dialogue: 0,0:07:39.93,0:07:44.77,Default,Sakurai,0,0,0,,Em chưa đến tuổi coi cái này!
Dialogue: 0,0:07:44.77,0:07:49.90,Default,Sakurai,0,0,0,,Em chưa đến tuổi coi cái này!
Dialogue: 0,0:07:51.36,0:07:53.03,Default,Mako,0,0,0,,Chị...
Dialogue: 0,0:07:56.28,0:07:57.20,Default,Mako,0,0,0,,Chị lấy...
Dialogue: 0,0:08:14.63,0:08:15.55,Default,Mako,0,0,0,,Cái gì đấy?
Dialogue: 0,0:08:15.55,0:08:16.89,Default,Mako,0,0,0,,Chả hiểu!
Dialogue: 0,0:08:16.89,0:08:17.89,Default,Mako,0,0,0,,Đâu phải của em!
Dialogue: 0,0:08:17.89,0:08:20.68,Default,Sakurai,0,0,0,,Thấy trong phòng em mà!
Dialogue: 0,0:08:20.68,0:08:21.60,Default,Mako,0,0,0,,Hả?
Dialogue: 0,0:08:21.60,0:08:23.31,Default,Mako,0,0,0,,Lần đầu thấy mà
Dialogue: 0,0:08:23.31,0:08:26.56,Default,Mako,0,0,0,,Đây phòng em hồi nào?
Dialogue: 0,0:08:26.56,0:08:28.77,Default,Mako,0,0,0,,Chắc gì là phòng em
Dialogue: 0,0:08:28.77,0:08:31.32,Default,Sakurai,0,0,0,,Nói chung chưa đủ tuổi!
Dialogue: 0,0:08:31.32,0:08:32.78,Default,Sakurai,0,0,0,,Dẹp đi!
Dialogue: 0,0:08:32.78,0:08:35.36,Default,Mako,0,0,0,,Chị nói gì đấy!
Dialogue: 0,0:08:35.36,0:08:37.70,Default,Mako,0,0,0,,Đùa thế không hay đâu!
Dialogue: 0,0:08:38.12,0:08:40.49,Default,Sakurai,0,0,0,,Vậy để chị vứt!
Dialogue: 0,0:09:02.35,0:09:04.44,Default,Mako,0,0,0,,Của hội trưởng mà!
Dialogue: 0,0:09:04.44,0:09:04.98,Default,Sakurai,0,0,0,,Hả?
Dialogue: 0,0:09:05.39,0:09:08.61,Default,Mako,0,0,0,,Đừng có vứt đồ người ta!
Dialogue: 0,0:09:08.61,0:09:09.82,Default,Sakurai,0,0,0,,Xin lỗi!
Dialogue: 0,0:09:10.36,0:09:16.28,Default,Sakurai,0,0,0,,Chị xem thử, thì thấy có hơi...
Dialogue: 0,0:09:16.28,0:09:18.91,Default,Sakurai,0,0,0,,Em đừng coi cái này
Dialogue: 0,0:09:18.91,0:09:20.58,Default,Sakurai,0,0,0,,Thành trẻ hư đó
Dialogue: 0,0:09:20.58,0:09:22.45,Default,Mako,0,0,0,,Biết rồi!
Dialogue: 0,0:09:22.45,0:09:25.13,Default,Mako,0,0,0,,Không xem, đừng vào phòng em!
Dialogue: 0,0:09:27.75,0:09:30.29,Default,Mako,0,0,0,,Trời, khổ quá
Dialogue: 0,0:09:30.29,0:09:32.13,Default,Mako,0,0,0,,Sao thầy Takasaki lại thích?
Dialogue: 0,0:09:32.13,0:09:33.78,Default,Sakurai,0,0,0,,Mako giận à?
Dialogue: 0,0:09:33.78,0:09:35.88,Default,Mako,0,0,0,,Gõ cửa hộ coi!
Dialogue: 0,0:09:36.28,0:09:39.72,Default,Sakurai,0,0,0,,Chị nghĩ em giận...
Dialogue: 0,0:09:39.72,0:09:40.85,Default,Mako,0,0,0,,Không giận!
Dialogue: 0,0:09:40.85,0:09:42.97,Default,Sakurai,0,0,0,,Đấy? Giận mà...
Dialogue: 0,0:09:42.97,0:09:44.35,Default,Mako,0,0,0,,Không giận!
Dialogue: 0,0:09:44.35,0:09:46.31,Default,Mako,0,0,0,,Không giận, ra khỏi phòng em!
Dialogue: 0,0:09:46.81,0:09:48.23,Default,Mako,0,0,0,,Đừng có vào!
Dialogue: 0,0:09:48.23,0:09:48.98,Default,Mako,0,0,0,,Coi chừng đó!
Dialogue: 0,0:09:48.98,0:09:51.11,Default,Mako,0,0,0,,Nhớ đấy!
Dialogue: 0,0:09:51.52,0:09:53.98,Default,Mako,0,0,0,,Muốn sống một mình quá...
Dialogue: 0,0:09:55.36,0:09:57.70,Default,Sakurai,0,0,0,,Mình bậy quá
Dialogue: 0,0:09:58.14,0:10:00.82,Default,Sakurai,0,0,0,,Nó bảo phòng thế được rồi
Dialogue: 0,0:10:00.82,0:10:03.16,Default,Sakurai,0,0,0,,sách không phải của Mako
Dialogue: 0,0:10:03.74,0:10:07.79,Default,Sakurai,0,0,0,,Dạo này trường kỉ cương lỏng quá
Dialogue: 0,0:10:07.79,0:10:11.88,Default,Sakurai,0,0,0,,Ở nhà mình cũng phải nghiêm!
Dialogue: 0,0:10:11.88,0:10:13.70,Default,Sakurai,0,0,0,,Mình là giám thị mà
Dialogue: 0,0:10:14.96,0:10:17.13,Default,Sakurai,0,0,0,,À, quên mất!
Dialogue: 0,0:10:17.46,0:10:19.41,Default,Sakurai,0,0,0,,Mako, chị tính đặt-
Dialogue: 0,0:10:19.41,0:10:22.14,Default,Mako,0,0,0,,Bao giờ mới chịu hiểu!
Dialogue: 0,0:10:30.23,0:10:34.53,Default,Sakurai,0,0,0,,À, phần định hướng tương lai
Dialogue: 0,0:10:34.53,0:10:38.36,Default,Sakurai,0,0,0,,Em có mục tiêu hay mơ ước gì không?
Dialogue: 0,0:10:38.36,0:10:39.40,Default,Yukko,0,0,0,,Có...
Dialogue: 0,0:10:47.08,0:10:48.00,Default,Sakurai,0,0,0,,À...
Dialogue: 0,0:10:48.41,0:10:51.26,Default,Yukko,0,0,0,,Em muốn được lên TV
Dialogue: 0,0:10:52.79,0:10:56.30,Default,Sakurai,0,0,0,,À, không phải thế
Dialogue: 0,0:10:56.30,0:10:57.51,Default,Sakurai,0,0,0,,Cô xin lỗi
Dialogue: 0,0:10:57.51,0:10:59.80,Default,,0,0,0,,Hỏi chắc chưa rõ
Dialogue: 0,0:10:59.80,0:11:04.60,Default,Sakurai,0,0,0,,Em muốn làm nghề nghiệp gì nhỉ?
Dialogue: 0,0:11:04.60,0:11:08.56,Default,Yukko,0,0,0,,À, để coi
Dialogue: 0,0:11:08.56,0:11:12.02,Default,Yukko,0,0,0,,À...
Dialogue: 0,0:11:12.69,0:11:15.52,Default,Yukko,0,0,0,,Làm giáo viên như cô Sakurai
Dialogue: 0,0:11:17.15,0:11:19.78,Default,Sakurai,0,0,0,,Cô à?
Dialogue: 0,0:11:22.64,0:11:23.82,Default,Yukko,0,0,0,,Dạ
Dialogue: 0,0:11:23.82,0:11:27.58,Default,Yukko,0,0,0,,À...
Dialogue: 0,0:11:31.50,0:11:34.92,Default,??,0,0,0,,Nỗ lực của mọi người...
Dialogue: 0,0:11:35.42,0:11:38.67,Default,??,0,0,0,,Cố lên nào Nakamura!
Dialogue: 0,0:11:38.67,0:11:43.55,Default,Nakamura,0,0,0,,Lần chạy cuối rồi!
Dialogue: 0,0:11:51.19,0:11:52.27,Default,Announcer,0,0,0,,Bỏ cuộc!
Dialogue: 0,0:11:52.27,0:11:55.13,Default,Announcer,0,0,0,,Trường Tokisadame bỏ cuộc
Dialogue: 0,0:12:04.70,0:12:05.99,Default,Mio,0,0,0,,Nano, Nano ơi
Dialogue: 0,0:12:05.99,0:12:07.33,Default,Nano,0,0,0,,Sao nào?
Dialogue: 0,0:12:07.74,0:12:11.33,Default,Mio,0,0,0,,Lúc về gặp giáo sư
Dialogue: 0,0:12:11.33,0:12:13.79,Default,Mio,0,0,0,,rồi bị chó cắn
Dialogue: 0,0:12:13.79,0:12:16.04,Default,Nano,0,0,0,,Tội các cậu quá
Dialogue: 0,0:12:16.04,0:12:19.59,Default,Yukko,0,0,0,,Bảo giáo sư mình đến nữa nhé
Dialogue: 0,0:12:20.05,0:12:21.38,Default,Nano,0,0,0,,Được rồi
Dialogue: 0,0:12:26.01,0:12:28.39,Default,Mako,0,0,0,,Thầy làm cố vấn đi ạ!
Dialogue: 0,0:12:28.39,0:12:31.02,Default,Takasaki,0,0,0,,Không được đi!
Dialogue: 0,0:12:31.02,0:12:32.17,Default,Mako,0,0,0,,Đùa à!
Dialogue: 0,0:12:32.17,0:12:35.06,Default,,0,0,0,,Bữa thầy có ý rồi!
Dialogue: 0,0:12:35.06,0:12:36.56,Default,Takasaki,0,0,0,,Đã nói gì đâu
Dialogue: 0,0:12:36.56,0:12:39.24,Default,Mako,0,0,0,,Không ngờ được...
Dialogue: 0,0:12:39.24,0:12:44.32,Default,Mako,0,0,0,,Thầy bảo em thắng thì thầy làm cố vấn!
Dialogue: 0,0:12:46.03,0:12:47.66,Default,Takasaki,0,0,0,,Hồi đó à?
Dialogue: 0,0:12:48.01,0:12:48.65,Default,Takasaki,0,0,0,,Khoan!
Dialogue: 0,0:12:48.65,0:12:51.41,Default,,0,0,0,,Chúng ta hòa nhau mà!
Dialogue: 0,0:12:52.37,0:12:56.00,Default,Mako,0,0,0,,Rồi, em nói vụ chị gái vậy!
Dialogue: 0,0:13:00.23,0:13:02.76,Default,Takasaki,0,0,0,,Phòng clb ở đâu?
Dialogue: 0,0:13:02.76,0:13:03.92,Default,Mako,0,0,0,,Lối này!
Dialogue: 0,0:13:03.92,0:13:04.72,Default,Mako,0,0,0,,Rồi!
Dialogue: 0,0:13:04.72,0:13:07.14,Default,Mako,0,0,0,,Hôm nay clb hồi sinh!
Dialogue: 0,0:13:07.14,0:13:08.48,Default,Mako,0,0,0,,Rồi!
Dialogue: 0,0:13:11.81,0:13:13.02,Default,Mako,0,0,0,,Hội trưởng!
Dialogue: 0,0:13:13.02,0:13:14.60,Default,Daiku,0,0,0,,Hả?
Dialogue: 0,0:13:14.60,0:13:17.06,Default,Mako,0,0,0,,E-em quấy rầy rồi!
Dialogue: 0,0:13:17.77,0:13:21.23,Default,Mako,0,0,0,,Em kiếm được cố vấn!
Dialogue: 0,0:13:21.23,0:13:22.65,Default,Mako,0,0,0,,Nè, khoan!
Dialogue: 0,0:13:22.65,0:13:24.24,Default,Mako,0,0,0,,Sao lại chạy!
Dialogue: 0,0:13:24.24,0:13:26.32,Default,Mako,0,0,0,,Thầy Takasaki
Dialogue: 0,0:13:26.74,0:13:28.53,Default,Mako,0,0,0,,Chạy mất rồi
Dialogue: 0,0:13:28.53,0:13:31.29,Default,Daiku,0,0,0,,Đang tính kiếm thành viên
Dialogue: 0,0:13:31.29,0:13:35.66,Default,Daiku,0,0,0,,Không có Sakurai sao giải thích về clb
Dialogue: 0,0:13:37.76,0:13:38.79,Default,Daiku,0,0,0,,Kệ đi
Dialogue: 0,0:13:38.79,0:13:41.84,Default,Daiku,0,0,0,,Clb mình mà
Dialogue: 0,0:13:49.53,0:13:52.14,Default,Fechan,0,0,0,,Lo quá...
Dialogue: 0,0:13:52.14,0:13:54.56,Default,Websey,0,0,0,,Lo gì đâu
Dialogue: 0,0:13:54.87,0:13:57.48,Default,Fechan,0,0,0,,Cậu đang đi bày tỏ...
Dialogue: 0,0:13:57.48,0:13:59.23,Default,Fechan,0,0,0,,với cái cậu Nakanojou mà
Dialogue: 0,0:13:59.23,0:13:59.90,Default,Websey,0,0,0,,Đúng!
Dialogue: 0,0:13:59.90,0:14:02.82,Default,Websey,0,0,0,,Côn đồ trong trường này
Dialogue: 0,0:14:02.82,0:14:04.57,Default,Websey,0,0,0,,Mào gà đẹp mà!
Dialogue: 0,0:14:05.07,0:14:08.78,Default,Feccan,0,0,0,,Đẹp gì, độc thì có
Dialogue: 0,0:14:08.78,0:14:09.91,Default,WebChan,0,0,0,,Úi!
Dialogue: 0,0:14:10.24,0:14:11.20,Default,Mako,0,0,0,,Thầy ơi!
Dialogue: 0,0:14:11.61,0:14:13.29,Default,Mako,0,0,0,,Đứng lại coi!
Dialogue: 0,0:14:28.38,0:14:31.55,Default,Mako,0,0,0,,Thầy Takasaki ơi!
Dialogue: 0,0:14:31.55,0:14:32.89,Default,Mako,0,0,0,,Sao thầy lại chạy?
Dialogue: 0,0:14:33.47,0:14:37.18,Default,Mako,0,0,0,,Em chưa đấu trận nào máu lửa thế!
Dialogue: 0,0:14:37.56,0:14:40.73,Default,Mako,0,0,0,,Ngoài thầy còn ai làm nổi!
Dialogue: 0,0:14:41.77,0:14:42.73,Default,Takasaki,0,0,0,,Xin lỗi
Dialogue: 0,0:14:42.73,0:14:46.61,Default,Takasaki,0,0,0,,Nhưng mà, thầy có...
Dialogue: 0,0:14:46.61,0:14:50.95,Default,Takasaki,0,0,0,,À, mấy cái...
Dialogue: 0,0:14:50.95,0:14:53.53,Default,Takasaki,0,0,0,,Là...như
Dialogue: 0,0:14:54.53,0:14:56.37,Default,Mako,0,0,0,,Thầy đến clb rồi!
Dialogue: 0,0:14:57.33,0:14:58.62,Default,Mako,0,0,0,,Đành thế!
Dialogue: 0,0:14:59.46,0:15:00.57,Default,Mako,0,0,0,,Thầy ơi!
Dialogue: 0,0:15:01.86,0:15:04.95,Default,Mako,0,0,0,,Nhìn cái này rồi vẫn chối sao?
Dialogue: 0,0:15:14.10,0:15:14.72,Default,Takasaki,0,0,0,,Em!
Dialogue: 0,0:15:14.72,0:15:16.52,Default,Takasaki,0,0,0,,Cái này...
Dialogue: 0,0:15:16.52,0:15:17.56,Default,Mako,0,0,0,,Đúng rồi
Dialogue: 0,0:15:17.56,0:15:20.48,Default,Mako,0,0,0,,Chị em hồi đi học
Dialogue: 0,0:15:20.86,0:15:22.90,Default,Mako,0,0,0,,Thầy tính sao nào?
Dialogue: 0,0:15:22.90,0:15:25.36,Default,Mako,0,0,0,,Đổi tấm ảnh này không?
Dialogue: 0,0:15:28.78,0:15:29.78,Default,Takasaki,0,0,0,,Mình...
Dialogue: 0,0:15:29.78,0:15:30.95,Default,Takasaki,0,0,0,,muốn quá!
Dialogue: 0,0:15:30.95,0:15:33.89,Default,Takasaki,0,0,0,,Siêu muốn luôn!
Dialogue: 0,0:15:33.89,0:15:39.06,Default,Takasaki,0,0,0,,Có linh cảm xấu nếu nhận lời làm!
Dialogue: 0,0:15:39.06,0:15:39.79,Default,Takasaki,0,0,0,,Sakurai...
Dialogue: 0,0:15:40.50,0:15:41.37,Default,Mako,0,0,0,,Dạ
Dialogue: 0,0:15:45.46,0:15:46.59,Default,Takasaki,0,0,0,,Xin phép...
Dialogue: 0,0:15:52.80,0:15:56.22,Default,Takasaki,0,0,0,,Giá này quá tốt!
Dialogue: 0,0:15:56.22,0:15:58.47,Default,Takasaki,0,0,0,,Làm gì thế?
Dialogue: 0,0:15:58.47,0:15:59.72,Default,Mako,0,0,0,,Tuyệt vời!
Dialogue: 0,0:15:59.72,0:16:01.60,Default,,0,0,0,,Thầy làm cố vấn ha?
Dialogue: 0,0:16:02.77,0:16:04.15,Default,Takasaki,0,0,0,,Tệ hại...
Dialogue: 0,0:16:04.15,0:16:06.31,Default,Takasaki,0,0,0,,Giáo viên mà lại thế...
Dialogue: 0,0:16:06.73,0:16:11.03,Default,Takasaki,0,0,0,,Dù là giáo viên, mình vẫn là sói!
Dialogue: 0,0:16:11.61,0:16:14.11,Default,Takasaki,0,0,0,,Khỏi cần ra vẻ!
Dialogue: 0,0:16:15.42,0:16:16.32,Default,Takasaki,0,0,0,,Nghe này, Sakurai!
Dialogue: 0,0:16:16.69,0:16:18.79,Default,Takasaki,0,0,0,,Làm phải hết sức!
Dialogue: 0,0:16:20.00,0:16:21.29,Default,Takasaki,0,0,0,,Biết chưa?
Dialogue: 0,0:16:21.29,0:16:21.83,Default,Mako,0,0,0,,Vâng!
Dialogue: 0,0:16:22.25,0:16:25.00,Default,Mako,0,0,0,,Clb cờ bóng khai trương!
Dialogue: 0,0:16:25.00,0:16:27.63,Default,Takasaki,0,0,0,,Mai mình luyện tập!
Dialogue: 0,0:16:27.63,0:16:28.59,Default,Mako,0,0,0,,Dạ!
Dialogue: 0,0:16:34.68,0:16:36.30,Default,Websey,0,0,0,,Đây này
Dialogue: 0,0:16:36.59,0:16:38.51,Default,Annaka,0,0,0,,Thế à
Dialogue: 0,0:16:38.51,0:16:39.76,Default,Websey,0,0,0,,Nè!
Dialogue: 0,0:16:39.76,0:16:40.43,Default,Annaka,0,0,0,,Dạ?
Dialogue: 0,0:16:41.10,0:16:42.85,Default,Websey,0,0,0,,Có Nakanojou không?
Dialogue: 0,0:16:42.85,0:16:44.60,Default,Annaka,0,0,0,,Có
Dialogue: 0,0:16:44.60,0:16:46.44,Default,Annaka,0,0,0,,Nakanojou ơi
Dialogue: 0,0:16:46.76,0:16:48.52,Default,Annaka,0,0,0,,Nakanojou ơi, có người gọi!
Dialogue: 0,0:16:52.65,0:16:55.11,Default,Nakanojo,0,0,0,,Đây, sao nào?
Dialogue: 0,0:17:01.04,0:17:04.37,Default,WebChan,0,0,0,,Chả có gì...
Dialogue: 0,0:17:26.14,0:17:28.61,Default,Girl,0,0,0,,Bố, xem hộ con nhé?
Dialogue: 0,0:17:28.61,0:17:29.69,Default,Dad,0,0,0,,Rồi
Dialogue: 0,0:17:38.82,0:17:40.49,Default,Girl,0,0,0,,Sao ạ?
Dialogue: 0,0:17:55.09,0:17:56.26,Default,Girl,0,0,0,,Là sao?
Dialogue: 0,0:18:20.37,0:18:22.46,Default,Train,0,0,0,,Sắp đến bến
Dialogue: 0,0:18:33.67,0:18:36.50,Default,Nano,0,0,0,,Minakami có gì thế?
Dialogue: 0,0:18:37.34,0:18:38.63,Default,Mai,0,0,0,,Mình đến chơi
Dialogue: 0,0:18:41.97,0:18:45.97,Default,Nano,0,0,0,,À, cậu mang quà luôn
Dialogue: 0,0:18:45.97,0:18:47.10,Default,Professor,0,0,0,,À!
Dialogue: 0,0:18:47.10,0:18:49.64,Default,Professor,0,0,0,,Người này để chó dọa em!
Dialogue: 0,0:18:49.64,0:18:50.10,Default,Nano,0,0,0,,Hả?
Dialogue: 0,0:18:57.19,0:18:58.95,Default,Professor,0,0,0,,Đúng rồi đó!
Dialogue: 0,0:19:00.95,0:19:03.78,Default,Nano,0,0,0,,Để mình pha trà
Dialogue: 0,0:19:09.83,0:19:11.50,Default,Professor,0,0,0,,Đến đây chi?
Dialogue: 0,0:19:16.00,0:19:17.13,Default,Mai,0,0,0,,Đây
Dialogue: 0,0:19:17.46,0:19:18.67,Default,Professor,0,0,0,,Gì đây?
Dialogue: 0,0:19:18.67,0:19:20.05,Default,Mai,0,0,0,,Yamantaka
Dialogue: 0,0:19:20.05,0:19:21.34,Default,Professor,0,0,0,,Không thích
Dialogue: 0,0:19:22.18,0:19:23.35,Default,Mai,0,0,0,,Ngầu mà
Dialogue: 0,0:19:23.35,0:19:25.22,Default,Professor,0,0,0,,Ngầu cái gì
Dialogue: 0,0:19:30.44,0:19:32.02,Default,Mai,0,0,0,,Hay mà
Dialogue: 0,0:19:32.02,0:19:33.19,Default,Professor,0,0,0,,Gì đó?
Dialogue: 0,0:19:33.19,0:19:35.15,Default,Mai,0,0,0,,Nhà kiểu 36
Dialogue: 0,0:19:35.46,0:19:37.11,Default,Professor,0,0,0,,Hay ho gì
Dialogue: 0,0:19:37.53,0:19:39.49,Default,Mai,0,0,0,,Không nhà vệ sinh
Dialogue: 0,0:19:39.49,0:19:40.90,Default,Professor,0,0,0,,Hay cái gì
Dialogue: 0,0:19:51.41,0:19:52.71,Default,Mai,0,0,0,,Vẽ gì đấy?
Dialogue: 0,0:19:52.71,0:19:54.08,Default,Professor,0,0,0,,Không nói
Dialogue: 0,0:20:03.01,0:20:03.97,Default,Mai,0,0,0,,Đây
Dialogue: 0,0:20:05.51,0:20:06.85,Default,Mai,0,0,0,,Cá mập xanh
Dialogue: 0,0:20:11.10,0:20:13.06,Default,Professor,0,0,0,,Ngầu ha!
Dialogue: 0,0:20:14.31,0:20:15.23,Default,Mai,0,0,0,,Cho đó!
Dialogue: 0,0:20:16.31,0:20:18.73,Default,Professor,0,0,0,,Nè, vẽ cá mập khác ha!
Dialogue: 0,0:20:18.73,0:20:19.32,Default,Mai,0,0,0,,Rồi!
Dialogue: 0,0:20:19.32,0:20:21.32,Default,Professor,0,0,0,,Vẽ cá mập cười!
Dialogue: 0,0:20:21.32,0:20:21.95,Default,Mai,0,0,0,,Rồi
Dialogue: 0,0:20:21.95,0:20:24.36,Default,Professor,0,0,0,,Mình đổi ha?
Dialogue: 0,0:20:24.36,0:20:25.38,Default,Mai,0,0,0,,Rồi
Dialogue: 0,0:20:26.66,0:20:28.41,Default,Nano,0,0,0,,Chờ lâu!
Dialogue: 0,0:20:31.33,0:20:32.46,Default,Nano,0,0,0,,Hả?
Dialogue: 0,0:20:35.79,0:20:39.25,Default,Nano,0,0,0,,Giáo sư, Minakami về rồi à?
Dialogue: 0,0:20:39.25,0:20:43.26,Default,Professor,0,0,0,,Ừ, em bảo nhớ quay lại vẽ
Dialogue: 0,0:20:43.26,0:20:44.59,Default,Nano,0,0,0,,Thế à
Dialogue: 0,0:20:46.22,0:20:48.35,Default,Nano,0,0,0,,Đến đây chi nhỉ

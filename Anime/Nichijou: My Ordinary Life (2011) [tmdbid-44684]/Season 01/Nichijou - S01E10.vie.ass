Dialogue: 0,0:21:54.75,0:21:58.00,ED English,,0,0,0,,Thức dậy mỗi sáng, đánh răng rửa mặt
Dialogue: 0,0:21:58.00,0:22:01.13,ED English,,0,0,0,,Mới ngoảnh lại đã 10 giờ đêm
Dialogue: 0,0:22:01.13,0:22:04.21,ED English,,0,0,0,,Hôm nay cười hoài à
Dialogue: 0,0:22:04.21,0:22:07.22,ED English,,0,0,0,,Chơi vui lắm cơ
Dialogue: 0,0:22:07.22,0:22:13.60,ED English,,0,0,0,,Ngốc nghếch vui vẻ bên bạn bè cả ngày nè
Dialogue: 0,0:22:13.60,0:22:16.68,ED English,,0,0,0,,Bỗng hai ta nhìn nhau
Dialogue: 0,0:22:16.68,0:22:19.35,ED English,,0,0,0,,tình cờ thôi nhỉ
Dialogue: 0,0:22:19.35,0:22:22.86,ED English,,0,0,0,,Không phải là tình đầu đâu nha
Dialogue: 0,0:22:22.86,0:22:26.03,ED English,,0,0,0,,Em không phải là thế này
Dialogue: 0,0:22:26.03,0:22:32.08,ED English,,0,0,0,,Nè, mai ta lại gặp nhau nữa nha
Dialogue: 0,0:22:32.08,0:22:35.16,ED English,,0,0,0,,Mắt nhắm chặt lại
Dialogue: 0,0:22:35.16,0:22:38.29,ED English,,0,0,0,,anh cứ lấp la lấp lánh trong em
Dialogue: 0,0:22:38.29,0:22:41.63,ED English,,0,0,0,,Hôm nay vui ghê ha
Dialogue: 0,0:22:41.63,0:22:45.55,ED English,,0,0,0,,A, em yêu mất rồi
Dialogue: 0,0:22:50.97,0:22:54.10,ED English,,0,0,0,,Vui quá, vui ghê
Dialogue: 0,0:22:54.10,0:22:57.18,ED English,,0,0,0,,Gặp mối tình này em vui lắm
Dialogue: 0,0:22:57.18,0:23:00.35,ED English,,0,0,0,,Mai ta lại gặp nhau nhé
Dialogue: 0,0:23:00.35,0:23:02.52,ED English,,0,0,0,,Chúc anh ngủ ngon
Dialogue: 0,0:23:03.44,0:23:07.57,ED English,,0,0,0,,Giờ ta gặp trong giấc mơ
Dialogue: 0,0:23:08.07,0:23:13.66,ED English,,0,0,0,,Chúc anh ngủ ngon ha
Dialogue: 0,0:00:08.15,0:00:10.03,Default,Mai,0,0,0,,Đèn đỏ...
Dialogue: 0,0:00:10.03,0:00:11.11,Default,Mai,0,0,0,,Đèn xanh...
Dialogue: 0,0:02:39.13,0:02:40.47,Default,Yukko,0,0,0,,Chết rồi
Dialogue: 0,0:02:40.47,0:02:41.64,Default,Yukko,0,0,0,,Chết rồi!
Dialogue: 0,0:02:41.64,0:02:43.51,Default,Yukko,0,0,0,,Sinh hoạt rồi!
Dialogue: 0,0:02:44.22,0:02:46.02,Default,Yukko,0,0,0,,Em giúp bà cụ!
Dialogue: 0,0:02:46.02,0:02:47.39,Default,Yukko,0,0,0,,Tuần trước rồi
Dialogue: 0,0:02:47.39,0:02:50.44,Default,Yukko,0,0,0,,Mẹ vắng nhà không được
Dialogue: 0,0:02:51.31,0:02:52.25,Default,Yukko,0,0,0,,Sao giờ?
Dialogue: 0,0:02:52.25,0:02:54.28,Default,,0,0,0,,Cần lí do!
Dialogue: 0,0:02:57.44,0:03:00.07,Default,Yukko,0,0,0,,Sao không có xe nào?
Dialogue: 0,0:03:03.03,0:03:05.79,Default,Yukko,0,0,0,,Kẹt đèn đỏ 4 lần lận
Dialogue: 0,0:03:05.79,0:03:09.58,Default,Yukko,0,0,0,,Vào tiết đầu luôn rồi
Dialogue: 0,0:03:14.25,0:03:16.71,Default,Yukko,0,0,0,,Đây là bẫy mà
Dialogue: 0,0:03:18.01,0:03:20.76,Default,Yukko,0,0,0,,Cái trò cũ rích-
Dialogue: 0,0:03:21.47,0:03:22.18,Default,Yukko,0,0,0,,Hai bên?
Dialogue: 0,0:03:22.64,0:03:23.34,Default,Yukko,0,0,0,,Hả?
Dialogue: 0,0:03:23.34,0:03:26.22,Default,,0,0,0,,Phạt mình đi trễ sao?
Dialogue: 0,0:03:26.22,0:03:28.35,Default,Yukko,0,0,0,,Mình làm gì sai
Dialogue: 0,0:03:28.35,0:03:29.35,Default,Yukko,0,0,0,,Trễ tí mà
Dialogue: 0,0:03:29.68,0:03:33.44,Default,Yukko,0,0,0,,Thôi thì đi cửa sau
Dialogue: 0,0:03:33.44,0:03:37.69,Default,Yukko,0,0,0,,Có gì đâu mà la
Dialogue: 0,0:03:41.45,0:03:42.66,Default,Yukko,0,0,0,,Bị...
Dialogue: 0,0:03:42.66,0:03:46.58,Default,Yukko,0,0,0,,khóa mất rồi
Dialogue: 0,0:03:46.58,0:03:47.34,Default,Yukko,0,0,0,,Đúng rồi
Dialogue: 0,0:03:47.34,0:03:50.12,Default,,0,0,0,,Đi cửa dưới
Dialogue: 0,0:03:50.12,0:03:54.75,Default,Yukko,0,0,0,,Rồi, lén chui vào lớp như không luôn
Dialogue: 0,0:03:56.79,0:03:57.75,Default,Yukko,0,0,0,,Tại sao?
Dialogue: 0,0:03:58.21,0:04:00.26,Default,Yukko,0,0,0,,Biết ai rồi!
Dialogue: 0,0:04:00.26,0:04:03.63,Default,Yukko,0,0,0,,Chỉ nữ hoàng mới làm được thôi
Dialogue: 0,0:04:03.63,0:04:06.17,Default,Yukko,0,0,0,,Đi cửa dưới bị chụp
Dialogue: 0,0:04:06.17,0:04:09.01,Default,,0,0,0,,thì còn mệt nữa
Dialogue: 0,0:04:09.01,0:04:10.14,Default,Yukko,0,0,0,,Nếu thế thì
Dialogue: 0,0:04:10.14,0:04:11.60,Default,,0,0,0,,dùng cửa sổ!
Dialogue: 0,0:04:33.62,0:04:36.83,Default,Yukko,0,0,0,,Có kính hồi nào đấy!
Dialogue: 0,0:04:36.83,0:04:38.38,Default,Yukko,0,0,0,,Khoan, khoan, khoan, khoan
Dialogue: 0,0:04:38.38,0:04:40.34,Default,Yukko,0,0,0,,Mắc gì lại chạy
Dialogue: 0,0:04:40.34,0:04:43.84,Default,Yukko,0,0,0,,Chạy cũng không được gì
Dialogue: 0,0:04:43.84,0:04:45.55,Default,Yukko,0,0,0,,Quay lại vậy
Dialogue: 0,0:04:46.09,0:04:50.26,Default,Yukko,0,0,0,,Đúng, quan trọng là giờ chọn lối nào
Dialogue: 0,0:04:50.60,0:04:54.10,Default,Yukko,0,0,0,,Nếu mà không thoát được
Dialogue: 0,0:04:54.10,0:04:57.35,Default,Yukko,0,0,0,,thì thà ráng chọn cách
Dialogue: 0,0:04:57.35,0:04:58.56,Default,Yukko,0,0,0,,ngầu nhất
Dialogue: 0,0:04:58.90,0:05:00.57,Default,Yukko,0,0,0,,Đúng, tốt nhất...
Dialogue: 0,0:05:00.57,0:05:02.07,Default,Yukko,0,0,0,,là cửa trước!
Dialogue: 0,0:05:02.53,0:05:06.49,Default,Yukko,0,0,0,,Cười lớn khi rớt trúng đầu!
Dialogue: 0,0:05:07.28,0:05:09.33,Default,Yukko,0,0,0,,Chào mọi người!
Dialogue: 0,0:05:10.28,0:05:13.29,Default,Yukko,0,0,0,,Cả lớp cười òa lên!
Dialogue: 0,0:05:13.29,0:05:15.83,Default,Yukko,0,0,0,,Biết mấy giờ rồi không
Dialogue: 0,0:05:17.88,0:05:19.96,Default,Takasaki,0,0,0,,Em đúng là
Dialogue: 0,0:05:19.96,0:05:22.09,Default,Takasaki,0,0,0,,nhanh về chỗ đi
Dialogue: 0,0:05:22.09,0:05:22.96,Default,Yukko,0,0,0,,Đúng rồi!
Dialogue: 0,0:05:23.30,0:05:24.76,Default,Yukko,0,0,0,,Chính xác Yukko!
Dialogue: 0,0:05:24.76,0:05:28.59,Default,Yukko,0,0,0,,Cứ vui vẻ thoải mái!
Dialogue: 0,0:05:28.59,0:05:29.14,Default,Yukko,0,0,0,,Rồi!
Dialogue: 0,0:05:29.51,0:05:30.14,Default,Yukko,0,0,0,,Vào trận!
Dialogue: 0,0:05:32.22,0:05:35.18,Default,Yukko,0,0,0,,Sáng vui vẻ nhé mọi người!
Dialogue: 0,0:05:35.18,0:05:35.89,Default,Yukko,0,0,0,,Đùa đó!
Dialogue: 0,0:06:19.40,0:06:23.11,Default,Yukko,0,0,0,,Oh ho ho, người ngoài hành tinh đây
Dialogue: 0,0:06:27.07,0:06:29.36,Default,Mio,0,0,0,,Làm trò gì đấy?
Dialogue: 0,0:06:29.36,0:06:31.03,Default,Mio,0,0,0,,Có phải tiểu học đâu
Dialogue: 0,0:06:31.03,0:06:35.04,Default,Yukko,0,0,0,,Người ngoài hành tinh không biết đau đâu!
Dialogue: 0,0:06:37.25,0:06:38.37,Default,Man,0,0,0,,Xin lỗi!
Dialogue: 0,0:06:38.37,0:06:40.50,Default,Man,0,0,0,,Có sao không?
Dialogue: 0,0:06:57.56,0:06:58.52,Default,Sakamoto,0,0,0,,Im đi!
Dialogue: 0,0:06:58.52,0:07:00.48,Default,Sakamoto,0,0,0,,Miles cũng phải đội mồ
Dialogue: 0,0:07:00.48,0:07:02.15,Default,Nano,0,0,0,,Cháu xin lỗi
Dialogue: 0,0:07:02.15,0:07:04.19,Default,Nano,0,0,0,,Có hơi quá đà
Dialogue: 0,0:07:04.19,0:07:05.44,Default,Professor,0,0,0,,Nghe không, Sakamoto?
Dialogue: 0,0:07:05.73,0:07:07.05,Default,Sakamoto,0,0,0,,Có nghe rồi
Dialogue: 0,0:07:07.05,0:07:08.69,Default,,0,0,0,,Tra tấn nhau à?
Dialogue: 0,0:07:08.69,0:07:09.57,Default,Sakamoto,0,0,0,,Muốn lòi cả tai
Dialogue: 0,0:07:09.86,0:07:12.24,Default,Professor,0,0,0,,Chơi nữa này, nghe không?
Dialogue: 0,0:07:12.24,0:07:13.28,Default,Sakamoto,0,0,0,,Có nghe không?
Dialogue: 0,0:07:43.19,0:07:48.69,Default,Annaka,0,0,0,,Mình đến mì ống trúc gần công viên
Dialogue: 0,0:07:49.02,0:07:54.16,Default,Annaka,0,0,0,,Mới thấy trên TV nên tò mò quá
Dialogue: 0,0:07:55.16,0:07:57.66,Default,Annaka,0,0,0,,Nếu không đứng trên
Dialogue: 0,0:07:57.66,0:08:00.50,Default,,0,0,0,,gắp mì kiểu gì nhỉ?
Dialogue: 0,0:08:00.50,0:08:05.50,Default,Annaka,0,0,0,,Cả ngày chưa ăn nên giờ ăn thoải mái
Dialogue: 0,0:08:17.85,0:08:18.51,Default,Lady,0,0,0,,Đây!
Dialogue: 0,0:08:25.27,0:08:26.90,Default,Lady,0,0,0,,Làm gì đấy?
Dialogue: 0,0:08:26.90,0:08:28.15,Default,Lady,0,0,0,,Chưa thấy ai thế!
Dialogue: 0,0:08:30.57,0:08:32.44,Default,Lady,0,0,0,,Người kế
Dialogue: 0,0:08:32.44,0:08:35.61,Default,Annaka,0,0,0,,À, cô ơi
Dialogue: 0,0:08:35.61,0:08:36.54,Default,Lady,0,0,0,,Cái gì?
Dialogue: 0,0:08:36.54,0:08:38.41,Default,,0,0,0,,Chưa ăn mà về à?
Dialogue: 0,0:08:39.33,0:08:40.66,Default,Lady,0,0,0,,Luật có cho
Dialogue: 0,0:08:40.66,0:08:42.54,Default,Lady,0,0,0,,nhưng tôi không
Dialogue: 0,0:08:45.54,0:08:49.55,Default,Annaka,0,0,0,,À, cháu không có đũa ạ
Dialogue: 0,0:08:49.55,0:08:52.30,Default,Lady,0,0,0,,Đúng là hậu đậu
Dialogue: 0,0:08:52.30,0:08:52.86,Default,Lady,0,0,0,,Đây
Dialogue: 0,0:08:52.86,0:08:53.84,Default,,0,0,0,,Cầm lấy!
Dialogue: 0,0:08:53.84,0:08:54.88,Default,Annaka,0,0,0,,Dạ
Dialogue: 0,0:08:59.47,0:09:03.73,Default,Annaka,0,0,0,,Cô ơi, đũa này xài rồi ạ
Dialogue: 0,0:09:03.73,0:09:04.98,Default,Lady,0,0,0,,Tiết kiệm
Dialogue: 0,0:09:06.31,0:09:07.06,Default,Lady,0,0,0,,Đây này!
Dialogue: 0,0:09:07.81,0:09:09.31,Default,Lady,0,0,0,,Không ai chờ đâu!
Dialogue: 0,0:09:09.82,0:09:12.36,Default,Annaka,0,0,0,,Đành đảo đầu vậy
Dialogue: 0,0:09:27.33,0:09:29.63,Default,Lady,0,0,0,,Cũng vui tính đấy
Dialogue: 0,0:09:32.42,0:09:34.21,Default,Lady,0,0,0,,Sẽ khó hơn đó!
Dialogue: 0,0:09:34.21,0:09:36.76,Default,Lady,0,0,0,,Cho biết mùi đời thês nào!
Dialogue: 0,0:09:43.81,0:09:47.73,Default,Lady,0,0,0,,Không có nước mãi đâu!
Dialogue: 0,0:09:50.31,0:09:52.48,Default,Lady,0,0,0,,Dùng sức mà lấy!
Dialogue: 0,0:10:01.66,0:10:02.45,Default,Annaka,0,0,0,,À...
Dialogue: 0,0:10:03.12,0:10:04.41,Default,Lady,0,0,0,,Đừng có giả hiền!
Dialogue: 0,0:10:04.95,0:10:07.58,Default,Lady,0,0,0,,Ăn sống luôn đi!
Dialogue: 0,0:10:09.92,0:10:12.04,Default,Lady,0,0,0,,Đừng đứng đó!
Dialogue: 0,0:10:12.04,0:10:14.09,Default,Lady,0,0,0,,Chiến tranh ai chờ?
Dialogue: 0,0:10:14.71,0:10:16.72,Default,Man,0,0,0,,Vẫn như xưa
Dialogue: 0,0:10:29.85,0:10:30.81,Default,Lady,0,0,0,,Anh Masaharu
Dialogue: 0,0:10:32.82,0:10:36.99,Default,Lady,0,0,0,,Anh Masaharu
Dialogue: 0,0:10:38.99,0:10:43.49,Default,Man,0,0,0,,Nghe đâu có mì ống trúc nên ghé thử
Dialogue: 0,0:10:43.49,0:10:48.41,Default,Lady,0,0,0,,Em làm mì ống trúc để mong anh đến
Dialogue: 0,0:10:50.08,0:10:54.46,Default,Lady,0,0,0,,30 năm nay anh bỏ đi tìm mì ống trúc
Dialogue: 0,0:10:54.46,0:10:56.13,Default,Lady,0,0,0,,Tìm thấy chưa?
Dialogue: 0,0:10:57.71,0:11:02.34,Default,Man,0,0,0,,Ăn đủ loại rồi thì khó nói quá
Dialogue: 0,0:11:04.18,0:11:10.31,Default,Man,0,0,0,,Dùng tiền tiết kiệm du hành nên sạch tiền rồi
Dialogue: 0,0:11:12.02,0:11:13.19,Default,Man,0,0,0,,Xin lỗi
Dialogue: 0,0:11:13.69,0:11:17.11,Default,Man,0,0,0,,Cho mì trôi theo dòng nước đi
Dialogue: 0,0:11:18.15,0:11:18.61,Default,Lady,0,0,0,,Vâng!
Dialogue: 0,0:11:22.11,0:11:23.72,Default,Lady,0,0,0,,Nhìn cái gì?
Dialogue: 0,0:11:23.72,0:11:25.91,Default,,0,0,0,,Không phải kịch đâu!
Dialogue: 0,0:11:25.91,0:11:30.16,Default,Man,0,0,0,,Xin lỗi, nhưng em thả mì xuống được không?
Dialogue: 0,0:11:40.42,0:11:42.68,Default,Teacher,0,0,0,,Năm nhất đủ chưa?
Dialogue: 0,0:11:42.68,0:11:43.58,Default,Teacher,0,0,0,,Nghe này
Dialogue: 0,0:11:43.58,0:11:46.97,Default,,0,0,0,,Tiến bộ do khen hay chê
Dialogue: 0,0:11:46.97,0:11:49.73,Default,Teacher,0,0,0,,thầy sẽ là người quyết
Dialogue: 0,0:11:49.73,0:11:51.48,Default,Teacher,0,0,0,,Hiểu chưa?
Dialogue: 0,0:11:51.48,0:11:55.23,Default,Narrator,0,0,0,,Mình khen mới tiến bộ mà chả ai khen
Dialogue: 0,0:12:15.67,0:12:18.17,Default,Voice,0,0,0,,Chuẩn bị phóng!
Dialogue: 0,0:12:19.53,0:12:19.95,Default,Woman,0,0,0,,What?!
Dialogue: 0,0:12:23.01,0:12:23.79,Default,Pilot,0,0,0,,What?!
Dialogue: 0,0:12:24.38,0:12:31.46,Default,Pilot,0,0,0,,What?!
Dialogue: 0,0:12:33.06,0:12:34.39,Default,Professor,0,0,0,,Nano!
Dialogue: 0,0:12:35.23,0:12:36.40,Default,Professor,0,0,0,,Nhìn nè!
Dialogue: 0,0:12:36.40,0:12:38.50,Default,Professor,0,0,0,,Buộc tóc nè
Dialogue: 0,0:12:39.61,0:12:40.69,Default,Professor,0,0,0,,Dễ thương không?
Dialogue: 0,0:12:41.15,0:12:44.91,Default,Nano,0,0,0,,Giáo sư nhìn như chị gái ấy
Dialogue: 0,0:12:48.91,0:12:54.04,Default,Professor,0,0,0,,Nè, nè giống chị đến mức nào?
Dialogue: 0,0:12:54.04,0:12:56.96,Default,Nano,0,0,0,,À...giống chị gái lắm
Dialogue: 0,0:12:58.46,0:12:59.60,Default,Professor,0,0,0,,Còn Nano thì sao?
Dialogue: 0,0:12:59.60,0:13:02.05,Default,,0,0,0,,Chị thế nào?
Dialogue: 0,0:13:02.05,0:13:06.80,Default,Nano,0,0,0,,Chị không ra dáng chị gái lắm
Dialogue: 0,0:13:06.80,0:13:08.64,Default,Professor,0,0,0,,Tội chưa
Dialogue: 0,0:13:08.64,0:13:09.24,Default,Professor,0,0,0,,Ra thế
Dialogue: 0,0:13:09.24,0:13:10.10,Default,,0,0,0,,Đúng đó
Dialogue: 0,0:13:10.47,0:13:14.35,Default,Professor,0,0,0,,Đành để em dọn dẹp thôi
Dialogue: 0,0:13:16.94,0:13:19.11,Default,Professor,0,0,0,,Dọn, dọn
Dialogue: 0,0:13:23.28,0:13:25.61,Default,Professor,0,0,0,,Nano đúng là
Dialogue: 0,0:13:25.61,0:13:28.45,Default,Nano,0,0,0,,Chị đang đọc
Dialogue: 0,0:13:28.45,0:13:29.28,Default,Professor,0,0,0,,Nano đúng là
Dialogue: 0,0:13:33.66,0:13:37.76,Default,Professor,0,0,0,,Đành giúp Sakamoto vậy
Dialogue: 0,0:13:37.76,0:13:39.21,Default,Sakamoto,0,0,0,,Làm gì đấy?
Dialogue: 0,0:13:39.21,0:13:40.42,Default,Professor,0,0,0,,Coi chừng cảm
Dialogue: 0,0:13:41.04,0:13:42.50,Default,Sakamoto,0,0,0,,Làm gì thế?
Dialogue: 0,0:13:42.50,0:13:43.26,Default,Sakamoto,0,0,0,,Muốn gì đây?
Dialogue: 0,0:13:43.59,0:13:45.80,Default,Professor,0,0,0,,Sakamoto đúng là
Dialogue: 0,0:13:45.80,0:13:46.97,Default,Sakamoto,0,0,0,,Sao?
Dialogue: 0,0:13:46.97,0:13:48.39,Default,Professor,0,0,0,,Coi chừng cảm
Dialogue: 0,0:13:48.39,0:13:49.64,Default,Sakamoto,0,0,0,,Không có
Dialogue: 0,0:13:49.64,0:13:51.45,Default,Sakamoto,0,0,0,,Lấy khăn mà đắp
Dialogue: 0,0:13:51.45,0:13:52.93,Default,,0,0,0,,Lấy sách chi?
Dialogue: 0,0:13:52.93,0:13:56.85,Default,Professor,0,0,0,,Sakamoto lúc nào cũng bướng
Dialogue: 0,0:13:57.77,0:13:59.86,Default,Sakamoto,0,0,0,,Đang làm gì đấy?
Dialogue: 0,0:14:01.27,0:14:03.86,Default,Nano,0,0,0,,Chị giặt đồ đây
Dialogue: 0,0:14:03.86,0:14:06.03,Default,Professor,0,0,0,,Để em!
Dialogue: 0,0:14:08.28,0:14:10.32,Default,Nano,0,0,0,,Rồi giáo sư!
Dialogue: 0,0:14:10.32,0:14:12.20,Default,Nano,0,0,0,,Còn bấm nút thôi
Dialogue: 0,0:14:12.70,0:14:14.71,Default,Professor,0,0,0,,Hết cách
Dialogue: 0,0:14:14.71,0:14:16.71,Default,,0,0,0,,để em bấm cho
Dialogue: 0,0:14:16.71,0:14:20.42,Default,Nano,0,0,0,,Không sao, cứ để chị lo
Dialogue: 0,0:14:20.42,0:14:23.71,Default,Professor,0,0,0,,Em bảo để em thì cứ để em
Dialogue: 0,0:14:23.71,0:14:25.88,Default,Nano,0,0,0,,Rồi
Dialogue: 0,0:14:25.88,0:14:28.38,Default,Nano,0,0,0,,Bấm đi
Dialogue: 0,0:14:42.31,0:14:45.15,Default,Professor,0,0,0,,Cần giúp gì cứ gọi em
Dialogue: 0,0:14:45.15,0:14:46.40,Default,Nano,0,0,0,,Rồi
Dialogue: 0,0:14:56.41,0:14:57.83,Default,Nano,0,0,0,,Chà
Dialogue: 0,0:15:16.31,0:15:20.10,Default,Professor,0,0,0,,Chị hậu đậu quá để em nấu cho
Dialogue: 0,0:15:20.10,0:15:23.77,Default,Nano,0,0,0,,Chị đang nấu mà
Dialogue: 0,0:15:23.77,0:15:26.44,Default,Professor,0,0,0,,Em nấu ngon hơn
Dialogue: 0,0:15:26.44,0:15:29.03,Default,Professor,0,0,0,,Nè, nè để em
Dialogue: 0,0:15:25.77,0:15:27.04,Default-Alt,Nano,0,0,0,,Đừng mà
Dialogue: 0,0:15:27.04,0:15:28.44,Default-Alt,,0,0,0,,Nguy hiểm!
Dialogue: 0,0:15:29.03,0:15:30.65,Default,Professor,0,0,0,,Không sao!
Dialogue: 0,0:15:30.65,0:15:33.03,Default,Nano,0,0,0,,Sao lại không sao được
Dialogue: 0,0:15:33.45,0:15:35.91,Default,Nano,0,0,0,,Giáo sư, khoan-
Dialogue: 0,0:15:39.37,0:15:42.21,Default,Sakamoto,0,0,0,,Nóng!
Dialogue: 0,0:15:42.21,0:15:43.83,Default,Sakamoto,0,0,0,,Nóng! Nóng! Nóng!
Dialogue: 0,0:15:43.83,0:15:46.55,Default,Nano,0,0,0,,Sao không chú Sakamoto?
Dialogue: 0,0:15:46.55,0:15:48.17,Default,Sakamoto,0,0,0,,Làm trò gì thế?
Dialogue: 0,0:15:48.17,0:15:50.09,Default,Nano,0,0,0,,Xin lỗi!
Dialogue: 0,0:15:50.09,0:15:53.34,Default,Professor,0,0,0,,Sakomoto ngốc, tự dưng vào chi
Dialogue: 0,0:15:53.34,0:15:55.10,Default,Sakamoto,0,0,0,,Ngốc con khỉ!
Dialogue: 0,0:15:55.10,0:15:56.18,Default,,0,0,0,,Giỡn mặt hả!
Dialogue: 0,0:15:56.18,0:15:58.03,Default,Professor,0,0,0,,Thôi Sakamoto 
Dialogue: 0,0:15:58.03,0:15:59.60,Default,,0,0,0,,Có gì mà giận
Dialogue: 0,0:15:59.60,0:16:01.35,Default,Sakamoto,0,0,0,,Thái độ gì đấy
Dialogue: 0,0:16:01.94,0:16:04.90,Default,Nano,0,0,0,,Làm lại vậy
Dialogue: 0,0:16:04.90,0:16:06.82,Default,Professor,0,0,0,,À, để em cho
Dialogue: 0,0:16:07.07,0:16:10.90,Default,Nano,0,0,0,,Không làm nhanh nguội món khác mất
Dialogue: 0,0:16:11.24,0:16:12.87,Default,Professor,0,0,0,,Nano đúng là
Dialogue: 0,0:16:12.87,0:16:15.24,Default,,0,0,0,,để em cho
Dialogue: 0,0:16:15.24,0:16:17.35,Default,Nano,0,0,0,,Giáo sư! Nguy hiểm lắm!
Dialogue: 0,0:16:17.35,0:16:19.37,Default,,0,0,0,,Rớt trứng đó!
Dialogue: 0,0:16:19.37,0:16:21.41,Default,Professor,0,0,0,,Không sao
Dialogue: 0,0:16:20.50,0:16:21.41,Default-Alt,Sakamoto,0,0,0,,Nè, coi chừng!
Dialogue: 0,0:16:21.41,0:16:24.04,Default,Professor,0,0,0,,Không sao mà
Dialogue: 0,0:16:25.42,0:16:26.75,Default,Professor,0,0,0,,Hậu đậu quá
Dialogue: 0,0:16:26.75,0:16:28.34,Default,Sakamoto,0,0,0,,Nhóc thì có!
Dialogue: 0,0:16:29.75,0:16:32.67,Default,Sakamoto,0,0,0,,Nè, nó bị sao thế?
Dialogue: 0,0:16:32.67,0:16:36.09,Default,Nano,0,0,0,,Đang muốn ra dáng chị 
Dialogue: 0,0:16:36.09,0:16:38.05,Default,Professor,0,0,0,,Em là chị mà
Dialogue: 0,0:16:38.05,0:16:38.93,Default,Sakamoto,0,0,0,,Khỉ
Dialogue: 0,0:16:40.68,0:16:41.48,Default,Sakamoto,0,0,0,,Nè, nhóc
Dialogue: 0,0:16:41.98,0:16:42.77,Default,Professor,0,0,0,,Sao?
Dialogue: 0,0:16:45.40,0:16:48.11,Default,Sakamoto,0,0,0,,Làm chị không ăn vặt đâu
Dialogue: 0,0:16:56.91,0:16:59.28,Default,Professor,0,0,0,,Là giáo sư này!
Dialogue: 0,0:17:03.23,0:17:03.79,Default,Sakamoto,0,0,0,,Nói dóc
Dialogue: 0,0:17:03.79,0:17:06.79,Default,Professor,0,0,0,,Thiệt mà!
Dialogue: 0,0:17:12.76,0:17:13.92,Default,Yukko,0,0,0,,To lắm đây
Dialogue: 0,0:17:17.73,0:17:18.80,Default,,0,0,0,,Là giày à
Dialogue: 0,0:17:21.43,0:17:22.52,Default,Yukko,0,0,0,,Cái gì đây!
Dialogue: 0,0:17:25.19,0:17:25.85,Default,Yukko,0,0,0,,Sao?
Dialogue: 0,0:17:25.85,0:17:26.85,Default,,0,0,0,,Ấm nước
Dialogue: 0,0:17:28.65,0:17:31.69,Default,Yukko,0,0,0,,Còn hay hơn cá ha
Dialogue: 0,0:17:33.15,0:17:35.28,Default,Yukko,0,0,0,,Biết bị thế này
Dialogue: 0,0:17:35.28,0:17:37.86,Default,,0,0,0,,Rủ người khác rồi
Dialogue: 0,0:17:37.86,0:17:39.32,Default,Yukko,0,0,0,,Thôi kệ
Dialogue: 0,0:17:56.05,0:17:57.51,Default,Mai,0,0,0,,Theo xu thế
Dialogue: 0,0:18:09.60,0:18:11.31,Default,Mai,0,0,0,,Tay không ăn sushi
Dialogue: 0,0:18:17.45,0:18:19.82,Default,Yukko,0,0,0,,Khỉ gió!
Dialogue: 0,0:18:19.82,0:18:22.83,Default,Yukko,0,0,0,,Không muốn làm bài nữa!
Dialogue: 0,0:18:23.58,0:18:26.58,Default,Yukko,0,0,0,,Sao mình phải làm bài về nhà?
Dialogue: 0,0:18:27.21,0:18:31.84,Default,Yukko,0,0,0,,Điểm thấp thì phải thế thôi
Dialogue: 0,0:18:31.84,0:18:35.15,Default,Yukko,0,0,0,,Nhưng mình biết cách lấy điểm
Dialogue: 0,0:18:35.15,0:18:36.80,Default,,0,0,0,,nên khỏi lo
Dialogue: 0,0:18:36.80,0:18:37.88,Default,Yukko,0,0,0,,Chắc thế
Dialogue: 0,0:18:38.63,0:18:44.10,Default,Yukko,0,0,0,,Tốt nghiệp xong chả muốn học đại học đâu
Dialogue: 0,0:18:44.10,0:18:44.89,Default,Yukko,0,0,0,,Hay đi làm?
Dialogue: 0,0:18:45.43,0:18:48.98,Default,Yukko,0,0,0,,Nhưng nghe đâu khó kiếm việc lắm
Dialogue: 0,0:18:48.98,0:18:52.06,Default,Yukko,0,0,0,,Nhất là không bằng cấp
Dialogue: 0,0:18:52.06,0:18:53.82,Default,Yukko,0,0,0,,Trung học
Dialogue: 0,0:18:53.82,0:18:55.73,Default,Yukko,0,0,0,,Đại học
Dialogue: 0,0:18:55.73,0:18:57.40,Default,Yukko,0,0,0,,Đi làm
Dialogue: 0,0:18:58.61,0:19:01.43,Default,Yukko,0,0,0,,Không học đại học thì không được
Dialogue: 0,0:19:01.43,0:19:05.79,Default,Yukko,0,0,0,,Nhưng không muốn học
Dialogue: 0,0:19:06.41,0:19:10.04,Default,Yukko,0,0,0,,Học đống này tương lai làm gì?
Dialogue: 0,0:19:10.04,0:19:12.13,Default,Yukko,0,0,0,,Mà khoan
Dialogue: 0,0:19:13.00,0:19:15.96,Default,Yukko,0,0,0,,Giấc mơ tương lai của mình
Dialogue: 0,0:19:17.13,0:19:20.18,Default,Yukko,0,0,0,,Ước mơ sau này, Aioi Yukko
Dialogue: 0,0:19:20.18,0:19:25.06,Default,Yukko,0,0,0,,Em muốn sau này làm thằn lằn cổ diềm
Dialogue: 0,0:19:25.68,0:19:27.56,Default,Yukko,0,0,0,,Cổ diềm
Dialogue: 0,0:19:27.56,0:19:29.89,Default,Yukko,0,0,0,,Cổ diềm
Dialogue: 0,0:19:30.94,0:19:32.65,Default,Yukko,0,0,0,,Mình không muốn học!
Dialogue: 0,0:19:33.27,0:19:38.15,Default,Yukko,0,0,0,,Không học cũng không làm thằn lằn
Dialogue: 0,0:19:38.86,0:19:40.53,Default,Yukko,0,0,0,,Mà khoan
Dialogue: 0,0:19:40.53,0:19:45.58,Default,Yukko,0,0,0,,Làm thằn lằn thì thoát được rồi
Dialogue: 0,0:19:45.58,0:19:51.79,Default,Yukko,0,0,0,,Biết đâu lại thành quốc bảo ấy chứ
Dialogue: 0,0:19:52.29,0:19:55.96,Default,Yukko,0,0,0,,Mà quốc bảo được trả bao nhiêu ta?
Dialogue: 0,0:19:55.96,0:19:58.80,Default,Yukko,0,0,0,,Làm sao thành quốc bảo?
Dialogue: 0,0:19:59.26,0:20:02.30,Default,Yukko,0,0,0,,Lại còn là quốc bảo sống?
Dialogue: 0,0:20:02.63,0:20:03.64,Default,Yukko,0,0,0,,Không, không, không
Dialogue: 0,0:20:03.97,0:20:06.47,Default,,0,0,0,,Thực tế cái nào
Dialogue: 0,0:20:06.47,0:20:12.19,Default,Yukko,0,0,0,,Mình muốn làm, muốn làm, muốn làm
Dialogue: 0,0:20:14.65,0:20:15.77,Default,Yukko,0,0,0,,Chán quá
Dialogue: 0,0:20:15.77,0:20:17.77,Default,Yukko,0,0,0,,Chả nghĩ được gì
Dialogue: 0,0:20:17.77,0:20:20.24,Default,Yukko,0,0,0,,Mình mới năm nhất mà
Dialogue: 0,0:20:20.24,0:20:22.78,Default,Yukko,0,0,0,,Còn 3 năm suy nghĩ
Dialogue: 0,0:20:22.78,0:20:25.45,Default,Yukko,0,0,0,,Nếu không nghĩ ra thì...
Dialogue: 0,0:20:25.45,0:20:26.20,Default,Yukko,0,0,0,,Đại học!
Dialogue: 0,0:20:26.58,0:20:27.53,Default,Yukko,0,0,0,,Thi cử!
Dialogue: 0,0:20:27.53,0:20:29.37,Default,Yukko,0,0,0,,Nhưng mà...
Dialogue: 0,0:20:29.66,0:20:33.96,Default,Yukko,0,0,0,,Không muốn học đâu!
Dialogue: 0,0:20:33.96,0:20:35.46,Default,Yukko,0,0,0,,Khoan, khoan, khoan, khoan
Dialogue: 0,0:20:35.46,0:20:37.09,Default,,0,0,0,,Buồn cũng thế
Dialogue: 0,0:20:37.46,0:20:39.71,Default,Yukko,0,0,0,,Suy nghĩ xem mình muốn gì
Dialogue: 0,0:20:40.26,0:20:43.51,Default,Yukko,0,0,0,,Nghĩ xem muốn làm gì!
Dialogue: 0,0:20:45.51,0:20:49.68,Default,Yukko,0,0,0,,Muốn làm gì à....
Dialogue: 0,0:21:02.28,0:21:03.03,Default,Yukko,0,0,0,,Mẹ ơi!
Dialogue: 0,0:21:10.33,0:21:12.16,Default,Mom,0,0,0,,Ồn ào quá đi!
Dialogue: 0,0:21:12.16,0:21:15.25,Default,Mom,0,0,0,,Biết mấy giờ không?
Dialogue: 0,0:21:38.19,0:21:42.03,Default,Yukko,0,0,0,,Chả hiểu mấy việc này lắm
Dialogue: 0,0:21:42.03,0:21:45.03,Default,Yukko,0,0,0,,Hay giờ làm bài nhỉ?
Dialogue: 0,0:23:18.96,0:23:21.37,Default,Yukko,0,0,0,,Chết!
Dialogue: 0,0:23:21.37,0:23:25.00,Default,Yukko,0,0,0,,Không muốn làm bài!

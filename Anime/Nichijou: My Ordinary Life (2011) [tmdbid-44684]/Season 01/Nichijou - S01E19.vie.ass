Dialogue: 0,0:12:50.91,0:12:52.63,Default,Professor,0,0,0,,Kệ người ta!
Dialogue: 0,0:12:54.00,0:12:55.00,Default,,0,0,0,,Nano
Dialogue: 0,0:12:55.36,0:12:58.25,Default,,0,0,0,,Em hiến Sakamoto
Dialogue: 0,0:12:58.25,0:12:59.17,Default,,0,0,0,,Được không?
Dialogue: 0,0:12:59.17,0:13:00.30,Default,,0,0,0,,Là sao?
Dialogue: 0,0:13:00.30,0:13:01.63,Default,,0,0,0,,Không sao
Dialogue: 0,0:13:01.63,0:13:04.38,Default,,0,0,0,,Giặt lại thì đồ sạch thôi
Dialogue: 0,0:13:05.63,0:13:06.80,Default,,0,0,0,,Đúng ha
Dialogue: 0,0:13:07.84,0:13:09.39,Default,,0,0,0,,Đúng ha!
Dialogue: 0,0:13:09.39,0:13:11.89,Default,,0,0,0,,Nhưng thời gian không quay lại
Dialogue: 0,0:13:13.06,0:13:16.40,Default,Nano,0,0,0,,Hai người không được ăn tối
Dialogue: 0,0:13:16.40,0:13:18.27,Default,Sakamoto,0,0,0,,Sao lại dính mình
Dialogue: 0,0:13:18.27,0:13:20.57,Default,Professor,0,0,0,,Sakamoto làm mà!
Dialogue: 0,0:13:20.57,0:13:24.78,Default,Professor,0,0,0,,Em hiến Sakamoto được không?
Dialogue: 0,0:13:24.78,0:13:26.03,Default,Nano,0,0,0,,Biết rồi
Dialogue: 0,0:13:26.03,0:13:28.66,Default,Nano,0,0,0,,Chị lấy chú Sakamoto
Dialogue: 0,0:13:29.10,0:13:31.73,Default,,0,0,0,,em không được ăn
Dialogue: 0,0:13:32.54,0:13:33.66,Default,Professor,0,0,0,,Khoan mà!
Dialogue: 0,0:13:33.66,0:13:35.87,Default,Professor,0,0,0,,Hiến em nè!
Dialogue: 0,0:13:35.87,0:13:37.83,Default,Professor,0,0,0,,Do làm rớt đó
Dialogue: 0,0:13:37.83,0:13:40.04,Default,,0,0,0,,em làm ướt đồ đó!
Dialogue: 0,0:13:44.59,0:13:45.67,Default,Nano,0,0,0,,{Tada!}
Dialogue: 0,0:13:45.67,0:13:48.09,Default,Nano,0,0,0,,Hôm nay ăn cơm trứng!
Dialogue: 0,0:13:49.59,0:13:51.26,Default,Nano,0,0,0,,Trứng giảm giá!
Dialogue: 0,0:13:51.26,0:13:54.49,Default,Professor,0,0,0,,Giáo sư được ăn ạ?
Dialogue: 0,0:13:54.49,0:13:57.48,Default,Nano,0,0,0,,Đúng rồi, quên mất!
Dialogue: 0,0:13:59.81,0:14:02.23,Default,Nano,0,0,0,,À
Dialogue: 0,0:14:02.55,0:14:05.35,Default,Nano,0,0,0,,Cho hôm nay thôi đó
Dialogue: 0,0:14:06.61,0:14:08.66,Default-Alt,Nano,0,0,0,,Mai không được ăn đó!
Dialogue: 0,0:14:09.82,0:14:11.78,Default,NanoProf,0,0,0,,Bơi, bơi
Dialogue: 0,0:14:11.78,0:14:13.79,Default,Nano,0,0,0,,Cá mập, cá mập, bõm!
Dialogue: 0,0:14:13.79,0:14:15.79,Default,Professor,0,0,0,,Cá mập, cá mập, bõm!
Dialogue: 0,0:14:15.79,0:14:18.96,Default,NanoProf,0,0,0,,Lúc đó thì vầy ha!
Dialogue: 0,0:14:39.81,0:14:41.74,Default,Yukko,0,0,0,,Mio, Mai ơi!
Dialogue: 0,0:14:41.74,0:14:43.48,Default,,0,0,0,,Selamat pagi!
Dialogue: 0,0:14:43.48,0:14:47.08,Default,Yukko,0,0,0,,Tự họa mỹ thuật mệt ghê
Dialogue: 0,0:14:47.08,0:14:49.36,Default,Mio,0,0,0,,Ủa, có à?
Dialogue: 0,0:14:49.36,0:14:50.45,Default,Yukko,0,0,0,,Gì thế?
Dialogue: 0,0:14:50.45,0:14:54.49,Default,Yukko,0,0,0,,Ngài Mio quên làm bài tập à?
Dialogue: 0,0:14:54.49,0:14:56.91,Default,Mio,0,0,0,,Cứ tưởng mai nộp
Dialogue: 0,0:14:56.91,0:14:58.50,Default,Mio,0,0,0,,Mai làm chưa?
Dialogue: 0,0:15:01.38,0:15:02.25,Default,Mai,0,0,0,,Chết
Dialogue: 0,0:15:03.79,0:15:05.30,Default,Yukko,0,0,0,,Mai nữa à?
Dialogue: 0,0:15:05.30,0:15:07.76,Default,Yukko,0,0,0,,Hôm nay chắc mưa rồi
Dialogue: 0,0:15:07.76,0:15:10.13,Default,Mio,0,0,0,,Mưa là nhẹ
Dialogue: 0,0:15:10.13,0:15:11.89,Default,Mio,0,0,0,,chắc bão quá
Dialogue: 0,0:15:11.89,0:15:13.76,Default,Mio,0,0,0,,Bão
Dialogue: 0,0:15:20.52,0:15:21.65,Default,Yukko,0,0,0,,Đúng là...
Dialogue: 0,0:15:22.10,0:15:23.56,Default,Mio,0,0,0,,mưa thật...
Dialogue: 0,0:16:44.43,0:16:46.65,Default,Man,0,0,0,,Các người....
Dialogue: 0,0:16:46.65,0:16:49.77,Default,Man,0,0,0,,Làm gì ngôi đền đấy?
Dialogue: 0,0:16:53.53,0:16:56.95,Default,Man,0,0,0,,Bọn trộm phước sương!
Dialogue: 0,0:17:03.04,0:17:04.29,Default,Man,0,0,0,,X-xém chết!
Dialogue: 0,0:17:10.05,0:17:11.80,Default,Man,0,0,0,,Nguyền rủa!
Dialogue: 0,0:17:12.51,0:17:14.30,Default,Man,0,0,0,,Thần báo tử!
Dialogue: 0,0:17:19.51,0:17:21.97,Default,Man,0,0,0,,Ra đi...
Dialogue: 0,0:17:21.97,0:17:24.56,Default,Man,0,0,0,,Ra khỏi đền ta!
Dialogue: 0,0:17:31.86,0:17:33.69,Default,,0,0,0,,Cứ kệ đi
Dialogue: 0,0:17:33.69,0:17:38.57,Default,,0,0,0,,các cô gái trong mưa nghĩ thế
Dialogue: 0,0:18:17.40,0:18:18.70,Default,Makoto,0,0,0,,Đi mà, hội trưởng
Dialogue: 0,0:18:19.20,0:18:21.37,Default,Makoto,0,0,0,,Em đấu với thầy Takasaki
Dialogue: 0,0:18:21.37,0:18:23.20,Default,Makoto,0,0,0,,Anh trọng tài nhé?
Dialogue: 0,0:18:23.53,0:18:25.45,Default,Daiku,0,0,0,,Trọng tài? Cờ bóng?
Dialogue: 0,0:18:25.45,0:18:26.20,Default,Makoto,0,0,0,,Đúng rồi
Dialogue: 0,0:18:26.58,0:18:30.75,Default,Makoto,0,0,0,,Em thắng thì thầy làm cố vấn cho
Dialogue: 0,0:18:30.75,0:18:32.46,Default,Makoto,0,0,0,,Em phải thắng!
Dialogue: 0,0:18:32.79,0:18:34.75,Default,Daiku,0,0,0,,Nhưng anh làm gì?
Dialogue: 0,0:18:34.75,0:18:35.46,Default,Makoto,0,0,0,,Không sao
Dialogue: 0,0:18:35.92,0:18:38.76,Default,Makoto,0,0,0,,Cuốn cờ bóng em cho anh mượn nãy
Dialogue: 0,0:18:38.76,0:18:41.22,Default,Makoto,0,0,0,,Đọc sơ qua là được
Dialogue: 0,0:18:43.05,0:18:46.02,Default,Daiku,0,0,0,,Mình chưa đọc chút nào
Dialogue: 0,0:18:46.02,0:18:50.60,Default,Daiku,0,0,0,,chả hiểu họ làm gì nữa
Dialogue: 0,0:18:50.60,0:18:52.73,Default,Daiku,0,0,0,,Cờ bóng thế à?
Dialogue: 0,0:18:53.48,0:18:55.98,Default,Ogi,0,0,0,,Có đọc cũng không hiểu đâu
Dialogue: 0,0:18:56.40,0:18:57.45,Default,Daiku,0,0,0,,Ogi!
Dialogue: 0,0:18:57.99,0:19:01.49,Default,Ogi,0,0,0,,Bọn họ xài bí thuật cấm rồi
Dialogue: 0,0:19:01.49,0:19:03.70,Default,,0,0,0,,trình độ thoát cảnh giới
Dialogue: 0,0:19:04.24,0:19:06.66,Default,Ogi,0,0,0,,Như cái tay dựng lên
Dialogue: 0,0:19:06.66,0:19:08.50,Default,Ogi,0,0,0,,Tượng trưng chân bàn cờ
Dialogue: 0,0:19:08.96,0:19:11.79,Default,Ogi,0,0,0,,còn thế kia là cờ lăn
Dialogue: 0,0:19:11.79,0:19:14.04,Default,Ogi,0,0,0,,Đồng hợp nhất quán
Dialogue: 0,0:19:14.04,0:19:16.21,Default,Ogi,0,0,0,,Tư thế giữ bóng đấy
Dialogue: 0,0:19:16.21,0:19:18.17,Default,Ogi,0,0,0,,Theo mình biết thì họ...
Dialogue: 0,0:19:20.05,0:19:21.80,Default,Ogi,0,0,0,,sẽ thành mặt trời
Dialogue: 0,0:19:22.97,0:19:26.22,Default,Daiku,0,0,0,,Chả hiểu nói gì nữa!
Dialogue: 0,0:19:27.22,0:19:27.97,Default,Ogi,0,0,0,,Di chuyển rồi!
Dialogue: 0,0:19:32.77,0:19:33.69,Default,Ogi,0,0,0,,Chết!
Dialogue: 0,0:19:34.15,0:19:35.61,Default,,0,0,0,,Tình yêu!
Dialogue: 0,0:19:39.65,0:19:41.36,Default,Daiku,0,0,0,,Ogi, thế là-
Dialogue: 0,0:19:41.36,0:19:42.07,Default,Ogi,0,0,0,,Im
Dialogue: 0,0:19:43.37,0:19:44.78,Default,Ogi,0,0,0,,Không hiểu à?
Dialogue: 0,0:19:44.78,0:19:47.70,Default,Ogi,0,0,0,,Ám khí bao trùm rồi
Dialogue: 0,0:19:47.70,0:19:49.33,Default,Ogi,0,0,0,,Chỉ cần sai lầm
Dialogue: 0,0:19:49.33,0:19:50.50,Default,Ogi,0,0,0,,phải trả mạng
Dialogue: 0,0:19:52.42,0:19:55.75,Default,Daiku,0,0,0,,Cái gì thế này?
Dialogue: 0,0:19:57.00,0:20:01.11,Default,Ogi,0,0,0,,Lâu rồi không thấy trận cao nhân đấu!
Dialogue: 0,0:20:01.68,0:20:03.76,Default,Ogi,0,0,0,,Gió đang phất phới!
Dialogue: 0,0:20:04.22,0:20:05.18,Default,Ogi,0,0,0,,Chết!
Dialogue: 0,0:20:05.18,0:20:05.80,Default,Ogi,0,0,0,,Thế đó!
Dialogue: 0,0:20:06.14,0:20:08.68,Default,Ogi,0,0,0,,Trận giao hữu mà thế sao?
Dialogue: 0,0:20:09.05,0:20:10.18,Default,#Ogi,0,0,0,,Điên rồi!
Dialogue: 0,0:20:11.14,0:20:14.98,Default,Ogi,0,0,0,,Mình không dám nhìn nữa
Dialogue: 0,0:20:14.98,0:20:17.40,Default,Ogi,0,0,0,,Phụ thân, con xin phép
Dialogue: 0,0:20:22.53,0:20:25.12,Default,Ogi,0,0,0,,Vì chân lý
Dialogue: 0,0:20:25.12,0:20:26.12,Default,Ogi,0,0,0,,Tỏa sáng
Dialogue: 0,0:20:26.12,0:20:27.99,Default,#Ogi,0,0,0,,mở lối!
Dialogue: 0,0:20:27.99,0:20:29.58,Default,Ogi,0,0,0,,Ogi cấm thuật!
Dialogue: 0,0:20:29.58,0:20:31.12,Default,Ogi,0,0,0,,Siêu Ogi!
Dialogue: 0,0:20:34.08,0:20:35.63,Default,Ogi,0,0,0,,Ogi tinh tú!
Dialogue: 0,0:20:49.89,0:20:50.89,Default,Ogi,0,0,0,,Đỡ này!
Dialogue: 0,0:20:50.89,0:20:53.27,Default,,0,0,0,,Tình bất diệt!
Dialogue: 0,0:20:59.61,0:21:00.78,Default,Takasaki,0,0,0,,Khá lắm!
Dialogue: 0,0:21:02.86,0:21:04.28,Default,Makoto,0,0,0,,Thầy nữa!
Dialogue: 0,0:21:07.78,0:21:09.28,Default,Daiku,0,0,0,,Chết cha
Dialogue: 0,0:21:10.08,0:21:13.29,Default,Daiku,0,0,0,,Mình lập clb chơi thôi mà
Dialogue: 0,0:21:13.91,0:21:15.54,Default,Daiku,0,0,0,,Cái này...
Dialogue: 0,0:21:16.04,0:21:18.67,Default,,0,0,0,,thành cái quái gì rồi

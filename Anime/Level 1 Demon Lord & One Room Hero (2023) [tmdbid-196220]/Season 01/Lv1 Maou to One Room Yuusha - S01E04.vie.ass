Dialogue: 0,0:21:58.55,0:22:00.51,4-normal,,0,0,0,,{\fs60\bord0\c&HEAEAEA&\pos(496,808.8)}MỤC VI PHẠM: NỘI DUNG PHÁT TRỰC TIẾP HOẶC\NVIDEO TẢI LÊN VI PHẠM THUẦN PHONG MỸ TỤC
Dialogue: 0,0:00:03.12,0:00:04.12,Default,,0,0,0,,Bệ hạ.
Dialogue: 0,0:00:04.41,0:00:06.20,Default,,0,0,0,,Đây là thứ người muốn.
Dialogue: 0,0:00:06.79,0:00:08.66,Default,,0,0,0,,Ngươi vất vả rồi, Zenia.
Dialogue: 0,0:00:10.87,0:00:12.91,Default,,0,0,0,,Chỉ cần kế hoạch này tiến triển suôn sẻ,
Dialogue: 0,0:00:12.91,0:00:15.70,Default,,0,0,0,,việc Ma tộc thống trị thế giới loài người
Dialogue: 0,0:00:15.70,0:00:18.50,Default,,0,0,0,,sẽ tiến thêm một bước dài.
Dialogue: 0,0:00:20.91,0:00:22.29,Top,,0,0,0,,Nhìn này.
Dialogue: 0,0:00:22.29,0:00:23.83,Top,,0,0,0,,Ngươi hãy phấn chấn lên, dũng sĩ.
Dialogue: 0,0:00:23.83,0:00:25.16,Top,,0,0,0,,Trẫm mang tin vui đến cho ngươi đây.
Dialogue: 0,0:00:25.45,0:00:27.95,Default,,0,0,0,,Cái quỷ gì thế? Để ta xem nào.
Dialogue: 0,0:00:27.95,0:00:30.58,Default,,0,0,0,,"Tập ảnh chân dung mỹ nữ Ma tộc"?
Dialogue: 0,0:00:31.91,0:00:34.66,Default,,0,0,0,,Trẫm thương xót nên mở lòng từ bi\Ntặng cho kẻ đáng thương như ngươi đấy.
Dialogue: 0,0:00:34.66,0:00:35.87,Default,,0,0,0,,Ngươi còn không cảm ơn trẫm?
Dialogue: 0,0:00:35.87,0:00:37.45,Default,,0,0,0,,Ngươi đang nói cái gì vậy?
Dialogue: 0,0:00:37.45,0:00:39.66,Default,,0,0,0,,Trước đây, ngươi có vẻ\Nkhá được lòng phụ nữ.
Dialogue: 0,0:00:39.66,0:00:42.16,Default,,0,0,0,,Nhưng giờ đây ngươi chẳng tìm nổi\Nmột đối tượng.
Dialogue: 0,0:00:42.16,0:00:43.41,Default,,0,0,0,,Không có người phụ nữ nào\Nbên cạnh ngươi, đúng không?
Dialogue: 0,0:00:43.41,0:00:44.54,Default,,0,0,0,,Nếu nói đến phụ nữ thì tôi đây.
Dialogue: 0,0:00:44.54,0:00:47.70,Default,,0,0,0,,Đối tượng gì? Ta không cần bạn gái.
Dialogue: 0,0:00:48.50,0:00:49.70,Default,,0,0,0,,Ngươi đừng giở trò này.
Dialogue: 0,0:00:49.70,0:00:52.50,Default,,0,0,0,,Ngươi nói vậy thôi chứ thực ra\Nrất cô đơn, đúng không?
Dialogue: 0,0:00:52.50,0:00:55.50,Default,,0,0,0,,Đây là ảnh và thông tin cá nhân của
Dialogue: 0,0:00:55.50,0:00:57.75,Default,,0,0,0,,những người đẹp chất lượng cao chọn ra từ cấp dưới của trẫm.
Dialogue: 0,0:00:58.16,0:01:00.12,Default,,0,0,0,,Trẫm nhờ Zenia làm đấy.
Dialogue: 0,0:01:00.12,0:01:04.25,Default,,0,0,0,,Ngươi chọn lấy một cô ưng ý,\Ntrẫm sẽ sắp xếp giúp ngươi.
Dialogue: 0,0:01:05.20,0:01:07.83,Default,,0,0,0,,Làm sao ta có thể yêu Ma tộc được?
Dialogue: 0,0:01:08.54,0:01:09.45,Default,,0,0,0,,Tại sao?
Dialogue: 0,0:01:09.45,0:01:14.04,Default,,0,0,0,,Làm ơn, dù sao ta cũng là người đã\Ngiết vô số Ma tộc mười năm trước.
Dialogue: 0,0:01:14.04,0:01:15.87,Default,,0,0,0,,Chẳng lẽ người ta không quan tâm?
Dialogue: 0,0:01:15.87,0:01:17.41,Default,,0,0,0,,Gì chứ, đó chỉ là chuyện nhỏ.
Dialogue: 0,0:01:17.41,0:01:19.20,Default,,0,0,0,,Ngươi không cần phải lo lắng.
Dialogue: 0,0:01:19.20,0:01:23.33,Default,,0,0,0,,Trước đây, trẫm đã nói\NMa tộc bọn ta có bảy cái mạng.
Dialogue: 0,0:01:23.33,0:01:26.50,Default,,0,0,0,,Bọn ta không quá quan tâm đến\Nsống chết của bản thân.
Dialogue: 0,0:01:26.50,0:01:28.16,Default,,0,0,0,,Đâu phải vấn đề đó.
Dialogue: 0,0:01:28.16,0:01:30.75,Default,,0,0,0,,Tóm lại, ngươi cứ xem đi đã rồi nói.
Dialogue: 0,0:01:30.75,0:01:34.00,Default,,0,0,0,,Tất cả đều là những cô gái xứng tầm đấy.
Dialogue: 0,0:01:43.12,0:01:44.66,Default,,0,0,0,,Sao vừa mở ra đã là bà này?
Dialogue: 0,0:01:45.95,0:01:48.08,Default,,0,0,0,,Hóa ra Zenia cũng có hứng thú.
Dialogue: 0,0:01:51.54,0:01:53.45,Default,,0,0,0,,Những tấm này là cô ta tự chụp hả?
Dialogue: 0,0:01:53.95,0:01:55.87,Default,,0,0,0,,Cô ta nghĩ gì trong đầu vậy?
Dialogue: 0,0:01:55.87,0:01:58.41,Default,,0,0,0,,Chắc cô ấy đang bày tỏ thiện chí\Nvới ngươi.
Dialogue: 0,0:01:58.41,0:01:59.58,Default,,0,0,0,,Ngươi cứ xem kỹ đi.
Dialogue: 0,0:01:59.58,0:02:04.50,Default,,0,0,0,,Cái đứa say rồi trần truồng đuổi theo ta\Nnhư người điên thì thôi đi...
Dialogue: 0,0:02:05.25,0:02:07.95,Default,,0,0,0,,Miễn không uống rượu thì cô ấy\Nlà một cô gái đàng hoàng, tử tế.
Dialogue: 0,0:02:07.95,0:02:11.79,Default,,0,0,0,,Ta nghi ngờ liệu người đàng hoàng\Ncó ăn mặc như vậy không.
Dialogue: 0,0:02:12.20,0:02:14.91,Default,,0,0,0,,Thôi, ngươi xem cả những cô gái khác đi.
Dialogue: 0,0:02:14.91,0:02:18.00,Default,,0,0,0,,Tha hồ lựa chọn nhé,\NNgươi làm người ta ghen tị thật đấy.
Dialogue: 0,0:02:19.70,0:02:21.00,Default,,0,0,0,,Đây là lợn mà?
Dialogue: 0,0:02:21.00,0:02:22.29,Default,,0,0,0,,Ngươi nói vớ vẩn gì đấy?
Dialogue: 0,0:02:22.29,0:02:26.25,Default,,0,0,0,,Rebecca là kiếm sĩ của tộc người bán thú\Nvà là tinh hoa của binh đoàn Ma thú.
Dialogue: 0,0:02:26.25,0:02:27.37,Default,,0,0,0,,Sao ngươi có thể chửi cô ấy là lợn?
Dialogue: 0,0:02:27.37,0:02:30.50,Default,,0,0,0,,Bởi vì cô ấy đúng là lợn mà,\Nlợn rõ ràng thứ thiệt.
Dialogue: 0,0:02:30.50,0:02:32.70,Default,,0,0,0,,Rebecca thực sự là một cô gái tốt đấy.
Dialogue: 0,0:02:32.70,0:02:34.45,Default,,0,0,0,,Đôi tay của cô ấy rất khéo léo.
Dialogue: 0,0:02:34.45,0:02:35.33,Default,,0,0,0,,Xem kỹ nhé.
Dialogue: 0,0:02:35.33,0:02:37.66,Default,,0,0,0,,Bộ giáp mà cô ấy thường dùng\Ndo chính cô ấy chế tạo.
Dialogue: 0,0:02:37.66,0:02:39.25,Default,,0,0,0,,Sức mạnh nữ tính của cô ấy\Nđáng kinh ngạc chưa?
Dialogue: 0,0:02:39.25,0:02:40.33,Default,,0,0,0,,Cô ấy cừ quá!
Dialogue: 0,0:02:40.58,0:02:44.50,Default,,0,0,0,,Không đúng, tự chế tạo áo giáp vượt quá\Ncả phạm vi sức mạnh nữ tính chứ?
Dialogue: 0,0:02:46.54,0:02:48.54,Default,,0,0,0,,Cô này trông có vẻ bình thường đấy.
Dialogue: 0,0:02:49.45,0:02:51.66,Default,,0,0,0,,Desdale à? Ngươi thật tinh tường.
Dialogue: 0,0:02:51.66,0:02:54.45,Default,,0,0,0,,Cô ấy là một chiến binh xuất sắc vừa thông minh vừa giỏi phép thuật.
Dialogue: 0,0:02:54.95,0:02:58.62,Default,,0,0,0,,Cô ấy mang một vẻ hơi lười biếng, nhưng\Ncó người nói như thế lại càng quyến rũ.
Dialogue: 0,0:03:01.29,0:03:06.08,Default,,0,0,0,,Cô ấy cũng toát lên vẻ đẹp chiến đấu,\Nkhi dùng thân dưới quấn chặt đối thủ.
Dialogue: 0,0:03:06.62,0:03:08.12,Default,,0,0,0,,Tại sao không có một ai bình thường?\NBố khỉ!
Dialogue: 0,0:03:08.54,0:03:11.16,Default,,0,0,0,,Tuy trông thì như vậy nhưng trẫm nhớ\Nsở thích của cô ấy là thể thao.
Dialogue: 0,0:03:11.16,0:03:13.45,Default,,0,0,0,,Con gái yêu thể thao\Nrất hấp dẫn, đúng không?
Dialogue: 0,0:03:13.45,0:03:17.04,Default,,0,0,0,,Điều đó không quan trọng,\Nquan trọng là tại sao cô ấy lại là rắn?
Dialogue: 0,0:03:17.04,0:03:18.87,Default,,0,0,0,,Cô ấy làm con người\Nkhông phải rất tốt sao?
Dialogue: 0,0:03:18.87,0:03:20.79,Default,,0,0,0,,Thế cô này thì sao?
Dialogue: 0,0:03:21.91,0:03:23.62,Top,,0,0,0,,Là con người từ đầu đến chân nhé.
Dialogue: 0,0:03:23.62,0:03:25.91,Top,,0,0,0,,''Đã từng'' là con người chứ?
Dialogue: 0,0:03:25.91,0:03:27.95,Top,,0,0,0,,Chết nhăn răng ra rồi\Nthì đừng gọi là con người.
Dialogue: 0,0:03:28.16,0:03:30.20,Default,,0,0,0,,Ngươi kén chọn quá đấy.
Dialogue: 0,0:03:30.70,0:03:31.83,Default,,0,0,0,,Bố khỉ!
Dialogue: 0,0:03:32.08,0:03:33.83,Default,,0,0,0,,Ngươi xem đi, có rất nhiều lựa chọn,
Dialogue: 0,0:03:33.83,0:03:37.08,Default,,0,0,0,,Chắc chắn sẽ tìm được một cô\Nphù hợp với sở thích của ngươi.
Dialogue: 0,0:03:37.08,0:03:39.75,Default,,0,0,0,,Ta nhớ con rồng khổng lồ với cá mực này.
Dialogue: 0,0:03:39.75,0:03:41.58,Default,,0,0,0,,Dù sao chúng cũng cực kì mạnh.
Dialogue: 0,0:03:42.29,0:03:44.75,Default,,0,0,0,,Nhưng hóa ra chúng là giống cái.
Dialogue: 0,0:03:44.75,0:03:47.79,Default,,0,0,0,,Người giới thiệu chúng cho ta\Nlà muốn ta phải làm thế nào?
Dialogue: 0,0:03:47.79,0:03:49.91,Default,,0,0,0,,Thế ngươi thích cô nào rồi?
Dialogue: 0,0:03:50.25,0:03:51.62,Default,,0,0,0,,Chẳng có ai hết, đồ ngốc!
Dialogue: 0,0:03:51.62,0:03:53.37,Default,,0,0,0,,Đây chỉ là cuốn từ điển quái vật\Nbằng tranh thôi.
Dialogue: 0,0:03:53.70,0:03:56.62,Default,,0,0,0,,Trời ạ, ngươi yêu cầu cao quá.
Dialogue: 0,0:03:56.62,0:03:59.62,Default,,0,0,0,,Thế tóm lại là ngươi thích\Nhình mẫu con gái nào?
Dialogue: 0,0:03:59.62,0:04:03.12,Default,,0,0,0,,Có nói cho ngươi biết cũng vô ích.
Dialogue: 0,0:04:03.12,0:04:05.50,Default,,0,0,0,,Nhưng điều kiện bắt buộc\Nlà phải trông giống người.
Dialogue: 0,0:04:06.62,0:04:09.29,Default,,0,0,0,,Ý ngươi là phải có tay chân,\Ndi chuyển bằng chân?
Dialogue: 0,0:04:09.29,0:04:10.20,Default,,0,0,0,,Đúng thế.
Dialogue: 0,0:04:10.20,0:04:11.91,Default,,0,0,0,,Kiểu có thể tháo đầu ra cũng được hả?
Dialogue: 0,0:04:11.91,0:04:13.08,Default,,0,0,0,,Tuyệt đối không được, đồ ngốc!
Dialogue: 0,0:04:13.58,0:04:16.91,Default,,0,0,0,,Vậy thì có thể coi Zenia\Nlà ứng cử viên sáng giá nhỉ?
Dialogue: 0,0:04:17.83,0:04:20.25,Default,,0,0,0,,Tôi không thích mấy con bé\Nngớ ngẩn như cô ta.
Dialogue: 0,0:04:21.41,0:04:23.37,Default,,0,0,0,,Tốt nhất là phải thông minh hơn.
Dialogue: 0,0:04:23.37,0:04:24.70,Default,,0,0,0,,Còn gì nữa không?
Dialogue: 0,0:04:24.70,0:04:26.58,Default,,0,0,0,,Tính cách quyết đoán, thẳng thắn,
Dialogue: 0,0:04:26.58,0:04:29.08,Default,,0,0,0,,nấu ăn giỏi, xinh đẹp,
Dialogue: 0,0:04:29.08,0:04:32.95,Default,,0,0,0,,dáng ngon, tóc dài đen mượt.
Dialogue: 0,0:04:32.95,0:04:35.04,Default,,0,0,0,,Như vậy ta không còn gì để chê nữa.
Dialogue: 0,0:04:35.04,0:04:37.08,Default,,0,0,0,,Cái đồ năng lực có hạn\Nmà thủ đoạn thì có thừa.
Dialogue: 0,0:04:37.08,0:04:39.29,Default,,0,0,0,,Nếu vậy thì trong quân đội của trẫm...
Dialogue: 0,0:04:42.16,0:04:43.62,Default,,0,0,0,,Biến hình!
Dialogue: 0,0:04:49.00,0:04:52.04,Default,,0,0,0,,Ngươi xem, thế nào hả, dũng sĩ?
Dialogue: 0,0:04:54.75,0:04:57.87,Default,,0,0,0,,Đây là vỏ ngụy trang trẫm dùng\Nđể tránh tai mắt.
Dialogue: 0,0:04:57.87,0:05:02.50,Default,,0,0,0,,Nhìn đi, mái tóc đen óng ả,\Ncùng với thân hình nóng bỏng của trẫm
Dialogue: 0,0:05:02.50,0:05:05.75,Default,,0,0,0,,chính là hình mẫu lý tưởng của ngươi.
Dialogue: 0,0:05:05.75,0:05:07.33,Default,,0,0,0,,Hơn nữa, như ngươi đã biết
Dialogue: 0,0:05:07.33,0:05:11.87,Default,,0,0,0,,nấu cơm, giặt giũ, dọn dẹp không làm khó\Nđược trẫm, xứng đáng là vợ hiền dâu thảo.
Dialogue: 0,0:05:11.87,0:05:15.29,Default,,0,0,0,,Tính cách tất nhiên cũng\Nthẳng thắn, quyết đoán.
Dialogue: 0,0:05:16.58,0:05:17.91,Default,,0,0,0,,Đau đầu quá nhỉ!
Dialogue: 0,0:05:17.91,0:05:20.75,Default,,0,0,0,,Trẫm và ngươi rõ ràng là\Nkẻ thù truyền kiếp
Dialogue: 0,0:05:21.00,0:05:25.00,Default,,0,0,0,,nhưng nếu chúng ta kết hôn,
Dialogue: 0,0:05:25.00,0:05:27.29,Default,,0,0,0,,trẫm sẽ có thêm một người trung gian\Nmạnh mẽ ở thế giới loài người.
Dialogue: 0,0:05:27.29,0:05:29.33,Default,,0,0,0,,Trẫm đùa ngươi thôi...
Dialogue: 0,0:05:55.25,0:05:56.95,Default,,0,0,0,,Có... có phải Zenia không?
Dialogue: 0,0:05:56.95,0:05:58.87,Default,,0,0,0,,Đây, trẫm ra mở cửa đây.
Dialogue: 0,0:05:59.25,0:06:00.95,Default,,0,0,0,,Cảm ơn ngài, bệ hạ
Dialogue: 0,0:06:02.33,0:06:04.66,Default,,0,0,0,,Ngài làm sao mà má ửng hồng vậy?
Dialogue: 0,0:06:05.37,0:06:06.95,Default,,0,0,0,,Chẳng lẽ ngài bị cảm?
Dialogue: 0,0:06:07.75,0:06:08.62,Default,,0,0,0,,Làm gì có, má trẫm có hồng đâu.
Dialogue: 0,0:06:08.62,0:06:10.95,Default,,0,0,0,,Ngươi yên tâm, trẫm rất khỏe.
Dialogue: 0,0:06:17.41,0:06:20.54,Default,,0,0,0,,Dũng sĩ, ngươi có muốn trở thành\Nthuộc hạ của ta không?
Dialogue: 0,0:06:20.54,0:06:23.41,Default,,0,0,0,,Như vậy thì ngươi cũng có thể có bảy mạng.
Dialogue: 0,0:06:23.41,0:06:28.54,Default,,0,0,0,,Tại sao không cùng trẫm\Ntận hưởng sự sống vĩnh cửu?
Dialogue: 0,0:06:28.54,0:06:29.70,Default,,0,0,0,,Ngươi đùa đấy à?
Dialogue: 0,0:06:29.70,0:06:32.58,Default,,0,0,0,,Muốn ta vứt bỏ phẩm giá con người\Ntrở thành Ma tộc,
Dialogue: 0,0:06:32.58,0:06:34.20,Default,,0,0,0,,ta thà chết ở đây còn hơn.
Dialogue: 0,0:06:34.58,0:06:36.66,Default,,0,0,0,,Thật đáng tiếc!
Dialogue: 0,0:06:36.66,0:06:40.00,Default,,0,0,0,,Vậy thì ngươi hãy chìm vào\Nsự lãng quên của cái chết đi.
Dialogue: 0,0:06:40.91,0:06:43.62,Default,,0,0,0,,Dũng sĩ, ngươi có muốn trở thành\Nthuộc hạ của trẫm không?
Dialogue: 0,0:06:43.62,0:06:47.37,Default,,0,0,0,,Hãy chuyển sinh thành Ma tộc\Nvà nhận "sức mạnh" nào.
Dialogue: 0,0:06:47.37,0:06:49.75,Default,,0,0,0,,Như vậy ngươi cũng có thể có bảy mạng.
Dialogue: 0,0:06:51.45,0:06:54.50,Default,,0,0,0,,Đây không phải là câu ngươi đã hỏi\Nmười năm trước sao?
Dialogue: 0,0:06:54.50,0:06:55.58,Default,,0,0,0,,Ừ nhỉ.
Dialogue: 0,0:06:55.58,0:06:59.00,Default,,0,0,0,,Lúc đó đã đoán trước là ngươi sẽ từ chối\Nlời đề nghị của trẫm.
Dialogue: 0,0:06:59.00,0:07:00.75,Default,,0,0,0,,Nhưng bây giờ, ý ngươi thế nào?
Dialogue: 0,0:07:00.75,0:07:01.79,Default,,0,0,0,,Ngươi bị ngáo à?
Dialogue: 0,0:07:01.79,0:07:03.08,Default,,0,0,0,,Trẫm nghiêm túc đấy.
Dialogue: 0,0:07:03.08,0:07:04.50,Default,,0,0,0,,Không đời nào...
Dialogue: 0,0:07:04.50,0:07:06.37,Default,,0,0,0,,Ngươi nói mớ thì vào trong mơ mà nói.
Dialogue: 0,0:07:06.37,0:07:08.91,Default,,0,0,0,,Ngươi đừng vội vàng chối bỏ bọn ta.
Dialogue: 0,0:07:08.91,0:07:11.20,Default,,0,0,0,,Ít nhất hãy nghe xem trẫm nói gì đã.
Dialogue: 0,0:07:11.20,0:07:13.37,Default,,0,0,0,,Sao ngươi cứ ngoan cố với chuyện này vậy?
Dialogue: 0,0:07:14.12,0:07:17.04,Default,,0,0,0,,Nào, ngươi cũng gia nhập vào Ma tộc đi.
Dialogue: 0,0:07:17.04,0:07:19.62,Default,,0,0,0,,Hội thảo giải thích việc chuyển sinh.
Dialogue: 0,0:07:20.16,0:07:22.08,Default,,0,0,0,,Hội thảo cực kì đáng ngờ này là sao?
Dialogue: 0,0:07:22.66,0:07:27.62,Default,,0,0,0,,Không làm con người, chuyển sinh thành\NMa tộc sẽ có những lợi ích gì.
Dialogue: 0,0:07:27.62,0:07:30.33,Default,,0,0,0,,Hôm nay, trẫm sẽ giải thích tận tình\Ncho ngươi nghe.
Dialogue: 0,0:07:30.33,0:07:34.00,Default,,0,0,0,,Đầu tiên là lợi ích thứ nhất.\NDù sao đi nữa
Dialogue: 0,0:07:34.00,0:07:35.20,Default,,0,0,0,,cũng là mức lương siêu cao.
Dialogue: 0,0:07:35.20,0:07:36.91,Default,,0,0,0,,Ngươi mở màn bằng tiền à?
Dialogue: 0,0:07:37.37,0:07:39.12,Default,,0,0,0,,Fred đã nói rồi còn gì?
Dialogue: 0,0:07:39.12,0:07:42.12,Default,,0,0,0,,Ngươi sắp hết tiền tiết kiệm rồi.
Dialogue: 0,0:07:43.33,0:07:46.45,Default,,0,0,0,,Chỉ cần làm thuộc hạ của trẫm,\Nngươi sẽ không phải lo thiếu tiền.
Dialogue: 0,0:07:46.45,0:07:49.16,Default,,0,0,0,,Nhưng ta vẫn còn một chút tiền tiết kiệm.
Dialogue: 0,0:07:49.16,0:07:51.04,Default,,0,0,0,,Trước đây ra kiếm được rất nhiều mà.
Dialogue: 0,0:07:51.04,0:07:54.04,Default,,0,0,0,,Không đến nỗi ngày mai hết tiền ăn.
Dialogue: 0,0:07:54.04,0:07:56.25,Default,,0,0,0,,Không nói ngày mai hay ngày kia vội.
Dialogue: 0,0:07:56.25,0:07:58.91,Default,,0,0,0,,Mười năm hay 20 năm nữa,\Nngươi sẽ làm thế nào?
Dialogue: 0,0:07:58.91,0:08:00.75,Default,,0,0,0,,Liên quan gì đến ngươi.
Dialogue: 0,0:08:00.75,0:08:03.08,Default,,0,0,0,,Lúc đó ta đã chết rồi, chẳng sao hết.
Dialogue: 0,0:08:03.08,0:08:05.58,Default,,0,0,0,,Lại nữa... lại giở bài này.
Dialogue: 0,0:08:05.58,0:08:09.20,Default,,0,0,0,,Những kẻ nói như vậy đều\Nchưa thấy quan tài chưa đổ lệ.
Dialogue: 0,0:08:09.20,0:08:11.41,Default,,0,0,0,,Đúng vậy, suy nghĩ của ngươi quá nông cạn.
Dialogue: 0,0:08:11.41,0:08:13.37,Default,,0,0,0,,Giả sử đến lâu đài của trẫm làm việc,
Dialogue: 0,0:08:13.37,0:08:16.12,Default,,0,0,0,,lương tháng khởi điểm là 480 nghìn yên.
Dialogue: 0,0:08:16.12,0:08:17.58,Default,,0,0,0,,Hàng năm có hai dịp thưởng.
Dialogue: 0,0:08:17.58,0:08:20.33,Default,,0,0,0,,Đảm bảo được tăng lương, thăng chức\Ncùng trợ cấp hưu trí.
Dialogue: 0,0:08:20.33,0:08:22.79,Default,,0,0,0,,Dĩ nhiên, đây là mức lương của\Nlính hạng xoàng.
Dialogue: 0,0:08:22.79,0:08:27.41,Default,,0,0,0,,Xét trên năng lực và kinh nghiệm xuất sắc\Ncủa ngươi,
Dialogue: 0,0:08:27.41,0:08:29.50,Default,,0,0,0,,ít nhất sẽ cao gấp ba.
Dialogue: 0,0:08:30.87,0:08:32.79,Default,,0,0,0,,Đợi đã, ngươi đang nói gì vậy?
Dialogue: 0,0:08:32.79,0:08:34.41,Default,,0,0,0,,Ngươi thấy chưa đủ à?
Dialogue: 0,0:08:34.41,0:08:37.70,Default,,0,0,0,,Vậy thì tính cho ngươi 2,5 triệu yên\Nlà gấp năm lần nhé?
Dialogue: 0,0:08:37.70,0:08:39.08,Default,,0,0,0,,Ngươi nghiêm túc đấy à?
Dialogue: 0,0:08:39.08,0:08:40.83,Default,,0,0,0,,Xã hội loài người chắc phát điên mất.
Dialogue: 0,0:08:41.37,0:08:43.08,Default,,0,0,0,,Khoan, thưa bệ hạ.
Dialogue: 0,0:08:43.08,0:08:48.33,Default,,0,0,0,,Thu nhập cao đến mấy mà không có ngày nghỉ\Nthì cuộc sống sẽ nhàm chán.
Dialogue: 0,0:08:48.33,0:08:50.00,Default,,0,0,0,,Ngươi không cần lo về điều này.
Dialogue: 0,0:08:50.00,0:08:54.37,Default,,0,0,0,,Ma tộc đều có bảo hiểm, môi trường lao động thoải mái.
Dialogue: 0,0:08:54.37,0:08:56.50,Default,,0,0,0,,Một ngày làm việc nhiều nhất năm tiếng.
Dialogue: 0,0:08:56.50,0:08:58.16,Default,,0,0,0,,Tất nhiên, không cần tăng ca.
Dialogue: 0,0:08:58.16,0:09:00.20,Default,,0,0,0,,Nghỉ cố định bốn ngày mỗi tuần.
Dialogue: 0,0:09:00.20,0:09:01.95,Default,,0,0,0,,Nghỉ bốn ngày một tuần?
Dialogue: 0,0:09:01.95,0:09:03.58,Default,,0,0,0,,Quả thực là quá tuyệt vời!
Dialogue: 0,0:09:03.58,0:09:06.04,Default,,0,0,0,,Chẳng những giữ được thời gian riêng tư,
Dialogue: 0,0:09:06.04,0:09:09.45,Default,,0,0,0,,thậm chí sẽ rảnh đến mức\Nkhông biết phải làm gì.
Dialogue: 0,0:09:09.45,0:09:12.62,Default,,0,0,0,,Đây là đặc quyền của nhân viên\Ntrong lâu đài à?
Dialogue: 0,0:09:13.41,0:09:16.50,Default,,0,0,0,,Không, trẫm làm việc\Nkhông thể bất công như vậy.
Dialogue: 0,0:09:16.50,0:09:19.87,Default,,0,0,0,,Bất kể doanh nghiệp tư nhân hay công chức\Nđều áp dụng đồng đều.
Dialogue: 0,0:09:19.87,0:09:22.04,Default,,0,0,0,,Đây là quyền lợi mà Ma tộc\Nđáng được hưởng.
Dialogue: 0,0:09:22.04,0:09:24.20,Default,,0,0,0,,Như vậy thì xã hội có thể vận hành không?
Dialogue: 0,0:09:24.20,0:09:26.54,Default,,0,0,0,,Tất nhiên là có thể,\Nvận hành nhẹ nhàng êm ái.
Dialogue: 0,0:09:26.54,0:09:27.75,Default,,0,0,0,,Khoan, thưa bệ hạ.
Dialogue: 0,0:09:28.37,0:09:30.45,Default,,0,0,0,,Nếu chế độ phúc lợi không hoàn thiện,
Dialogue: 0,0:09:30.45,0:09:32.95,Default,,0,0,0,,không phải làm việc\Nsẽ kém ý nghĩa hơn sao?
Dialogue: 0,0:09:32.95,0:09:35.79,Default,,0,0,0,,Tất nhiên, bọn ta cũng có\Nchế độ phúc lợi đầy đủ.
Dialogue: 0,0:09:35.79,0:09:40.29,Default,,0,0,0,,Bọn ta đảm bảo ngươi sẽ có nhà ở quanh\Nlâu đài hoặc khu dân cư trong thành phố.
Dialogue: 0,0:09:40.29,0:09:44.75,Default,,0,0,0,,Hỗ trợ xin giấy chứng nhận nghỉ thai sản, nghỉ chăm con.\NCó quy định rõ ràng để bảo vệ quyền và lợi ích.
Dialogue: 0,0:09:44.75,0:09:48.91,Default,,0,0,0,,Ngoài ra, có nhiều chế độ khác\Nđể hỗ trợ đầy đủ cho thuộc hạ của trẫm.
Dialogue: 0,0:09:48.91,0:09:50.66,Default,,0,0,0,,Thì ra các người cũng sinh con à?
Dialogue: 0,0:09:50.66,0:09:52.62,Default,,0,0,0,,Còn gì nữa nhỉ... Đúng rồi.
Dialogue: 0,0:09:52.62,0:09:54.70,Default,,0,0,0,,Cơ thể khỏe mạnh không bệnh tật,
Dialogue: 0,0:09:54.70,0:09:56.62,Default,,0,0,0,,trí não không bị lão hóa,
Dialogue: 0,0:09:56.62,0:09:58.91,Default,,0,0,0,,cộng thêm ma lực dồi dào bất tận,
Dialogue: 0,0:09:58.91,0:10:03.12,Default,,0,0,0,,cùng với linh hồn vĩnh hằng dù có chết\Ncũng chỉ cần ngủ say là hồi sinh.
Dialogue: 0,0:10:03.12,0:10:05.87,Default,,0,0,0,,Ngươi đừng hấp tấp bỏ qua đặc trưng của Ma tộc.
Dialogue: 0,0:10:09.75,0:10:11.62,Default,,0,0,0,,Chỉ cần trở thành một thành viên\Ncủa bọn ta,
Dialogue: 0,0:10:11.62,0:10:15.45,Default,,0,0,0,,ngươi sẽ thoát khỏi hết thảy phiền muộn\Nvà bất an,
Dialogue: 0,0:10:15.45,0:10:18.00,Default,,0,0,0,,cùng nỗi sợ cái chết của con người.
Dialogue: 0,0:10:27.29,0:10:30.95,Default,,0,0,0,,Hãy cùng trẫm sống cuộc sống vĩnh cửu.
Dialogue: 0,0:10:30.95,0:10:32.08,Default,,0,0,0,,Max.
Dialogue: 0,0:10:44.91,0:10:46.12,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,0:10:46.12,0:10:47.66,Default,,0,0,0,,Việc này thì thôi đi.
Dialogue: 0,0:10:49.83,0:10:51.37,Default,,0,0,0,,Tại sao?
Dialogue: 0,0:10:51.37,0:10:53.45,Default,,0,0,0,,Vì ngươi không muốn\Nlàm thuộc hạ của trẫm à?
Dialogue: 0,0:10:54.00,0:10:57.12,Default,,0,0,0,,Thuộc hạ chỉ là hình thức.
Dialogue: 0,0:10:57.12,0:10:58.75,Default,,0,0,0,,Chúng ta có thể duy trì mối quan hệ hiện tại.
Dialogue: 0,0:10:59.29,0:11:03.20,Default,,0,0,0,,Hay là vì "phẩm giá con người"\Nmà ngươi nói mười năm trước?
Dialogue: 0,0:11:04.00,0:11:07.66,Default,,0,0,0,,"Làm một con người" quan trọng\Nvới ngươi vậy ư?
Dialogue: 0,0:11:08.70,0:11:10.41,Default,,0,0,0,,Ngươi định nghĩa phẩm giá con người là gì?
Dialogue: 0,0:11:13.04,0:11:16.12,Default,,0,0,0,,Người đừng hỏi thẳng\Nmột câu hóc búa như vậy.
Dialogue: 0,0:11:16.12,0:11:18.20,Default,,0,0,0,,Đây là lời ngươi từng nói còn gì?
Dialogue: 0,0:11:18.20,0:11:19.95,Default,,0,0,0,,Lúc đó, ta còn trẻ người non dạ.
Dialogue: 0,0:11:19.95,0:11:23.12,Default,,0,0,0,,Tuy nói hùng hồn đấy\Nnhưng thực ra, ta không hiểu mình nói gì.
Dialogue: 0,0:11:23.12,0:11:28.41,Default,,0,0,0,,Bây giờ, ta cũng không cố chấp\Nmuốn "làm một con người"
Dialogue: 0,0:11:28.41,0:11:30.08,Default,,0,0,0,,nhưng phải nói sao nhỉ?
Dialogue: 0,0:11:30.08,0:11:33.75,Default,,0,0,0,,Ta không thể dễ dàng vứt bỏ "chính mình".
Dialogue: 0,0:11:33.75,0:11:35.58,Default,,0,0,0,,Ý ngươi là sao?
Dialogue: 0,0:11:35.58,0:11:38.25,Default,,0,0,0,,Cho dù chuyển sinh,\Nngươi cũng không mất ký ức mà.
Dialogue: 0,0:11:38.25,0:11:42.45,Default,,0,0,0,,Trở thành Ma tộc có thể sống mãi,\Nkiếm được cả tiền lẫn sức mạnh.
Dialogue: 0,0:11:42.45,0:11:46.75,Default,,0,0,0,,Khi đó ta sẽ không còn là ta\Nmà là một sinh vật khác mất rồi.
Dialogue: 0,0:11:46.75,0:11:49.08,Default,,0,0,0,,Mặc dù so với ta của mười năm trước,
Dialogue: 0,0:11:49.08,0:11:51.41,Default,,0,0,0,,ta bây giờ cũng là một người\Nhoàn toàn khác.
Dialogue: 0,0:11:52.25,0:11:57.33,Default,,0,0,0,,Hơn nữa, nếu ta không chết\Nthì thật sự phiền phức.
Dialogue: 0,0:11:57.33,0:11:59.41,Default,,0,0,0,,Ta không chịu nổi cơ thể bất tử.
Dialogue: 0,0:11:59.87,0:12:01.37,Default,,0,0,0,,Đối với loài người bọn ta là vậy đấy.
Dialogue: 0,0:12:03.91,0:12:05.62,Default,,0,0,0,,Cảm ơn đề nghị của các ngươi.
Dialogue: 0,0:12:05.62,0:12:08.12,Default,,0,0,0,,Ta xin từ chối lời mời này.
Dialogue: 0,0:12:08.95,0:12:10.66,Default,,0,0,0,,Trẫm...
Dialogue: 0,0:12:10.66,0:12:13.16,Default,,0,0,0,,Trẫm chỉ muốn bên ngươi mãi mãi...
Dialogue: 0,0:12:15.33,0:12:16.95,Default,,0,0,0,,Không, không có gì.
Dialogue: 0,0:12:17.50,0:12:19.16,Default,,0,0,0,,Thật là đáng tiếc, thưa bệ hạ.
Dialogue: 0,0:12:21.12,0:12:22.91,Default,,0,0,0,,Cũng đành chịu thôi.
Dialogue: 0,0:12:22.91,0:12:25.66,Default,,0,0,0,,Max đã suy nghĩ kỹ càng rồi.
Dialogue: 0,0:12:26.20,0:12:27.79,Default,,0,0,0,,Anh Max.
Dialogue: 0,0:12:27.79,0:12:29.50,Default,,0,0,0,,Tôi cứ tạm giữ lại hợp đồng.
Dialogue: 0,0:12:29.50,0:12:32.20,Default,,0,0,0,,Nếu anh muốn chuyển sinh,\Ncứ nói với tôi bất cứ lúc nào.
Dialogue: 0,0:12:32.20,0:12:34.41,Default,,0,0,0,,Bản hợp đồng này tùy hứng vậy à?
Dialogue: 0,0:12:34.41,0:12:35.91,Default,,0,0,0,,Nhưng dũng sĩ ạ.
Dialogue: 0,0:12:35.91,0:12:37.75,Default,,0,0,0,,Ngươi đã từ chối đề nghị của ta
Dialogue: 0,0:12:37.75,0:12:40.16,Default,,0,0,0,,thì tự ngươi phải phấn chấn lên.
Dialogue: 0,0:12:40.16,0:12:42.54,Default,,0,0,0,,Trước tiên, ngươi phải lên kế hoạch\Ncho cuộc sống của mình.
Dialogue: 0,0:12:42.54,0:12:46.95,Default,,0,0,0,,Ngươi không được nói cái gì mà\N"mười năm sau sẽ chết" nữa.
Dialogue: 0,0:12:46.95,0:12:49.87,Default,,0,0,0,,Xin lỗi, để ta nghĩ...
Dialogue: 0,0:12:51.58,0:12:53.29,Default,,0,0,0,,Được, ta biết rồi.
Dialogue: 0,0:12:53.29,0:12:54.95,Default,,0,0,0,,Ta sẽ tìm cách giải quyết chuyện tiền bạc.
Dialogue: 0,0:12:55.70,0:12:56.79,Default,,0,0,0,,Cứ chờ xem.
Dialogue: 0,0:12:56.79,0:12:59.87,Default,,0,0,0,,Ta nhất định sẽ kiếm được\Ngấp nhiều lần lương của Ma tộc.
Dialogue: 0,0:12:59.87,0:13:02.58,Default,,0,0,0,,Dũng sĩ đã lấy lại ý chí chiến đấu.
Dialogue: 0,0:13:04.20,0:13:05.70,Default,,0,0,0,,Đây là cái gì?
Dialogue: 0,0:13:05.70,0:13:07.08,Default,,0,0,0,,Ta muốn quay video.
Dialogue: 0,0:13:07.08,0:13:08.37,Default,,0,0,0,,Video?
Dialogue: 0,0:13:08.37,0:13:12.16,Default,,0,0,0,,Ta muốn sản xuất video,\Nrồi đăng lên mạng kiếm tiền.
Dialogue: 0,0:13:12.16,0:13:13.91,Default,,0,0,0,,Cái thứ gì mà khó hiểu quá.
Dialogue: 0,0:13:13.91,0:13:15.12,Default,,0,0,0,,Ngươi không biết à?
Dialogue: 0,0:13:15.12,0:13:17.08,Default,,0,0,0,,Cái này gọi là "Poocuber".
Dialogue: 0,0:13:17.08,0:13:20.37,Default,,0,0,0,,Thu nhập hàng năm của người đứng đầu\Ntrong đó có thể lên tới hơn 100 triệu.
Dialogue: 0,0:13:20.37,0:13:23.12,Default,,0,0,0,,Thu nhập cả năm mà 100 triệu á?\NNgươi nói thật đấy à?
Dialogue: 0,0:13:24.04,0:13:25.87,Default,,0,0,0,,Dù không cần đạt đến mức đó,
Dialogue: 0,0:13:25.87,0:13:30.54,Default,,0,0,0,,có những video chỉ hét lên khi chơi game\Ncũng có thể đạt đến một triệu lượt xem.
Dialogue: 0,0:13:30.54,0:13:32.54,Default,,0,0,0,,Tính một lượt xem bằng một yên Nhật,
Dialogue: 0,0:13:32.54,0:13:35.16,Default,,0,0,0,,là có thể kiếm được một triệu.\NThật sự kiếm tiền quá dễ.
Dialogue: 0,0:13:36.91,0:13:38.91,Default,,0,0,0,,Đơn giản như vậy thật á?
Dialogue: 0,0:13:38.91,0:13:40.91,Default,,0,0,0,,Được, ta bắt đầu làm việc đây.
Dialogue: 0,0:13:40.91,0:13:44.00,Default,,0,0,0,,Mình nên quay video gì nhỉ?\NChọn stream game vẫn hơn.
Dialogue: 0,0:13:44.54,0:13:46.20,Default,,0,0,0,,Hắn không sao chứ?
Dialogue: 0,0:13:47.83,0:13:50.41,Default,,0,0,0,,Anh Max muốn làm Poocuber à?
Dialogue: 0,0:13:50.41,0:13:54.08,Default,,0,0,0,,Đúng thế, đột nhiên hắn nói vậy.
Dialogue: 0,0:13:54.08,0:13:56.87,Default,,0,0,0,,Nghề đó không đơn giản\Nnhư bệ hạ nghe đâu.
Dialogue: 0,0:13:56.87,0:13:59.29,Default,,0,0,0,,Ngươi cũng nghĩ như vậy à?
Dialogue: 0,0:13:59.29,0:14:03.37,Default,,0,0,0,,Rốt cuộc thì Poocuber bình thường\Nkiếm được bao nhiêu tiền?
Dialogue: 0,0:14:03.37,0:14:06.79,Default,,0,0,0,,Dũng sĩ nói mỗi lượt xem là một yên.
Dialogue: 0,0:14:06.79,0:14:09.95,Default,,0,0,0,,Làm gì được? Nghe nói giá rẻ hơn.
Dialogue: 0,0:14:10.50,0:14:15.33,Default,,0,0,0,,Theo dữ liệu, tải lên hầu như mỗi ngày\Nvà mỗi video có hơn 100.000 lượt xem
Dialogue: 0,0:14:15.33,0:14:17.08,Default,,0,0,0,,cũng chỉ miễn cưỡng được no ấm.
Dialogue: 0,0:14:17.08,0:14:19.37,Default,,0,0,0,,Thế thì vất vả quá còn gì.
Dialogue: 0,0:14:19.37,0:14:22.04,Default,,0,0,0,,Nghề này không dành cho người lười.
Dialogue: 0,0:14:22.04,0:14:25.16,Default,,0,0,0,,Thật ra tôi nghĩ anh Max\Nkhông thích hợp với công việc này lắm.
Dialogue: 0,0:14:25.91,0:14:27.87,Default,,0,0,0,,Nói thì nói vậy
Dialogue: 0,0:14:27.87,0:14:30.87,Default,,0,0,0,,nhưng hắn đã quyết tâm rồi.
Dialogue: 0,0:14:30.87,0:14:33.37,Default,,0,0,0,,Bệ hạ, người quá nuông chiều hắn.
Dialogue: 0,0:14:33.37,0:14:35.50,Default,,0,0,0,,Người nên cho hắn một đòn cảnh tỉnh.
Dialogue: 0,0:14:35.50,0:14:38.75,Default,,0,0,0,,Nhưng mà... nói thế nào nhỉ...
Dialogue: 0,0:14:40.95,0:14:44.95,Default,,0,0,0,,Đã lâu ta không thấy dáng vẻ nghiêm túc đó\Ncủa dũng sĩ.
Dialogue: 0,0:14:47.83,0:14:51.75,Default,,0,0,0,,Dường như hắn đã quay lại\Nlúc đối đầu với trẫm mười năm trước.
Dialogue: 0,0:14:51.75,0:14:53.08,Default,,0,0,0,,Đến mức đó ạ?
Dialogue: 0,0:14:54.12,0:14:55.54,Default,,0,0,0,,Ngươi làm thế nào rồi?
Dialogue: 0,0:14:55.54,0:14:57.33,Default,,0,0,0,,Ngươi đừng nói chuyện với ta.
Dialogue: 0,0:14:57.33,0:14:58.95,Default,,0,0,0,,Micro sẽ ghi âm tiếng của ngươi đấy.
Dialogue: 0,0:14:58.95,0:15:00.00,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,0:15:00.50,0:15:02.20,Default,,0,0,0,,Theo ngươi, liệu có được\Nmột triệu lượt xem không?
Dialogue: 0,0:15:02.20,0:15:03.91,Default,,0,0,0,,Mới bắt đầu không thể nhiều thế được.
Dialogue: 0,0:15:03.91,0:15:05.87,Default,,0,0,0,,Ta chỉ vừa cất bước thôi mà.
Dialogue: 0,0:15:05.87,0:15:07.20,Default,,0,0,0,,Vậy nếu thực sự lên tới một triệu thì sao?
Dialogue: 0,0:15:07.20,0:15:08.41,Default,,0,0,0,,Tất nhiên là...
Dialogue: 0,0:15:15.66,0:15:16.95,Default,,0,0,0,,Ngươi sao vậy?
Dialogue: 0,0:15:16.95,0:15:18.54,Default,,0,0,0,,Video...
Dialogue: 0,0:15:18.54,0:15:20.33,Default,,0,0,0,,Chẳng có ai xem...
Dialogue: 0,0:15:20.87,0:15:24.04,Default,,0,0,0,,Ngươi làm video gì đấy? Để trẫm xem nào.
Dialogue: 0,0:15:24.58,0:15:27.70,Default,,0,0,0,,Dừng lại! Ngươi đừng xem, xấu hổ lắm.
Dialogue: 0,0:15:27.87,0:15:29.00,Default,,0,0,0,,Tên ngốc này.
Dialogue: 0,0:15:29.00,0:15:31.87,Default,,0,0,0,,Ngươi công khai cho cả thế giới xem\Nrồi còn gì?
Dialogue: 0,0:15:31.87,0:15:33.04,Default,,0,0,0,,Để trẫm xem nào.
Dialogue: 0,0:15:33.04,0:15:34.37,Default,,0,0,0,,Nhấp vào đây là xem được hả?
Dialogue: 0,0:15:35.41,0:15:36.33,Default,,0,0,0,,Bắt đầu rồi.
Dialogue: 0,0:15:36.33,0:15:37.83,Default,,0,0,0,,Chào mọi người, ờ...
Dialogue: 0,0:15:37.83,0:15:39.29,Default,,0,0,0,,Tôi là Poocuber Mackie.
Dialogue: 0,0:15:39.29,0:15:41.12,Default,,0,0,0,,Đây là giọng nói của ngươi.
Dialogue: 0,0:15:41.12,0:15:42.54,Default,,0,0,0,,Ngươi còn đặt tên là Mackie nữa.
Dialogue: 0,0:15:42.54,0:15:43.54,Default,,0,0,0,,Lải nhải ít thôi.
Dialogue: 0,0:15:43.54,0:15:46.50,Default,,0,0,0,,Ờ, lần này à, trò chơi...
Dialogue: 0,0:15:46.50,0:15:50.25,Default,,0,0,0,,Trò chơi nhỏ... trò chơi nhỏ miễn phí...
Dialogue: 0,0:15:50.25,0:15:53.04,Default,,0,0,0,,Chủ đề muốn chơi... muốn giới thiệu.
Dialogue: 0,0:15:53.04,0:15:55.87,Default,,0,0,0,,- Còn về tôi,\N- Sao ngươi cứ lắp bắp nghe ỉu xìu thế?
Dialogue: 0,0:15:55.87,0:15:58.25,Default,,0,0,0,,- Đây là lần đầu tiên của tôi...\N- Sao ngươi không nói rõ ràng một chút?
Dialogue: 0,0:15:58.25,0:16:02.83,Default,,0,0,0,,Tôi tên là Mackie. Sau này sẽ tiếp tục...\Ndự định tiếp tục tải video lên.
Dialogue: 0,0:16:02.83,0:16:08.29,Default,,0,0,0,,Ờ, đây là video đầu tiên của tôi.
Dialogue: 0,0:16:08.29,0:16:12.37,Default,,0,0,0,,Sau này mong mọi người ủng hộ nhiều.
Dialogue: 0,0:16:12.37,0:16:14.00,Default,,0,0,0,,Ủng hộ nhiều... ngươi đang nói gì vậy?
Dialogue: 0,0:16:14.37,0:16:17.08,Default,,0,0,0,,Đây là màn hình tiêu đề.
Dialogue: 0,0:16:17.08,0:16:18.83,Default,,0,0,0,,Chữ của tiêu đề...
Dialogue: 0,0:16:18.83,0:16:21.12,Default,,0,0,0,,À, có thể thấy chữ của tiêu đề trò chơi.
Dialogue: 0,0:16:21.12,0:16:24.87,Default,,0,0,0,,Và phía dưới là\N"Bắt đầu trò chơi mới, Chơi tiếp, Thoát."
Dialogue: 0,0:16:25.33,0:16:27.04,Default,,0,0,0,,Bắt đầu nhanh lên hộ cái.
Dialogue: 0,0:16:27.04,0:16:30.79,Default,,0,0,0,,Vậy chúng ta hãy chọn\N"Thoát" ngay bây giờ.
Dialogue: 0,0:16:30.79,0:16:33.33,Default,,0,0,0,,Tôi đùa thôi.\NPhải "Bắt đầu trò chơi mới" chứ. Haha.
Dialogue: 0,0:16:34.37,0:16:37.37,Default,,0,0,0,,- Chúng ta hãy nhanh chóng bắt đầu.\N- Cuối cùng cũng bắt đầu.
Dialogue: 0,0:16:38.50,0:16:41.37,Default,,0,0,0,,Thì ra là thế,\Nđánh bại quân địch như thế này.
Dialogue: 0,0:16:41.37,0:16:43.79,Default,,0,0,0,,Hết tên này đến tên khác. Hay quá.
Dialogue: 0,0:16:43.79,0:16:46.95,Default,,0,0,0,,Hãy xem đây,\Nđánh bại quân địch như thế này.
Dialogue: 0,0:16:46.95,0:16:50.70,Default,,0,0,0,,Rồi chúng ta tiếp tục tiến lên.\NHãy xem đây...
Dialogue: 0,0:16:50.70,0:16:53.70,Default,,0,0,0,,Lại có địch xuất hiện. Hãy xem đây...
Dialogue: 0,0:16:54.54,0:16:56.33,Default,,0,0,0,,Hãy xem đây...
Dialogue: 0,0:16:56.33,0:16:59.04,Default,,0,0,0,,Địch lại xuất hiện nữa. Hãy xem đây...
Dialogue: 0,0:16:59.04,0:17:01.45,Default,,0,0,0,,Tuyệt quá, hey, hãy xem đây!
Dialogue: 0,0:17:01.45,0:17:04.04,Default,,0,0,0,,Ngươi không biết phải nói gì, đúng không?
Dialogue: 0,0:17:04.91,0:17:07.33,Default,,0,0,0,,Ma vương kìa, Ma vương, Ma vương đấy.
Dialogue: 0,0:17:07.33,0:17:09.08,Default,,0,0,0,,Tốt, đánh bại hắn.
Dialogue: 0,0:17:09.08,0:17:11.41,Default,,0,0,0,,Làm tôi nổi máu chiến đấu. Hãy xem đây...
Dialogue: 0,0:17:11.41,0:17:14.12,Default,,0,0,0,,Bố khỉ, ai lại thua. Hãy xem đây...
Dialogue: 0,0:17:14.12,0:17:18.45,Default,,0,0,0,,Hãy xem đây. Hải âu kìa. Buồn cười nhỉ?
Dialogue: 0,0:17:26.04,0:17:27.70,Default,,0,0,0,,Được 14 lượt xem à...
Dialogue: 0,0:17:29.45,0:17:31.58,Default,,0,0,0,,Có đến 14 người xem cái này cơ đấy.
Dialogue: 0,0:17:32.45,0:17:34.58,Default,,0,0,0,,Đừng nản lòng, dũng sĩ.
Dialogue: 0,0:17:34.58,0:17:38.29,Default,,0,0,0,,Ngươi đã rất cố gắng rồi,\Nlàm video đến tận nửa đêm.
Dialogue: 0,0:17:38.29,0:17:40.75,Default,,0,0,0,,Chỉ như vậy thôi đã tuyệt vời lắm rồi,\Nđúng không?
Dialogue: 0,0:17:42.58,0:17:46.12,Default,,0,0,0,,Ngươi nói đúng. Ta không thể chán nản\Nvì chuyện nhỏ nhặt này.
Dialogue: 0,0:17:46.12,0:17:47.87,Default,,0,0,0,,Nếu stream game không xong,
Dialogue: 0,0:17:47.87,0:17:50.45,Default,,0,0,0,,ta sẽ làm video mới vậy.
Dialogue: 0,0:17:50.45,0:17:52.95,Default,,0,0,0,,Tôi nghe nói video về trải nghiệm\Nsử dụng sản phẩm rất được ưa chuộng.
Dialogue: 0,0:17:52.95,0:17:54.29,Default,,0,0,0,,Để ra thử xem nào.
Dialogue: 0,0:17:54.95,0:17:57.29,Default,,0,0,0,,Ờ, hôm nay tôi mua bánh kẹo truyền thống.
Dialogue: 0,0:17:57.29,0:18:00.25,Default,,0,0,0,,Tôi sẽ ăn thử xem loại nào ngon nhất.
Dialogue: 0,0:18:00.25,0:18:02.37,Default,,0,0,0,,Ờ, mặc dù tôi không thể lộ mặt.
Dialogue: 0,0:18:06.75,0:18:09.33,Default,,0,0,0,,Tất cả đều ngon, toàn số dách.
Dialogue: 0,0:18:09.33,0:18:12.54,Default,,0,0,0,,Dạo này hình như có ca sĩ Poocuber.
Dialogue: 0,0:18:12.54,0:18:14.20,Default,,0,0,0,,Mình cũng thử debut làm ca sĩ xem sao.
Dialogue: 0,0:18:14.20,0:18:21.50,Default,,0,0,0,,Mối tình như những vì sao lấp lánh.
Dialogue: 0,0:18:21.50,0:18:23.95,Default,,0,0,0,,Khụ khụ...
Dialogue: 0,0:18:24.16,0:18:24.95,Default,,0,0,0,,Đợi một chút...
Dialogue: 0,0:18:25.75,0:18:28.04,Default,,0,0,0,,Tôi sẽ hát lại lần nữa...
Dialogue: 0,0:18:28.33,0:18:30.50,Default,,0,0,0,,Chủ đề thời sự sẽ luôn có người xem.
Dialogue: 0,0:18:30.50,0:18:32.66,Default,,0,0,0,,Xem mình kiếm tiền dễ dàng đây.
Dialogue: 0,0:18:43.25,0:18:46.16,Default,,0,0,0,,Tóm lại là ta không làm được Poocuber.
Dialogue: 0,0:18:47.58,0:18:51.04,Default,,0,0,0,,Công việc này khó hơn nhiều\Nso với bề ngoài.
Dialogue: 0,0:18:51.04,0:18:52.95,Default,,0,0,0,,Ngươi muốn từ bỏ cũng không sao.
Dialogue: 0,0:18:52.95,0:18:54.91,Default,,0,0,0,,Không cần ép mình cố quá.
Dialogue: 0,0:18:56.41,0:18:57.91,Default,,0,0,0,,Bố khỉ...
Dialogue: 0,0:18:59.08,0:19:01.50,Default,,0,0,0,,Hay là ngươi quay một video cuối cùng\Nxem sao.
Dialogue: 0,0:19:01.50,0:19:03.29,Default,,0,0,0,,Hãy dùng hết khả năng của ngươi để quay.
Dialogue: 0,0:19:03.29,0:19:06.58,Default,,0,0,0,,Nếu vẫn không được ưa thích,\Nngươi có từ bỏ cũng thoải mái hơn.
Dialogue: 0,0:19:06.58,0:19:08.41,Default,,0,0,0,,Trẫm cũng sẽ không ép buộc ngươi.
Dialogue: 0,0:19:09.16,0:19:11.66,Default,,0,0,0,,Nếu cần, trẫm cũng có thể giúp ngươi.
Dialogue: 0,0:19:16.12,0:19:18.41,Default,,0,0,0,,Thật à, ngươi sẵn lòng giúp ta sao?
Dialogue: 0,0:19:18.79,0:19:19.95,Default,,0,0,0,,Tất nhiên.
Dialogue: 0,0:19:19.95,0:19:22.04,Default,,0,0,0,,Làm nhiếp ảnh gia cũng được,\Nviệc gì trẫm cũng làm.
Dialogue: 0,0:19:31.87,0:19:34.16,Default,,0,0,0,,Có thấy trẫm trên màn hình không?
Dialogue: 0,0:19:34.58,0:19:36.50,Default,,0,0,0,,Có vẻ ổn.
Dialogue: 0,0:19:37.08,0:19:40.50,Default,,0,0,0,,Chào mọi người, trẫm là Vua Ba Mắt.
Dialogue: 0,0:19:40.50,0:19:44.04,Default,,0,0,0,,Ờ, hôm nay trẫm muốn thử...
Dialogue: 0,0:19:44.04,0:19:48.04,Default,,0,0,0,,"phát trực tiếp".
Dialogue: 0,0:19:48.79,0:19:51.12,Default,,0,0,0,,Có người xem trẫm thật à?
Dialogue: 0,0:19:51.45,0:19:53.87,Default,,0,0,0,,Tóm lại là xin hãy chỉ bảo.
Dialogue: 0,0:19:59.79,0:20:03.87,Default,,0,0,0,,Hình như có khá nhiều người đến,\Nhay quá...
Dialogue: 0,0:20:05.41,0:20:07.41,Default,,0,0,0,,Bạn nói về cách trẫm nói chuyện à?
Dialogue: 0,0:20:07.41,0:20:08.79,Default,,0,0,0,,Có lạ lắm không? Hahaha.
Dialogue: 0,0:20:12.16,0:20:13.79,Default,,0,0,0,,Bạn nói về bộ quần áo này à?
Dialogue: 0,0:20:13.79,0:20:15.54,Default,,0,0,0,,Đây là đồng phục học sinh.
Dialogue: 0,0:20:15.54,0:20:17.41,Default,,0,0,0,,Đúng vậy, đây là đồng phục hải quân.
Dialogue: 0,0:20:17.41,0:20:19.08,Default,,0,0,0,,Đậm phong cách con người...
Dialogue: 0,0:20:19.08,0:20:21.58,Default,,0,0,0,,Không, ý tôi là phong cách rất gần gũi.
Dialogue: 0,0:20:24.50,0:20:25.37,Default,,0,0,0,,Các bạn xem này.
Dialogue: 0,0:20:26.45,0:20:29.16,Default,,0,0,0,,Nhìn kiểu gì thì đây cũng là\Nđồng phục học sinh bình thường, đúng chứ?
Dialogue: 0,0:20:33.58,0:20:36.75,Default,,0,0,0,,Bạn nói đáng yêu à. Ngại thật.
Dialogue: 0,0:20:36.79,0:20:39.62,Default,,0,0,0,,Mọi người bình luận nhiệt tình quá.
Dialogue: 0,0:20:39.62,0:20:41.45,Default,,0,0,0,,Cũng vui đấy chứ.
Dialogue: 0,0:20:44.04,0:20:45.66,Default,,0,0,0,,Đúng rồi.
Dialogue: 0,0:20:45.66,0:20:48.70,Default,,0,0,0,,Hắn bảo trẫm ăn trong lúc\Nphát trực tiếp... Không phải.
Dialogue: 0,0:20:48.70,0:20:50.41,Default,,0,0,0,,Là tự nhiên trẫm muốn ăn kem.
Dialogue: 0,0:20:51.08,0:20:55.33,Default,,0,0,0,,Được, vậy thì bây giờ trẫm sẽ ăn thử\Nvà đánh giá cây kem này.
Dialogue: 0,0:20:55.33,0:20:56.79,Default,,0,0,0,,Mọi người nhìn kỹ nhé.
Dialogue: 0,0:20:59.41,0:20:59.95,Default,,0,0,0,,Ngòn ngọt lành lạnh, rất mát...
Dialogue: 0,0:20:59.95,0:21:01.41,Default,,0,0,0,,Ngòn ngọt lành lạnh, hương vị thanh mát...
Dialogue: 0,0:21:02.04,0:21:03.33,Default,,0,0,0,,Tại sao?
Dialogue: 0,0:21:15.25,0:21:17.66,Default,,0,0,0,,Có nhiều người xem như vậy à?
Dialogue: 0,0:21:17.66,0:21:19.91,Default,,0,0,0,,Mọi người bình luận cũng rất nhiệt tình.
Dialogue: 0,0:21:20.87,0:21:24.12,Default,,0,0,0,,Ấy chết, trẫm sơ ý làm rớt lên quần áo.
Dialogue: 0,0:21:30.95,0:21:32.66,Default,,0,0,0,,Đúng lúc trẫm thấy nóng quá.
Dialogue: 0,0:21:33.54,0:21:36.87,Default,,0,0,0,,Cởi phắt ra cho rồi.
Dialogue: 0,0:22:07.12,0:22:09.33,Default,,0,0,0,,Chúng ta thật ngu ngốc.

Dialogue: 0,0:03:40.61,0:03:41.81,Default,,0000,0000,0000,,Mình...
Dialogue: 0,0:03:42.71,0:03:44.62,Default,,0000,0000,0000,,Kanda-kun?
Dialogue: 0,0:03:44.62,0:03:45.74,Default,,0000,0000,0000,,Aoyama.
Dialogue: 0,0:03:45.74,0:03:46.70,Default,,0000,0000,0000,,Chào buổi sáng.
Dialogue: 0,0:03:46.70,0:03:48.15,Default,,0000,0000,0000,,Ừ. Chào buổi sáng.
Dialogue: 0,0:03:51.61,0:03:54.62,Default,,0000,0000,0000,,Cậu... thấy khỏe chứ, Kanda-kun?
Dialogue: 0,0:03:54.62,0:03:56.34,Default,,0000,0000,0000,,Còn cậu thì sao, Aoyama?
Dialogue: 0,0:03:58.19,0:04:00.50,Default,,0000,0000,0000,,Chắc là tớ không nên hỏi.
Dialogue: 0,0:04:01.03,0:04:03.22,Default,,0000,0000,0000,,Nói thật thì tớ thấy khỏe hơn rất nhiều.
Dialogue: 0,0:04:03.22,0:04:04.14,Default,,0000,0000,0000,,Vậy thì tốt quá.
Dialogue: 0,0:04:04.14,0:04:06.36,Default,,0000,0000,0000,,Bản mặt tớ hôm bữa cậu quên đi nhé?
Dialogue: 0,0:04:06.36,0:04:07.30,Default,,0000,0000,0000,,Ý cậu là "cửa hậu" của ngựa ấy à?
Dialogue: 0,0:04:07.63,0:04:10.59,Default,,0000,0000,0000,,Thẳng thắn với con gái kiểu đó thô lỗ lắm đó.
Dialogue: 0,0:04:15.37,0:04:18.60,Default,,0000,0000,0000,,Ước gì tớ có thể quên hết mọi chuyện xảy ra hôm qua.
Dialogue: 0,0:04:20.01,0:04:21.98,Default,,0000,0000,0000,,Tớ thì ước có thể làm lại từ đầu.
Dialogue: 0,0:04:23.01,0:04:24.40,Default,,0000,0000,0000,,Kanda-kun?
Dialogue: 0,0:04:24.40,0:04:25.46,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:04:25.46,0:04:27.26,Default,,0000,0000,0000,,Cậu quên những gì tớ mới nói đi nhé?
Dialogue: 0,0:04:28.57,0:04:31.74,Default,,0000,0000,0000,,Xin lỗi, giờ thì tớ phải lên đánh thức Shiina dậy.
Dialogue: 0,0:04:31.74,0:04:32.40,Default,,0000,0000,0000,,Ừ.
Dialogue: 0,0:04:33.95,0:04:35.02,Default,,0000,0000,0000,,Shiina.
Dialogue: 0,0:04:36.01,0:04:37.86,Default,,0000,0000,0000,,Shiina, tớ vào đấy.
Dialogue: 0,0:04:39.81,0:04:41.29,Default,,0000,0000,0000,,Dậy coi, Shii—
Dialogue: 0,0:04:45.81,0:04:46.72,Default,,0000,0000,0000,,Shiina?
Dialogue: 0,0:04:50.91,0:04:51.96,Default,,0000,0000,0000,,Shiina!
Dialogue: 0,0:04:55.93,0:04:56.58,Default,,0000,0000,0000,,Gì vậy?
Dialogue: 0,0:04:56.58,0:04:58.26,Default,,0000,0000,0000,,Cậu sao thế, Kanda-kun?
Dialogue: 0,0:04:59.05,0:05:00.77,Default,,0000,0000,0000,,Shiina đâu mất rồi!
Dialogue: 0,0:05:01.11,0:05:02.77,Default,,0000,0000,0000,,Con bé đi lúc sáng sớm rồi.
Dialogue: 0,0:05:03.60,0:05:04.74,Default,,0000,0000,0000,,Hả?
Dialogue: 0,0:05:04.74,0:05:05.44,Default,,0000,0000,0000,,Sao lại...?
Dialogue: 0,0:05:05.83,0:05:08.69,Default,,0000,0000,0000,,Vì Shiina là lý do khiến Sakurasou phải bị dở bỏ.
Dialogue: 0,0:05:09.29,0:05:11.08,Default,,0000,0000,0000,,Có cần phải hỏi gì nữa không?
Dialogue: 0,0:05:11.69,0:05:14.20,Default,,0000,0000,0000,,Ý cậu... là cậu ấy biết cả rồi ư?
Dialogue: 0,0:05:14.20,0:05:15.10,Default,,0000,0000,0000,,Tại sao?!
Dialogue: 0,0:05:15.10,0:05:17.30,Default,,0000,0000,0000,,Sao cô không ngăn cậu ấy lại chứ?!
Dialogue: 0,0:05:17.30,0:05:19.08,Default,,0000,0000,0000,,Cô cũng có hỏi nó rồi.
Dialogue: 0,0:05:20.03,0:05:23.12,Default,,0000,0000,0000,,"Cháu đã nói cho Kanda biết chưa?"
Dialogue: 0,0:05:27.25,0:05:33.32,Default,,0000,0000,0000,,Lúc con bé mới chuyển về đây nó cũng chẳng quan tâm cậu suy nghĩ gì.
Dialogue: 0,0:05:34.95,0:05:37.80,Default,,0000,0000,0000,,Cũng nhờ có mấy đứa, mà con bé đã thay đổi hẳn.
Dialogue: 0,0:05:37.80,0:05:41.01,Default,,0000,0000,0000,,Cho dù là trong cả tình hình bây giờ.
Dialogue: 0,0:05:42.39,0:05:43.14,Default,,0000,0000,0000,,Là do em.
Dialogue: 0,0:05:44.21,0:05:46.31,Default,,0000,0000,0000,,GPS nói là cổ đang ở trường.
Dialogue: 0,0:05:46.31,0:05:48.15,Default,,0000,0000,0000,,Mashiro-sama, khóa mục tiêu!
Dialogue: 0,0:05:48.49,0:05:49.54,Default,,0000,0000,0000,,Cậu chắc chứ?
Dialogue: 0,0:05:49.54,0:05:51.22,Default,,0000,0000,0000,,Cứ để cho tôi lo!
Dialogue: 0,0:05:51.22,0:05:52.66,Default,,0000,0000,0000,,Đi thôi, Kouhai-kun!
Dialogue: 0,0:05:53.28,0:05:54.64,Default,,0000,0000,0000,,Tới trường nào!
Dialogue: 0,0:05:54.64,0:05:56.04,Default,,0000,0000,0000,,Sao tớ cũng phải đi thế này?
Dialogue: 0,0:05:56.04,0:05:56.70,Default,,0000,0000,0000,,Tụi em đi đây.

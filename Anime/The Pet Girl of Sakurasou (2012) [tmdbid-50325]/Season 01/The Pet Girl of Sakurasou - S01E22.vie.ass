Dialogue: 0,0:13:14.31,0:13:15.34,Default,,0000,0000,0000,,Cổ không có ở đây?
Dialogue: 0,0:13:16.41,0:13:18.44,Default,,0000,0000,0000,,Tớ cũng tìm quanh trường đại học rồi, nhưng—
Dialogue: 0,0:13:19.07,0:13:21.94,Default,,0000,0000,0000,,Tụi mình phải ra ngoài trường tìm nữa mới được!
Dialogue: 0,0:13:21.94,0:13:22.43,Default,,0000,0000,0000,,Không.
Dialogue: 0,0:13:24.59,0:13:29.22,Default,,0000,0000,0000,,Mấy đứa nó để mình tìm trong trường là vì không muốn tụi mình bị trễ lễ tốt nghiệp.
Dialogue: 0,0:13:29.22,0:13:30.70,Default,,0000,0000,0000,,Phải tin mấy đứa nó chứ.
Dialogue: 0,0:13:30.70,0:13:32.61,Default,,0000,0000,0000,,Và cũng có vài thứ tụi mình cần phải làm.
Dialogue: 0,0:13:33.57,0:13:34.76,Default,,0000,0000,0000,,Cũng phải.
Dialogue: 0,0:13:35.27,0:13:37.50,Default,,0000,0000,0000,,2 cậu vẫn thân mật như mọi khi nhỉ.
Dialogue: 0,0:13:37.50,0:13:41.16,Default,,0000,0000,0000,,Không thân mật như ai kia với Hauhau, lúc nào gặp mặt là cũng thấy kè kè nhau.
Dialogue: 0,0:13:41.16,0:13:43.12,Default,,0000,0000,0000,,C-Có... kè kè gì đâu.
Dialogue: 0,0:13:44.29,0:13:46.80,Default,,0000,0000,0000,,Đây. Thư tình của tớ.
Dialogue: 0,0:13:47.85,0:13:48.75,Default,,0000,0000,0000,,Cậu không đọc nó ngay à?
Dialogue: 0,0:13:48.75,0:13:50.96,Default,,0000,0000,0000,,Nếu cậu thật sự nghiêm túc thì chẳng có lý do gì phải đọc cả.
Dialogue: 0,0:13:52.75,0:13:55.23,Default,,0000,0000,0000,,Bạn trai của cậu ngầu đấy chứ, Hauhau?
Dialogue: 0,0:13:55.23,0:13:56.88,Default,,0000,0000,0000,,Bạn trai gì chứ.
Dialogue: 0,0:13:56.88,0:14:00.26,Default,,0000,0000,0000,,Nào, hy vọng là mọi chuyện sẽ theo như dự kiến, cựu hội trưởng hội học sinh.
Dialogue: 0,0:14:00.59,0:14:01.42,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:14:01.42,0:14:04.00,Default,,0000,0000,0000,,Nếu cậu thật sự thấy có lỗi, thì đã không kéo tôi vào rồi.
Dialogue: 0,0:14:04.00,0:14:04.94,Default,,0000,0000,0000,,Thật là...
Dialogue: 0,0:14:04.94,0:14:06.77,Default,,0000,0000,0000,,Bình tĩnh nhé, Misaki.
Dialogue: 0,0:14:07.63,0:14:10.06,Default,,0000,0000,0000,,Tớ cũng đang háo hức chờ buổi biểu diễn của cậu đây, Hauhau!
Dialogue: 0,0:14:11.55,0:14:12.73,Default,,0000,0000,0000,,Mashiro!
Dialogue: 0,0:14:12.73,0:14:15.00,Default,,0000,0000,0000,,A, nhóc Kanda đây mà!
Dialogue: 0,0:14:15.00,0:14:16.11,Default,,0000,0000,0000,,Bác có thấy Mashiro không?
Dialogue: 0,0:14:16.67,0:14:19.88,Default,,0000,0000,0000,,Hồi nãy cô có thấy con bé đang đi tới nhà ga đấy.
Dialogue: 0,0:14:19.88,0:14:21.45,Default,,0000,0000,0000,,Cám ơn cô ạ!
Dialogue: 0,0:14:22.45,0:14:24.34,Default,,0000,0000,0000,,Không biết là có chuyện gì, nhưng cố lên nhé!
Dialogue: 0,0:14:24.34,0:14:25.79,Default,,0000,0000,0000,,Bùng nổ sức trẻ đi nào!
Dialogue: 0,0:14:26.84,0:14:28.75,Default,,0000,0000,0000,,Mỗi ngày đều như mỗi ngày.
Dialogue: 0,0:14:29.29,0:14:31.54,Default,,0000,0000,0000,,Sâu thẳm, mình thấy chán nản vô cùng.
Dialogue: 0,0:14:32.61,0:14:36.20,Default,,0000,0000,0000,,Mình muốn trút nỗi chán nản này là vì người khác.
Dialogue: 0,0:14:37.69,0:14:41.24,Default,,0000,0000,0000,,Nhưng mùa xuân vừa rồi, sau khi gặp được Mashiro, mình nhận ra rằng...
Dialogue: 0,0:14:41.89,0:14:43.48,Default,,0000,0000,0000,,Nếu cậu thật sự muốn như vậy,
Dialogue: 0,0:14:43.48,0:14:45.72,Default,,0000,0000,0000,,thì thế giới có thể thay đổi muôn màu trong chớp nhoáng.
Dialogue: 0,0:14:54.45,0:14:55.38,Default,,0000,0000,0000,,{\i1}Này.
Dialogue: 0,0:14:55.38,0:14:57.24,Default,,0000,0000,0000,,{\i1}Cậu muốn trở thành màu nào?
Dialogue: 0,0:14:59.45,0:15:00.32,Default,,0000,0000,0000,,Mình...
Dialogue: 0,0:15:01.19,0:15:02.16,Default,,0000,0000,0000,,Mình muốn...
Dialogue: 0,0:15:09.65,0:15:10.66,Default,,0000,0000,0000,,Mashiro?
Dialogue: 0,0:15:13.69,0:15:14.50,Default,,0000,0000,0000,,Nè, cậu kìa!
Dialogue: 0,0:15:14.50,0:15:15.46,Default,,0000,0000,0000,,Cháu sẽ trả tiền sau!
Dialogue: 0,0:15:18.85,0:15:19.96,Default,,0000,0000,0000,,Mashiro!
Dialogue: 0,0:15:21.47,0:15:23.36,Default,,0000,0000,0000,,Cậu làm gì thế hả?!
Dialogue: 0,0:15:23.36,0:15:24.50,Default,,0000,0000,0000,,Sorata?
Dialogue: 0,0:15:25.01,0:15:25.96,Default,,0000,0000,0000,,Sao cậu lại...?
Dialogue: 0,0:15:25.96,0:15:28.83,Default,,0000,0000,0000,,Là tại cậu thình lình biến mất đấy!
Dialogue: 0,0:15:28.83,0:15:31.69,Default,,0000,0000,0000,,Nhưng nếu tớ ờ lại, Sakurasou sẽ...
Dialogue: 0,0:15:31.69,0:15:33.16,Default,,0000,0000,0000,,Chẳng sao cả!
Dialogue: 0,0:15:33.16,0:15:33.92,Default,,0000,0000,0000,,Chẳng sao cả!
Dialogue: 0,0:15:33.92,0:15:35.44,Default,,0000,0000,0000,,Có sao đấy!
Dialogue: 0,0:15:36.23,0:15:40.88,Default,,0000,0000,0000,,Hôm nay đã là ngày tốt nghiệp, nhưng tớ vẫn chưa tìm đủ phiếu đầy quyển vở đó.
Dialogue: 0,0:15:40.88,0:15:42.23,Default,,0000,0000,0000,,Mashiro...
Dialogue: 0,0:15:42.23,0:15:44.59,Default,,0000,0000,0000,,Tớ phải có trách nhiệm với chuyện đó...
Dialogue: 0,0:15:44.59,0:15:45.70,Default,,0000,0000,0000,,Không cần đâu!
Dialogue: 0,0:15:46.29,0:15:48.10,Default,,0000,0000,0000,,Là do tớ.
Dialogue: 0,0:15:48.69,0:15:50.38,Default,,0000,0000,0000,,Tớ đã rất cố gắng,
Dialogue: 0,0:15:50.38,0:15:52.25,Default,,0000,0000,0000,,nhưng Sakurasou sẽ phải biến mất...
Dialogue: 0,0:15:52.89,0:15:54.54,Default,,0000,0000,0000,,Là tại vì tớ!
Dialogue: 0,0:15:54.54,0:15:57.00,Default,,0000,0000,0000,,Và tớ cũng làm cậu tổn thương nữa...
Dialogue: 0,0:15:57.37,0:15:59.92,Default,,0000,0000,0000,,Bây giờ cậu rất ghét tớ, và... tớ...
Dialogue: 0,0:16:00.39,0:16:01.13,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:16:01.81,0:16:05.06,Default,,0000,0000,0000,,Tớ không muốn ở lại Sakurasou để khiến cậu càng ghét tớ thêm nữa!
Dialogue: 0,0:16:05.06,0:16:06.04,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:16:06.04,0:16:06.72,Default,,0000,0000,0000,,Không hề!
Dialogue: 0,0:16:06.72,0:16:07.84,Default,,0000,0000,0000,,Cậu ghét tớ!
Dialogue: 0,0:16:07.84,0:16:09.20,Default,,0000,0000,0000,,Nghe đây này!
Dialogue: 0,0:16:20.55,0:16:21.72,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:16:21.72,0:16:25.62,Default,,0000,0000,0000,,Những điều nó nói hôm qua là cảm giác thật sự của tớ.
Dialogue: 0,0:16:32.23,0:16:33.80,Default,,0000,0000,0000,,Lúc nào tớ cũng cảm thấy như thế.
Dialogue: 0,0:16:34.41,0:16:37.59,Default,,0000,0000,0000,,Cậu có mọi thứ mà tớ muốn,
Dialogue: 0,0:16:37.59,0:16:39.10,Default,,0000,0000,0000,,điều đó khiến tớ rất ghen tị.
Dialogue: 0,0:16:40.19,0:16:42.34,Default,,0000,0000,0000,,Tại sao lúc nào cũng là cậu?
Dialogue: 0,0:16:43.03,0:16:44.68,Default,,0000,0000,0000,,Tớ rất chán nản.
Dialogue: 0,0:16:44.68,0:16:47.10,Default,,0000,0000,0000,,Có những lúc tớ gần như căm thù cậu.
Dialogue: 0,0:16:50.85,0:16:52.52,Default,,0000,0000,0000,,Nhưng đó không phải là tất cả!
Dialogue: 0,0:16:53.07,0:16:56.73,Default,,0000,0000,0000,,Vì cậu ở đó mà tớ đã tìm thấy mục đích của đời mình!
Dialogue: 0,0:16:57.15,0:16:59.80,Default,,0000,0000,0000,,Tớ đã rất nỗ lực và cố nắm lấy,
Dialogue: 0,0:16:59.80,0:17:01.48,Default,,0000,0000,0000,,nhưng càng lúc tớ lại thấy chán nản hơn.
Dialogue: 0,0:17:01.48,0:17:05.28,Default,,0000,0000,0000,,Cho dù là thế, đó cũng là quãng thời gian đẹp nhất trong đời tớ!
Dialogue: 0,0:17:06.49,0:17:10.83,Default,,0000,0000,0000,,Nếu ngày đó không gặp được cậu, tớ sẽ không bao giờ hiểu được cảm giác này!
Dialogue: 0,0:17:10.83,0:17:14.28,Default,,0000,0000,0000,,Nhờ có cậu mà tớ đã nhận ra được!
Dialogue: 0,0:17:14.28,0:17:15.92,Default,,0000,0000,0000,,Tớ yêu cậu, Mashiro!
Dialogue: 0,0:17:15.92,0:17:17.42,Default,,0000,0000,0000,,Tớ yêu cậu!
Dialogue: 0,0:17:17.42,0:17:19.84,Default,,0000,0000,0000,,Tớ không bao giờ ghét cậu!
Dialogue: 0,0:17:19.84,0:17:23.13,Default,,0000,0000,0000,,Vậy nên cậu đừng đi!
Dialogue: 0,0:17:49.01,0:17:50.28,Default,,0000,0000,0000,,Không...
Dialogue: 0,0:17:51.91,0:17:53.06,Default,,0000,0000,0000,,Sorata!
Dialogue: 0,0:17:54.81,0:17:55.54,Default,,0000,0000,0000,,Mashiro?
Dialogue: 0,0:17:56.62,0:17:57.74,Default,,0000,0000,0000,,Tớ...
Dialogue: 0,0:17:59.57,0:18:01.05,Default,,0000,0000,0000,,Tớ muốn ở lại với cậu!
Dialogue: 0,0:18:02.55,0:18:03.51,Default,,0000,0000,0000,,Tất nhiên là được mà.
Dialogue: 0,0:18:03.51,0:18:05.94,Default,,0000,0000,0000,,Tớ muốn ở lại Sakurasou!
Dialogue: 0,0:18:05.94,0:18:06.97,Default,,0000,0000,0000,,Ừ, được mà.
Dialogue: 0,0:18:06.97,0:18:11.39,Default,,0000,0000,0000,,Nhưng nếu ở lại đó, Sakurasou sẽ bị dở bỏ.
Dialogue: 0,0:18:11.39,0:18:13.46,Default,,0000,0000,0000,,Tớ đã gây rắc rối cho mọi người,
Dialogue: 0,0:18:13.46,0:18:15.52,Default,,0000,0000,0000,,và làm cậu buồn nữa.
Dialogue: 0,0:18:16.47,0:18:19.01,Default,,0000,0000,0000,,Cậu có thể hành hạ tớ bao nhiêu cũng được.
Dialogue: 0,0:18:19.01,0:18:23.36,Default,,0000,0000,0000,,Và tớ cũng khiến Nanami phải chịu đựng rất nhiều sau buổi thử giọng.
Dialogue: 0,0:18:25.21,0:18:29.26,Default,,0000,0000,0000,,Tớ không muốn ở lại Sakurasou khi đã làm tổn thương ai đó.
Dialogue: 0,0:18:29.26,0:18:31.73,Default,,0000,0000,0000,,Cậu nỏi nhảm gì thế hả?
Dialogue: 0,0:18:31.73,0:18:32.64,Default,,0000,0000,0000,,Aoyama?
Dialogue: 0,0:18:32.64,0:18:35.76,Default,,0000,0000,0000,,Đừng có đẩy thất bại của tớ là do lỗi của cậu vậy chứ.
Dialogue: 0,0:18:37.51,0:18:40.10,Default,,0000,0000,0000,,Tớ trượt là do tớ.
Dialogue: 0,0:18:40.10,0:18:41.64,Default,,0000,0000,0000,,Tất cả là lỗi ở tớ.
Dialogue: 0,0:18:41.64,0:18:44.58,Default,,0000,0000,0000,,Người phải trải qua toàn bộ là tớ, biết không hả?
Dialogue: 0,0:18:44.58,0:18:45.76,Default,,0000,0000,0000,,Nhưng...
Dialogue: 0,0:18:45.76,0:18:51.14,Default,,0000,0000,0000,,Tớ đã mặc kệ lời khuyên của Kanda-kun để rồi giữ nó trong lòng.
Dialogue: 0,0:18:51.61,0:18:53.72,Default,,0000,0000,0000,,Đừng có trách nhiệm ảo thế.
Dialogue: 0,0:18:53.72,0:18:55.04,Default,,0000,0000,0000,,Gây phiền phức cho tớ lắm đấy.
Dialogue: 0,0:18:55.04,0:18:57.96,Default,,0000,0000,0000,,Aoyama này, có cần phải nói thẳng quá thế không...
Dialogue: 0,0:18:57.96,0:18:59.14,Default,,0000,0000,0000,,Cậu im ngay, Kanda-kun.
Dialogue: 0,0:18:59.49,0:19:01.76,Default,,0000,0000,0000,,Nanami... Cậu giận à?
Dialogue: 0,0:19:01.76,0:19:03.76,Default,,0000,0000,0000,,Bộ trông tớ đang vui hả?
Dialogue: 0,0:19:05.23,0:19:06.70,Default,,0000,0000,0000,,Nhưng Mashiro nè...
Dialogue: 0,0:19:07.67,0:19:09.78,Default,,0000,0000,0000,,Tớ cũng rất vui vì cậu đã nghĩ đến tớ.
Dialogue: 0,0:19:11.71,0:19:12.62,Default,,0000,0000,0000,,Nanami...
Dialogue: 0,0:19:12.62,0:19:14.10,Default,,0000,0000,0000,,Cám ơn nhé.
Dialogue: 0,0:19:14.10,0:19:15.50,Default,,0000,0000,0000,,Tớ thật sự rất vui.
Dialogue: 0,0:19:16.76,0:19:21.50,Default,,0000,0000,0000,,Vì vậy đừng tự ý bỏ đi mà không nói với tớ nhé.
Dialogue: 0,0:19:21.83,0:19:23.18,Default,,0000,0000,0000,,Nanami!
Dialogue: 0,0:19:23.18,0:19:25.97,Default,,0000,0000,0000,,Cậu làm tớ cứ như con ngốc vậy.
Dialogue: 0,0:19:25.97,0:19:31.44,Default,,0000,0000,0000,,Nhưng chuyện của Sakurasou là do tớ...
Dialogue: 0,0:19:31.44,0:19:34.36,Default,,0000,0000,0000,,Tớ không muốn, nhưng không đi không được...
Dialogue: 0,0:19:34.36,0:19:36.48,Default,,0000,0000,0000,,Đã nói là không cần rồi mà.
Dialogue: 0,0:19:37.94,0:19:41.85,Default,,0000,0000,0000,,Tớ chưa bao giờ thấy như thế này bao giờ. Tớ không biết phải làm gì nữa.
Dialogue: 0,0:19:42.27,0:19:43.96,Default,,0000,0000,0000,,Ngực tớ như siết chặt lại.
Dialogue: 0,0:19:43.96,0:19:45.70,Default,,0000,0000,0000,,Nó đau lắm.
Dialogue: 0,0:19:46.93,0:19:48.56,Default,,0000,0000,0000,,Khổ thân cậu quá.
Dialogue: 0,0:19:54.45,0:19:58.12,Default,,0000,0000,0000,,Nếu cậu cũng có gì muốn nói thì nói luôn đi.
Dialogue: 0,0:19:59.39,0:20:02.12,Default,,0000,0000,0000,,Cậu ấy không có buồn vì đang khóc đâu.
Dialogue: 0,0:20:02.12,0:20:04.14,Default,,0000,0000,0000,,U-Ừ.
Dialogue: 0,0:20:05.73,0:20:08.02,Default,,0000,0000,0000,,Cậu không được mấy khi thấy Mashiro khóc chứ gì.
Dialogue: 0,0:20:13.57,0:20:14.74,Default,,0000,0000,0000,,Mashiro.
Dialogue: 0,0:20:14.74,0:20:16.18,Default,,0000,0000,0000,,Gì?
Dialogue: 0,0:20:16.18,0:20:19.32,Default,,0000,0000,0000,,Cậu hoàn toàn không biết Sakurasou là thế nào cả.
Dialogue: 0,0:20:19.32,0:20:20.46,Default,,0000,0000,0000,,Tớ biết mà.
Dialogue: 0,0:20:20.46,0:20:21.79,Default,,0000,0000,0000,,Cậu không hề biết.
Dialogue: 0,0:20:21.79,0:20:22.60,Default,,0000,0000,0000,,Tớ biết mà!
Dialogue: 0,0:20:22.99,0:20:27.78,Default,,0000,0000,0000,,Sakurasou mà tớ nói không phải là ngôi nhà cũ kĩ đó.
Dialogue: 0,0:20:29.29,0:20:31.20,Default,,0000,0000,0000,,Misaki-senpai và Jin-san,
Dialogue: 0,0:20:31.20,0:20:33.04,Default,,0000,0000,0000,,Aoyama và Chihiro-sensei,
Dialogue: 0,0:20:33.04,0:20:34.50,Default,,0000,0000,0000,,Akasaka...
Dialogue: 0,0:20:34.50,0:20:37.12,Default,,0000,0000,0000,,Và cậu. Đã làm nên Sakurasou.
Dialogue: 0,0:20:38.35,0:20:39.24,Default,,0000,0000,0000,,Sorata...
Dialogue: 0,0:20:39.65,0:20:42.18,Default,,0000,0000,0000,,Đó mới là Sakurasou vì mọi người.
Dialogue: 0,0:20:43.04,0:20:44.94,Default,,0000,0000,0000,,Tóm lại, ý tớ muốn nói là—
Dialogue: 0,0:20:44.94,0:20:45.96,Default,,0000,0000,0000,,Tớ hiểu rồi.
Dialogue: 0,0:20:46.51,0:20:47.56,Default,,0000,0000,0000,,Thật không?
Dialogue: 0,0:20:47.56,0:20:48.59,Default,,0000,0000,0000,,Ừ.
Dialogue: 0,0:20:48.59,0:20:51.84,Default,,0000,0000,0000,,Sakurasou chỉ là Sakurasou vì mọi người ở đó.
Dialogue: 0,0:20:51.84,0:20:53.60,Default,,0000,0000,0000,,Chúng ta là Sakurasou.
Dialogue: 0,0:20:53.60,0:20:54.50,Default,,0000,0000,0000,,Ừ.
Dialogue: 0,0:20:54.50,0:20:55.14,Default,,0000,0000,0000,,Vì vậy...
Dialogue: 0,0:20:56.59,0:20:58.35,Default,,0000,0000,0000,,Chúng ta sẽ luôn luôn ở mãi Sakurasou.
Dialogue: 0,0:21:00.85,0:21:02.23,Default,,0000,0000,0000,,Nanami...
Dialogue: 0,0:21:04.91,0:21:06.02,Default,,0000,0000,0000,,Cám ơn cậu.
Dialogue: 0,0:21:07.17,0:21:10.00,Default,,0000,0000,0000,,Cậu đã nói là rất vui vì tớ ở đây.
Dialogue: 0,0:21:11.56,0:21:13.44,Default,,0000,0000,0000,,Tớ cũng vui lắm.
Dialogue: 0,0:21:13.99,0:21:14.66,Default,,0000,0000,0000,,Cám ơn cậu.
Dialogue: 0,0:21:15.15,0:21:16.52,Default,,0000,0000,0000,,Lẽ tự nhiên thôi mà.
Dialogue: 0,0:21:16.52,0:21:17.53,Default,,0000,0000,0000,,Tụi mình là bạn, đúng không?
Dialogue: 0,0:21:19.39,0:21:20.58,Default,,0000,0000,0000,,Bạn.
Dialogue: 0,0:21:20.58,0:21:23.76,Default,,0000,0000,0000,,Nè khoan— đau quá, Mashiro!
Dialogue: 0,0:21:23.76,0:21:24.80,Default,,0000,0000,0000,,Này...
Dialogue: 0,0:21:24.80,0:21:26.72,Default,,0000,0000,0000,,Cậu có thể chịu được mà, Nanami.
Dialogue: 0,0:21:26.72,0:21:28.34,Default,,0000,0000,0000,,Quác?!
Dialogue: 0,0:21:28.34,0:21:30.05,Default,,0000,0000,0000,,Xin lỗi đã xen vào "chuyện tình 2 cô."
Dialogue: 0,0:21:30.67,0:21:32.94,Default,,0000,0000,0000,,Nhưng tới giờ phải tới trường rồi đó.
Dialogue: 0,0:21:33.81,0:21:34.76,Default,,0000,0000,0000,,Rita?
Dialogue: 0,0:21:34.76,0:21:36.09,Default,,0000,0000,0000,,Nhớ mọi người quá.
Dialogue: 0,0:21:36.41,0:21:37.49,Default,,0000,0000,0000,,Sao cậu...?
Dialogue: 0,0:21:37.49,0:21:38.60,Default,,0000,0000,0000,,Kanda-kun, mấy giờ rồi?!
Dialogue: 0,0:21:39.27,0:21:41.68,Default,,0000,0000,0000,,Lễ tốt nghiệp sắp bắt đầu rồi.
Dialogue: 0,0:21:41.68,0:21:42.22,Default,,0000,0000,0000,,Bỏ mợ!
Dialogue: 0,0:21:42.22,0:21:43.34,Default,,0000,0000,0000,,Đi mau!
Dialogue: 0,0:21:43.34,0:21:44.20,Default,,0000,0000,0000,,Mashiro, đi nào.
Dialogue: 0,0:21:43.75,0:21:44.92,Default1,,0000,0000,0000,,Tớ đang đi nè.
Dialogue: 0,0:21:44.20,0:21:44.92,Default,,0000,0000,0000,,Ryuunosuke!
Dialogue: 0,0:21:44.92,0:21:46.23,Default,,0000,0000,0000,,Tui thì miễn cái—
Dialogue: 0,0:21:46.67,0:21:47.68,Default,,0000,0000,0000,,Nè, Mashiro!
Dialogue: 0,0:21:47.68,0:21:48.27,Default,,0000,0000,0000,,Gì vậy?
Dialogue: 0,0:21:48.27,0:21:51.52,Default,,0000,0000,0000,,Tớ vẫn chưa biết được mình muốn trở thành màu gì,
Dialogue: 0,0:21:51.52,0:21:53.49,Default,,0000,0000,0000,,nhưng có một màu mà hôm nay tớ rất muốn trở thành.
Dialogue: 0,0:21:53.49,0:21:57.18,Default,,0000,0000,0000,,Cho dù đây có là ngày cuối cùng chúng ta cùng được ở bên nhau,
Dialogue: 0,0:21:57.18,0:21:58.95,Default,,0000,0000,0000,,tớ muốn lễ tốt nghiệp ngập tràn trong màu hoa anh đào nở!
Dialogue: 0,0:22:04.77,0:22:09.13,Default,,0000,0000,0000,,Tớ... rất thích màu đó.
Dialogue: 0,0:23:31.25,0:23:32.36,Default,,0000,0000,0000,,Tốt nghiệp!
Dialogue: 0,0:23:32.36,0:23:33.48,Default,,0000,0000,0000,,Tốt nghiệp rồi!
Dialogue: 0,0:23:33.48,0:23:35.04,Default,,0000,0000,0000,,Chúng ta ai cũng tươi cười ở lễ hội văn hóa!

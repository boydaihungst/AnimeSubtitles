Dialogue: 0,0:02:03.97,0:02:08.44,sign_1_1_Scandinavian_Pen,Aide,0,0,0,,Vương quốc Britannia\N{\fs45}Dinh thự bá tước Redford
Dialogue: 0,0:02:05.02,0:02:09.98,sign_2998_8_Episode_7,,0,0,0,,{\fad(480,480)}Tập 7
Dialogue: 0,0:02:05.02,0:02:09.98,sign_2998_9_The_Battle_of_So,,0,0,0,,{\fad(480,480)}Trận chiến ở Sognefjord
Dialogue: 0,0:02:08.44,0:02:13.64,Main,,0,0,0,,{\an3}Nó đã rời cảng Rostock ở Germania\N và tiến về phía Little Belt,
Dialogue: 0,0:02:13.64,0:02:17.65,Main,,0,0,0,,rồi nhòm ngó vùng Sognefjord của Vương quốc Nord chúng tôi à?
Dialogue: 0,0:02:18.47,0:02:19.41,Main,Man,0,0,0,,Vương quốc của ông?
Dialogue: 0,0:02:19.41,0:02:23.41,sign_3343_13_Prince_of_The_Ki,Man,0,0,0,,{\pos(1216.5,1002)}Hoàng tử Vương quốc Nord \N{\b1\fs45}Magnus IX
Dialogue: 0,0:02:19.77,0:02:21.32,Main,Magnus,0,0,0,,{\a6}Tuy Germania đã chiếm mất nó,
Dialogue: 0,0:02:21.32,0:02:23.38,Main,Magnus,0,0,0,,{\a6}nhưng Nord là đất nước của chúng tôi!
Dialogue: 0,0:02:24.31,0:02:27.69,Main,Benoit,0,0,0,,{\a6}Chúng hạ thủy nó trong khi chưa hoàn tất
Dialogue: 0,0:02:25.66,0:02:29.71,sign_3493_16_Free_Thermidor_R,Magnus,0,0,0,,{\pos(1216.5,1002)}{\fad(1,520)}Đại sứ Thermidor\N{\b1\fs45}Tướng Benoit
Dialogue: 0,0:02:27.69,0:02:31.55,Main,,0,0,0,,{\a6}để không bị chặn đầu tại biển Baltic.
Dialogue: 0,0:02:31.55,0:02:33.17,Main,Benoit,0,0,0,,{\a6}Nhưng sao lại ngay lúc này?
Dialogue: 0,0:02:33.17,0:02:37.21,sign_3673_20_Prime_Minister_o,Benoit,0,0,0,,{\pos(1216.5,1002)}Thủ tướng Britannia\N{\fs45\b1}Burns
Dialogue: 0,0:02:33.83,0:02:37.21,Main,Burns,0,0,0,,{\a6}Là trò chơi của Otto đúng không?
Dialogue: 0,0:02:39.28,0:02:42.78,Main,Burns,0,0,0,,Một chiến hạm không thể thay đổi cục diện cuộc chiến.
Dialogue: 0,0:02:42.78,0:02:46.81,Main,Burns,0,0,0,,Nhưng giờ ta vẫn phải tập trung vào nó.
Dialogue: 0,0:02:46.81,0:02:52.73,Main,Burns,0,0,0,,Nhưng hắn có thể nhân cơ hội này tấn công hệ thống phòng thủ ở Normandy.
Dialogue: 0,0:02:52.73,0:02:58.28,Main,Redford,0,0,0,,Chúng tôi cũng nhận được tin hắn đang tập trung không quân tại Dover.
Dialogue: 0,0:02:58.28,0:03:00.16,Main,Benoit,0,0,0,,Tên Otto chết tiệt.
Dialogue: 0,0:03:00.16,0:03:02.03,Main,Benoit,0,0,0,,Hắn chỉ làm điều mình muốn thôi.
Dialogue: 0,0:03:02.03,0:03:04.24,Main,Burns,0,0,0,,Nhưng phải thừa nhận,
Dialogue: 0,0:03:04.24,0:03:08.50,Main,Burns,0,0,0,,chúng ta không thể làm gì để ngăn hắn cả.
Dialogue: 0,0:03:10.00,0:03:15.00,Main,Magnus,0,0,0,,{\a6}Ngài Stanley, chúng tôi cần Hiệp chủng quốc Atlanta hỗ trợ.
Dialogue: 0,0:03:14.08,0:03:18.09,sign_4654_32_Atlantan_Ambassa,Burns,0,0,0,,Đại sứ toàn quyền Atlantan\N{\b1\fs45}Stanley
Dialogue: 0,0:03:15.00,0:03:17.37,Main,Magnus,0,0,0,,{\a6}Phía ngài không thể đưa viện binh sao?
Dialogue: 0,0:03:17.37,0:03:22.46,Main,Stanley,0,0,0,,{\a6}Tổng thống cũng rất thông cảm với tình hình của châu Âu,
Dialogue: 0,0:03:22.46,0:03:25.08,Main,Stanley,0,0,0,,{\a6}nhưng dân chúng và dư luận không đồng ý.
Dialogue: 0,0:03:24.89,0:03:28.89,sign_4913_37_Louisiana_Ambass,Stanley,0,0,0,,{\fad(500,500)}Đại sứ Louisiana\N{\b1\fs45}MacLeod
Dialogue: 0,0:03:25.08,0:03:27.56,Main,McCloud,0,0,0,,{\a6}Tên chó Otto...
Dialogue: 0,0:03:27.56,0:03:32.85,Main,McCloud,0,0,0,,{\a6}Sao hắn không cứ nhắm vào "phù thủy" của Eylstadt tiếp nhỉ.
Dialogue: 0,0:03:33.28,0:03:34.54,Main,Redford,0,0,0,,Phải rồi.
Dialogue: 0,0:03:34.54,0:03:38.88,Main,Redford,0,0,0,,Tuy nãy giờ toàn bàn về mẫu hạm,
Dialogue: 0,0:03:38.88,0:03:42.88,Main,Redford,0,0,0,,nhưng hôm nay ta cũng định bàn về Eylstadt mà?
Dialogue: 0,0:03:42.88,0:03:45.19,Main,Benoit,0,0,0,,Có gì để bàn chứ?
Dialogue: 0,0:03:45.62,0:03:49.24,Main,Benoit,0,0,0,,Đám nhóc đó muốn tuyên truyền gì tôi không biết,
Dialogue: 0,0:03:49.24,0:03:50.93,Main,Benoit,0,0,0,,nhưng "phù thủy"?
Dialogue: 0,0:03:50.93,0:03:53.69,Main,Benoit,0,0,0,,Ta đang ở thế kỷ XX đấy!
Dialogue: 0,0:03:53.69,0:03:55.95,Main,Magnus,0,0,0,,Nhưng nếu đó là thật thì rất đáng quan tâm.
Dialogue: 0,0:03:55.95,0:03:57.87,Main,Burns,0,0,0,,Tôi cũng đã xem đoạn phim.
Dialogue: 0,0:03:57.87,0:04:00.43,Main,Burns,0,0,0,,Nếu là thật thì vụ này lớn đấy.
Dialogue: 0,0:04:00.43,0:04:05.87,Main,Burns,0,0,0,,Nếu thực sự đó là cơ hội để chấm dứt việc Germania đang thống trị châu Âu,
Dialogue: 0,0:04:05.87,0:04:07.60,Main,Burns,0,0,0,,thì tất nhiên là thú vị rồi.
Dialogue: 0,0:04:08.07,0:04:09.32,Main,Redford,0,0,0,,Thưa các ngài,
Dialogue: 0,0:04:09.32,0:04:12.88,Main,Redford,0,0,0,,cứ ngồi đây mà đoán mò về mục đích của Eylstadt là gì
Dialogue: 0,0:04:12.88,0:04:15.85,Main,Redford,0,0,0,,hay phù thủy có thật hay không cũng vô ích.
Dialogue: 0,0:04:15.85,0:04:19.88,Main,Redford,0,0,0,,Nên tôi đã mời tới hai vị khách đặc biệt.
Dialogue: 0,0:04:20.80,0:04:24.53,Main,Redford,0,0,0,,Sao chúng ta không hỏi họ thử xem?
Dialogue: 0,0:04:29.38,0:04:32.10,Main,Fine,0,0,0,,Lâu rồi không gặp, Magnus-sama.
Dialogue: 0,0:04:32.50,0:04:34.50,Main,Fine,0,0,0,,Thật vinh hạnh được gặp lại ngài.
Dialogue: 0,0:04:34.78,0:04:36.35,Main,Magnus,0,0,0,,Ortfiné-sama!
Dialogue: 0,0:04:36.35,0:04:37.21,Main,Benoit,0,0,0,,Không thể nào!
Dialogue: 0,0:04:38.48,0:04:41.20,Main,Fine,0,0,0,,Tôi là Ortfiné von Eylstadt.
Dialogue: 0,0:04:41.20,0:04:41.88,Main,Fine,0,0,0,,Và...
Dialogue: 0,0:04:49.07,0:04:50.18,Main,Benoit,0,0,0,,Như vậy là sao?
Dialogue: 0,0:04:50.18,0:04:52.25,Main,Redford,0,0,0,,Xin đừng lo.
Dialogue: 0,0:04:52.25,0:04:54.69,Main,Redford,0,0,0,,Chỉ là muốn giới thiệu một người thôi.
Dialogue: 0,0:04:59.16,0:05:00.47,Main,Benoit,0,0,0,,Sao có thể...
Dialogue: 0,0:05:03.24,0:05:05.80,Main,Izetta,0,0,0,,Hân hạnh được gặp! Tôi là Izetta!
Dialogue: 0,0:05:14.41,0:05:18.42,Main,Burns,0,0,0,,Cảm ơn cả hai vị đã đến đây trong thời điểm nguy hiểm này.
Dialogue: 0,0:05:18.93,0:05:23.68,Main,Fine,0,0,0,,Tôi đến vì nghe tin phe Đồng minh sẽ họp mặt.
Dialogue: 0,0:05:23.68,0:05:26.56,Main,Fine,0,0,0,,Với sức mạnh của Izetta,
Dialogue: 0,0:05:26.56,0:05:29.89,Main,Fine,0,0,0,,thì đến Britannia chỉ là chuyện nhỏ thôi.
Dialogue: 0,0:05:29.89,0:05:31.53,Main,Burns,0,0,0,,Ra vậy.
Dialogue: 0,0:05:32.15,0:05:35.27,Main,Burns,0,0,0,,Nhưng màn trình diễn khá ấn tượng đấy.
Dialogue: 0,0:05:38.69,0:05:41.52,Main,Benoit,0,0,0,,Tuy cô gái này là phù thủy,
Dialogue: 0,0:05:41.52,0:05:43.32,Main,Benoit,0,0,0,,nhưng chỉ có một người.
Dialogue: 0,0:05:43.32,0:05:45.41,Main,Benoit,0,0,0,,Đâu thể làm được tất cả!
Dialogue: 0,0:05:45.41,0:05:47.20,Main,Fine,0,0,0,,Quả là vậy.
Dialogue: 0,0:05:47.20,0:05:52.45,Main,Fine,0,0,0,,Nhưng chính sức mạnh đó đã đem đến chiến thắng cho chúng tôi tại Coenenberg và Veile.
Dialogue: 0,0:05:52.88,0:05:57.92,Main,Fine,0,0,0,,Sức mạnh của Izetta có thể xem như một mũi kiếm sắc bén.
Dialogue: 0,0:05:58.39,0:06:02.38,Main,Fine,0,0,0,,Nhưng đâm vào một chỗ không thể hạ gục gã khổng lồ Germania được.
Dialogue: 0,0:06:02.38,0:06:05.42,Main,Burns,0,0,0,,Vì vậy người cần quân lực?
Dialogue: 0,0:06:06.29,0:06:10.39,Main,Fine,0,0,0,,Tôi biết đề nghị bất ngờ thế này thì rất khó.
Dialogue: 0,0:06:11.32,0:06:14.84,Main,Fine,0,0,0,,Các vị sẽ phải thuyết phục người dân và chính phủ.
Dialogue: 0,0:06:15.41,0:06:17.35,Main,Fine,0,0,0,,Vì vậy tôi sẽ làm nó đơn giản hơn
Dialogue: 0,0:06:17.35,0:06:22.47,Main,Fine,0,0,0,,bằng cách giải quyết vấn đề lớn nhất của các vị hiện tại.
Dialogue: 0,0:06:23.42,0:06:24.23,Main,Magnus,0,0,0,,Không lẽ là...
Dialogue: 0,0:06:26.24,0:06:28.29,Main,Fine,0,0,0,,Mẫu hạm Drachenfels...
Dialogue: 0,0:06:28.29,0:06:31.82,Main,Fine,0,0,0,,Cái răng nanh mới của Otto sẽ bị sức mạnh của Izetta...
Dialogue: 0,0:06:33.36,0:06:35.19,Main,,0,0,0,,bẻ gãy làm hai!
Dialogue: 0,0:06:41.79,0:06:44.63,Main,Fine,0,0,0,,Cảm ơn ngài Redford,
Dialogue: 0,0:06:45.48,0:06:47.42,Main,Fine,0,0,0,,vì đã giúp tôi đàm phán.
Dialogue: 0,0:06:47.96,0:06:51.39,Main,Redford,0,0,0,,Tôi vẫn nợ người lần ở Westria mà.
Dialogue: 0,0:06:51.97,0:06:56.58,Main,Redford,0,0,0,,Nhưng người quả thật rất quả quyết về chuyện mẫu hạm.
Dialogue: 0,0:06:56.58,0:07:00.56,Main,Fine,0,0,0,,Suốt trên đường đến đây, chúng tôi đã tìm hiểu về nó,
Dialogue: 0,0:07:01.48,0:07:03.19,Main,Fine,0,0,0,,và suy nghĩ kỹ càng,
Dialogue: 0,0:07:05.22,0:07:08.07,Main,Fine,0,0,0,,cuối cùng cả hai đã quyết định như vậy.
Dialogue: 0,0:07:08.07,0:07:08.82,Main,Izetta,0,0,0,,Vâng.
Dialogue: 0,0:07:09.14,0:07:10.50,Main,Redford,0,0,0,,Ra thế.
Dialogue: 0,0:07:11.30,0:07:13.76,Main,Butler,0,0,0,,Thưa ngài, có khách đến.
Dialogue: 0,0:07:13.76,0:07:15.38,Main,Redford,0,0,0,,Đang chờ đây.
Dialogue: 0,0:07:15.38,0:07:16.29,Main,Redford,0,0,0,,Cho anh ta vào.
Dialogue: 0,0:07:18.25,0:07:20.90,Main,Redford,0,0,0,,Đây sẽ là người hỗ trợ nhiệm vụ lần này.
Dialogue: 0,0:07:20.90,0:07:24.44,Main,Redford,0,0,0,,Trung tá Groman của phi đội không quân 633.
Dialogue: 0,0:07:24.44,0:07:25.77,Main,Groman,0,0,0,,Tôi là Groman.
Dialogue: 0,0:07:25.77,0:07:28.97,Main,Groman,0,0,0,,Ông nội tôi là người Nord.
Dialogue: 0,0:07:28.97,0:07:32.54,Main,Groman,0,0,0,,Nên đó là quê hương thứ hai của tôi.
Dialogue: 0,0:07:33.14,0:07:36.76,Main,Groman,0,0,0,,Tôi sẽ làm bất cứ gì để dạy lũ Germania đó một bài học.
Dialogue: 0,0:07:37.28,0:07:39.09,Main,Fine,0,0,0,,Thật là tốt quá.
Dialogue: 0,0:07:39.09,0:07:41.91,Main,Fine,0,0,0,,Mong anh giúp đỡ, Trung tá.
Dialogue: 0,0:07:41.91,0:07:43.61,Main,Izetta,0,0,0,,Ưm, Trung tá-san,
Dialogue: 0,0:07:43.61,0:07:45.93,Main,Izetta,0,0,0,,em sẽ cố gắng hết mình,
Dialogue: 0,0:07:45.93,0:07:48.23,Main,Izetta,0,0,0,,mong anh giúp đỡ!
Dialogue: 0,0:07:48.23,0:07:49.49,Main,Izetta,0,0,0,,Xin cảm ơn!
Dialogue: 0,0:07:51.84,0:07:54.16,Main,Groman,0,0,0,,Tôi cũng vậy, tiểu thư phù thủy.
Dialogue: 0,0:07:56.64,0:08:02.46,Main,Fine,0,0,0,,Cũng may là mẫu hạm đó đi vào vùng vịnh nhiều ma lực.
Dialogue: 0,0:08:03.78,0:08:06.49,Main,Fine,0,0,0,,Nhưng theo như bản đồ,
Dialogue: 0,0:08:06.49,0:08:09.62,Main,Fine,0,0,0,,mạch ma lực khu vực này có nhiều chỗ ngắt quãng.
Dialogue: 0,0:08:12.80,0:08:13.51,Main,Fine,0,0,0,,Izetta?
Dialogue: 0,0:08:15.04,0:08:16.33,Main,Izetta,0,0,0,,Vâng?
Dialogue: 0,0:08:16.33,0:08:17.34,Main,Fine,0,0,0,,Sao vậy?
Dialogue: 0,0:08:17.34,0:08:18.66,Main,Fine,0,0,0,,Mặt cậu đỏ lên kìa.
Dialogue: 0,0:08:19.44,0:08:20.41,Main,Fine,0,0,0,,Không lẽ...
Dialogue: 0,0:08:22.18,0:08:23.10,Main,Izetta,0,0,0,,Ưm...
Dialogue: 0,0:08:25.30,0:08:26.61,Main,fine,0,0,0,,Cậu bị sốt?
Dialogue: 0,0:08:27.20,0:08:28.23,Main,Izetta,0,0,0,,K-Không ạ
Dialogue: 0,0:08:28.72,0:08:30.11,Main,Izetta,0,0,0,,Em vẫn ổn!
Dialogue: 0,0:08:30.11,0:08:33.21,Main,Izetta,0,0,0,,Nhưng ở chung phòng với Công chúa,
Dialogue: 0,0:08:33.64,0:08:35.51,Main,Izetta,0,0,0,,còn nằm chung giường nữa...
Dialogue: 0,0:08:35.51,0:08:36.37,Main,Izetta,0,0,0,,Nên...
Dialogue: 0,0:08:37.70,0:08:38.68,Main,Fine,0,0,0,,Không thích à?
Dialogue: 0,0:08:39.88,0:08:41.70,Main,Fine,0,0,0,,Vậy đâu có sao!
Dialogue: 0,0:08:44.12,0:08:44.79,Main,Fine,0,0,0,,Bắt được rồi!
Dialogue: 0,0:08:47.90,0:08:50.50,Main,Fine,0,0,0,,Chỗ này thì sao?
Dialogue: 0,0:08:50.50,0:08:51.19,Main,,0,0,0,,Khoan—
Dialogue: 0,0:08:51.19,0:08:52.33,Main,Izetta,0,0,0,,Công chúa!
Dialogue: 0,0:08:54.44,0:08:56.41,Main,Fine,0,0,0,,Cù này!
Dialogue: 0,0:09:04.95,0:09:07.78,Main,Fine,0,0,0,,Mình đòi hỏi cậu nhiều quá.
Dialogue: 0,0:09:08.87,0:09:13.82,Main,Fine,0,0,0,,Tất cả mọi người ở đây đều đại diện cho đất nước mình.
Dialogue: 0,0:09:14.71,0:09:16.52,Main,Fine,0,0,0,,Nếu muốn họ giúp,
Dialogue: 0,0:09:17.03,0:09:22.68,Main,Fine,0,0,0,,phải cho họ thấy chúng ta có thể làm được gì.
Dialogue: 0,0:09:23.30,0:09:25.71,Main,Izetta,0,0,0,,Xin cứ tin ở em, Công chúa.
Dialogue: 0,0:09:26.18,0:09:31.40,Main,Izetta,0,0,0,,Em đã thề sẽ làm tất cả để bảo vệ mọi người.
Dialogue: 0,0:09:31.96,0:09:36.12,Main,Izetta,0,0,0,,Nên em sẵn sàng phá hủy chiến hạm đó hay bất cứ gì.
Dialogue: 0,0:09:37.24,0:09:38.36,Main,Fine,0,0,0,,Izetta...
Dialogue: 0,0:09:46.77,0:09:48.27,Main,Soldier,0,0,0,,Yosh, theo.
Dialogue: 0,0:09:49.01,0:09:50.19,Main,Soldier,0,0,0,,Ba cây.
Dialogue: 0,0:09:52.74,0:09:54.04,Main,Soldier,0,0,0,,Quả là Đại úy.
Dialogue: 0,0:09:54.04,0:09:56.03,Main,Soldier,0,0,0,,Chơi poker giỏi không kém gì lái máy bay.
Dialogue: 0,0:09:56.03,0:09:59.03,Main,Basler,0,0,0,,Gì chứ, ăn hên thôi.
Dialogue: 0,0:09:59.03,0:10:00.81,Main,Basler,0,0,0,,Mấy cậu thì sao?
Dialogue: 0,0:10:00.81,0:10:03.04,Main,Basler,0,0,0,,Thấy mẫu hạm đầu tiên của Germania thế nào?
Dialogue: 0,0:10:03.04,0:10:07.20,Main,Soldier,0,0,0,,Chả biết nữa, một nửa trang bị vẫn chưa lắp đặt.
Dialogue: 0,0:10:07.20,0:10:11.10,Main,Soldier,0,0,0,,Khoang thuyền còn chỗ chưa ráp vỏ.
Dialogue: 0,0:10:11.10,0:10:14.44,Main,Soldier,0,0,0,,Phải rồi, tôi đã thấy máy bay của Đại úy.
Dialogue: 0,0:10:14.44,0:10:15.80,Main,Soldier,0,0,0,,Loại mới nhỉ?
Dialogue: 0,0:10:16.45,0:10:17.92,Main,Soldier,0,0,0,,Nó thế nào vậy?
Dialogue: 0,0:10:17.92,0:10:19.63,Main,Basler,0,0,0,,Để xem...
Dialogue: 0,0:10:19.63,0:10:21.71,Main,Basler,0,0,0,,Rất là cơ động.
Dialogue: 0,0:10:21.71,0:10:24.04,Main,Basler,0,0,0,,Hai cánh khá nhẹ,
Dialogue: 0,0:10:24.04,0:10:26.17,Main,Basler,0,0,0,,nên quay tròn dễ dàng.
Dialogue: 0,0:10:26.17,0:10:28.70,Main,Basler,0,0,0,,Động cơ thì rất mạnh.
Dialogue: 0,0:10:28.70,0:10:30.64,Main,Basler,0,0,0,,Ngon hơn con 109 nhiều.
Dialogue: 0,0:10:30.64,0:10:32.44,Main,Basler,0,0,0,,Khi mà tăng hết tốc—
Dialogue: 0,0:10:33.44,0:10:37.15,Main,Rickert,0,0,0,,Thông tin đó là tối mật đấy, Đại úy.
Dialogue: 0,0:10:37.15,0:10:39.23,Main,Basler,0,0,0,,Chán vậy.
Dialogue: 0,0:10:39.23,0:10:41.88,Main,Basler,0,0,0,,Nói chung nó là một con ngựa hoang.
Dialogue: 0,0:10:41.88,0:10:44.18,Main,Basler,0,0,0,,Ngoài tôi không ai cưỡi nổi đâu.
Dialogue: 0,0:10:44.95,0:10:46.92,Main,Basler,0,0,0,,Tố thêm 20 nữa.
Dialogue: 0,0:10:48.53,0:10:50.37,Main,Basler,0,0,0,,Mà đã kéo đến tận đây rồi.
Dialogue: 0,0:10:51.08,0:10:53.22,Main,Basler,0,0,0,,Thông tin của cậu nên chính xác thì hơn.
Dialogue: 0,0:10:53.22,0:10:54.44,Main,Rickert,0,0,0,,Vâng.
Dialogue: 0,0:10:54.44,0:10:59.85,Main,Rickert,0,0,0,,Theo Thiếu tá nói, nguồn tin này đáng tin.
Dialogue: 0,0:11:01.65,0:11:03.85,Main,Rickert,0,0,0,,Thế này có vẻ hơi khó.
Dialogue: 0,0:11:06.84,0:11:09.32,Main,Rickert,0,0,0,,Thôi kệ. Tố hết.
Dialogue: 0,0:11:12.63,0:11:13.66,Main,Basler,0,0,0,,Tôi rút.
Dialogue: 0,0:11:15.20,0:11:17.25,Main,Basler,0,0,0,,Mặt cậu hiện hết ra rồi.
Dialogue: 0,0:11:17.25,0:11:21.40,Main,Basler,0,0,0,,Cậu không bắt chước Berkman được đâu, đừng cố.
Dialogue: 0,0:11:21.40,0:11:23.57,Main,Soldier,0,0,0,,Đừng để ý làm gì.
Dialogue: 0,0:11:26.94,0:11:30.67,Main,Groman,0,0,0,,Cô ấy sẽ bay theo thung lũng này và tiến vào vịnh
Dialogue: 0,0:11:30.67,0:11:31.92,Main,Groman,0,0,0,,rồi tấn công bất ngờ.
Dialogue: 0,0:11:32.20,0:11:34.33,Main,Benoit,0,0,0,,Sao lại theo hướng đó?
Dialogue: 0,0:11:35.25,0:11:38.83,Main,Groman,0,0,0,,Đây là đề nghị từ phía Eylstadt.
Dialogue: 0,0:11:38.83,0:11:42.94,Main,Groman,0,0,0,,Hiện tại không có máy bay nào có thể vượt qua thung lũng này,
Dialogue: 0,0:11:42.94,0:11:45.45,Main,Groman,0,0,0,,nên có lẽ sẽ không có pháo phòng không.
Dialogue: 0,0:11:46.08,0:11:47.68,Main,Magnus,0,0,0,,Cảnh giác quá nhỉ.
Dialogue: 0,0:11:47.68,0:11:48.86,Main,Groman,0,0,0,,Tất nhiên.
Dialogue: 0,0:11:48.86,0:11:52.69,Main,Groman,0,0,0,,Tiểu thư Izetta sẽ mang theo ngư lôi.
Dialogue: 0,0:11:53.22,0:11:55.90,Main,Groman,0,0,0,,Bốn trái loại 760 kg.
Dialogue: 0,0:11:55.90,0:11:57.34,Main,Murmurs,0,0,0,,Cái gì?
Dialogue: 0,0:12:00.37,0:12:02.28,Main,Fine,0,0,0,,Sáng mai sẽ tiến hành.
Dialogue: 0,0:12:02.28,0:12:06.15,Main,Fine,0,0,0,,Nếu muốn các vị có thể dùng máy bay để theo dõi.
Dialogue: 0,0:12:06.60,0:12:11.20,Main,Fine,0,0,0,,Tốt nhất là nên tự đi, hoặc cử ai đó đáng tin cũng được.
Dialogue: 0,0:12:12.07,0:12:13.47,Main,Mechanics,0,0,0,,Ngư lôi đã sẵn sàng chưa?
Dialogue: 0,0:12:13.47,0:12:14.21,Main,Mechanics,0,0,0,,Rồi!
Dialogue: 0,0:12:14.21,0:12:15.45,Main,Mechanics,0,0,0,,Kiểm tra lại.
Dialogue: 0,0:12:15.45,0:12:17.21,Main,,0,0,0,,Số hai ổn! Số ba ổn!
Dialogue: 0,0:12:17.59,0:12:20.01,Main,Izetta,0,0,0,,Em đi đây, Công chúa.
Dialogue: 0,0:12:22.84,0:12:24.55,Main,Fine,0,0,0,,Nhớ bình an quay về.
Dialogue: 0,0:12:25.97,0:12:26.74,Main,Izetta,0,0,0,,Vâng!
Comment: 0,0:12:46.07,0:12:48.03,Show_Title_Small_UR,,0,0,0,,{\fad(984,1)}I z e t t a: T h e L a s t {\c&H000080&}W {\c&HFFFFFF&}i t c h
Comment: 0,0:12:50.08,0:12:52.04,Show_Title_Small_LR,,0,0,0,,{\fad(984,1)}I z e t t a: T h e L a s t {\c&H000080&}W {\c&HFFFFFF&}i t c h
Dialogue: 0,0:13:09.16,0:13:11.60,Main,Groman,0,0,0,,Hai người thân quá nhỉ?
Dialogue: 0,0:13:11.60,0:13:13.12,Main,Groman,0,0,0,,Cô với Công chúa...
Dialogue: 0,0:13:13.12,0:13:15.44,Main,Groman,0,0,0,,À quên, với nữ Đại công tước chứ.
Dialogue: 0,0:13:15.96,0:13:19.06,Main,Groman,0,0,0,,Nói thật thì nhiệm vụ này rất nguy hiểm,
Dialogue: 0,0:13:19.06,0:13:23.57,Main,Groman,0,0,0,,cũng có thể nói là điên rồ, ngay cả đối với phù thủy.
Dialogue: 0,0:13:23.57,0:13:25.99,Main,Groman,0,0,0,,Nhưng trông cô rất vui.
Dialogue: 0,0:13:25.99,0:13:29.13,Main,Izetta,0,0,0,,Em hạnh phúc lắm.
Dialogue: 0,0:13:29.84,0:13:31.87,Main,Izetta,0,0,0,,Vì có thể dùng sức mạnh bị căm ghét này
Dialogue: 0,0:13:31.87,0:13:35.54,Main,,0,0,0,,để bảo vệ Công chúa và người dân Eylstadt em yêu quý.
Dialogue: 0,0:13:39.33,0:13:42.23,Main,Groman,0,0,0,,Có điều gì để bảo vệ thật tuyệt.
Dialogue: 0,0:13:42.69,0:13:45.38,Main,Groman,0,0,0,,Nhất là khi có sức mạnh để làm điều đó.
Dialogue: 0,0:13:46.80,0:13:48.92,Main,Groman,0,0,0,,Chúng ta sắp đến rồi!
Dialogue: 0,0:13:48.92,0:13:49.89,Main,Groman,0,0,0,,Sẵn sàng chưa?
Dialogue: 0,0:13:50.39,0:13:51.35,Main,Izetta,0,0,0,,Vâng!
Dialogue: 0,0:13:53.26,0:13:54.36,Main,Man,0,0,0,,Chúc may mắn!
Dialogue: 0,0:13:54.36,0:13:55.14,Main,Izetta,0,0,0,,Mọi người cũng vậy!
Dialogue: 0,0:13:58.53,0:13:59.64,Main,Men,0,0,0,,Bay thật kìa!
Dialogue: 0,0:14:12.69,0:14:14.41,Main,Men,0,0,0,,Rồi, thả chúng ra!
Dialogue: 0,0:14:14.41,0:14:15.91,Main,Men,0,0,0,,Rõ!
Dialogue: 0,0:14:15.91,0:14:16.95,Main,Men,0,0,0,,Bắt đầu thả!
Dialogue: 0,0:14:23.38,0:14:24.22,Main,Izetta,0,0,0,,Đi thôi!
Dialogue: 0,0:14:26.33,0:14:27.38,Main,Man,0,0,0,,Kinh thật...
Dialogue: 0,0:14:46.71,0:14:49.20,Main,izetta,0,0,0,,Nhắm vào... đáy tàu?
Dialogue: 0,0:14:49.57,0:14:55.29,Main,Groman,0,0,0,,Nếu cho ngư lôi nổ ở nơi thấp nhất và ngay chính giữa chiến hạm,
Dialogue: 0,0:14:55.29,0:14:58.08,Main,Groman,0,0,0,,vụ nổ sẽ đẩy nó lên cao,
Dialogue: 0,0:14:58.08,0:15:01.79,Main,Groman,0,0,0,,và sức nặng từ hai bên sẽ gây áp lực lên phần thân.
Dialogue: 0,0:15:01.79,0:15:05.22,Main,Groman,0,0,0,,Nhưng dư chấn từ vụ nổ sẽ nhanh chóng biến mất.
Dialogue: 0,0:15:05.89,0:15:08.67,Main,Groman,0,0,0,,Lúc này trọng lực sẽ kéo phần giữa thân xuống,
Dialogue: 0,0:15:08.67,0:15:11.76,Main,Groman,0,0,0,,rồi bẻ cong con tàu theo hướng ngược lại,
Dialogue: 0,0:15:11.76,0:15:13.26,Main,Groman,0,0,0,,cứ như vậy và...
Dialogue: 0,0:15:14.93,0:15:18.71,Main,Groman,0,0,0,,Nhưng nếu muốn phá hủy một chiến hạm cỡ lớn,
Dialogue: 0,0:15:18.71,0:15:23.40,Main,Groman,0,0,0,,cần cho nổ cả bốn ngư lôi đúng thời điểm ở gần trung tâm thân tàu,
Dialogue: 0,0:15:24.07,0:15:27.20,Main,Groman,0,0,0,,và dưới độ sâu nhất định.
Dialogue: 0,0:15:27.20,0:15:28.28,Main,Groman,0,0,0,,Có được không?
Dialogue: 0,0:15:30.10,0:15:31.41,Italics,Izetta,0,0,0,,Sẽ ổn thôi.
Dialogue: 0,0:15:31.41,0:15:35.83,Italics,Izetta,0,0,0,,Ở đây và trung tâm vịnh rất dồi dào ma lực.
Dialogue: 0,0:15:36.58,0:15:37.77,Italics,Izetta,0,0,0,,Mình sẽ làm được!
Dialogue: 0,0:15:52.45,0:15:54.01,Main,Izetta,0,0,0,,Qua khỏi đây là...
Dialogue: 0,0:16:08.94,0:16:09.53,Main,Izetta,0,0,0,,Không có!
Dialogue: 0,0:16:13.24,0:16:14.44,Main,Izetta,0,0,0,,Ra khơi rồi sao!
Dialogue: 0,0:16:15.32,0:16:16.21,Main,Izetta,0,0,0,,Phải đuổi theo!
Dialogue: 0,0:16:25.25,0:16:26.89,Italics,Gu,0,0,0,,Mục tiêu đã tiến vào vịnh!
Dialogue: 0,0:16:26.89,0:16:28.21,Italics,Gu,0,0,0,,Là Phù thủy Eylstadt!
Dialogue: 0,0:16:28.21,0:16:30.06,Italics,Gu,0,0,0,,Tất cả phi công xuất kích!
Dialogue: 0,0:16:30.06,0:16:32.32,Main,Basler,0,0,0,,Nhớ chắn phía trên như kế hoạch!
Dialogue: 0,0:16:32.32,0:16:32.92,Main,,0,0,0,,Rõ!
Dialogue: 0,0:16:57.44,0:17:01.41,Main,Groman,0,0,0,,Cần phải kích nổ cả bốn ngư lôi cùng một lúc.
Dialogue: 0,0:17:06.31,0:17:08.28,Main,pilot,0,0,0,,Cô ấy đã tiếp cận địch, nhưng...
Dialogue: 0,0:17:08.28,0:17:09.09,Main,pilot,0,0,0,,Đây là...
Dialogue: 0,0:17:09.40,0:17:10.41,Main,pilot,0,0,0,,Sao vậy?
Dialogue: 0,0:17:10.41,0:17:11.71,Main,pilot,0,0,0,,Bị phục kích rồi!
Dialogue: 0,0:17:11.71,0:17:13.34,Main,pilot,0,0,0,,Chúng đã biết kế hoạch của ta!
Dialogue: 0,0:17:18.20,0:17:19.72,Main,Officer,0,0,0,,Nó chưa được thử nghiệm thực chiến!
Dialogue: 0,0:17:19.72,0:17:20.51,Main,Officer,0,0,0,,Ngài chắc chứ?
Dialogue: 0,0:17:20.81,0:17:22.90,Main,Basler,0,0,0,,Cậu nghĩ tôi là ai?
Dialogue: 0,0:17:22.90,0:17:24.08,Main,Basler,0,0,0,,Xuống đi.
Dialogue: 0,0:17:24.08,0:17:26.25,Main,Basler,0,0,0,,Hay là muốn đi chung?
Dialogue: 0,0:17:26.25,0:17:27.50,Main,Officer,0,0,0,,Sieg Reich!
Dialogue: 0,0:17:54.16,0:17:55.85,Main,Basler,0,0,0,,Không tệ.
Dialogue: 0,0:17:55.85,0:17:57.64,Main,Basler,0,0,0,,Nhưng đây là kết thúc rồi.
Dialogue: 0,0:17:57.64,0:18:00.90,Main,Basler,0,0,0,,Hãy trả giá cho trận chiến ở dãy An-pơ!
Dialogue: 0,0:18:07.07,0:18:08.02,Main,Izetta,0,0,0,,Theo kịp sao?
Dialogue: 0,0:18:16.49,0:18:18.61,Main,Rickert,0,0,0,,Yosh, có thể được!
Dialogue: 0,0:18:38.55,0:18:40.09,Main,Basler,0,0,0,,Chịu thua rồi sao?
Dialogue: 0,0:18:40.09,0:18:41.55,Main,Basler,0,0,0,,Đừng hòng thoát!
Dialogue: 0,0:18:49.99,0:18:50.75,Italics,Pilot,0,0,0,,Đại úy!
Dialogue: 0,0:18:50.75,0:18:51.95,Main,Basler,0,0,0,,Bao vây cô ta!
Dialogue: 0,0:18:51.95,0:18:53.23,Main,Basler,0,0,0,,Đừng để cô ta ra khỏi vịnh!
Dialogue: 0,0:18:59.20,0:19:02.16,Main,Pilot,0,0,0,,Có máy bay cất cánh từ mẫu hạm!
Dialogue: 0,0:19:04.07,0:19:05.79,Main,Groman,0,0,0,,Đi thôi anh em!
Dialogue: 0,0:19:44.40,0:19:45.82,Italics,Izetta,0,0,0,,Có chỗ không có ma lực.
Dialogue: 0,0:19:45.82,0:19:46.95,Italics,Izetta,0,0,0,,Phải cẩn thận mới được.
Dialogue: 0,0:19:47.28,0:19:49.57,Main,Rickert,0,0,0,,Vừa rồi là sao?
Dialogue: 0,0:19:49.57,0:19:51.05,Main,Rickert,0,0,0,,Cô ta giảm tốc ư?
Dialogue: 0,0:19:51.05,0:19:51.79,Main,Berkman,0,0,0,,Biết ngay mà.
Dialogue: 0,0:19:55.21,0:19:56.96,Main,Basler,0,0,0,,Giờ tính sao đây?
Dialogue: 0,0:19:57.36,0:19:59.17,Main,Basler,0,0,0,,Cô nằm trong tầm nhắm rồi!
Dialogue: 0,0:20:04.70,0:20:05.76,Main,,0,0,0,,Không lẽ là...
Dialogue: 0,0:20:07.50,0:20:10.48,Italics,Izetta,0,0,0,,Mình đã hứa là sẽ làm được!
Dialogue: 0,0:20:16.97,0:20:18.36,Main,Basler,0,0,0,,Vẫn chưa chết à?
Dialogue: 0,0:20:32.71,0:20:33.41,Main,Izetta,0,0,0,,Tiến lên!
Dialogue: 0,0:20:37.66,0:20:38.60,Main,Basler,0,0,0,,Cái gì?
Dialogue: 0,0:20:38.60,0:20:41.84,Italics,Pilot,0,0,0,,Cô ta trực tiếp tấn công hệ thống nâng?
Dialogue: 0,0:20:41.84,0:20:42.76,Main,Basler,0,0,0,,Đừng có hòng!
Dialogue: 0,0:20:43.25,0:20:44.84,Main,Basler,0,0,0,,Tất cả theo tôi!
Dialogue: 0,0:20:53.01,0:20:54.39,Main,Basler,0,0,0,,Chết đi!
Dialogue: 0,0:20:59.98,0:21:04.53,Main,Groman,0,0,0,,Danh dự Không quân Britannia sẽ bị bôi nhọ nếu bỏ rơi một cô gái!
Dialogue: 0,0:21:05.03,0:21:06.11,Main,Izetta,0,0,0,,Groman-san!
Dialogue: 0,0:21:06.11,0:21:07.49,Main,Basler,0,0,0,,Tên chết tiệt!
Dialogue: 0,0:21:15.42,0:21:15.92,Main,Izetta,0,0,0,,Ngay bây giờ!
Dialogue: 0,0:21:23.30,0:21:24.76,Main,groman,0,0,0,,Được lắm!
Dialogue: 0,0:21:24.76,0:21:26.24,Main,groman,0,0,0,,Trúng khoang nhiên liệu rồi!
Dialogue: 0,0:21:26.72,0:21:29.08,Main,Basler,0,0,0,,Rơi cùng con phù thủy đó đi.
Dialogue: 0,0:21:29.08,0:21:29.93,Main,Basler,0,0,0,,Đừng có—
Dialogue: 0,0:21:29.44,0:21:31.84,Italics,Berkman,0,0,0,,Đủ rồi, Đại úy.
Dialogue: 0,0:21:31.84,0:21:32.43,Main,Basler,0,0,0,,Đến rồi à?
Dialogue: 0,0:21:32.77,0:21:34.90,Main,Berkman,0,0,0,,Nhiệm vụ kết thúc.
Dialogue: 0,0:21:34.90,0:21:37.40,Main,Berkman,0,0,0,,Không cần tiếp tục truy đuổi.
Dialogue: 0,0:21:37.65,0:21:38.92,Italics,Basler,0,0,0,,Nhưng mà...
Dialogue: 0,0:21:38.92,0:21:41.36,Main,Berkman,0,0,0,,Đây là lệnh, Đại úy.
Dialogue: 0,0:21:41.64,0:21:43.16,Italics,Berkman,0,0,0,,Hy vọng anh tuân thủ.
Dialogue: 0,0:21:57.09,0:21:59.50,Main,Pilot,0,0,0,,Gió thổi vào mát ghê!
Dialogue: 0,0:22:00.69,0:22:03.76,Main,Groman,0,0,0,,Chả biết bằng cách nào, nhưng ta thành công rồi.
Dialogue: 0,0:22:05.04,0:22:06.84,Main,Groman,0,0,0,,Tốt lắm, cô gái!
Dialogue: 0,0:22:21.21,0:22:24.11,Main,Rickert,0,0,0,,Đó là điểm yếu của phù thủy sao?
Dialogue: 0,0:22:24.72,0:22:27.24,Main,Berkman,0,0,0,,Sau trận hôm nay thì tôi chắc chắn.
Dialogue: 0,0:22:27.91,0:22:31.58,Main,Berkman,0,0,0,,Cũng đáng khi dùng Drachenfels làm mồi.
Dialogue: 0,0:22:31.58,0:22:32.33,Main,Rickert,0,0,0,,Mồi?
Dialogue: 0,0:22:32.66,0:22:33.56,Main,Berkman,0,0,0,,Ờ.
Dialogue: 0,0:22:34.04,0:22:36.48,Main,Berkman,0,0,0,,Nếu đem nó ra vùng biển Bắc,
Dialogue: 0,0:22:36.48,0:22:39.78,Main,,0,0,0,,chắc chắn Britannia sẽ tấn công để đánh chìm nó,
Dialogue: 0,0:22:40.21,0:22:44.60,Main,Berkman,0,0,0,,nên Bệ hạ cho phép tôi dùng nó để dụ địch tới đây.
Dialogue: 0,0:22:45.91,0:22:48.26,Main,Rickert,0,0,0,,Chỉ để tìm ra điểm yếu của cô ta sao?
Dialogue: 0,0:22:48.60,0:22:49.51,Main,Berkman,0,0,0,,Phải.
Dialogue: 0,0:22:49.92,0:22:53.81,Main,Berkman,0,0,0,,Và để bắt đầu kế hoạch mới của chúng ta.
Dialogue: 0,0:22:54.21,0:22:55.92,Main,Rickert,0,0,0,,Kế hoạch mới?
Dialogue: 0,0:22:55.92,0:22:56.70,Main,Berkman,0,0,0,,Ờ.
Dialogue: 0,0:22:57.70,0:23:01.58,Main,Berkman,0,0,0,,Nhưng trước đó còn một chuyện cần làm rõ.
Dialogue: 0,0:23:03.19,0:23:06.20,Main,Berkman,0,0,0,,Sự thật về Phù thủy trắng,
Dialogue: 0,0:23:06.20,0:23:08.90,Main,,0,0,0,,được cho là nằm bên dưới lâu đài Eylstadt cổ.
Dialogue: 0,0:23:11.33,0:23:15.37,Main,Berkman,0,0,0,,Hiện tại Laurence, gián điệp ta cài vào vẫn chưa bị lộ,
Dialogue: 0,0:23:15.69,0:23:17.99,Main,Berkman,0,0,0,,chắc tôi sẽ giao cho anh ta...
Dialogue: 0,0:23:19.09,0:23:21.08,Main,Rickert,0,0,0,,Xin chờ đã, Thiếu tá.
Dialogue: 0,0:23:23.03,0:23:26.80,Main,Rickert,0,0,0,,Có thể giao nhiệm vụ đó cho tôi không?

Dialogue: 0,0:00:00.00,0:00:10.00,Default,,0,0,0,,[Chương trình này có phụ đề tiếng Việt]
Dialogue: 0,0:00:04.45,0:00:10.87,Default,,0,0,0,,[CAO THỦ BÓNG RỔ]
Dialogue: 0,0:00:29.07,0:00:35.79,Default,,0,0,0,,♫ Ánh mặt trời chói rọi phía sau lưng,\Ntôi bước trên con đường này ♫
Dialogue: 0,0:00:35.84,0:00:41.98,Default,,0,0,0,,♫ Cũng như thường lệ, lại bị vỗ vai ♫
Dialogue: 0,0:00:42.58,0:00:49.08,Default,,0,0,0,,♫ Cậu là nữ thần của tôi, không có lý do gì cả ♫
Dialogue: 0,0:00:49.14,0:00:54.58,Default,,0,0,0,,♫ Nhưng lại chưa từng khoác lấy tay tôi ♫
Dialogue: 0,0:00:54.66,0:01:01.22,Default,,0,0,0,,♫ Không biết từ lúc nào,\Nánh mắt đã không thể rời đi ♫
Dialogue: 0,0:01:01.36,0:01:07.94,Default,,0,0,0,,♫ Không buông ra, không do dự,\Nđiên cuồng vì cậu ♫
Dialogue: 0,0:01:07.96,0:01:14.88,Default,,0,0,0,,♫ Tôi muốn hét to rằng tôi thích cậu,\Nmuốn thử thay đổi ngày mai ♫
Dialogue: 0,0:01:14.98,0:01:21.44,Default,,0,0,0,,♫ Tôi muốn xé toạc\Nquãng thời gian đông cứng ♫
Dialogue: 0,0:01:21.46,0:01:28.68,Default,,0,0,0,,♫ Tôi muốn hét to rằng tôi thích cậu,\Ndũng cảm tiến lên một bước ♫
Dialogue: 0,0:01:28.84,0:01:36.06,Default,,0,0,0,,♫ Xin cậu hãy chấp nhận\Ntrái tim nồng cháy này của tôi ♫
Dialogue: 0,0:01:45.90,0:01:46.95,Default,,0,0,0,,[Sakuragi Hanamichi,]
Dialogue: 0,0:01:46.95,0:01:50.13,Default,,0,0,0,,[người bị 50 bạn nữ\Nliên tục từ chối tời cấp hai.]
Dialogue: 0,0:01:51.69,0:01:52.79,Default,,0,0,0,,[Trước mặt một Hanamichi như thế,]
Dialogue: 0,0:01:53.16,0:01:55.50,Default,,0,0,0,,[đấng cứu thế Haruko xuất hiện.]
Dialogue: 0,0:01:57.65,0:01:59.11,Default,,0,0,0,,[Để tiếp cận Haruko,]
Dialogue: 0,0:01:59.26,0:02:01.96,Default,,0,0,0,,[Hanamichi đã nhận lời mời\Ntham gia đội bóng rổ.]
Dialogue: 0,0:02:02.56,0:02:03.83,Default,,0,0,0,,[Nhưng lại cứ xảy ra xung đột]
Dialogue: 0,0:02:03.94,0:02:06.47,Default,,0,0,0,,[với anh trai của Haruko,\Nđội trưởng đội bóng rổ - Akagi Takenori.]
Dialogue: 0,0:02:10.68,0:02:11.45,Default,,0,0,0,,[Chính vào lúc này,]
Dialogue: 0,0:02:11.65,0:02:15.73,Default,,0,0,0,,[Shohoku có một trận đấu giao hữu\Nvới trường Ryonan, top bốn toàn tỉnh.]
Dialogue: 0,0:02:16.58,0:02:18.90,Default,,0,0,0,,[Dưới sự cố gắng của Hanamichi\Nvà sự phối hợp của Rukawa,]
Dialogue: 0,0:02:19.03,0:02:22.30,Default,,0,0,0,,[đã đưa tỷ số của đội bóng Shohoku\Ntừ khoảng cách xa kéo lại chỉ còn]
Dialogue: 0,0:02:22.53,0:02:23.83,Default,,0,0,0,,[chênh lệch một điểm với đội Ryonan.]
Dialogue: 0,0:02:24.30,0:02:26.33,Default,,0,0,0,,[Pha ném bóng xuất sắc của Hanamichi\Nđã lật ngược tình thế,]
Dialogue: 0,0:02:26.52,0:02:29.13,Default,,0,0,0,,[có được số điểm dẫn trước quyết định.]
Dialogue: 0,0:02:33.03,0:02:36.14,Default,,0,0,0,,[Nhưng Sendoh đã nhân lúc\NShohoku không chú ý, ném bóng vào rổ,]
Dialogue: 0,0:02:36.31,0:02:38.20,Default,,0,0,0,,[khiến cho Shohoku nếm trải mùi vị thất bại.]
Dialogue: 0,0:02:45.03,0:02:46.66,Default,,0,0,0,,[Dưới sự cổ vũ của Haruko,]
Dialogue: 0,0:02:46.69,0:02:49.16,Default,,0,0,0,,[Hanamichi đã nhanh chóng đứng lên\Nsau cú sốc thua bóng,]
Dialogue: 0,0:02:49.36,0:02:51.56,Default,,0,0,0,,[đồng thời còn mua một đôi giày bóng rổ mới.]
Dialogue: 0,0:02:52.39,0:02:54.06,Default,,0,0,0,,[Bây giờ, đội bóng Shohoku]
Dialogue: 0,0:02:54.24,0:02:57.86,Default,,0,0,0,,[đã đốt cháy lại ý chí\Nhướng đến mục tiêu chinh phục toàn quốc.]
Dialogue: 0,0:02:59.46,0:03:00.36,Default,,0,0,0,,Cái thằng nhóc đó,
Dialogue: 0,0:03:01.26,0:03:02.76,Default,,0,0,0,,chắc là đang nóng lòng ra sân đây mà.
Dialogue: 0,0:03:02.79,0:03:03.96,Default,,0,0,0,,Cái thằng nhóc đó?
Dialogue: 0,0:03:04.46,0:03:05.16,Default,,0,0,0,,Đừng có giả vờ nữa.
Dialogue: 0,0:03:06.76,0:03:08.16,Default,,0,0,0,,Đương nhiên chỉ có người đó rồi.
Dialogue: 0,0:03:09.59,0:03:12.06,Default,,0,0,0,,Này, mày tên gì vậy?
Dialogue: 0,0:03:12.80,0:03:15.46,Default,,0,0,0,,Mito Yohei, còn mày?
Dialogue: 0,0:03:17.56,0:03:18.06,Default,,0,0,0,,Miyagi.
Dialogue: 0,0:03:19.57,0:03:20.96,Default,,0,0,0,,Miyagi Ryota.
Dialogue: 0,0:03:22.29,0:03:29.71,Default,,0,0,0,,[Đứa trẻ rắc rối!\NHanamichi đấu với Miyagi.]
Dialogue: 0,0:03:24.50,0:03:28.31,Default,,0,0,0,,[Đứa trẻ rắc rối!\NHanamichi đấu với Miyagi.]
Dialogue: 0,0:03:38.93,0:03:40.86,Default,,0,0,0,,Cuối cùng cũng mua được rồi à Hanamichi?
Dialogue: 0,0:03:40.99,0:03:44.37,Default,,0,0,0,,Đây là giày bóng rổ của Hanamichi ư?\NĐẹp quá đi.
Dialogue: 0,0:03:44.50,0:03:46.57,Default,,0,0,0,,Cho tao xem đi, mượn xem chút đi.
Dialogue: 0,0:03:47.50,0:03:49.31,Default,,0,0,0,,Mày đừng dùng bàn tay bẩn thỉu của mày\Nđụng nó.
Dialogue: 0,0:03:49.77,0:03:52.03,Default,,0,0,0,,Bọn mày tưởng đôi giày này là gì chứ!
Dialogue: 0,0:03:52.63,0:03:53.63,Default,,0,0,0,,Là gì?
Dialogue: 0,0:03:53.93,0:03:56.53,Default,,0,0,0,,Ngẩng đầu cao quá, thấp chút, thấp chút.
Dialogue: 0,0:03:57.43,0:03:58.53,Default,,0,0,0,,Bọn mày nghe rồi đừng hú hồn đó.
Dialogue: 0,0:03:58.83,0:04:03.43,Default,,0,0,0,,Đây là kiểu giày\Nmà cầu thủ siêu sao ABC từng mang đó.
Dialogue: 0,0:04:03.46,0:04:05.23,Default,,0,0,0,,ABC?
Dialogue: 0,0:04:06.33,0:04:07.23,Default,,0,0,0,,Không phải.
Dialogue: 0,0:04:07.30,0:04:08.53,Default,,0,0,0,,Là CIA.
Dialogue: 0,0:04:09.96,0:04:11.03,Default,,0,0,0,,Không phải.
Dialogue: 0,0:04:11.05,0:04:13.34,Default,,0,0,0,,Là FBI.
Dialogue: 0,0:04:15.15,0:04:16.15,Default,,0,0,0,,Cũng không phải.
Dialogue: 0,0:04:16.18,0:04:18.53,Default,,0,0,0,,Là PKO.
Dialogue: 0,0:04:19.01,0:04:20.33,Default,,0,0,0,,Nói chung là
Dialogue: 0,0:04:20.36,0:04:22.35,Default,,0,0,0,,một đôi giày bóng rổ rất có tiếng.
Dialogue: 0,0:04:22.82,0:04:24.23,Default,,0,0,0,,Lũ ngốc bọn mày hú hồn chưa?
Dialogue: 0,0:04:26.19,0:04:29.63,Default,,0,0,0,,Vậy đôi giày này\Nchắc đắt lắm phải không Hanamichi?
Dialogue: 0,0:04:30.46,0:04:32.56,Default,,0,0,0,,Cũng không bao nhiêu, chỉ có 30 yên thôi.
Dialogue: 0,0:04:32.90,0:04:34.06,Default,,0,0,0,,30 yên ư?
Dialogue: 0,0:04:34.41,0:04:36.36,Default,,0,0,0,,Sakuragi, cậu cảm thấy thế nào
Dialogue: 0,0:04:36.39,0:04:37.59,Default,,0,0,0,,khi mang đôi giày này?
Dialogue: 0,0:04:38.24,0:04:40.29,Default,,0,0,0,,Haruko! Chào cậu chào cậu.
Dialogue: 0,0:04:40.39,0:04:44.10,Default,,0,0,0,,Có lẽ trước kia là Rukawa cứ oai hơn mình
Dialogue: 0,0:04:44.13,0:04:45.37,Default,,0,0,0,,là vì đôi giày này nhỉ?
Dialogue: 0,0:04:45.40,0:04:48.86,Default,,0,0,0,,Giày bóng rổ mà nó mang khác mình, mình\Nchỉ mang giày thể thao bình thường thôi.
Dialogue: 0,0:04:50.81,0:04:52.37,Default,,0,0,0,,Chỉ hơn thua ở đôi giày.
Dialogue: 0,0:04:52.40,0:04:54.07,Default,,0,0,0,,Mình chỉ thua nó ở đôi giày nhỉ?
Dialogue: 0,0:04:56.61,0:04:57.46,Default,,0,0,0,,Trời cũng biết mà.
Dialogue: 0,0:05:02.94,0:05:06.16,Default,,0,0,0,,Một khi mà bổn thiên tài đây\Ncó được đôi giày bóng rổ này,
Dialogue: 0,0:05:06.19,0:05:07.06,Default,,0,0,0,,tên Rukawa đó
Dialogue: 0,0:05:07.09,0:05:08.26,Default,,0,0,0,,chẳng là gì cả.
Dialogue: 0,0:05:09.73,0:05:11.87,Default,,0,0,0,,Cộng thêm sự chênh lệch về thực lực,
Dialogue: 0,0:05:11.89,0:05:13.66,Default,,0,0,0,,mình tuyệt đối đẹp trai hơn nó.
Dialogue: 0,0:05:13.69,0:05:15.76,Default,,0,0,0,,Chỉ có đôi giày là thắng thôi, đôi giày.
Dialogue: 0,0:05:19.32,0:05:21.11,Default,,0,0,0,,Cậu cứ chờ xem đi Haruko.
Dialogue: 0,0:05:21.25,0:05:25.76,Default,,0,0,0,,Dẫn bóng, chuyền bóng, ba bước, ném vào rổ.
Dialogue: 0,0:05:28.22,0:05:29.62,Default,,0,0,0,,Thế nào?
Dialogue: 0,0:05:29.71,0:05:31.24,Default,,0,0,0,,Cậu giỏi lắm Sakuragi.
Dialogue: 0,0:05:31.42,0:05:33.97,Default,,0,0,0,,Đây chính là "như mèo mọc thêm cánh" ư?
Dialogue: 0,0:05:34.11,0:05:36.64,Default,,0,0,0,,Không phải như mèo, mà là "như hổ mọc thêm cánh".
Dialogue: 0,0:05:38.97,0:05:41.24,Default,,0,0,0,,Thiên tài Sakuragi\Nthật là hoàn hảo không chỗ chê.
Dialogue: 0,0:05:50.18,0:05:51.54,Default,,0,0,0,,[Nếu thằng nhóc này làm được,]
Dialogue: 0,0:05:51.78,0:05:54.04,Default,,0,0,0,,[việc xưng bá toàn quốc không còn là mơ nữa.]
Dialogue: 0,0:05:54.27,0:05:54.97,Default,,0,0,0,,[Nhưng mà...]
Dialogue: 0,0:05:55.12,0:05:56.44,Default,,0,0,0,,Thiên tài.
Dialogue: 0,0:05:57.23,0:05:57.94,Default,,0,0,0,,[Nhưng mà...]
Dialogue: 0,0:05:58.73,0:06:00.34,Default,,0,0,0,,[gặp phải Sakuragi]
Dialogue: 0,0:06:00.47,0:06:01.44,Default,,0,0,0,,[thằng nhóc đó sẽ...]
Dialogue: 0,0:06:14.72,0:06:15.74,Default,,0,0,0,,Miyagi.
Dialogue: 0,0:06:19.89,0:06:21.06,Default,,0,0,0,,Xuất viện rồi à?
Dialogue: 0,0:06:21.18,0:06:22.68,Default,,0,0,0,,Phải thông báo cho tao sớm hơn chứ.
Dialogue: 0,0:06:24.21,0:06:25.20,Default,,0,0,0,,Miyagi.
Dialogue: 0,0:06:28.38,0:06:29.28,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:06:34.45,0:06:36.45,Default,,0,0,0,,Ryota.
Dialogue: 0,0:06:37.05,0:06:38.87,Default,,0,0,0,,Cậu nói Ryota đi học lại rồi ư?
Dialogue: 0,0:06:38.99,0:06:40.56,Default,,0,0,0,,Cậu nói bậy, cậu ấy đâu có đến.
Dialogue: 0,0:06:40.66,0:06:43.05,Default,,0,0,0,,Mình học chung lớp với cậu ấy,\Nđến giờ cậu ấy vẫn còn nghỉ phép.
Dialogue: 0,0:06:43.19,0:06:44.99,Default,,0,0,0,,Nhưng mà Yasuko nói vừa gặp cậu ấy.
Dialogue: 0,0:06:45.32,0:06:47.06,Default,,0,0,0,,Với tính cách của Miyagi,
Dialogue: 0,0:06:47.46,0:06:49.16,Default,,0,0,0,,chắc cậu ấy\Nkhông tham gia hoạt động ngoại khóa đâu.
Dialogue: 0,0:06:51.94,0:06:53.42,Default,,0,0,0,,Tạm biệt Ayako.
Dialogue: 0,0:06:54.80,0:06:56.66,Default,,0,0,0,,Mình nghĩ mình nên đi tìm cậu ấy thì tốt hơn.
Dialogue: 0,0:06:58.44,0:07:01.15,Default,,0,0,0,,♫ Tôi là thiên tài ♫
Dialogue: 0,0:07:02.61,0:07:05.59,Default,,0,0,0,,♫ Thiên tài, thiên tài ♫
Dialogue: 0,0:07:05.99,0:07:09.50,Default,,0,0,0,,♫ Vận động viên bóng rổ Hanamichi ♫
Dialogue: 0,0:07:09.59,0:07:11.80,Default,,0,0,0,,Sakuragi Hanamichi.
Dialogue: 0,0:07:12.37,0:07:13.19,Default,,0,0,0,,Ayako.
Dialogue: 0,0:07:13.56,0:07:15.19,Default,,0,0,0,,Cậu đang hát cái gì thế?
Dialogue: 0,0:07:17.69,0:07:19.89,Default,,0,0,0,,Nghe ghê chết đi được.
Dialogue: 0,0:07:22.54,0:07:23.45,Default,,0,0,0,,Không mà không mà.
Dialogue: 0,0:07:23.82,0:07:26.00,Default,,0,0,0,,Hôm nay Haruko có đến xem bóng không ta?
Dialogue: 0,0:07:26.73,0:07:28.60,Default,,0,0,0,,Em ấy không có rảnh vậy đâu.
Dialogue: 0,0:07:29.50,0:07:31.40,Default,,0,0,0,,[Cái cậu đó đã xuất viện rồi sao?]
Dialogue: 0,0:07:32.63,0:07:35.03,Default,,0,0,0,,[Vậy thì đội bóng rổ\Nlại thêm một nhân vật rắc rối nữa rồi.]
Dialogue: 0,0:07:35.33,0:07:36.93,Default,,0,0,0,,[Lần này tiêu rồi.]
Dialogue: 0,0:07:38.98,0:07:41.03,Default,,0,0,0,,Thôi kệ, bây giờ có lo cũng chẳng ích gì.
Dialogue: 0,0:07:41.98,0:07:42.65,Default,,0,0,0,,Cái gì cơ?
Dialogue: 0,0:07:42.76,0:07:46.22,Default,,0,0,0,,Không có gì, có nghĩ cũng không có cách.
Dialogue: 0,0:07:46.65,0:07:48.04,Default,,0,0,0,,Được rồi, tôi đi tìm cậu ấy đây.
Dialogue: 0,0:07:48.24,0:07:49.82,Default,,0,0,0,,Siêu nhân bóng rổ, cậu đi với tôi đi.
Dialogue: 0,0:07:51.48,0:07:53.14,Default,,0,0,0,,Nhưng tuyệt đối đừng mê em quá đó.
Dialogue: 0,0:07:54.66,0:07:55.94,Default,,0,0,0,,Bọn mày đừng có quá đáng.
Dialogue: 0,0:07:56.39,0:07:58.04,Default,,0,0,0,,Tao vừa mới xuất viện đó.
Dialogue: 0,0:07:59.55,0:08:01.74,Default,,0,0,0,,Ở bệnh viện tao đã tu tâm dưỡng tính rồi.
Dialogue: 0,0:08:02.01,0:08:04.41,Default,,0,0,0,,Miyagi, mấy lời này không giống mày đó.
Dialogue: 0,0:08:04.55,0:08:05.65,Default,,0,0,0,,Sợ rồi ư?
Dialogue: 0,0:08:06.30,0:08:08.09,Default,,0,0,0,,Người hung hăng hơn tao
Dialogue: 0,0:08:08.21,0:08:09.93,Default,,0,0,0,,không phải đã vào trường chúng ta rồi sao?
Dialogue: 0,0:08:10.46,0:08:12.56,Default,,0,0,0,,Cái nhóm bốn người lớp 10 đó.
Dialogue: 0,0:08:17.34,0:08:18.64,Default,,0,0,0,,Đừng hòng trốn thoát.
Dialogue: 0,0:08:18.77,0:08:20.76,Default,,0,0,0,,Bọn tao muốn xử mày truớc, Miyagi.
Dialogue: 0,0:08:21.14,0:08:23.97,Default,,0,0,0,,Yên tâm đi, bọn nó sớm muộn gì cũng bị xử thôi.
Dialogue: 0,0:08:24.57,0:08:25.58,Default,,0,0,0,,Bọn mày nói sớm muộn sao?
Dialogue: 0,0:08:26.66,0:08:28.85,Default,,0,0,0,,Thật ra họ chẳng xem bọn mày ra gì.
Dialogue: 0,0:08:31.08,0:08:33.44,Default,,0,0,0,,Cái thằng tên Mito kia
Dialogue: 0,0:08:33.55,0:08:35.47,Default,,0,0,0,,thực lực hơn mày đó.
Dialogue: 0,0:08:36.61,0:08:37.61,Default,,0,0,0,,Hotta.
Dialogue: 0,0:08:43.12,0:08:44.21,Default,,0,0,0,,Mitsui.
Dialogue: 0,0:08:45.04,0:08:47.51,Default,,0,0,0,,Miyagi, tinh thần coi bộ ổn đó.
Dialogue: 0,0:08:48.08,0:08:49.61,Default,,0,0,0,,Như vậy thì tôi yên tâm rồi.
Dialogue: 0,0:08:53.75,0:08:54.92,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:08:55.70,0:08:56.96,Default,,0,0,0,,Miyagi đi học lại rồi sao?
Dialogue: 0,0:08:57.43,0:08:59.26,Default,,0,0,0,,Không, cậu ấy vẫn chưa bắt đầu đi học lại,
Dialogue: 0,0:08:57.59,0:09:00.43,Default,,0,0,0,,[Câu lạc bộ bóng rổ - Không được xông vào]
Dialogue: 0,0:08:59.50,0:09:00.86,Default,,0,0,0,,nhưng có người gặp cậu ấy trong trường.
Dialogue: 0,0:09:01.37,0:09:02.07,Default,,0,0,0,,Vậy sao?
Dialogue: 0,0:09:02.89,0:09:04.86,Default,,0,0,0,,Vậy là cậu ấy đã xuất viện rồi.
Dialogue: 0,0:09:05.08,0:09:06.46,Default,,0,0,0,,Nhanh hơn tôi tưởng đó.
Dialogue: 0,0:09:07.12,0:09:08.46,Default,,0,0,0,,Cậu nói cái người nằm viện
Dialogue: 0,0:09:08.49,0:09:10.06,Default,,0,0,0,,là một thành viên lớp 11 khác sao?
Dialogue: 0,0:09:10.25,0:09:10.86,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,0:09:11.31,0:09:12.06,Default,,0,0,0,,Thì ra là vậy.
Dialogue: 0,0:09:12.42,0:09:14.46,Default,,0,0,0,,Miyagi kịp trận đấu loại rồi.
Dialogue: 0,0:09:14.72,0:09:15.32,Default,,0,0,0,,Cái này tốt quá rồi.
Dialogue: 0,0:09:15.58,0:09:17.52,Default,,0,0,0,,Kogure, cái người tên Miyagi đó
Dialogue: 0,0:09:17.66,0:09:18.76,Default,,0,0,0,,thực lực mạnh vậy sao?
Dialogue: 0,0:09:20.35,0:09:22.15,Default,,0,0,0,,Cậu tự mình đi kiểm chứng là được rồi Kuwata.
Dialogue: 0,0:09:22.76,0:09:24.26,Default,,0,0,0,,Cậu ấy cũng là hậu vệ như cậu đó.
Dialogue: 0,0:09:24.82,0:09:25.56,Default,,0,0,0,,Hậu vệ!
Dialogue: 0,0:09:26.76,0:09:29.02,Default,,0,0,0,,Tôi đến rồi.
Dialogue: 0,0:09:29.33,0:09:32.86,Default,,0,0,0,,Kogure, Miyagi hình như về rồi đó.
Dialogue: 0,0:09:33.37,0:09:34.06,Default,,0,0,0,,Tôi cũng vừa nghe nói rồi.
Dialogue: 0,0:09:34.29,0:09:36.56,Default,,0,0,0,,Hy vọng là\Ntrong quãng thời gian đấu loại
Dialogue: 0,0:09:37.08,0:09:39.96,Default,,0,0,0,,không xảy ra vấn đề gì.
Dialogue: 0,0:09:40.35,0:09:42.55,Default,,0,0,0,,Đúng vậy,\Ntôi không muốn bị cấm thi đấu đâu.
Dialogue: 0,0:09:47.17,0:09:48.16,Default,,0,0,0,,Sao có thể chứ.
Dialogue: 0,0:09:48.54,0:09:50.06,Default,,0,0,0,,Sao lại cấm thi đấu?
Dialogue: 0,0:09:50.51,0:09:52.76,Default,,0,0,0,,Miyagi đã làm gì thế ạ?
Dialogue: 0,0:09:53.77,0:09:55.56,Default,,0,0,0,,Lẽ nào anh ấy đáng sợ vậy sao?
Dialogue: 0,0:09:57.86,0:10:02.08,Default,,0,0,0,,Gì cơ, Mitsui, anh cũng xuất viện rồi sao?
Dialogue: 0,0:10:02.73,0:10:04.73,Default,,0,0,0,,Trông cậu khỏe mạnh lắm đấy, Miyagi!
Dialogue: 0,0:10:05.62,0:10:07.22,Default,,0,0,0,,Vậy thì tôi yên tâm rồi.
Dialogue: 0,0:10:10.00,0:10:12.35,Default,,0,0,0,,Cậu không hề nương tay chút nào cả.
Dialogue: 0,0:10:15.33,0:10:18.02,Default,,0,0,0,,Không thể dùng từ "đáng sợ" với cậu ta nữa.
Dialogue: 0,0:10:18.31,0:10:19.58,Default,,0,0,0,,Lần trước nhập viện
Dialogue: 0,0:10:19.87,0:10:23.37,Default,,0,0,0,,là vì đã gây ra chuyện lớn\Nnên mới bị khiêng vào bệnh viện đấy!
Dialogue: 0,0:10:23.63,0:10:24.69,Default,,0,0,0,,Chuyện đó!
Dialogue: 0,0:10:25.40,0:10:26.56,Default,,0,0,0,,Nếu công khai chuyện đó
Dialogue: 0,0:10:26.58,0:10:28.68,Default,,0,0,0,,thì chắc chắn sẽ bị cấm thi đấu.
Dialogue: 0,0:10:28.71,0:10:31.18,Default,,0,0,0,,Cộng thêm việc năm nay có Sakuragi.
Dialogue: 0,0:10:31.44,0:10:33.18,Default,,0,0,0,,Đúng đó, hai người đó
Dialogue: 0,0:10:33.28,0:10:34.86,Default,,0,0,0,,đều ở trong đội bóng của chúng ta,
Dialogue: 0,0:10:34.98,0:10:38.54,Default,,0,0,0,,y như hai quả bom nguyên tử va vào nhau đấy!
Dialogue: 0,0:10:44.74,0:10:46.10,Default,,0,0,0,,Phải làm sao đây?
Dialogue: 0,0:10:48.04,0:10:53.54,Default,,0,0,0,,♫ Tôi là siêu nhân thiên tài bóng rổ ♫
Dialogue: 0,0:10:54.51,0:10:56.47,Default,,0,0,0,,♫ Nếu có đôi giày bóng rổ ♫
Dialogue: 0,0:10:57.22,0:11:00.07,Default,,0,0,0,,- ♫ Thì y như hổ mọc thêm cánh! ♫\N- Nhưng không ở đây.
Dialogue: 0,0:11:00.10,0:11:03.67,Default,,0,0,0,,- ♫ Rukawa thì có là gì chứ? ♫\N- [Hay là đừng nói ra thì hơn.]
Dialogue: 0,0:11:07.02,0:11:07.97,Default,,0,0,0,,[Hay đánh nhau?]
Dialogue: 0,0:11:22.55,0:11:25.63,Default,,0,0,0,,Ayako, thằng này là ai vậy?
Dialogue: 0,0:11:27.58,0:11:31.25,Default,,0,0,0,,Sao cậu lại làm như thế sau lưng tôi\Nvới thằng này? Tại sao?
Dialogue: 0,0:11:31.82,0:11:33.88,Default,,0,0,0,,Rõ ràng cậu nói tôi không hợp với cậu mà!
Dialogue: 0,0:11:34.78,0:11:35.98,Default,,0,0,0,,Chuyện này là sao đây?
Dialogue: 0,0:11:36.96,0:11:40.09,Default,,0,0,0,,- Ồn quá!\N- Ayako sao cậu lại làm vậy?
Dialogue: 0,0:11:43.84,0:11:44.72,Default,,0,0,0,,Đừng khóc nữa.
Dialogue: 0,0:11:45.56,0:11:46.47,Default,,0,0,0,,Thằng này!
Dialogue: 0,0:11:46.60,0:11:48.40,Default,,0,0,0,,Ayako, là sao...
Dialogue: 0,0:11:52.69,0:11:54.83,Default,,0,0,0,,[Một thanh niên rắc rối khác\Ntrong đội bóng rổ Shohoku,]
Dialogue: 0,0:11:54.99,0:11:56.40,Default,,0,0,0,,[Miyagi Ryota, lớp 11.]
Dialogue: 0,0:11:56.89,0:12:00.68,Default,,0,0,0,,[Cậu ấy và Ayako lại là chuyện thế nào đây?]
Dialogue: 0,0:12:16.45,0:12:17.77,Default,,0,0,0,,Khốn nạn!
Dialogue: 0,0:12:21.40,0:12:22.38,Default,,0,0,0,,Ryota!
Dialogue: 0,0:12:26.52,0:12:27.52,Default,,0,0,0,,[Đáng ghét!]
Dialogue: 0,0:12:29.50,0:12:33.40,Default,,0,0,0,,Nó đánh gục Sakuragi chỉ bằng một nắm đấm!
Dialogue: 0,0:12:33.75,0:12:35.25,Default,,0,0,0,,Anh làm gì vậy?
Dialogue: 0,0:12:35.35,0:12:37.93,Default,,0,0,0,,Miyagi, cậu nhầm đối thủ rồi!
Dialogue: 0,0:12:37.95,0:12:40.59,Default,,0,0,0,,Ryota, cậu hiểu lầm rồi!
Dialogue: 0,0:12:40.62,0:12:42.19,Default,,0,0,0,,Đợi đã!
Dialogue: 0,0:12:43.06,0:12:44.14,Default,,0,0,0,,[Góc nhìn của Miyagi Ryota]
Dialogue: 0,0:12:57.85,0:13:00.49,Default,,0,0,0,,Cái người tên Ryota đó\Nđã đụng phải chuyện gì vậy?
Dialogue: 0,0:13:00.82,0:13:03.49,Default,,0,0,0,,Shiozaki, anh nói cho em biết đi.
Dialogue: 0,0:13:05.23,0:13:07.79,Default,,0,0,0,,Tóm lại là chuyện dài lắm.
Dialogue: 0,0:13:08.04,0:13:10.01,Default,,0,0,0,,Cậu Miyagi đó đúng là quá khoe khoang rồi,
Dialogue: 0,0:13:10.03,0:13:12.59,Default,,0,0,0,,bị các đàn anh cho vào danh sách đen.
Dialogue: 0,0:13:16.33,0:13:19.59,Default,,0,0,0,,[Bị sáu, bảy người vây quanh,\NMiyaki đương nhiên là không thắng nổi.]
Dialogue: 0,0:13:39.22,0:13:40.19,Default,,0,0,0,,Mitsui!
Dialogue: 0,0:13:42.24,0:13:43.79,Default,,0,0,0,,Dừng tay! Dừng tay!
Dialogue: 0,0:13:44.19,0:13:45.26,Default,,0,0,0,,Thằng này!
Dialogue: 0,0:13:45.28,0:13:47.33,Default,,0,0,0,,Dừng tay! Dừng tay!
Dialogue: 0,0:13:56.38,0:13:58.79,Default,,0,0,0,,Dừng tay! Dừng tay lại!
Dialogue: 0,0:13:58.82,0:14:00.29,Default,,0,0,0,,Dừng tay!
Dialogue: 0,0:14:01.21,0:14:02.81,Default,,0,0,0,,Miyagi biết rõ không thắng được,
Dialogue: 0,0:14:03.47,0:14:06.37,Default,,0,0,0,,nên mặc kệ những người khác,\Nchỉ ra tay với Mitsui mà thôi.
Dialogue: 0,0:14:07.49,0:14:10.89,Default,,0,0,0,,Dù cho bị người khác đánh,\Ncậu ta cũng quyết đánh gục Mitsui.
Dialogue: 0,0:14:13.18,0:14:15.02,Default,,0,0,0,,Cho nên lúc Miyagi bị đánh\Nđến nổi không đứng dậy được
Dialogue: 0,0:14:15.32,0:14:17.58,Default,,0,0,0,,thì Mitsui đã ngất đi rồi.
Dialogue: 0,0:14:18.15,0:14:20.09,Default,,0,0,0,,Cho nên cả hai mới nhập viện chung.
Dialogue: 0,0:14:23.14,0:14:24.72,Default,,0,0,0,,Này, Miyagi!
Dialogue: 0,0:14:25.64,0:14:28.59,Default,,0,0,0,,Cậu nhầm đối thủ rồi!
Dialogue: 0,0:14:28.71,0:14:32.13,Default,,0,0,0,,Không tốt sao?\NCậu ta có thể chia sẻ nỗi đau giúp anh!
Dialogue: 0,0:14:32.16,0:14:34.04,Default,,0,0,0,,Gì cơ?
Dialogue: 0,0:14:50.06,0:14:51.79,Default,,0,0,0,,Xe máy của mình!
Dialogue: 0,0:14:53.89,0:14:55.67,Default,,0,0,0,,Tên đầu đỏ đáng ghét!
Dialogue: 0,0:14:55.84,0:14:57.94,Default,,0,0,0,,Hanamichi! Ryota!
Dialogue: 0,0:14:58.04,0:14:59.61,Default,,0,0,0,,Sao vậy, Ayako?
Dialogue: 0,0:15:00.77,0:15:01.72,Default,,0,0,0,,Haruko!
Dialogue: 0,0:15:03.39,0:15:06.27,Default,,0,0,0,,Tên đầu đỏ đáng ghét!
Dialogue: 0,0:15:06.47,0:15:07.45,Default,,0,0,0,,Mitsui!
Dialogue: 0,0:15:07.48,0:15:08.45,Default,,0,0,0,,Mitsui!
Dialogue: 0,0:15:19.15,0:15:21.65,Default,,0,0,0,,Sakuragi, chịu chết đi!
Dialogue: 0,0:15:29.13,0:15:30.43,Default,,0,0,0,,Norio!
Dialogue: 0,0:15:38.23,0:15:39.47,Default,,0,0,0,,Thằng khốn!
Dialogue: 0,0:15:41.79,0:15:43.77,Default,,0,0,0,,Dừng tay lại!
Dialogue: 0,0:15:43.85,0:15:45.47,Default,,0,0,0,,Vâng.
Dialogue: 0,0:15:45.77,0:15:47.17,Default,,0,0,0,,Ayako!
Dialogue: 0,0:15:48.24,0:15:49.57,Default,,0,0,0,,Haruko!
Dialogue: 0,0:15:54.97,0:15:57.47,Default,,0,0,0,,Tên khốn, đúng là cậu thích Ayako!
Dialogue: 0,0:15:58.42,0:15:59.80,Default,,0,0,0,,Mình biết rồi!
Dialogue: 0,0:15:59.99,0:16:04.04,Default,,0,0,0,,Anh ta đang nghi ngờ mối quan hệ\Ncủa mình và Ayako, thằng điên!
Dialogue: 0,0:16:04.77,0:16:09.37,Default,,0,0,0,,Đợi đã, nếu để tên này nghĩ ngợi lung tung,
Dialogue: 0,0:16:09.44,0:16:11.07,Default,,0,0,0,,nếu Haruko...
Dialogue: 0,0:16:12.15,0:16:15.67,Default,,0,0,0,,Sakuragi, cậu thích chị Ayako đúng không?
Dialogue: 0,0:16:16.00,0:16:19.27,Default,,0,0,0,,Vậy tôi sẽ đến bên Miyagi đây.
Dialogue: 0,0:16:20.22,0:16:23.31,Default,,0,0,0,,Đừng mà, Haruko, hiểu lầm thôi!
Dialogue: 0,0:16:24.02,0:16:26.37,Default,,0,0,0,,Tôi sẽ không tha cho cậu đâu!
Dialogue: 0,0:16:26.77,0:16:28.37,Default,,0,0,0,,Là tôi nói câu đó mới đúng!
Dialogue: 0,0:16:32.87,0:16:35.07,Default,,0,0,0,,Dừng tay lại hết cho tôi!
Dialogue: 0,0:16:38.84,0:16:39.63,Default,,0,0,0,,Ayako!
Dialogue: 0,0:16:39.66,0:16:41.35,Default,,0,0,0,,Mau đến câu lạc bộ luyện tập đi!
Dialogue: 0,0:16:41.35,0:16:42.56,Default,,0,0,0,,Tuân lệnh!
Dialogue: 0,0:16:43.33,0:16:44.71,Default,,0,0,0,,[Nhưng mà nói gì thì nói,]
Dialogue: 0,0:16:44.94,0:16:48.24,Default,,0,0,0,,[Ayako không thể nào có gì\Nvới tên đầu đỏ này được!]
Dialogue: 0,0:16:50.14,0:16:51.37,Default,,0,0,0,,Giày bóng rổ.
Dialogue: 0,0:16:52.31,0:16:54.97,Default,,0,0,0,,Sakuragi, cậu nhất định\Nphải luyện tập chăm chỉ nhé.
Dialogue: 0,0:16:56.13,0:16:58.80,Default,,0,0,0,,Đúng vậy, không thể ở lại nơi này,\Nmình phải đi luyện tập!
Dialogue: 0,0:17:02.87,0:17:04.17,Default,,0,0,0,,[Một tên ngoài ngành như anh!]
Dialogue: 0,0:17:04.20,0:17:07.17,Default,,0,0,0,,[Giày bóng rổ là phải như thế này!]
Dialogue: 0,0:17:07.20,0:17:11.17,Default,,0,0,0,,[Đôi giày bóng rổ này...\Nlà đôi mà mình luôn mơ ước có được!]
Dialogue: 0,0:17:11.26,0:17:12.57,Default,,0,0,0,,Thế nào hả?
Dialogue: 0,0:17:15.97,0:17:17.57,Default,,0,0,0,,[Bỏ đi.]
Dialogue: 0,0:17:17.60,0:17:21.41,Default,,0,0,0,,Một thằng không hiểu giá trị của đôi giày\Nnhư cậu chỉ biết chạy theo trào lưu mà thôi.
Dialogue: 0,0:17:25.75,0:17:26.87,Default,,0,0,0,,Mình thắng rồi!
Dialogue: 0,0:17:27.58,0:17:29.78,Default,,0,0,0,,Lâu quá không đến,\Nkhông biết câu lạc bộ nằm ở đâu.
Dialogue: 0,0:17:36.34,0:17:38.47,Default,,0,0,0,,- Không lẽ cậu cũng là!\N- Không lẽ anh cũng là!
Dialogue: 0,0:17:53.50,0:17:56.27,Default,,0,0,0,,- Miyagi!\N- Miyagi! - Miyagi!
Dialogue: 0,0:17:58.83,0:17:59.53,Default,,0,0,0,,Tôi về rồi đây.
Dialogue: 0,0:18:00.99,0:18:03.17,Default,,0,0,0,,[Không ngờ anh ta cũng trong đội bóng rổ!]
Dialogue: 0,0:18:05.47,0:18:06.27,Default,,0,0,0,,Miyagi!
Dialogue: 0,0:18:07.22,0:18:08.77,Default,,0,0,0,,Người đó là Miyagi sao?
Dialogue: 0,0:18:09.37,0:18:10.73,Default,,0,0,0,,Đúng là trông rất đáng sợ!
Dialogue: 0,0:18:12.70,0:18:15.01,Default,,0,0,0,,Ryota, cậu khỏe hơn rồi chứ?
Dialogue: 0,0:18:16.75,0:18:18.40,Default,,0,0,0,,Yasuda, chúng ta một chọi một đi!
Dialogue: 0,0:18:23.23,0:18:24.77,Default,,0,0,0,,Mits... Mitsui!
Dialogue: 0,0:18:27.91,0:18:29.67,Default,,0,0,0,,Thằng khốn Miyagi!
Dialogue: 0,0:18:31.27,0:18:33.67,Default,,0,0,0,,Đúng rồi, thằng đầu đỏ là ai vậy?
Dialogue: 0,0:18:34.04,0:18:35.57,Default,,0,0,0,,Học sinh lớp 10 mới vào trường sao?
Dialogue: 0,0:18:36.85,0:18:41.05,Default,,0,0,0,,Vâng, nó là Sakuragi Hanamichi lớp 10-7.\NCũng trong đội bóng rổ.
Dialogue: 0,0:18:41.92,0:18:44.57,Default,,0,0,0,,Lần trước bọn tao định\Ndạy dỗ nó trên sân thượng,
Dialogue: 0,0:18:44.96,0:18:47.17,Default,,0,0,0,,nhưng lại có người đến làm phiền,\Nnên đã không thành công.
Dialogue: 0,0:18:47.25,0:18:50.57,Default,,0,0,0,,Nói vậy là nó cũng trong đội bóng rổ sao?
Dialogue: 0,0:18:50.89,0:18:52.97,Default,,0,0,0,,Người có để tóc mái, tên là Rukawa Kaede,
Dialogue: 0,0:18:53.86,0:18:55.37,Default,,0,0,0,,nó cũng rất lợi hại.
Dialogue: 0,0:18:55.78,0:18:58.16,Default,,0,0,0,,Sao toàn là đội bóng rổ không vậy?
Dialogue: 0,0:19:02.53,0:19:04.08,Default,,0,0,0,,Norio, mày định ngủ đến khi nào nữa?
Dialogue: 0,0:19:06.60,0:19:08.92,Default,,0,0,0,,Đây là đâu? Tôi là ai?
Dialogue: 0,0:19:09.20,0:19:10.16,Default,,0,0,0,,Đáng ghét!
Dialogue: 0,0:19:10.86,0:19:12.77,Default,,0,0,0,,Tao sẽ không tha cho thằng đó!
Dialogue: 0,0:19:13.29,0:19:15.27,Default,,0,0,0,,Tao tuyệt đối không tha cho bọn chúng!
Dialogue: 0,0:19:28.27,0:19:29.17,Default,,0,0,0,,Động tác nhanh quá!
Dialogue: 0,0:19:29.47,0:19:34.47,Default,,0,0,0,,Nhanh đến nỗi hậu vệ dẫn bóng Yasuda\Ncủa chúng ta cũng phản ứng không kịp!
Dialogue: 0,0:19:34.50,0:19:37.77,Default,,0,0,0,,Dù không cao nhưng tốc độ lại rất nhanh!
Dialogue: 0,0:19:42.22,0:19:43.90,Default,,0,0,0,,Lợi hại thế!
Dialogue: 0,0:19:45.49,0:19:46.67,Default,,0,0,0,,Ayako!
Dialogue: 0,0:19:47.68,0:19:51.18,Default,,0,0,0,,[Lại ra vẻ nữa rồi, thật là!]
Dialogue: 0,0:19:52.50,0:19:53.90,Default,,0,0,0,,Dù cậu ấy có rất nhiều vấn đề,
Dialogue: 0,0:19:54.38,0:19:57.18,Default,,0,0,0,,nhưng năng lực bóng rổ thật sự rất mạnh,\Nđược xem là đội trưởng kỳ tiếp theo của đội.
Dialogue: 0,0:19:59.46,0:20:02.87,Default,,0,0,0,,Tôi thua cậu thật rồi này,\Nnăng lực của cậu không thụt lùi chút nào!
Dialogue: 0,0:20:02.99,0:20:05.18,Default,,0,0,0,,Vốn dĩ hôm nay tôi muốn ngăn cậu lại đấy!
Dialogue: 0,0:20:05.73,0:20:07.31,Default,,0,0,0,,Yasuda, cậu còn non quá!
Dialogue: 0,0:20:07.47,0:20:08.77,Default,,0,0,0,,Lúc cậu nằm viện...
Dialogue: 0,0:20:15.70,0:20:16.40,Default,,0,0,0,,Làm gì?
Dialogue: 0,0:20:19.44,0:20:20.97,Default,,0,0,0,,Tôi mới là đội trưởng kỳ tiếp theo!
Dialogue: 0,0:20:23.06,0:20:24.26,Default,,0,0,0,,Cậu muốn đánh tay đôi sao?
Dialogue: 0,0:20:35.20,0:20:36.81,Default,,0,0,0,,Tôi có một dự cảm không lành.
Dialogue: 0,0:20:37.22,0:20:38.27,Default,,0,0,0,,Tôi cũng vậy.
Dialogue: 0,0:20:44.56,0:20:47.97,Default,,0,0,0,,[Đứa trẻ rắc rối Miyagi đánh nhau\Nvới đàn anh du côn]
Dialogue: 0,0:20:48.27,0:20:50.47,Default,,0,0,0,,[đã quay trở lại.]
Dialogue: 0,0:20:52.38,0:20:53.38,Default,,0,0,0,,[Hanamichi và Ryota,]
Dialogue: 0,0:20:54.31,0:20:57.33,Default,,0,0,0,,[đội bóng rổ Shohoku đã nhận\Ncả hai nhân vật rắc rối này,]
Dialogue: 0,0:20:57.89,0:20:59.99,Default,,0,0,0,,[một dự cảm phong ba bão táp lại xuất hiện.]
Dialogue: 0,0:21:06.69,0:21:10.37,Default,,0,0,0,,♫ Đôi mắt luôn hướng về phía cậu ♫
Dialogue: 0,0:21:10.51,0:21:14.72,Default,,0,0,0,,♫ Từ lúc vừa mới gặp cho đến hiện tại ♫
Dialogue: 0,0:21:14.74,0:21:18.49,Default,,0,0,0,,♫ Chỉ cần có cậu ở bên ♫
Dialogue: 0,0:21:18.51,0:21:22.17,Default,,0,0,0,,♫ Những thứ khác đều không cần a ♫
Dialogue: 0,0:21:22.82,0:21:26.30,Default,,0,0,0,,♫ Căng thẳng tột độ vì ước mơ ♫
Dialogue: 0,0:21:31.92,0:21:36.47,Default,,0,0,0,,♫ Nhìn thấy vẻ mặt cậu vui như thế ♫
Dialogue: 0,0:21:36.50,0:21:39.98,Default,,0,0,0,,♫ Không có ngụy trang gì cả ♫
Dialogue: 0,0:21:40.00,0:21:46.23,Default,,0,0,0,,♫ Luôn mang theo máy nhắn tin\Nđể cậu có thể tìm mình bất cứ lúc nào ♫
Dialogue: 0,0:21:46.70,0:21:52.13,Default,,0,0,0,,♫ Am hiểu hơn về xe ♫
Dialogue: 0,0:21:52.53,0:21:56.16,Default,,0,0,0,,♫ Cũng đang cố gắng\Nhọc cách tự chăm sóc mình ♫
Dialogue: 0,0:21:57.98,0:22:00.44,Default,,0,0,0,,♫ Tuy là có hơi bối rối ♫
Dialogue: 0,0:22:00.68,0:22:06.28,Default,,0,0,0,,♫ Nhưng vẫn quyết định\Ntheo người này cả đời ♫
Dialogue: 0,0:22:09.02,0:22:12.62,Default,,0,0,0,,♫ Trong mắt chỉ có mỗi mình cậu ♫
Dialogue: 0,0:22:12.82,0:22:16.99,Default,,0,0,0,,♫ Một mình chờ đợi\Ntrong căn phòng của đôi ta ♫
Dialogue: 0,0:22:17.01,0:22:24.40,Default,,0,0,0,,♫ Nụ cười của cậu\Ngiống như là cái khóa màu hoa hồng ♫
Dialogue: 0,0:22:25.34,0:22:31.41,Default,,0,0,0,,♫ Tiến về trước, cô gái không có ước mơ ♫
Dialogue: 0,0:22:36.41,0:22:38.55,Default,,0,0,0,,[CAO THỦ BÓNG RỔ]
Dialogue: 0,0:22:39.65,0:22:40.88,Default,,0,0,0,,Này, Bố Già!
Dialogue: 0,0:22:41.24,0:22:43.20,Default,,0,0,0,,Em nghĩ nên để anh ta rời đội sẽ tốt hơn!
Dialogue: 0,0:22:43.35,0:22:44.84,Default,,0,0,0,,Dùng quyền của thầy đi, Bố Già!
Dialogue: 0,0:22:45.15,0:22:48.23,Default,,0,0,0,,Anh ta là người sẽ ngáng đường em đấy!
Dialogue: 0,0:22:50.05,0:22:51.95,Default,,0,0,0,,Năng lực thì bình thường,
Dialogue: 0,0:22:52.15,0:22:53.45,Default,,0,0,0,,tính cách lại tệ hại như vậy,
Dialogue: 0,0:22:53.65,0:22:55.15,Default,,0,0,0,,cho anh ta rời đội thì tốt hơn!
Dialogue: 0,0:22:55.55,0:22:56.35,Default,,0,0,0,,Từ giờ trở đi
Dialogue: 0,0:22:56.46,0:22:58.55,Default,,0,0,0,,Miyagi phải cố gắng hết sức mới được.
Dialogue: 0,0:22:58.75,0:22:59.65,Default,,0,0,0,,"Cao thủ bóng rổ!"
Dialogue: 0,0:23:00.06,0:23:02.46,Default,,0,0,0,,"Nhóm ngu ngốc đáng ghét nhất lịch sử ra đời."
Dialogue: 0,0:23:02.65,0:23:03.65,Default,,0,0,0,,Đừng bỏ qua đấy.

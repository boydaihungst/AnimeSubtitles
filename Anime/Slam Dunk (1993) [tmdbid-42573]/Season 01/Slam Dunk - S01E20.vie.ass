Dialogue: 0,0:00:00.00,0:00:10.00,Default,,0,0,0,,[Chương trình này có phụ đề tiếng Việt]
Dialogue: 0,0:00:04.34,0:00:10.76,Default,,0,0,0,,[CAO THỦ BÓNG RỔ]
Dialogue: 0,0:00:28.96,0:00:35.53,Default,,0,0,0,,♫ Ánh mặt trời chói rọi phía sau lưng,\Ntôi bước trên con đường này ♫
Dialogue: 0,0:00:35.68,0:00:41.82,Default,,0,0,0,,♫ Cũng như thường lệ, lại bị vỗ vai ♫
Dialogue: 0,0:00:42.42,0:00:48.92,Default,,0,0,0,,♫ Cậu là nữ thần của tôi, không có lý do gì cả ♫
Dialogue: 0,0:00:48.98,0:00:54.42,Default,,0,0,0,,♫ Nhưng lại chưa từng khoác lấy tay tôi ♫
Dialogue: 0,0:00:54.50,0:01:01.06,Default,,0,0,0,,♫ Không biết từ lúc nào,\Nánh mắt đã không thể rời đi ♫
Dialogue: 0,0:01:01.20,0:01:07.78,Default,,0,0,0,,♫ Không buông ra, không do dự,\Nđiên cuồng vì cậu ♫
Dialogue: 0,0:01:07.80,0:01:14.72,Default,,0,0,0,,♫ Tôi muốn hét to rằng tôi thích cậu,\Nmuốn thử thay đổi ngày mai ♫
Dialogue: 0,0:01:14.82,0:01:21.28,Default,,0,0,0,,♫ Tôi muốn xé toạc\Nquãng thời gian đông cứng ♫
Dialogue: 0,0:01:21.30,0:01:28.52,Default,,0,0,0,,♫ Tôi muốn hét to rằng tôi thích cậu,\Ndũng cảm tiến lên một bước ♫
Dialogue: 0,0:01:28.68,0:01:35.90,Default,,0,0,0,,♫ Xin cậu hãy chấp nhận\Ntrái tim nồng cháy này của tôi ♫
Dialogue: 0,0:01:49.53,0:01:53.72,Default,,0,0,0,,[Sakuragi Hanamichi bị liên tiếp 50 cô gái\Nbỏ rơi trong thời cấp hai.]
Dialogue: 0,0:01:53.75,0:01:56.60,Default,,0,0,0,,[Một Hanamichi như vậy\Nđã tương kế tựu kế vào đội bóng rổ]
Dialogue: 0,0:01:56.62,0:01:58.02,Default,,0,0,0,,[trước lời khuyên của Haruko.]
Dialogue: 0,0:01:58.46,0:02:00.78,Default,,0,0,0,,[Nhưng chuyện vẫn chưa kết thúc tại đây.]
Dialogue: 0,0:02:07.84,0:02:10.88,Default,,0,0,0,,[Trong trận đấu với đội Ryonan\Nmạnh top bốn toàn tỉnh,]
Dialogue: 0,0:02:10.90,0:02:13.84,Default,,0,0,0,,[Hanamichi đã ném quả bóng\Nchuyển bại thành thắng.]
Dialogue: 0,0:02:14.05,0:02:19.08,Default,,0,0,0,,[Nhưng lại bị Sendoh của Ryonan\Nnhân sơ hở thay đổi tình thế và thua trận.]
Dialogue: 0,0:02:21.50,0:02:22.98,Default,,0,0,0,,Chuyền bóng.
Dialogue: 0,0:02:23.67,0:02:25.92,Default,,0,0,0,,Kết thúc rồi, chúng ta thua rồi.
Dialogue: 0,0:02:31.77,0:02:33.07,Default,,0,0,0,,Cậu đã làm tốt lắm.
Dialogue: 0,0:02:38.75,0:02:47.01,Default,,0,0,0,,[Giày bóng rổ]
Dialogue: 0,0:02:40.50,0:02:42.17,Default,,0,0,0,,[Giày bóng rổ.]
Dialogue: 0,0:02:55.44,0:02:56.90,Default,,0,0,0,,Chào buổi sáng.
Dialogue: 0,0:03:12.13,0:03:14.69,Default,,0,0,0,,Chào buổi sáng Hanamichi, hôm qua...
Dialogue: 0,0:03:21.01,0:03:21.94,Default,,0,0,0,,Đau quá!
Dialogue: 0,0:03:22.10,0:03:23.63,Default,,0,0,0,,Bọn tao không nói gì hết.
Dialogue: 0,0:03:23.75,0:03:26.02,Default,,0,0,0,,Bọn tao chỉ muốn an ủi mày thôi mà.
Dialogue: 0,0:03:26.02,0:03:28.78,Default,,0,0,0,,Mới lạ, rõ ràng bọn mày\Nmuốn lấy tao làm trò tiêu khiển.
Dialogue: 0,0:03:28.81,0:03:30.43,Default,,0,0,0,,Tao nhìn ánh mắt bọn mày là biết rồi.
Dialogue: 0,0:03:31.60,0:03:32.87,Default,,0,0,0,,Cũng khó trách mà.
Dialogue: 0,0:03:33.09,0:03:33.94,Default,,0,0,0,,Yohei.
Dialogue: 0,0:03:34.49,0:03:37.48,Default,,0,0,0,,Do hôm qua thằng kia thua Ryonan
Dialogue: 0,0:03:37.58,0:03:39.24,Default,,0,0,0,,nên cực không cam tâm,\Nhình như cả đêm không ngủ đó.
Dialogue: 0,0:03:47.67,0:03:49.20,Default,,0,0,0,,Nhìn có vẻ bực bội lắm.
Dialogue: 0,0:03:49.30,0:03:52.47,Default,,0,0,0,,Đúng vậy, chúng ta\Nđừng nhắc đến chuyện hôm qua thì hơn.
Dialogue: 0,0:03:57.45,0:03:58.42,Default,,0,0,0,,Sakuragi.
Dialogue: 0,0:03:59.92,0:04:01.76,Default,,0,0,0,,Nghe nói hôm qua cậu thua rồi à?
Dialogue: 0,0:04:01.94,0:04:04.51,Default,,0,0,0,,Chắc bây giờ\Ncậu bắt đầu ghét đội bóng rổ rồi nhỉ?
Dialogue: 0,0:04:07.18,0:04:09.43,Default,,0,0,0,,Sakuragi, vào đội Judo của bọn tôi đi.
Dialogue: 0,0:04:14.56,0:04:16.47,Default,,0,0,0,,Cái tên Thủ Lĩnh Người Vượn đáng ghét đó.
Dialogue: 0,0:04:16.49,0:04:17.93,Default,,0,0,0,,Chờ chút, Sakuragi.
Dialogue: 0,0:04:18.15,0:04:21.24,Default,,0,0,0,,Trong đội Judo của Aota tôi\Nkhông có hai chữ thua trận đâu.
Dialogue: 0,0:04:21.72,0:04:23.62,Default,,0,0,0,,Chỉ cần cậu vào thì càng khỏi nói luôn.
Dialogue: 0,0:04:26.45,0:04:28.52,Default,,0,0,0,,Nguy hiểm, tên này đang ngủ à?
Dialogue: 0,0:04:28.76,0:04:30.60,Default,,0,0,0,,Thành viên đội bóng rổ đáng ghét.
Dialogue: 0,0:04:31.13,0:04:32.86,Default,,0,0,0,,[Tôi sẽ không bao giờ bỏ cuộc đâu.]
Dialogue: 0,0:04:33.29,0:04:35.98,Default,,0,0,0,,[Sakuragi, vào đội Judo đi.]
Dialogue: 0,0:04:37.75,0:04:39.97,Default,,0,0,0,,Hôm nay tâm trạng cậu ta không được tốt lắm.
Dialogue: 0,0:04:40.11,0:04:43.01,Default,,0,0,0,,Hình như do\Ncậu ta thua trận bóng rổ hôm qua đó.
Dialogue: 0,0:04:43.28,0:04:44.84,Default,,0,0,0,,Tốt nhất đừng đụng chạm gì cậu ta.
Dialogue: 0,0:04:47.55,0:04:49.49,Default,,0,0,0,,Nếu chuyền hết bóng cho Rukawa,
Dialogue: 0,0:04:49.60,0:04:51.12,Default,,0,0,0,,tôi nghĩ chắc chắn trận đấu này\Nsẽ không bị thua.
Dialogue: 0,0:04:51.19,0:04:53.19,Default,,0,0,0,,Chắc chắn là vậy, tôi cũng nghĩ vậy.
Dialogue: 0,0:04:53.39,0:04:56.45,Default,,0,0,0,,Nên cậu ấy là thiên tài, đúng vậy đúng vậy!
Dialogue: 0,0:04:56.96,0:04:57.78,Default,,0,0,0,,Thích chơi trội.
Dialogue: 0,0:05:01.90,0:05:03.30,Default,,0,0,0,,Đáng ghét.
Dialogue: 0,0:05:04.80,0:05:06.79,Default,,0,0,0,,Trận đấu hôm qua đáng tiếc ghê.
Dialogue: 0,0:05:07.34,0:05:09.86,Default,,0,0,0,,Đồ chết tiệt, tôi bảo đủ rồi mà!
Dialogue: 0,0:05:13.31,0:05:14.37,Default,,0,0,0,,Chào buổi sáng.
Dialogue: 0,0:05:14.42,0:05:15.54,Default,,0,0,0,,Haruko.
Dialogue: 0,0:05:16.95,0:05:20.61,Default,,0,0,0,,[Không được, mình không còn mặt mũi\Ngặp cậu ấy nữa, bọn mình thua rồi.]
Dialogue: 0,0:05:20.95,0:05:23.75,Default,,0,0,0,,Sakuragi,\Nthật ra cậu không cần phải tự trách mình đâu,
Dialogue: 0,0:05:24.02,0:05:26.01,Default,,0,0,0,,vì cậu đã thể hiện tốt lắm rồi.
Dialogue: 0,0:05:26.49,0:05:28.25,Default,,0,0,0,,Vậy, vậy sao?
Dialogue: 0,0:05:28.36,0:05:29.33,Default,,0,0,0,,Đúng đó.
Dialogue: 0,0:05:29.36,0:05:31.60,Default,,0,0,0,,Huống chi đối phương là đội Ryonan mà.
Dialogue: 0,0:05:31.71,0:05:33.96,Default,,0,0,0,,Không, nhưng rốt cuộc vẫn thua.
Dialogue: 0,0:05:35.19,0:05:36.22,Default,,0,0,0,,Sakuragi.
Dialogue: 0,0:05:37.15,0:05:38.95,Default,,0,0,0,,Cậu, cậu là bạn của Haruko à?
Dialogue: 0,0:05:39.22,0:05:40.08,Default,,0,0,0,,Tôi tên Fujii.
Dialogue: 0,0:05:40.35,0:05:41.45,Default,,0,0,0,,Fujii.
Dialogue: 0,0:05:41.59,0:05:43.35,Default,,0,0,0,,Tuy cậu đã thua,
Dialogue: 0,0:05:44.63,0:05:45.72,Default,,0,0,0,,nhưng tôi
Dialogue: 0,0:05:46.46,0:05:49.12,Default,,0,0,0,,xem rồi vẫn thấy cảm động cực kỳ.
Dialogue: 0,0:05:51.07,0:05:54.30,Default,,0,0,0,,Cậu ấy khóc ngất luôn đó.
Dialogue: 0,0:05:54.38,0:05:55.61,Default,,0,0,0,,Chỉ là một trận tập luyện thôi,
Dialogue: 0,0:05:56.64,0:05:57.83,Default,,0,0,0,,bạn No.2.
Dialogue: 0,0:05:57.86,0:05:59.76,Default,,0,0,0,,Ai là No.2 hả, tôi tên Matsui.
Dialogue: 0,0:05:59.82,0:06:03.75,Default,,0,0,0,,Đúng đó, nhìn không ra là\Ntrận đầu tiên ra sân luôn.
Dialogue: 0,0:06:03.87,0:06:07.27,Default,,0,0,0,,Anh tôi cũng từng nói biểu hiện\Ncủa Sakuragi vượt sức tưởng tượng.
Dialogue: 0,0:06:08.21,0:06:10.55,Default,,0,0,0,,Dù anh ấy đã dặn tôi không được nói,
Dialogue: 0,0:06:11.07,0:06:13.88,Default,,0,0,0,,nhưng cậu có thể khiến người anh trai\Ncứng đầu của tôi nói ra những lời này
Dialogue: 0,0:06:14.02,0:06:16.26,Default,,0,0,0,,thì nghĩa là giỏi thật đó, Sakuragi giỏi lắm.
Dialogue: 0,0:06:17.02,0:06:18.24,Default,,0,0,0,,Thật không?
Dialogue: 0,0:06:18.37,0:06:19.19,Default,,0,0,0,,Thật mà.
Dialogue: 0,0:06:19.30,0:06:22.59,Default,,0,0,0,,Hơn nữa quả đầu tiên cậu ném vào\Nlàm thay đổi tình thế,
Dialogue: 0,0:06:22.72,0:06:25.24,Default,,0,0,0,,chắc chắn tâm trạng của cậu khi đó\Nthoải mái lắm nhỉ? Xoay chuyển tình thế đó.
Dialogue: 0,0:06:25.35,0:06:28.68,Default,,0,0,0,,Không, mình chỉ áp dụng\Ncách mà cậu dạy mình thôi.
Dialogue: 0,0:06:32.89,0:06:34.36,Default,,0,0,0,,Úp rổ!
Dialogue: 0,0:06:34.38,0:06:35.32,Default,,0,0,0,,Sakuragi.
Dialogue: 0,0:06:39.96,0:06:40.73,Default,,0,0,0,,Bỏ vào!
Dialogue: 0,0:06:44.80,0:06:45.89,Default,,0,0,0,,Sakuragi.
Dialogue: 0,0:06:46.80,0:06:50.60,Default,,0,0,0,,Hãy gọi là chàng trai kêu gọi chiến thắng.
Dialogue: 0,0:06:52.94,0:06:55.60,Default,,0,0,0,,Nếu cứ vậy mà xong
Dialogue: 0,0:06:57.05,0:07:01.27,Default,,0,0,0,,thì Sakuragi, quả nhiên cậu là một thiên tài,\Nchàng trai kêu gọi chiến thắng.
Dialogue: 0,0:07:01.40,0:07:04.03,Default,,0,0,0,,Rukawa vốn không bằng được cậu,\Ncậu giỏi quá đi.
Dialogue: 0,0:07:06.57,0:07:07.66,Default,,0,0,0,,Không có gì.
Dialogue: 0,0:07:10.24,0:07:11.81,Default,,0,0,0,,Nếu thắng nhờ quả bóng đó
Dialogue: 0,0:07:11.83,0:07:14.60,Default,,0,0,0,,thì Sakuragi sẽ trở thành anh hùng rồi,\Nđại anh hùng đó.
Dialogue: 0,0:07:14.75,0:07:15.76,Default,,0,0,0,,Không có chuyện này đâu.
Dialogue: 0,0:07:18.66,0:07:21.21,Default,,0,0,0,,Hơn nữa trong thời gian ngắn như vậy
Dialogue: 0,0:07:21.23,0:07:23.82,Default,,0,0,0,,cậu đã đi rách đôi giày thể thao này rồi.
Dialogue: 0,0:07:24.19,0:07:27.07,Default,,0,0,0,,Đó là chứng cứ mà cậu đã tập luyện nghiêm túc.
Dialogue: 0,0:07:29.07,0:07:30.56,Default,,0,0,0,,Đó là vì mình có tài bẩm sinh.
Dialogue: 0,0:07:30.67,0:07:32.86,Default,,0,0,0,,Mình là thiên tài mà.
Dialogue: 0,0:07:33.89,0:07:36.30,Default,,0,0,0,,Ngây thơ quá, lại chứng nào tật nấy rồi.
Dialogue: 0,0:07:36.91,0:07:38.41,Default,,0,0,0,,Đúng rồi, Sakuragi.
Dialogue: 0,0:07:38.54,0:07:39.68,Default,,0,0,0,,Cùng đi mua giày chơi bóng rổ nhé.
Dialogue: 0,0:07:39.80,0:07:41.80,Default,,0,0,0,,[Tan học]
Dialogue: 0,0:07:42.42,0:07:44.90,Default,,0,0,0,,Chắc chắn phải về trước khi bắt đầu buổi tập.
Dialogue: 0,0:07:45.56,0:07:48.26,Default,,0,0,0,,Nhiều kiểu dáng cũng ngầu lắm.
Dialogue: 0,0:07:48.54,0:07:52.21,Default,,0,0,0,,Cái này cũng đẹp, cái kia cũng tốt,\Nđúng không Sakuragi?
Dialogue: 0,0:07:52.34,0:07:52.89,Default,,0,0,0,,Đúng đó.
Dialogue: 0,0:07:53.80,0:07:55.80,Default,,0,0,0,,[Đây rõ ràng là hẹn hò đó.]
Dialogue: 0,0:07:56.15,0:07:57.67,Default,,0,0,0,,[Vui quá đi mất.]
Dialogue: 0,0:07:58.14,0:08:00.87,Default,,0,0,0,,Chào mừng quý khách, có thích đôi nào\Nthì nói số size cho tôi nhé.
Dialogue: 0,0:08:01.00,0:08:02.80,Default,,0,0,0,,[Ông đừng đến đây cản trở chứ.]
Dialogue: 0,0:08:03.37,0:08:05.92,Default,,0,0,0,,Cậu cao ghê đó,\Ncó phải thành viên của đội bóng rổ không?
Dialogue: 0,0:08:06.70,0:08:07.63,Default,,0,0,0,,Phải thì sao?
Dialogue: 0,0:08:08.68,0:08:10.82,Default,,0,0,0,,[Sao khí chất hung hãn vậy?]
Dialogue: 0,0:08:11.27,0:08:12.67,Default,,0,0,0,,Cậu học trường cấp ba nào?
Dialogue: 0,0:08:12.76,0:08:15.82,Default,,0,0,0,,Shohoku, phiền chết được,\Nông già chết tiệt sang bên kia đi.
Dialogue: 0,0:08:16.01,0:08:18.18,Default,,0,0,0,,Sakuragi, có muốn mang thử không?
Dialogue: 0,0:08:18.20,0:08:18.83,Default,,0,0,0,,Được chứ.
Dialogue: 0,0:08:20.18,0:08:22.82,Default,,0,0,0,,Xin lỗi ông chủ, có được mang thử không ạ?
Dialogue: 0,0:08:22.94,0:08:24.46,Default,,0,0,0,,Cứ tự nhiên, cứ tự nhiên.
Dialogue: 0,0:08:24.55,0:08:25.87,Default,,0,0,0,,Cũng cừ đấy nhóc.
Dialogue: 0,0:08:28.82,0:08:30.20,Default,,0,0,0,,Làm gì vậy ông già chết tiệt?
Dialogue: 0,0:08:30.36,0:08:34.07,Default,,0,0,0,,Bạn gái xinh ghê, làm người ta ganh tỵ đó.
Dialogue: 0,0:08:36.27,0:08:37.82,Default,,0,0,0,,[Bạn gái?]
Dialogue: 0,0:08:38.80,0:08:40.96,Default,,0,0,0,,Chúng tôi giống không? Giống không ông chủ?
Dialogue: 0,0:08:40.99,0:08:43.63,Default,,0,0,0,,Sakuragi, cậu mang giày size bao nhiêu?
Dialogue: 0,0:08:43.75,0:08:44.79,Default,,0,0,0,,28.
Dialogue: 0,0:08:44.96,0:08:46.01,Default,,0,0,0,,28 à?
Dialogue: 0,0:08:46.13,0:08:50.39,Default,,0,0,0,,Tôi cũng không thua kém thanh niên trẻ,\Ntôi cũng mang size 28 đó.
Dialogue: 0,0:08:50.42,0:08:51.32,Default,,0,0,0,,Vậy à?
Dialogue: 0,0:08:52.54,0:08:53.76,Default,,0,0,0,,Được rồi, mau mang thử, mau mang thử đi.
Dialogue: 0,0:08:55.50,0:08:58.30,Default,,0,0,0,,Đang mặc đồng phục,\Nkhông nhìn ra là đẹp hay không.
Dialogue: 0,0:08:58.77,0:09:00.74,Default,,0,0,0,,Có thể mặc thử đồ thể thao không ạ?
Dialogue: 0,0:09:02.51,0:09:03.27,Default,,0,0,0,,Được chứ.
Dialogue: 0,0:09:05.51,0:09:06.57,Default,,0,0,0,,Được thật à.
Dialogue: 0,0:09:07.58,0:09:10.81,Default,,0,0,0,,Đúng là mang giày bóng rổ vào là cảm thấy\Ngiống hệt vận động viên bóng rổ ngay luôn.
Dialogue: 0,0:09:10.83,0:09:11.62,Default,,0,0,0,,Vậy sao?
Dialogue: 0,0:09:13.62,0:09:16.48,Default,,0,0,0,,Chắc đôi này cũng được lắm đó Sakuragi.
Dialogue: 0,0:09:18.03,0:09:18.78,Default,,0,0,0,,Chiếc này cũng tốt lắm.
Dialogue: 0,0:09:19.19,0:09:20.02,Default,,0,0,0,,Chiếc đó cũng tốt luôn.
Dialogue: 0,0:09:21.39,0:09:24.85,Default,,0,0,0,,Chiếc này cũng tốt, ôi tốn nơ-ron não quá.
Dialogue: 0,0:09:24.95,0:09:27.03,Default,,0,0,0,,Mang thử xem, mang thử xem.
Dialogue: 0,0:09:31.78,0:09:32.97,Default,,0,0,0,,Chút thôi là dừng lại được rồi.
Dialogue: 0,0:09:33.08,0:09:33.95,Default,,0,0,0,,Khoan đã,
Dialogue: 0,0:09:34.17,0:09:36.07,Default,,0,0,0,,đừng làm bẩn tiệm của tôi chứ.
Dialogue: 0,0:09:46.12,0:09:48.73,Default,,0,0,0,,Chút thôi là dừng lại được rồi, dọa chết luôn.
Dialogue: 0,0:09:48.95,0:09:49.90,Default,,0,0,0,,Sakuragi.
Dialogue: 0,0:09:50.02,0:09:54.05,Default,,0,0,0,,Giày bóng rổ còn một tác dụng nữa,\Nđó là giảm xóc khi tiếp đất.
Dialogue: 0,0:09:54.86,0:09:57.29,Default,,0,0,0,,Cô ơi, đừng nói thêm\Nmấy câu dư thừa nữa được không?
Dialogue: 0,0:10:05.37,0:10:06.70,Default,,0,0,0,,Cô đừng có nói nữa mà.
Dialogue: 0,0:10:08.01,0:10:13.32,Default,,0,0,0,,Nhảy còn cao hơn bình thường nữa.
Dialogue: 0,0:10:13.42,0:10:15.08,Default,,0,0,0,,Khoan đã, đừng quậy nữa.
Dialogue: 0,0:10:20.36,0:10:22.46,Default,,0,0,0,,Chiếc Jordan quý báu của tôi.
Dialogue: 0,0:10:22.48,0:10:23.41,Default,,0,0,0,,Quý báu?
Dialogue: 0,0:10:24.37,0:10:28.27,Default,,0,0,0,,Đúng đó, là mẫu mà siêu sao bóng rổ NBA,\NMichael Jordan đã mang đấy.
Dialogue: 0,0:10:28.55,0:10:32.32,Default,,0,0,0,,Tôi là fan bóng rổ,\Ntôi có hết tất cả mẫu giày Jordan.
Dialogue: 0,0:10:33.52,0:10:34.55,Default,,0,0,0,,Chưa nghe qua.
Dialogue: 0,0:10:35.26,0:10:36.56,Default,,0,0,0,,Nhưng hình như có vẻ mang sướng lắm.
Dialogue: 0,0:10:38.47,0:10:39.94,Default,,0,0,0,,Đôi giày này bao nhiêu tiền?
Dialogue: 0,0:10:41.29,0:10:44.28,Default,,0,0,0,,Giá tiền? Tôi...
Dialogue: 0,0:10:44.41,0:10:45.60,Default,,0,0,0,,Tôi cứ muốn lấy đôi này đó.
Dialogue: 0,0:10:49.47,0:10:51.19,Default,,0,0,0,,Đây là loại cũ rích rồi, chiết khấu cho tôi đi.
Dialogue: 0,0:10:56.91,0:10:58.04,Default,,0,0,0,,30 đồng nhé?
Dialogue: 0,0:10:59.65,0:11:01.98,Default,,0,0,0,,Ba...
Dialogue: 0,0:11:07.36,0:11:09.07,Default,,0,0,0,,Chào buổi sáng.
Dialogue: 0,0:11:09.73,0:11:11.52,Default,,0,0,0,,Sakuragi, cậu đến muộn rồi.
Dialogue: 0,0:11:15.41,0:11:18.76,Default,,0,0,0,,Thiên tài Sakuragi\Nmang giày bóng rổ ra sân đây.
Dialogue: 0,0:11:18.99,0:11:21.20,Default,,0,0,0,,Tôi đến rồi.
Dialogue: 0,0:11:22.26,0:11:25.59,Default,,0,0,0,,Tương lai tên này\Nchắc chắn sẽ ghê gớm lắm đây.
Dialogue: 0,0:11:25.59,0:11:27.59,Default,,0,0,0,,[Tạm ngừng kinh doanh]
Dialogue: 0,0:11:53.79,0:11:55.26,Default,,0,0,0,,Kakuta, mau đổi tay!
Dialogue: 0,0:12:14.55,0:12:15.36,Default,,0,0,0,,Đáng ghét!
Dialogue: 0,0:12:19.09,0:12:20.31,Default,,0,0,0,,Sakuragi, thủ tốt lắm.
Dialogue: 0,0:12:22.59,0:12:24.89,Default,,0,0,0,,Đâu có, do em là thiên tài thôi.
Dialogue: 0,0:12:26.21,0:12:27.81,Default,,0,0,0,,Bớt đắc ý đi.
Dialogue: 0,0:12:28.06,0:12:31.72,Default,,0,0,0,,[Rukawa chết tiệt đáng ghét,\Nchắc chắn mày đang đố kỵ thiên tài như tao.]
Dialogue: 0,0:12:32.30,0:12:35.83,Default,,0,0,0,,Nhưng hễ nghĩ đến\Ndáng vẻ dịu dàng của Haruko là mình sẽ...
Dialogue: 0,0:12:36.94,0:12:39.93,Default,,0,0,0,,Làm người ta nhìn không ra\Nlà lần đầu tiên ra sân luôn.
Dialogue: 0,0:12:40.21,0:12:43.30,Default,,0,0,0,,Anh tôi cũng từng nói biểu hiện\Ncủa Sakuragi vượt sức tưởng tượng.
Dialogue: 0,0:12:43.58,0:12:46.81,Default,,0,0,0,,Cậu có thể khiến người anh trai\Ncứng đầu của tôi nói ra những lời này
Dialogue: 0,0:12:47.05,0:12:49.35,Default,,0,0,0,,thì nghĩa là cậu giỏi thật đó, giỏi thật đó.
Dialogue: 0,0:12:49.77,0:12:54.19,Default,,0,0,0,,[Haruko chẳng những đáng yêu\Nmà còn dịu dàng cực kỳ.]
Dialogue: 0,0:12:55.29,0:12:56.98,Default,,0,0,0,,Cũng cừ đấy nhóc.
Dialogue: 0,0:12:57.10,0:12:58.42,Default,,0,0,0,,Làm gì vậy ông già chết tiệt?
Dialogue: 0,0:12:58.54,0:13:02.53,Default,,0,0,0,,Bạn gái đáng yêu vậy làm người ta ganh tỵ đó.
Dialogue: 0,0:13:03.24,0:13:05.23,Default,,0,0,0,,[Bạn gái sao?]
Dialogue: 0,0:13:05.48,0:13:07.34,Default,,0,0,0,,[Haruko là bạn gái của mình sao?]
Dialogue: 0,0:13:07.58,0:13:10.05,Default,,0,0,0,,Đúng là đau đầu quá mà.
Dialogue: 0,0:13:10.75,0:13:11.30,Default,,0,0,0,,Sakuragi.
Dialogue: 0,0:13:15.12,0:13:16.18,Default,,0,0,0,,Haruko.
Dialogue: 0,0:13:17.75,0:13:21.35,Default,,0,0,0,,Đang tập bóng mà cậu ngơ ngẩn gì vậy?
Dialogue: 0,0:13:22.98,0:13:25.03,Default,,0,0,0,,Chán quá đi.
Dialogue: 0,0:13:25.84,0:13:28.36,Default,,0,0,0,,Không có chuyện gì thú vị hết.
Dialogue: 0,0:13:28.48,0:13:31.56,Default,,0,0,0,,Dạo này cũng không nghe tin\NHanamichi bị đá nữa.
Dialogue: 0,0:13:31.67,0:13:33.09,Default,,0,0,0,,Chán thật.
Dialogue: 0,0:13:33.63,0:13:37.86,Default,,0,0,0,,Người Haruko thích là Rukawa,\Ntao thấy nó nên từ bỏ tình cảm này sớm đi.
Dialogue: 0,0:13:38.84,0:13:41.93,Default,,0,0,0,,Từ sau khi lên cấp ba,\Nthằng nhóc đó đã ít khi bị đá lắm rồi.
Dialogue: 0,0:13:42.00,0:13:43.19,Default,,0,0,0,,Nhưng như vậy
Dialogue: 0,0:13:43.43,0:13:47.07,Default,,0,0,0,,thì nó không có cách nào\Nlàm mới kỷ lục của thời cấp hai rồi.
Dialogue: 0,0:13:48.97,0:13:52.11,Default,,0,0,0,,Thằng nhóc đó đã đam mê bóng rổ rồi.
Dialogue: 0,0:13:53.81,0:13:56.57,Default,,0,0,0,,Xem ra nó đã tìm được nơi gửi gắm.
Dialogue: 0,0:14:05.09,0:14:07.32,Default,,0,0,0,,Tao nghĩ mày cũng nên tìm chút chuyện làm đi.
Dialogue: 0,0:14:08.53,0:14:12.03,Default,,0,0,0,,Yohei, sao rồi? Vậy bản thân mày thì sao?
Dialogue: 0,0:14:15.87,0:14:16.70,Default,,0,0,0,,Tao...
Dialogue: 0,0:14:17.03,0:14:19.37,Default,,0,0,0,,Chuyện này à? Tao cũng không biết nữa.
Dialogue: 0,0:14:21.94,0:14:23.87,Default,,0,0,0,,Quả nhiên
Dialogue: 0,0:14:24.01,0:14:26.18,Default,,0,0,0,,chán thật.
Dialogue: 0,0:14:33.72,0:14:34.63,Default,,0,0,0,,Làm sao vậy?
Dialogue: 0,0:14:34.77,0:14:35.93,Default,,0,0,0,,Mày nhìn kìa, mày nhìn kìa.
Dialogue: 0,0:14:39.21,0:14:41.27,Default,,0,0,0,,Hai tên nhóc đó, bọn nó đang làm gì vậy?
Dialogue: 0,0:14:51.07,0:14:52.67,Default,,0,0,0,,Nhìn dáng vẻ bị dọa của bọn nó kìa.
Dialogue: 0,0:14:55.52,0:14:58.42,Default,,0,0,0,,Thằng ngu này,\Nchúng ta đang xem đến chỗ đặc sắc mà.
Dialogue: 0,0:14:59.43,0:15:02.09,Default,,0,0,0,,- Đó là ai?\N- Ai biết? - Đừng lớn tiếng vậy chứ.
Dialogue: 0,0:15:03.60,0:15:04.63,Default,,0,0,0,,Xin lỗi nhé.
Dialogue: 0,0:15:05.11,0:15:08.57,Default,,0,0,0,,Tôi đã có người trong lòng rồi,\Nnên tôi không thể quen cậu được.
Dialogue: 0,0:15:09.61,0:15:10.54,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,0:15:10.81,0:15:12.23,Default,,0,0,0,,Thất bại!
Dialogue: 0,0:15:12.25,0:15:13.97,Default,,0,0,0,,Thất bại!
Dialogue: 0,0:15:15.12,0:15:19.58,Default,,0,0,0,,Người thứ mười kể từ khi lên cấp ba,\Ntrong vòng một năm.
Dialogue: 0,0:15:19.58,0:15:21.19,Default,,0,0,0,,Cậu vốn đã không còn nghiêm túc rồi.
Dialogue: 0,0:15:22.13,0:15:24.52,Default,,0,0,0,,Bạn bè ở Shohoku của tôi đã nói với tôi
Dialogue: 0,0:15:24.79,0:15:27.59,Default,,0,0,0,,thật ra cậu đã có người mình thích từ lâu rồi,
Dialogue: 0,0:15:27.90,0:15:30.20,Default,,0,0,0,,đó là bạn cùng lớp ở Shohoku của cậu.
Dialogue: 0,0:15:34.03,0:15:38.03,Default,,0,0,0,,Đại ca, anh đừng đau khổ như vậy nữa,\Nngười này không được thì còn người khác.
Dialogue: 0,0:15:38.16,0:15:40.07,Default,,0,0,0,,So với Hanamichi thì anh còn kém xa lắm.
Dialogue: 0,0:15:40.22,0:15:41.55,Default,,0,0,0,,Mới mười người có là gì chứ?
Dialogue: 0,0:15:52.55,0:15:54.38,Default,,0,0,0,,Bọn mày học lớp 10 phải không?
Dialogue: 0,0:15:57.82,0:16:00.19,Default,,0,0,0,,Được rồi, ngừng, tập họp.
Dialogue: 0,0:16:00.48,0:16:01.33,Default,,0,0,0,,Vâng!
Dialogue: 0,0:16:07.27,0:16:08.97,Default,,0,0,0,,Trong đội chúng ta\Nvốn có rất nhiều học sinh mới lớp 10,
Dialogue: 0,0:16:09.00,0:16:11.60,Default,,0,0,0,,bây giờ chỉ còn lại năm người các cậu thôi.
Dialogue: 0,0:16:12.14,0:16:13.13,Default,,0,0,0,,Vâng.
Dialogue: 0,0:16:13.21,0:16:15.47,Default,,0,0,0,,Mấy tên chết tiệt không có nghị lực.
Dialogue: 0,0:16:15.78,0:16:18.07,Default,,0,0,0,,Được rồi, năm nào cũng vậy à.
Dialogue: 0,0:16:18.68,0:16:20.44,Default,,0,0,0,,Năm nay cũng xem như khá rồi.
Dialogue: 0,0:16:20.99,0:16:22.32,Default,,0,0,0,,Sắp đến trận đấu tuyển trong khu vực rồi.
Dialogue: 0,0:16:22.86,0:16:24.99,Default,,0,0,0,,Bây giờ chỉ dựa vào\Nnhững người chúng ta đi thi đấu thôi.
Dialogue: 0,0:16:26.24,0:16:28.61,Default,,0,0,0,,Trong tỉnh chúng ta\Ncó hơn 200 đội bóng rổ cấp ba,
Dialogue: 0,0:16:29.12,0:16:32.35,Default,,0,0,0,,trong đó gồm đội quán quân năm ngoái,\Ntrường cấp ba trực thuộc Đại học Kainan.
Dialogue: 0,0:16:32.62,0:16:35.21,Default,,0,0,0,,Hơn nữa 10 năm qua, năm nào họ cũng\Ncó mặt trong giải đấu các trường đại học.
Dialogue: 0,0:16:35.38,0:16:37.29,Default,,0,0,0,,[Cũng là một đội mạnh nổi tiếng trong nước.]
Dialogue: 0,0:16:38.01,0:16:42.70,Default,,0,0,0,,[Ngoài ra, cấp ba Shoyo hạng hai năm ngoái\Nthực lực ngang cấp ba trực thuộc Đại học Kainan,]
Dialogue: 0,0:16:43.24,0:16:48.07,Default,,0,0,0,,[và trường cấp ba Ryonan\Ncó thiên tài Sendoh và quái vật Uozumi.]
Dialogue: 0,0:16:56.03,0:16:56.86,Default,,0,0,0,,Sakuragi.
Dialogue: 0,0:16:58.13,0:17:01.37,Default,,0,0,0,,Nếu cậu muốn đánh bại tôi\Nthì phải dốc hết sức mà luyện tập mới được.
Dialogue: 0,0:17:02.34,0:17:03.33,Default,,0,0,0,,Sendoh.
Dialogue: 0,0:17:09.38,0:17:11.01,Default,,0,0,0,,Sendoh chết tiệt,
Dialogue: 0,0:17:11.41,0:17:13.97,Default,,0,0,0,,chắc chắn lần sau tôi sẽ đánh bại anh.
Dialogue: 0,0:17:15.72,0:17:16.79,Default,,0,0,0,,Các cậu nghe rõ cho tôi.
Dialogue: 0,0:17:16.92,0:17:20.45,Default,,0,0,0,,Cho dù đối thủ của các cậu là Kainan,\NShoyo hoặc là Ryonan,
Dialogue: 0,0:17:20.59,0:17:22.06,Default,,0,0,0,,kẻ có thể đánh bại những đội mạnh này...
Dialogue: 0,0:17:22.22,0:17:23.58,Default,,0,0,0,,Là Shohoku!
Dialogue: 0,0:17:24.25,0:17:25.06,Default,,0,0,0,,Đúng vậy!
Dialogue: 0,0:17:25.39,0:17:26.95,Default,,0,0,0,,Một đội có thiên tài chơi bóng Sakuragi,
Dialogue: 0,0:17:27.12,0:17:31.08,Default,,0,0,0,,và đại quái thú,\NShohuku chắc chắn sẽ thắng họ thôi!
Dialogue: 0,0:17:33.30,0:17:34.89,Default,,0,0,0,,Cậu đang nói ai là đại quái thú hả?
Dialogue: 0,0:17:35.43,0:17:36.87,Default,,0,0,0,,Đồ óc heo.
Dialogue: 0,0:17:36.93,0:17:38.06,Default,,0,0,0,,Sakuragi Hanamichi,
Dialogue: 0,0:17:38.94,0:17:41.66,Default,,0,0,0,,nếu cậu đã mua giày chơi bóng mới rồi\Nthì cố lên nhé.
Dialogue: 0,0:17:42.84,0:17:45.03,Default,,0,0,0,,Thấy hợp không Ayako?
Dialogue: 0,0:17:45.15,0:17:46.96,Default,,0,0,0,,Được đó, nhìn đẹp lắm.
Dialogue: 0,0:17:47.47,0:17:49.46,Default,,0,0,0,,Nhưng đi chân trần thì không tốt.
Dialogue: 0,0:17:54.85,0:17:57.34,Default,,0,0,0,,Đám khốn nạn mấy người, mấy người làm gì vậy?
Dialogue: 0,0:17:57.47,0:17:58.64,Default,,0,0,0,,Sao giẫm lên giày bóng rổ của tôi?
Dialogue: 0,0:17:58.75,0:18:01.54,Default,,0,0,0,,Vì giày bóng rổ mới hơi cứng,\Ndễ làm chân bị thương.
Dialogue: 0,0:18:01.57,0:18:05.09,Default,,0,0,0,,Giẫm như vậy thì giày sẽ tốt,\Nđề phòng bị thương thôi mà.
Dialogue: 0,0:18:05.62,0:18:08.22,Default,,0,0,0,,Vậy à? Thì ra là vậy.
Dialogue: 0,0:18:08.24,0:18:09.69,Default,,0,0,0,,Tao vẫn chưa giẫm đây.
Dialogue: 0,0:18:10.41,0:18:11.49,Default,,0,0,0,,Rukawa.
Dialogue: 0,0:18:11.83,0:18:13.36,Default,,0,0,0,,Nghe đây, đừng quên
Dialogue: 0,0:18:13.93,0:18:15.59,Default,,0,0,0,,mục tiêu cuối cùng của chúng ta là...
Dialogue: 0,0:18:15.77,0:18:17.26,Default,,0,0,0,,Xưng bá cả nước!
Dialogue: 0,0:18:17.54,0:18:20.60,Default,,0,0,0,,Đúng vậy Sakuragi,\Nnhưng đừng nói huỵch toẹt ra vậy chứ.
Dialogue: 0,0:18:21.21,0:18:22.70,Default,,0,0,0,,Tuy tập luyện cực khổ lắm
Dialogue: 0,0:18:22.88,0:18:24.87,Default,,0,0,0,,nhưng dù thế nào\Ncũng phải vượt qua, biết chưa?
Dialogue: 0,0:18:25.11,0:18:26.90,Default,,0,0,0,,Rõ!
Dialogue: 0,0:18:28.72,0:18:29.91,Default,,0,0,0,,Huấn luyện viên.
Dialogue: 0,0:18:30.22,0:18:32.12,Default,,0,0,0,,Hôm qua là một trận đấu rất tốt.
Dialogue: 0,0:18:32.92,0:18:37.15,Default,,0,0,0,,Chỉ cần cố gắng hơn, chắc chắn sẽ có ngày\Nkỹ thuật chơi bóng của mọi người sẽ cực tốt.
Dialogue: 0,0:18:39.53,0:18:41.15,Default,,0,0,0,,Các cậu sẽ trở nên cực mạnh.
Dialogue: 0,0:18:43.50,0:18:44.62,Default,,0,0,0,,Vâng!
Dialogue: 0,0:18:44.93,0:18:48.26,Default,,0,0,0,,Được, phải cố gắng lên, chắc chắn chúng ta\Nsẽ giành quán quân của tỉnh.
Dialogue: 0,0:18:48.50,0:18:49.56,Default,,0,0,0,,Được!
Dialogue: 0,0:18:54.91,0:18:55.50,Default,,0,0,0,,Ayako.
Dialogue: 0,0:18:57.64,0:18:58.67,Default,,0,0,0,,Tên nhóc đó
Dialogue: 0,0:18:59.58,0:19:01.07,Default,,0,0,0,,có kịp trận đấu loại không?
Dialogue: 0,0:19:01.41,0:19:02.28,Default,,0,0,0,,Anh đang nói ai?
Dialogue: 0,0:19:02.88,0:19:03.61,Default,,0,0,0,,Đừng vờ vịt nữa,
Dialogue: 0,0:19:05.22,0:19:06.65,Default,,0,0,0,,ngoài cậu ta ra thì còn ai vào đây?
Dialogue: 0,0:19:09.62,0:19:10.49,Default,,0,0,0,,Chính là tên nhóc đó.
Dialogue: 0,0:19:13.93,0:19:14.83,Default,,0,0,0,,Cái gì?
Dialogue: 0,0:19:15.40,0:19:17.73,Default,,0,0,0,,Thằng nhóc mày đúng là\Nkhông biết hài hước mà,
Dialogue: 0,0:19:19.90,0:19:21.70,Default,,0,0,0,,còn ra sức cào tao như vậy.
Dialogue: 0,0:19:22.30,0:19:24.24,Default,,0,0,0,,Có muốn so một trận không?
Dialogue: 0,0:19:34.98,0:19:36.38,Default,,0,0,0,,[Tên nhóc này là đại ca rồi nhỉ?]
Dialogue: 0,0:19:39.55,0:19:40.95,Default,,0,0,0,,Mày đừng có nhầm,
Dialogue: 0,0:19:41.19,0:19:43.99,Default,,0,0,0,,bọn tao sẽ không lấy bốn đánh một đâu,\Nbọn tao không hèn hạ như vậy.
Dialogue: 0,0:19:44.29,0:19:45.99,Default,,0,0,0,,Tao với mày solo thôi, tới đi.
Dialogue: 0,0:19:46.29,0:19:48.99,Default,,0,0,0,,[Mới xuất viện thôi mà.]
Dialogue: 0,0:19:56.55,0:19:59.64,Default,,0,0,0,,Đừng xúc động như thế đã,\Ndù thế nào cũng do bọn tao sai.
Dialogue: 0,0:20:02.26,0:20:03.23,Default,,0,0,0,,Thôi bỏ đi.
Dialogue: 0,0:20:10.44,0:20:11.90,Default,,0,0,0,,Yohei, mày đang làm gì vậy?
Dialogue: 0,0:20:15.66,0:20:17.24,Default,,0,0,0,,Mày tên gì?
Dialogue: 0,0:20:17.88,0:20:19.30,Default,,0,0,0,,Tao tên Mito Yohei.
Dialogue: 0,0:20:19.91,0:20:20.64,Default,,0,0,0,,Còn mày?
Dialogue: 0,0:20:22.20,0:20:24.20,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:20:45.84,0:20:46.70,Default,,0,0,0,,Miyagi.
Dialogue: 0,0:20:48.21,0:20:49.70,Default,,0,0,0,,Miyagi Ryota.
Dialogue: 0,0:20:52.47,0:20:55.80,Default,,0,0,0,,[Bọn Hanamichi lần nữa sục sôi ý chí\Nchiến đấu, đặt mục tiêu là xưng bá cả nước.]
Dialogue: 0,0:20:56.92,0:21:00.21,Default,,0,0,0,,[Lúc này Miyagi Ryota xuất hiện, rốt cuộc là...]
Dialogue: 0,0:21:06.86,0:21:10.48,Default,,0,0,0,,♫ Đôi mắt luôn hướng về phía cậu ♫
Dialogue: 0,0:21:10.75,0:21:14.71,Default,,0,0,0,,♫ Từ lúc vừa mới gặp cho đến hiện tại ♫
Dialogue: 0,0:21:14.99,0:21:18.48,Default,,0,0,0,,♫ Chỉ cần có cậu ở bên ♫
Dialogue: 0,0:21:18.72,0:21:22.16,Default,,0,0,0,,♫ Những thứ khác đều không cần a ♫
Dialogue: 0,0:21:23.13,0:21:26.29,Default,,0,0,0,,♫ Căng thẳng tột độ vì ước mơ ♫
Dialogue: 0,0:21:32.34,0:21:36.67,Default,,0,0,0,,♫ Nhìn thấy vẻ mặt cậu vui như thế ♫
Dialogue: 0,0:21:36.94,0:21:39.97,Default,,0,0,0,,♫ Không có ngụy trang gì cả ♫
Dialogue: 0,0:21:40.45,0:21:46.21,Default,,0,0,0,,♫ Luôn mang theo máy nhắn tin\Nđể cậu có thể tìm mình bất cứ lúc nào ♫
Dialogue: 0,0:21:47.09,0:21:52.11,Default,,0,0,0,,♫ Am hiểu hơn về xe ♫
Dialogue: 0,0:21:52.86,0:21:56.09,Default,,0,0,0,,♫ Cũng đang cố gắng\Nhọc cách tự chăm sóc mình ♫
Dialogue: 0,0:21:56.66,0:22:00.43,Default,,0,0,0,,♫ Tuy là có hơi bối rối ♫
Dialogue: 0,0:22:00.67,0:22:06.26,Default,,0,0,0,,♫ Nhưng vẫn quyết định\Ntheo người này cả đời ♫
Dialogue: 0,0:22:09.01,0:22:12.60,Default,,0,0,0,,♫ Trong mắt chỉ có mỗi mình cậu ♫
Dialogue: 0,0:22:12.81,0:22:16.98,Default,,0,0,0,,♫ Một mình chờ đợi\Ntrong căn phòng của đôi ta ♫
Dialogue: 0,0:22:17.25,0:22:24.42,Default,,0,0,0,,♫ Nụ cười của cậu\Ngiống như là cái khóa màu hoa hồng ♫
Dialogue: 0,0:22:25.32,0:22:33.15,Default,,0,0,0,,♫ Tiến về trước, cô gái không có ước mơ ♫
Dialogue: 0,0:22:36.76,0:22:38.22,Default,,0,0,0,,[CAO THỦ BÓNG RỔ]
Dialogue: 0,0:22:39.93,0:22:41.93,Default,,0,0,0,,Giày bóng rổ đã vào tay,
Dialogue: 0,0:22:42.25,0:22:43.88,Default,,0,0,0,,như vậy sẽ không bị hai người Tinh Tinh Bự
Dialogue: 0,0:22:43.88,0:22:46.23,Default,,0,0,0,,và Rukawa xem thường nữa nhỉ? Ayako.
Dialogue: 0,0:22:46.48,0:22:48.65,Default,,0,0,0,,Ayako, rốt cuộc tên đó là ai vậy?
Dialogue: 0,0:22:48.65,0:22:50.65,Default,,0,0,0,,Rõ ràng cậu nói tôi không thích hợp với cậu.
Dialogue: 0,0:22:50.65,0:22:54.55,Default,,0,0,0,,Dám nhân lúc không có tôi\Nmà đi theo thằng đó, khốn khiếp.
Dialogue: 0,0:22:54.85,0:22:59.08,Default,,0,0,0,,Tên này đang nghi ngờ\Nquan hệ giữa mình và Ayako.
Dialogue: 0,0:22:59.08,0:23:00.38,Default,,0,0,0,,"Cao thủ bóng rổ."
Dialogue: 0,0:23:00.40,0:23:03.28,Default,,0,0,0,,"Đứa trẻ rắc rối!\NHanamichi đấu với Miyagi."
Dialogue: 0,0:23:03.31,0:23:04.98,Default,,0,0,0,,Đừng bỏ qua đấy.

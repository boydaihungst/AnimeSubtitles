Dialogue: 0,0:16:53.31,0:16:56.04,Viet,K,0000,0000,0000,,Mọi người ở đây có quen biết tôi sao?
Dialogue: 0,0:16:57.24,0:16:59.34,Viet,A,0000,0000,0000,,Cậu quên cả điều đó ư?
Dialogue: 0,0:16:59.34,0:17:00.48,Viet,K,0000,0000,0000,,Xin lỗi.
Dialogue: 0,0:17:00.48,0:17:02.82,Viet,K,0000,0000,0000,,Nhưng tôi thật sự không nhớ được gì cả.
Dialogue: 0,0:17:02.82,0:17:04.23,Viet,Ma,0000,0000,0000,,Senpai...
Dialogue: 0,0:17:04.23,0:17:08.02,Viet,Mi,0000,0000,0000,,Người luôn miệng nói về harem mà lại xin lỗi sao?
Dialogue: 0,0:17:08.02,0:17:10.52,Viet,A,0000,0000,0000,,Sugisaki không phải là Sugisaki nữa rồi.
Dialogue: 0,0:17:10.52,0:17:12.18,Viet,C,0000,0000,0000,,Bây giờ có khóc cũng chẳng được gì đâu.
Dialogue: 0,0:17:12.18,0:17:15.29,Viet,C,0000,0000,0000,,Vậy thì làm thôi, mọi người cùng nhau đem kí ức Key-kun trở lại!
Dialogue: 0,0:17:15.29,0:17:19.07,Viet,Mi,0000,0000,0000,,Ờ. Cậu ấy là kiểu người đơn giản, và dễ quên.
Dialogue: 0,0:17:19.07,0:17:21.92,Viet,Mi,0000,0000,0000,,Nếu chúng ta bật công tắc, chắc chắn cậu ấy sẽ tỉnh lại ngay.
Dialogue: 0,0:17:21.92,0:17:23.84,Viet,A,0000,0000,0000,,Phải rồi…
Dialogue: 0,0:17:23.84,0:17:25.00,Viet,Ma,0000,0000,0000,,Senpai!
Dialogue: 0,0:17:25.00,0:17:28.84,Viet,Ma,0000,0000,0000,,Anh có nhớ lần em tỏ tình với anh không?
Dialogue: 0,0:17:30.41,0:17:31.62,Viet,K,0000,0000,0000,,Anou...
Dialogue: 0,0:17:31.62,0:17:34.54,Viet,K,0000,0000,0000,,Vậy lúc đó… tôi có đồng ý không?
Dialogue: 0,0:17:34.54,0:17:39.98,Viet,Ma,0000,0000,0000,,Thật ra thì, anh mới là người đã nói yêu em trước đó.
Dialogue: 0,0:17:39.98,0:17:42.55,Viet,K,0000,0000,0000,,Vậy... chúng ta đang hẹn hò à?
Dialogue: 0,0:17:42.55,0:17:49.36,Viet,Ma,0000,0000,0000,,Không. Em nghĩ việc anh cặp với Nakameguro-senpai mới là quan trọng hơn cả.
Dialogue: 0,0:17:49.36,0:17:51.10,Viet,K,0000,0000,0000,,Mối tình tay ba?
Dialogue: 0,0:17:51.57,0:17:55.14,Viet,K,0000,0000,0000,,Vậy cô gái tên Nakameguro-san mới thực sự là bạn gái tôi à?
Dialogue: 0,0:17:55.14,0:17:57.94,Viet,Ma,0000,0000,0000,,Nakameguro-senpai là con trai đấy!
Dialogue: 0,0:17:58.94,0:17:59.57,Viet,K,0000,0000,0000,,Nói cách khác…
Dialogue: 0,0:17:59.57,0:18:00.59,Viet,Ma,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:18:01.51,0:18:03.57,Viet,Mi,0000,0000,0000,,Đừng "đột biến" quá khứ kiểu đó!
Dialogue: 0,0:18:07.89,0:18:09.22,Viet,Mi,0000,0000,0000,,Ken nữa!
Dialogue: 0,0:18:09.22,0:18:12.62,Viet,Mi,0000,0000,0000,,Đừng có như một con gà mới đẻ và tin bất cứ thứ gì nghe được!
Dialogue: 0,0:18:12.62,0:18:15.22,Viet,K,0000,0000,0000,,Trước khi mất trí nhớ mình như thế nào?
Dialogue: 0,0:18:15.22,0:18:18.83,Viet,A,0000,0000,0000,,Có rất nhiều tập phim nói về cậu đó.
Dialogue: 0,0:18:18.83,0:18:21.13,Viet,K,0000,0000,0000,,Chắc chắn tất cả đều rất tệ!
Dialogue: 0,0:18:21.13,0:18:22.94,Viet,A,0000,0000,0000,,K-Không hẳn vậy đâu.
Dialogue: 0,0:18:22.94,0:18:24.59,Viet,A,0000,0000,0000,,Cậu đã làm được nhiều việc tốt.
Dialogue: 0,0:18:25.43,0:18:27.40,Viet,K,0000,0000,0000,,Đó là những việc gì?
Dialogue: 0,0:18:27.40,0:18:28.98,Viet,A,0000,0000,0000,,P-Phải rồi nhỉ...
Dialogue: 0,0:18:28.98,0:18:30.10,Viet,A,0000,0000,0000,,Để coi…
Dialogue: 0,0:18:30.10,0:18:31.38,Viet,A,0000,0000,0000,,Etou...
Dialogue: 0,0:18:31.38,0:18:34.21,Viet,K,0000,0000,0000,,Quả nhiên tất cả đều rất tệ mà.
Dialogue: 0,0:18:34.57,0:18:38.15,Viet,A,0000,0000,0000,,Không, chỉ là nhiều quá nên không biết chọn cái nào thôi.
Dialogue: 0,0:18:38.15,0:18:39.87,Viet,K,0000,0000,0000,,Không sao đâu.
Dialogue: 0,0:18:39.87,0:18:42.47,Viet,K,0000,0000,0000,,Tôi biết tôi là người rất tệ mà.
Dialogue: 0,0:18:42.47,0:18:44.61,Viet,Mi,0000,0000,0000,,Hình như trí nhớ của cậu ấy đang bắt đầu trở lại!
Dialogue: 0,0:18:44.61,0:18:46.27,Viet,C,0000,0000,0000,,Chỉ cần một chút lực đẩy nữa thôi!
Dialogue: 0,0:18:46.27,0:18:49.64,Viet,A,0000,0000,0000,,Nhưng... phải làm cách nào để mang kí ức cậu ấy trở lại đúng hướng...
Dialogue: 0,0:18:49.64,0:18:53.65,Viet,Ma,0000,0000,0000,,Anou... để anh ấy đọc cái này thì sao ạ?
